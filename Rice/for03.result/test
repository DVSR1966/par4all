Check the proper effects

void for03(int n)
{
//               <must be written>: j
   int j;
//               <may be written >: e1[.x][*]
//               <must be written>: e1[.next]
   al_t e1;
//               <may be written >: e2[.x][*]
//               <must be written>: e2[.next]
   al_t e2;
//               <must be written>: delta_t t t_max
   float t, delta_t, t_max;
//               <must be written>: e1[.next]

   e1.next = &e2;
//               <must be written>: j

   for(j = 0; j <= 99; j += 1) {
//               <must be read   >: j
//               <must be written>: e1[.x][j]
      (e1.x)[j] = 0.;
//               <must be read   >: e1[.next] j
//               <must be written>: e1[.next][0][.x][j]
      (e1.next->x)[j] = 1.;
   }
}

Check the regions

void for03(int n)
{
   int j;

//  <e1[.next]-W-EXACT-{}>
//  <e1[.x][PHI2]-W-MAY-{}>

   al_t e1;

//  <e2[.next]-W-EXACT-{}>
//  <e2[.x][PHI2]-W-MAY-{}>

   al_t e2;
   float t, delta_t, t_max;

//  <e1[.next]-W-EXACT-{}>


   e1.next = &e2;

//  <e1[.next]-R-EXACT-{}>
//  <e1[.x][PHI2]-W-EXACT-{0<=PHI2, PHI2<=99}>
//  <e1[.next][PHI2][.x][PHI4]-W-EXACT-{PHI2==0, 0<=PHI4, PHI4<=99}>


   for(j = 0; j <= 99; j += 1) {

//  <e1[.x][PHI2]-W-EXACT-{PHI2==j, 0<=j, j<=99}>

      (e1.x)[j] = 0.;

//  <e1[.next]-R-EXACT-{0<=j, j<=99}>
//  <e1[.next][PHI2][.x][PHI4]-W-EXACT-{PHI2==0, PHI4==j, 0<=j,
//    j<=99}>

      (e1.next->x)[j] = 1.;
   }
}

