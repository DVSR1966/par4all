      PROGRAM TEST5
      REAL V1(10,10),V2(10,10)

      DOALL I = 1, 10                                                   0001
         DOALL J = 1, 10                                                0002
            V2(I,J) = V2(I,J)+V1(I+1,J)                                 0004
         ENDDO
      ENDDO
      DOALL I = 1, 10                                                   0001
         DOALL J = 1, 10                                                0002
            V1(I,J) = 0.0                                               0003
         ENDDO
      ENDDO
      END
