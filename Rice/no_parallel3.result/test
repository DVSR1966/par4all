// OMP style
int main()
{
   float a[10];                                                         /*0003*/
   int m = 0;                                                           /*0004*/
   int n = 0;                                                           /*0005*/

   while (n<100)                                                        /*0007*/
      for(m = 0; m <= 9; m += 1)
         n = n+1;                                                       /*0009*/


   for(m = 0; m <= 9; m += 1)
      n = n+1;                                                          /*0014*/

   n = n+2;                                                             /*0017*/

   return 0;                                                            /*0019*/
}
