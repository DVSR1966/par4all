// OMP style
int main()
{
   float a[10];
   int m = 0;
   int n = 0;

   while (n<100)                                                        /*0007*/
      for(m = 0; m <= 9; m += 1)                                        /*0008*/
         n = n+1;                                                       /*0009*/

   for(m = 0; m <= 9; m += 1)                                           /*0013*/
      n = n+1;                                                          /*0014*/

   n = n+2;                                                             /*0017*/

   return 0;                                                            /*0019*/
}
