void invariant02(int ni, int nj)
{
   int n = 10;
   float y[n];
   int i, j;
   float s, a, b;


   i = MAX(INT(ni), 0);
   s = a*b;
#pragma omp parallel for 
   for(i = 0; i <= ni-1; i += 1)
      y[i] = s;
}

