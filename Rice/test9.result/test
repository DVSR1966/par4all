      PROGRAM TEST9
      REAL V1(10,10),V2(10,10)

      DOALL I = 1, 10                                                   0001
         DO J = 1, 10                                                   0002
            V1(I,J) = 0.0                                               0003
            V2(I,J) = V1(I,J)+V1(I,J+1)                                 0004
         ENDDO
      ENDDO
      END
