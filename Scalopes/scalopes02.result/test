int main(int *argc, char **argv)
{
   int i, j;
   int a[100];

   for(i = 0; i <= 99; i += 1)
      for(j = 0; j <= 99; j += 1)
         a[j] += 1;

   return 0;
}
int main(int *argc, char **argv)
{
   int i, j;
   int a[100];

   task_0(&j, &i, a);

   return 0;
}


extern int main(int *argc, char **argv);
void task_0(int *j, int *i, int a[100])
{

   for(i = 0; i <= 99; i += 1)
      for(j = 0; j <= 99; j += 1)
         a[*j] += 1;
}
