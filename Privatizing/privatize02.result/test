void j_aime_les_moulinettes_a_fromage()
{
   int i, j = 0;
   for(i = 0; i <= 11; i += 1)
      j++;
}
extern void j_aime_les_moulinettes_a_fromage();
void j_aime_les_moulinettes_a_fromage()
{
   int i, j = 0;
#pragma omp DO private(i)
   for(i = 0; i <= 11; i += 1)
      j++;
      private(i)
}
extern void j_aime_les_moulinettes_a_fromage();
