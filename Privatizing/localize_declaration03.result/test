void getimage(char *filename)
{
   double z;
   double amp;
   double sum;
   double squares;
   int i;
   int j;
   int conj;
   int nx;
   int ny;
   unsigned char c;
   FILE *fp;
   
   /* Erase the memory, in case the image is not big enough: */
   for (i = 0;i<64;i++)                                                 /*0031*/
      for (j = 0;j<64;j++) {                                            /*0032*/
         imagein[i][j].re = 0.0;                                        /*0033*/
         imagein[i][j].im = 0.0;                                        /*0034*/
      }
}
