  "14 -" -> "15 &" [label="t"];
  "13 *" [shape=trapezium,orientation=270];
  "13 *" -> "14 -" [label="t"];
  "i1" -> "13 *";
  "i0" -> "15 &";
  "i0" -> "14 -";
  "i0" -> "13 *";
}
