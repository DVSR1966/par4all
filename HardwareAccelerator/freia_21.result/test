#
# AIPO
#

! initial function

freia_status freia_21(freia_data2d *o, freia_data2d *i0, freia_data2d *i1)
{

   freia_data2d *t = freia_common_create_data(16, 128, 128);            /*0007*/
   
   // internal variable reuses
   // t = i0 * i1
   // t = t - i0
   // o = t & i0
   freia_aipo_mul(t, i0, i1);                                           /*0013*/
   freia_aipo_sub(t, t, i0);                                            /*0014*/
   freia_aipo_and(o, t, i0);                                            /*0015*/

   freia_common_destruct_data(t);                                       /*0017*/

   return FREIA_OK;                                                     /*0019*/
}

