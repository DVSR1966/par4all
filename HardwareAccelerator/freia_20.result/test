   freia_aipo_erode_6c(t1, t0, k);                                      /*0022*/
   freia_aipo_dilate_8c(t2, t1, k);                                     /*0023*/
   freia_aipo_dilate_6c(t3, t2, k);                                     /*0024*/
   freia_aipo_erode_8c(t4, t3, k);                                      /*0025*/
   freia_aipo_erode_6c(t5, t4, k);                                      /*0026*/
   freia_aipo_dilate_8c(t6, t5, k);                                     /*0027*/
