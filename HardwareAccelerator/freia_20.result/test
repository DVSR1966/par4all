
! initial function

freia_status freia_20(freia_data2d *o, freia_data2d *i, int32_t *k)
{
   freia_data2d *t0 = freia_common_create_data(16, 128, 128);
   freia_data2d *t1 = freia_common_create_data(16, 128, 128);
   freia_data2d *t2 = freia_common_create_data(16, 128, 128);
   freia_data2d *t3 = freia_common_create_data(16, 128, 128);
   freia_data2d *t4 = freia_common_create_data(16, 128, 128);
   freia_data2d *t5 = freia_common_create_data(16, 128, 128);
   freia_data2d *t6 = freia_common_create_data(16, 128, 128);
   freia_data2d *t7 = freia_common_create_data(16, 128, 128);
   freia_data2d *t8 = freia_common_create_data(16, 128, 128);
   freia_data2d *t9 = freia_common_create_data(16, 128, 128);
   freia_data2d *ta = freia_common_create_data(16, 128, 128);
   freia_data2d *tb = freia_common_create_data(16, 128, 128);
   
   // long pipeline that must be cut
   freia_aipo_erode_8c(t0, i, k);                                       /*0020*/
   freia_aipo_erode_6c(t1, t0, k);                                      /*0021*/
   freia_aipo_dilate_8c(t2, t1, k);                                     /*0022*/
   freia_aipo_dilate_6c(t3, t2, k);                                     /*0023*/
   freia_aipo_erode_8c(t4, t3, k);                                      /*0024*/
   freia_aipo_erode_6c(t5, t4, k);                                      /*0025*/
   freia_aipo_dilate_8c(t6, t5, k);                                     /*0026*/
   freia_aipo_dilate_6c(t7, t6, k);                                     /*0027*/
   // should cut here
   freia_aipo_erode_8c(t8, t7, k);                                      /*0029*/
   freia_aipo_erode_6c(t9, t8, k);                                      /*0030*/
   freia_aipo_dilate_8c(ta, t9, k);                                     /*0031*/
   freia_aipo_dilate_6c(tb, ta, k);                                     /*0032*/
   freia_aipo_erode_8c(o, tb, k);                                       /*0033*/

   freia_common_destruct_data(t0);                                      /*0035*/
   freia_common_destruct_data(t1);                                      /*0036*/
   freia_common_destruct_data(t2);                                      /*0037*/
   freia_common_destruct_data(t3);                                      /*0038*/
   freia_common_destruct_data(t4);                                      /*0039*/
   freia_common_destruct_data(t5);                                      /*0040*/
   freia_common_destruct_data(t6);                                      /*0041*/
   freia_common_destruct_data(t7);                                      /*0042*/
   freia_common_destruct_data(t8);                                      /*0043*/
   freia_common_destruct_data(t9);                                      /*0044*/
   freia_common_destruct_data(ta);                                      /*0045*/
   freia_common_destruct_data(tb);                                      /*0046*/

   return FREIA_OK;                                                     /*0048*/
}

