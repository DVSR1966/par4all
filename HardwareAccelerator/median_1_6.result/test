  mcu_macro[1][13].addrStart = TERAPIX_UCODE_SUP3;

  // inputs:
  // - imagelet 1 is i0 for i
  tile_in[0][0].x = imagelet_io_1_0;
  tile_in[0][0].y = 0;
  tile_in[1][0].x = imagelet_io_1_1;
  tile_in[1][0].y = 0;

  // outputs:
  // - imagelet 2 is o0 for t
  tile_out[0][0].x = imagelet_io_2_0;
  tile_out[0][0].y = 0;
  tile_out[1][0].x = imagelet_io_2_1;
  tile_out[1][0].y = 0;
  // - imagelet 2 is o1 for t
  tile_out[0][1].x = imagelet_io_2_0;
  tile_out[0][1].y = 0;
  tile_out[1][1].x = imagelet_io_2_1;
  tile_out[1][1].y = 0;
  // - imagelet 1 is o2 for o
  tile_out[0][2].x = imagelet_io_1_0;
  tile_out[0][2].y = 0;
  tile_out[1][2].x = imagelet_io_1_1;
  tile_out[1][2].y = 0;


  // imagelet erosion for the computation
  mcu_instr.borderTop    = 12;
  mcu_instr.borderBottom = 12;
  mcu_instr.borderLeft   = 12;
  mcu_instr.borderRight  = 12;
  mcu_instr.imagelet_height = imagelet_size;
  mcu_instr.imagelet_width  = TERAPIX_PE_NUMBER;

  // outputs
  mcu_instr.nbout = 3;
  mcu_instr.out0 = tile_out[0];
  mcu_instr.out1 = tile_out[1];

  // inputs
  mcu_instr.nbin = 1;
  mcu_instr.in0 = tile_in[0];
  mcu_instr.in1 = tile_in[1];

  // actual instructions
  mcu_instr.nbinstr = 14;
  mcu_instr.instr0   = mcu_macro[0];
  mcu_instr.instr1   = mcu_macro[1];

  // call terapix runtime
  ret = freia_cg_template_process(&param, o0, o1, o2, i0);

  // extract measures

  return ret;
}


! updated function

freia_status median_1(freia_data2d *o, freia_data2d *i)
{
   freia_status ret;                                                    /*0005*/
   freia_data2d *t = freia_common_create_data(i->bpp, i->widthWa, i->heightWa);        /*0006*/
   
   // int32_t c = 8;
   // ret =  freia_cipo_close(t, i, c, 1);
   ret = median_1_helper_0_0(t, t, o, i, freia_morpho_kernel_8c);       /*0010*/
   ret |= 0;                                                            /*0011*/
   
   // ret |= freia_cipo_open(t, t, c, 1);
   ret |= 0;                                                            /*0014*/
   ret |= 0;                                                            /*0015*/
   
   // ret |= freia_cipo_close(t, t, c, 1);
   ret |= 0;                                                            /*0018*/
   ret |= 0;                                                            /*0019*/

   ret |= 0;                                                            /*0021*/
   
   // ret |= freia_cipo_open(t, i, c, 1);
   ret |= 0;                                                            /*0024*/
   ret |= 0;                                                            /*0025*/
   
   // ret |= freia_cipo_close(t, t, c, 1);
   ret |= 0;                                                            /*0028*/
   ret |= 0;                                                            /*0029*/
   
   // ret |= freia_cipo_open(t, t, c, 1);
   ret |= 0;                                                            /*0032*/
   ret |= 0;                                                            /*0033*/

   ret |= 0;                                                            /*0035*/
   
   // cleanup
   ret |= freia_common_destruct_data(t);                                /*0038*/
   return ret;                                                          /*0039*/
}

