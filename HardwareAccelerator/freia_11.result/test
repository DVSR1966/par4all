// graph for dag "freia_11_helper_0_0" of module "freia_11" in dot format
digraph "freia_11_helper_0_0" {
  // inputs
  "i" [shape=box];

  // outputs
  "o" [shape=box];

  // computation vertices
  "7 sub" [shape=circle];
  "i" -> "7 sub";
  "i" -> "7 sub";
  "7 sub" -> "o";
}
#include <freiaCommon.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// FREIA-SPoC helper function for module freia_11
freia_status freia_11_helper_0_0_0(freia_data2d * o0, freia_data2d * i0)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_sel op = 0;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // i [0 input 0] -> [0 alu] 7 sub
  // nope
  // i [0 input 1] -> [0 alu] 7 sub
  // nope
  // alu 7 sub stage 0 flipped
  si.alu[0][0].op = SPOC_ALU_SUB_IN1_IN0;


  // output image o on 0
  // o [0 alu] -> [0 output 0] 0 null
  si.mux[0][0].op = SPOC_MUX_IN1;
  si.mux[0][2].op = SPOC_MUX_IN0;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(op, param, o0, NULL, i0, i0);

  return ret;
}

! new function

freia_status freia_11(freia_data2d *o, freia_data2d *i)
{
   // input used twice
   // o = i - i
   freia_11_helper_0_0_0(o, i);                                         /*0007*/
   return FREIA_OK;                                                     /*0008*/
}
