
! initial function

freia_status freia_11(freia_data2d *o, freia_data2d *i)
{
   // input used twice
   // o = i - i
   freia_aipo_sub(o, i, i);                                             /*0007*/
   return FREIA_OK;                                                     /*0008*/
}

! accelerated function

