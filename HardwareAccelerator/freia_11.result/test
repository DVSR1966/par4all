digraph "dag_0" {
  // inputs
  "i" [shape=box];

  // outputs
  "o" [shape=box];

  // computation vertices
  "7 sub" [shape=circle];
  "i" -> "7 sub";
  "i" -> "7 sub";
  "7 sub" -> "o";
}
