! initial function

freia_status freia_41(freia_data2d *o, freia_data2d *i, int32_t *kern)
{
   
   
   freia_data2d *e = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *d = freia_common_create_data(i->bpp, i->widthWa, i->heightWa);    /*0006*/
   
   // bad scheduled example for terapix tests
   
   freia_aipo_erode_8c(e, i, kern);                                     /*0010*/
   freia_aipo_erode_8c(e, e, kern);                                     /*0011*/
   freia_aipo_erode_8c(e, e, kern);                                     /*0012*/
   freia_aipo_erode_8c(e, e, kern);                                     /*0013*/

   freia_aipo_dilate_8c(d, i, kern);                                    /*0015*/
   freia_aipo_dilate_8c(d, d, kern);                                    /*0016*/
   freia_aipo_dilate_8c(d, d, kern);                                    /*0017*/
   freia_aipo_dilate_8c(d, d, kern);                                    /*0018*/

   freia_aipo_sub(o, d, e);                                             /*0020*/

   freia_common_destruct_data(e);                                       /*0022*/
   freia_common_destruct_data(d);                                       /*0023*/

   return FREIA_OK;                                                     /*0025*/
}

