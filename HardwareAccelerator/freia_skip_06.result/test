   // live allocation in an intermediate statement
   freia_skip_06_helper_0_0_0(i, t0, i, 111);                           /*0010*/
   freia_skip_06_helper_0_1_0(o, t0, i, 111);                           /*0012*/
   freia_common_destruct_data(t0);                                      /*0013*/
}
