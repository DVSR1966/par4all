
! initial function

void freia_skip_06(freia_data2d *o, freia_data2d *i)
{
   freia_data2d *t0, *t1;                                               /*0005*/
   // live allocation in an intermediate statement
   freia_aipo_add(i, i, i);                                             /*0007*/
   t0 = freia_common_create_data(16, 128, 128);                         /*0008*/
   t1 = freia_common_create_data(16, 128, 128);                         /*0009*/
   freia_aipo_and_const(t0, i, 111);                                    /*0010*/
   freia_aipo_xor_const(t1, i, 111);                                    /*0011*/
   freia_aipo_addsat(o, t0, t1);                                        /*0012*/
   freia_common_destruct_data(t0);                                      /*0013*/
   freia_common_destruct_data(t1);                                      /*0014*/
}

