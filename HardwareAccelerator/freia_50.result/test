! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// dag 0: 3 ops (0 copies), optimized to 3 (0+0 copies)
// split 0: 1 cut
// # SPOC calls: 1

// FREIA-SPoC helper function for module freia_50
freia_status freia_50_helper_0_0_0(freia_data2d * o0, freia_data2d * i0, freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // i0 [0 input 0] -> [0 alu] 9 inf
  // nope
  // i1 [0 input 1] -> [0 alu] 9 inf
  // nope
  // alu 9 inf stage 0
  si.alu[0][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 1
  // i0 [0 input 0] -> [1 poc 0] 8 erode_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // i0 [1 input 0] -> [1 poc 0] 8 erode_8c
  // nope
  // poc 8 erode_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_ERODE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];

  // 0`t1 [1 poc 0] -> [1 alu] 10 sub
  // nope
  // 0`t2 [0 alu] -> [1 alu] 10 sub
  si.mux[0][1].op = SPOC_MUX_IN1;
  si.mux[0][3].op = SPOC_MUX_IN0;
  // 0`t2 [1 input 1] -> [1 alu] 10 sub
  // nope
  // alu 10 sub stage 1
  si.alu[1][0].op = SPOC_ALU_SUB_IN0_IN1;


  // output image out on 0
  // out [1 alu] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN1;
  si.mux[1][2].op = SPOC_MUX_IN0;

  // fill in to the end...
  // out [1 output 0] -> [7 output 0] 0 null
  // out [2 input 0] -> [7 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;
  // out [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // out [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // out [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // out [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // out [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i1);

  return ret;
}

! updated function

freia_status freia_50(freia_data2d *out, freia_data2d *i0, freia_data2d *i1)
{

   freia_50_helper_0_0_0(out, i0, i1, freia_morpho_kernel_8c);          /*0008*/

   return FREIA_OK;                                                     /*0014*/
}

