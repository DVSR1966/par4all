freia_status median_1(freia_data2d *o, freia_data2d *i)
{
   freia_status ret = 0;                                                /*0005*/
   freia_data2d *t = freia_common_create_data(i->bpp, i->widthWa, i->heightWa);        /*0006*/
   // hardware accelerator generated variable
   freia_data2d *o_1 = freia_common_create_data(o->bpp, o->widthWa, o->heightWa);
   
   // int32_t c = 8;
   // ret =  freia_cipo_close(t, i, c, 1);
   median_1_helper_0_0_0(t, i, freia_morpho_kernel_8c);                 /*0010*/
   
   // ret |= freia_cipo_open(t, t, c, 1);
   median_1_helper_0_0_1(t, t, freia_morpho_kernel_8c);                 /*0014*/
   
   // ret |= freia_cipo_close(t, t, c, 1);
   median_1_helper_0_0_2(t, o_1, t, i, freia_morpho_kernel_8c);         /*0018*/
   
   
   // ret |= freia_cipo_open(t, i, c, 1);
   median_1_helper_0_0_3(t, t, freia_morpho_kernel_8c);                 /*0024*/
   
   // ret |= freia_cipo_close(t, t, c, 1);
   median_1_helper_0_0_4(t, t, freia_morpho_kernel_8c);                 /*0028*/
   
   // ret |= freia_cipo_open(t, t, c, 1);
   median_1_helper_0_0_5(o, t, o_1, freia_morpho_kernel_8c);            /*0032*/

   freia_common_destruct_data(o_1);
   
   // cleanup
   ret |= freia_common_destruct_data(t);                                /*0038*/
   return ret;                                                          /*0039*/
}

