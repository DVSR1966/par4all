   freia_aipo_dilate_8c(od, od, freia_morpho_kernel_8c);                /*0049*/
   freia_aipo_dilate_8c(od, od, freia_morpho_kernel_8c);                /*0050*/
   i_0 = 10;                                                            /*0051*/

