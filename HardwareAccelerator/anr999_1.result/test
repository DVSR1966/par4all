   freia_data2d *og;                                                    /*0017*/
   freia_data2d *od;                                                    /*0018*/
   int32_t measure_min;                                                 /*0019*/
   int32_t measure_vol;                                                 /*0020*/
   int i_0;                                                             /*0021*/
   int I_10_0;                                                          /*0022*/
   int I_3_0;                                                           /*0023*/
   freia_status ret_0;                                                  /*0024*/
   freia_data2d *t;                                                     /*0025*/
