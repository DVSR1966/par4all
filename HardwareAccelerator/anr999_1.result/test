  mcu_instr[16].macrocode.xmin2 = imagelet_1;
  mcu_instr[16].macrocode.ymin2 = 0;
  mcu_instr[16].macrocode.xmin3 = x_2;
  mcu_instr[16].macrocode.ymin3 = y_2;
  mcu_instr[16].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[16].macrocode.iter2 = imagelet_size;
  mcu_instr[16].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  // 17: E8(3) -> 2
  mcu_instr[17].macrocode.xmin1 = imagelet_3;
  mcu_instr[17].macrocode.ymin1 = 0;
  mcu_instr[17].macrocode.xmin2 = imagelet_2;
  mcu_instr[17].macrocode.ymin2 = 0;
  mcu_instr[17].macrocode.xmin3 = x_2;
  mcu_instr[17].macrocode.ymin3 = y_2;
  mcu_instr[17].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[17].macrocode.iter2 = imagelet_size;
  mcu_instr[17].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  // 18: D8(1) -> 3
  mcu_instr[18].macrocode.xmin1 = imagelet_1;
  mcu_instr[18].macrocode.ymin1 = 0;
  mcu_instr[18].macrocode.xmin2 = imagelet_3;
  mcu_instr[18].macrocode.ymin2 = 0;
  mcu_instr[18].macrocode.xmin3 = x_2;
  mcu_instr[18].macrocode.ymin3 = y_2;
  mcu_instr[18].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[18].macrocode.iter2 = imagelet_size;
  mcu_instr[18].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  // 19: E8(2) -> 1
  mcu_instr[19].macrocode.xmin1 = imagelet_2;
  mcu_instr[19].macrocode.ymin1 = 0;
  mcu_instr[19].macrocode.xmin2 = imagelet_1;
  mcu_instr[19].macrocode.ymin2 = 0;
  mcu_instr[19].macrocode.xmin3 = x_2;
  mcu_instr[19].macrocode.ymin3 = y_2;
  mcu_instr[19].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[19].macrocode.iter2 = imagelet_size;
  mcu_instr[19].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  // 20: D8(3) -> 2
  mcu_instr[20].macrocode.xmin1 = imagelet_3;
  mcu_instr[20].macrocode.ymin1 = 0;
  mcu_instr[20].macrocode.xmin2 = imagelet_2;
  mcu_instr[20].macrocode.ymin2 = 0;
  mcu_instr[20].macrocode.xmin3 = x_2;
  mcu_instr[20].macrocode.ymin3 = y_2;
  mcu_instr[20].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[20].macrocode.iter2 = imagelet_size;
  mcu_instr[20].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  // 21: E8(1) -> 3
  mcu_instr[21].macrocode.xmin1 = imagelet_1;
  mcu_instr[21].macrocode.ymin1 = 0;
  mcu_instr[21].macrocode.xmin2 = imagelet_3;
  mcu_instr[21].macrocode.ymin2 = 0;
  mcu_instr[21].macrocode.xmin3 = x_2;
  mcu_instr[21].macrocode.ymin3 = y_2;
  mcu_instr[21].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[21].macrocode.iter2 = imagelet_size;
  mcu_instr[21].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  // 22: -(2,3) -> 1
  mcu_instr[22].macrocode.xmin1 = imagelet_2;
  mcu_instr[22].macrocode.ymin1 = 0;
  mcu_instr[22].macrocode.xmin2 = imagelet_3;
  mcu_instr[22].macrocode.ymin2 = 0;
  mcu_instr[22].macrocode.xmin3 = imagelet_1;
  mcu_instr[22].macrocode.ymin3 = 0;
  mcu_instr[22].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[22].macrocode.iter2 = imagelet_size;
  mcu_instr[22].macrocode.addrStart = TERAPIX_UCODE_SUB3;

  // inputs:
  // - imagelet 1 is i0 for in

  // outputs:
  // - imagelet 2 is o0 for od
  // - imagelet 1 is o1 for og

  // double buffer management:
  // - buffer 1/4
  // - buffer 2/5

  // output imagelet erosion:
  //   north: 10
  //   south: 10
  //    west: 10
  //    east: 10

  ret = freia_cg_template_process_1i_2o(&param, o0, o1, i0);

  // get measure min result from 1023
  // get measure vol result from 1021

  return ret;
}


