   freia_aipo_add_const(o0, i, c);                                      /*0008*/
   freia_aipo_erode_6c(o1, i, k);                                       /*0009*/
   freia_aipo_global_min(i, r0);                                        /*0010*/
   freia_aipo_global_max(i, r1);                                        /*0011*/
   freia_aipo_global_vol(i, r2);                                        /*0012*/
   return FREIA_OK;                                                     /*0013*/
}

