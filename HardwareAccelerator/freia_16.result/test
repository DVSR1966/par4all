{
   // test re ordering of mes
   freia_aipo_erode_6c(o1, i, k);                                       /*0009*/
   freia_16_helper_0_0_1(o0, i, c, r0, r1, r2);                         /*0010*/
   return FREIA_OK;                                                     /*0013*/
}

