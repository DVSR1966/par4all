! initial function

void freia_3muls(freia_data2d *o1, freia_data2d *o2, freia_data2d *in0, freia_data2d *in1)
{
   freia_data2d *t0 = freia_common_create_data(16, 128, 128);           /*0004*/
   freia_aipo_mul(t0, in0, in1);                                        /*0005*/
   freia_aipo_mul(o1, in0, in1);                                        /*0006*/
   freia_aipo_mul(o2, t0, o1);                                          /*0007*/
   freia_common_destruct_data(t0);                                      /*0008*/
}

! graphs

// graph for dag "dag_0" of module "freia_3muls" in dot format
digraph "dag_0" {
  // inputs
  "in0" [shape=circle];
  "in1" [shape=circle];

  // outputs
  "o1" [shape=circle];
  "o2" [shape=circle];

  // computation vertices
  "7 *" [shape=trapezium,orientation=270];
  "7 *" -> "o2";
  "6 *" [shape=trapezium,orientation=270];
  "6 *" -> "7 *" [label="o1"];
  "6 *" -> "o1";
  "5 *" [shape=trapezium,orientation=270];
  "5 *" -> "7 *" [label="t0"];
  "in1" -> "6 *";
  "in1" -> "5 *";
  "in0" -> "6 *";
  "in0" -> "5 *";
}
