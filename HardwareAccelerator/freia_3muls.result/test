  // inputs
  "in0" [shape=circle];
  "in1" [shape=circle];

  // outputs
  "o1" [shape=circle];
  "o2" [shape=circle];

  // computation vertices
  "7 *" [shape=trapezium,orientation=270];
  "7 *" -> "o2";
  "5 *" [shape=trapezium,orientation=270];
  "5 *" -> "7 *" [label="o1"];
  "5 *" -> "7 *" [label="o1"];
  "5 *" -> "o1";
  "in1" -> "5 *";
  "in0" -> "5 *";
}

! updated function for

void freia_3muls(freia_data2d *o1, freia_data2d *o2, freia_data2d *in0, freia_data2d *in1)
{
   freia_aipo_mul(o1, in0, in1);                                        /*0005*/
   freia_aipo_mul(o2, o1, o1);                                          /*0007*/
}
