! acceleration function

#include <freiaCommon.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

! updated function

freia_status freia_19(freia_data2d *o, freia_data2d *i)
{
   
   
   // one useless copy
   // t = i
   // o = t
   freia_aipo_copy(o, i);
   
   
   return FREIA_OK;                                                     /*0017*/
}
