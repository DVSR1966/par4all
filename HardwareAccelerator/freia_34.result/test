  "15 -" -> "o";
  "14 D8" [shape=box];
  "14 D8" -> "15 -" [label="t2"];
  "13 E8" [shape=box];
  "13 E8" -> "15 -" [label="t1"];
  "i" -> "14 D8";
  "i" -> "13 E8";
}
