
! initial function

freia_status freia_33(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   freia_data2d *X0 = freia_common_create_data(16, 128, 128);
   freia_data2d *X1 = freia_common_create_data(16, 128, 128);
   freia_data2d *tB = freia_common_create_data(16, 128, 128);
   freia_data2d *tB2 = freia_common_create_data(16, 128, 128);
   freia_data2d *tC = freia_common_create_data(16, 128, 128);
   freia_data2d *tC2 = freia_common_create_data(16, 128, 128);
   freia_data2d *tD = freia_common_create_data(16, 128, 128);
   int sA;
   int sB;
   int sC;
   
   // complex dag for split
   // A
   freia_aipo_global_max(i, &sA);                                       /*0017*/
   // B
   freia_aipo_threshold(tB, i, sA-10, sA-1, false);                     /*0019*/
   // A
   freia_aipo_erode_8c(X0, i, k);                                       /*0021*/
   // A
   freia_aipo_global_min(X0, &sB);                                      /*0023*/
   // C
   freia_aipo_threshold(tC, X0, sB-10, sB-1, false);                    /*0025*/
   freia_aipo_add(tC2, tC, tC);                                         /*0026*/
   // B
   freia_aipo_dilate_8c(tB2, tB, k);                                    /*0028*/
   freia_aipo_mul(X1, tB2, tB);                                         /*0029*/
   freia_aipo_global_vol(X1, &sC);                                      /*0030*/
   // D
   freia_aipo_threshold(tD, X1, sC-10, sC-1, false);                    /*0032*/
   freia_aipo_add(o, tC2, tD);                                          /*0033*/

   freia_common_destruct_data(X0);                                      /*0035*/
   freia_common_destruct_data(X1);                                      /*0036*/
   freia_common_destruct_data(tB);                                      /*0037*/
   freia_common_destruct_data(tB2);                                     /*0038*/
   freia_common_destruct_data(tC);                                      /*0039*/
   freia_common_destruct_data(tC2);                                     /*0040*/
   freia_common_destruct_data(tD);                                      /*0041*/
   return FREIA_OK;                                                     /*0042*/
}

