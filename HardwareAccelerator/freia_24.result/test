freia_status freia_24(freia_data2d *o, const freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tC = freia_common_create_data(16, 128, 128);         /*0011*/
   int sA, sB, sC;                                                      /*0012*/
   
   // complex dag for split
   freia_24_helper_0_0_0(X0, i, &sA, k, &sB);                           /*0015*/
   freia_24_helper_0_1_0(tC, X1, X0, i, sA-10, sA-1, false, k, &sC, sB-10, sB-1, false);              /*0016*/
   freia_24_helper_0_2_0(o, X1, tC, sC-10, sC-1, false);                /*0022*/

   freia_common_destruct_data(X0);                                      /*0025*/
   freia_common_destruct_data(X1);                                      /*0026*/
   freia_common_destruct_data(tC);                                      /*0028*/
   return FREIA_OK;                                                     /*0030*/
}

! cleaned up function

freia_status freia_24(freia_data2d *o, const freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tC = freia_common_create_data(16, 128, 128);         /*0011*/
   int sA, sB, sC;                                                      /*0012*/
   
   // complex dag for split
   freia_24_helper_0_0_0(X0, i, &sA, k, &sB);                           /*0015*/
   freia_24_helper_0_1_0(tC, X1, X0, i, sA-10, sA-1, 0, k, &sC, sB-10, sB-1, 0);       /*0016*/
   freia_24_helper_0_2_0(o, X1, tC, sC-10, sC-1, 0);                    /*0022*/

   freia_common_destruct_data(X0);                                      /*0025*/
   freia_common_destruct_data(X1);                                      /*0026*/
   freia_common_destruct_data(tC);                                      /*0028*/
   return 0;                                                            /*0030*/
}
#
# TERAPIX
#

! initial function

freia_status freia_24(freia_data2d *o, const freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tB = freia_common_create_data(16, 128, 128), *tC = freia_common_create_data(16, 128, 128), *tD = freia_common_create_data(16, 128, 128);       /*0011*/
   int sA, sB, sC;                                                      /*0012*/
   
   // complex dag for split
   freia_aipo_global_max(i, &sA);                                       /*0015*/
   freia_aipo_threshold(tB, i, sA-10, sA-1, false);                     /*0016*/
   freia_aipo_erode_8c(X0, i, k);                                       /*0017*/
   freia_aipo_global_min(X0, &sB);                                      /*0018*/
   freia_aipo_threshold(tC, X0, sB-10, sB-1, false);                    /*0019*/
   freia_aipo_dilate_8c(X1, tB, k);                                     /*0020*/
   freia_aipo_global_vol(X1, &sC);                                      /*0021*/
   freia_aipo_threshold(tD, X1, sC-10, sC-1, false);                    /*0022*/
   freia_aipo_add(o, tC, tD);                                           /*0023*/

   freia_common_destruct_data(X0);                                      /*0025*/
   freia_common_destruct_data(X1);                                      /*0026*/
   freia_common_destruct_data(tB);                                      /*0027*/
   freia_common_destruct_data(tC);                                      /*0028*/
   freia_common_destruct_data(tD);                                      /*0029*/
   return FREIA_OK;                                                     /*0030*/
}

