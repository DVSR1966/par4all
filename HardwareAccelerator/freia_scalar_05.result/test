    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t23 = PIXEL_THRESHOLD(in0, c0, c1, c2);\n"
    "    PIXEL t24 = PIXEL_ADD(in1, t23);\n"
    "    // set output pixels\n"
    "    p0[i] = t24;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
