#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_scalar_05

// opencl for dag 0

// opencl function freia_scalar_05_helper_0_1_0
KERNEL void freia_scalar_05_helper_0_1_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width,
  int pitch,
  int c0,
  int c1,
  int c2,
  int c3,
  int c4,
  int c5)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * p1 = o1 + ofs_o1;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;
  GLOBAL PIXEL * j1 = i1 + ofs_i1;
  int gid = get_global_id(1)*pitch + get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t16 = PIXEL_THRESHOLD(in1, c0, c1, c2);
    PIXEL t19 = PIXEL_THRESHOLD(in0, c3, c4, c5);
    PIXEL t20 = PIXEL_ADD(t19, t19);
    // set output pixels
    p0[i] = t16;
    p1[i] = t20;
  }
}

// opencl function freia_scalar_05_helper_0_2_0
KERNEL void freia_scalar_05_helper_0_2_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width,
  int pitch,
  int c0,
  int c1,
  int c2)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;
  GLOBAL PIXEL * j1 = i1 + ofs_i1;
  int gid = get_global_id(1)*pitch + get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t23 = PIXEL_THRESHOLD(in0, c0, c1, c2);
    PIXEL t24 = PIXEL_ADD(in1, t23);
    // set output pixels
    p0[i] = t24;
  }
}

! updated function

freia_status freia_scalar_05(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   freia_data2d *X = freia_common_create_data(16, 128, 128), *tB = freia_common_create_data(16, 128, 128), *tC2 = freia_common_create_data(16, 128, 128);         /*0011*/
   int sA, sB, sC;                                                      /*0012*/
   // hardware accelerator generated variable
   freia_data2d *X_1 = freia_common_create_data(X->bpp, X->widthWa, X->heightWa);
   
   // look for order issues when reusing a temporary image
   freia_aipo_global_max(i, &sA);                                       /*0015*/
   freia_scalar_05_helper_0_1_0(tB, tC2, X_1, i, sA-10, sA-1, false, sB-10, sB-1, false);             /*0016*/
   freia_aipo_erode_8c(X_1, i, k);                                      /*0017*/
   freia_aipo_global_min(X_1, &sB);                                     /*0018*/
   freia_aipo_dilate_8c(X, tB, k);                                      /*0021*/
   freia_aipo_global_vol(X, &sC);                                       /*0022*/
   freia_scalar_05_helper_0_2_0(o, X, tC2, sC-10, sC-1, false);         /*0023*/

   freia_common_destruct_data(X);                                       /*0026*/
   freia_common_destruct_data(X_1);
   freia_common_destruct_data(tB);                                      /*0027*/
   freia_common_destruct_data(tC2);                                     /*0029*/
   return FREIA_OK;                                                     /*0031*/
}

! cleaned up function

freia_status freia_scalar_05(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   freia_data2d *X = freia_common_create_data(16, 128, 128), *tB = freia_common_create_data(16, 128, 128), *tC2 = freia_common_create_data(16, 128, 128);         /*0011*/
   int sA, sB, sC;                                                      /*0012*/
   // hardware accelerator generated variable
   freia_data2d *X_1 = freia_common_create_data(X->bpp, X->widthWa, X->heightWa);
   
   // look for order issues when reusing a temporary image
   freia_aipo_global_max(i, &sA);                                       /*0015*/
   freia_scalar_05_helper_0_1_0(tB, tC2, X_1, i, sA-10, sA-1, 0, sB-10, sB-1, 0);      /*0016*/
   freia_aipo_erode_8c(X_1, i, k);                                      /*0017*/
   freia_aipo_global_min(X_1, &sB);                                     /*0018*/
   freia_aipo_dilate_8c(X, tB, k);                                      /*0021*/
   freia_aipo_global_vol(X, &sC);                                       /*0022*/
   freia_scalar_05_helper_0_2_0(o, X, tC2, sC-10, sC-1, 0);             /*0023*/

   freia_common_destruct_data(X);                                       /*0026*/
   freia_common_destruct_data(X_1);
   freia_common_destruct_data(tB);                                      /*0027*/
   freia_common_destruct_data(tC2);                                     /*0029*/
   return 0;                                                            /*0031*/
}
