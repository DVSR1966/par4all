// compile kernels for freia_scalar_05_helper_0_2_0
static freia_status freia_scalar_05_helper_0_2_0_compile(void)
{
  // OpenCL source for freia_scalar_05_helper_0_2_0
  const char * freia_scalar_05_helper_0_2_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
