
! initial function

freia_status freia_dup_07(freia_data2d *o, freia_data2d *i0, freia_data2d *i1)
{
   freia_data2d *t = freia_common_create_data(16, 128, 128);            /*0006*/
   // same operation performed twice, with commutation
   // t = inf(i0, i1)
   freia_aipo_inf(t, i0, i1);                                           /*0009*/
   // o = inf(i1, i0)
   // could be replaced by o = t
   freia_aipo_inf(o, i1, i0);                                           /*0012*/
   freia_common_destruct_data(t);                                       /*0013*/
   return FREIA_OK;                                                     /*0014*/
}

