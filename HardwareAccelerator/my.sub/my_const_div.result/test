! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "freia_opencl_runtime.h"


// dag 0: 1 ops and 0 copies, optimized to 1 ops and 0+0 copies

// hold kernels formy_const_div_helper_0_0_0
static cl_kernel my_const_div_helper_0_0_0_kernel[2];

// compile kernels for my_const_div_helper_0_0_0
static freia_status my_const_div_helper_0_0_0_compile(void)
{
  // OpenCL source for my_const_div_helper_0_0_0
  const char * my_const_div_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function my_const_div_helper_0_0_0\n"
    "KERNEL void my_const_div_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0)\n"
    "{\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0;\n"
    "  int gid = get_global_id(1)*pitch + get_global_id(0);\n"
    "  int i;\n"
    "  for (i=gid; i < (gid+width); i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t5 = PIXEL_DIVC(in0, c0);\n"
    "    // set output pixels\n"
    "    p0[i] = t5;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(my_const_div_helper_0_0_0_source, "my_const_div_helper_0_0_0", "-DPIXEL8", &my_const_div_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(my_const_div_helper_0_0_0_source, "my_const_div_helper_0_0_0", "-DPIXEL16", &my_const_div_helper_0_0_0_kernel[1]);
  return err;
}

// helper function my_const_div_helper_0_0_0
freia_status my_const_div_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  int32_t c0)
{
  freia_status err = FREIA_OK;
  freia_data2d_opencl *pool= frclTarget.pool;
  int pitch = o0->row[1] - o0->row[0];
  int width = o0->widthWa;
  size_t workSize[2];
  workSize[0] = o0->heightWa;
  uint32_t bpp = o0->bpp>>4;
  cl_kernel kernel;

  // handle on the fly compilation...
  static int to_compile = 1;

  if (to_compile) {
    freia_status cerr = my_const_div_helper_0_0_0_compile();
    // compilation may have failed
    if (cerr) return cerr;
    to_compile = 0;
  }

  // now get kernel, which must be there...
  kernel = my_const_div_helper_0_0_0_kernel[bpp];
  cl_int ofs_o0 = freia_common_data_get_offset(o0);
  cl_int ofs_i0 = freia_common_data_get_offset(i0);

  // set kernel parameters
  err |= clSetKernelArg(kernel, 0, sizeof(cl_mem), &pool[o0->clId]);
  err |= clSetKernelArg(kernel, 1, sizeof(cl_int), &ofs_o0);
  err |= clSetKernelArg(kernel, 2, sizeof(cl_mem), &pool[i0->clId]);
  err |= clSetKernelArg(kernel, 3, sizeof(cl_int), &ofs_i0);
  err |= clSetKernelArg(kernel, 4, sizeof(cl_int), &width);
  err |= clSetKernelArg(kernel, 5, sizeof(cl_int), &pitch);
  err |= clSetKernelArg(kernel, 6, sizeof(cl_int), &c0);

  // call kernel my_const_div_helper_0_0_0
  err |= clEnqueueNDRangeKernel(
            frclTarget.queue,
            kernel,
            1, // number of dimensions
            NULL, // undefined for OpenCL 1.0
            workSize, // x and y dimensions
            NULL, // local size
            0, // don't wait events
            NULL,
            NULL); // do not produce an event

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function my_const_div

// opencl for dag 0

// opencl function my_const_div_helper_0_0_0
KERNEL void my_const_div_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width,
  int pitch,
  int c0)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;
  int gid = get_global_id(1)*pitch + get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t5 = PIXEL_DIVC(in0, c0);
    // set output pixels
    p0[i] = t5;
  }
}

