
! initial function

freia_status freia_28(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   freia_data2d *X0 = freia_common_create_data(16, 128, 128);
   freia_data2d *X1 = freia_common_create_data(16, 128, 128);
   freia_data2d *tB = freia_common_create_data(16, 128, 128);
   freia_data2d *tC = freia_common_create_data(16, 128, 128);
   freia_data2d *tC2 = freia_common_create_data(16, 128, 128);
   freia_data2d *tD = freia_common_create_data(16, 128, 128);
   int sA;
   int sB;
   int sC;
   
   // complex dag for split
   freia_aipo_global_max(i, &sA);                                       /*0015*/
   freia_aipo_threshold(tB, i, sA-10, sA-1, false);                     /*0016*/
   freia_aipo_erode_8c(X0, i, k);                                       /*0017*/
   freia_aipo_global_min(X0, &sB);                                      /*0018*/
   freia_aipo_threshold(tC, X0, sB-10, sB-1, false);                    /*0019*/
   freia_aipo_add(tC2, tC, tC);                                         /*0020*/
   freia_aipo_dilate_8c(X1, tB, k);                                     /*0021*/
   freia_aipo_global_vol(X1, &sC);                                      /*0022*/
   freia_aipo_threshold(tD, X1, sC-10, sC-1, false);                    /*0023*/
   freia_aipo_add(o, tC2, tD);                                          /*0024*/

   freia_common_destruct_data(X0);                                      /*0026*/
   freia_common_destruct_data(X1);                                      /*0027*/
   freia_common_destruct_data(tB);                                      /*0028*/
   freia_common_destruct_data(tC);                                      /*0029*/
   freia_common_destruct_data(tD);                                      /*0030*/
   return FREIA_OK;                                                     /*0031*/
}

