! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 10 ops and 0 copies, optimized to 10 ops and 0+0 copies

// hold kernels for copy_00_helper_0_0_1
static cl_kernel copy_00_helper_0_0_1_kernel[2];

// compile kernels for copy_00_helper_0_0_1
static freia_status copy_00_helper_0_0_1_compile(void)
{
  // OpenCL source for copy_00_helper_0_0_1
  const char * copy_00_helper_0_0_1_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function copy_00_helper_0_0_1\n"
    "KERNEL void copy_00_helper_0_0_1(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  GLOBAL PIXEL * i2, // const?\n"
    "  int ofs_i2,\n"
    "  GLOBAL PIXEL * i3, // const?\n"
    "  int ofs_i3,\n"
    "  int width,\n"
    "  int pitch)\n"
    "{\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1;\n"
    "  GLOBAL PIXEL * j2 = i2 + ofs_i2;\n"
    "  GLOBAL PIXEL * j3 = i3 + ofs_i3;\n"
    "  int gid = pitch*get_global_id(0);\n"
    "  int i;\n"
    "  for (i=gid; i < (gid+width); i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in2 = j2[i];\n"
    "    PIXEL in3 = j3[i];\n"
    "    // pixel computations\n"
    "    PIXEL t22 = PIXEL_SUB(in1, in0);\n"
    "    PIXEL t27 = PIXEL_SUB(in3, in2);\n"
    "    // set output pixels\n"
    "    p0[i] = t22;\n"
    "    p1[i] = t27;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(copy_00_helper_0_0_1_source, "copy_00_helper_0_0_1", "-DPIXEL8", &copy_00_helper_0_0_1_kernel[0]);
  err |= freia_op_compile_kernel(copy_00_helper_0_0_1_source, "copy_00_helper_0_0_1", "-DPIXEL16", &copy_00_helper_0_0_1_kernel[1]);
  return err;
}

// helper function copy_00_helper_0_0_1
freia_status copy_00_helper_0_0_1(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const freia_data2d * i2,
  const freia_data2d * i3)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= copy_00_helper_0_0_1_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = copy_00_helper_0_0_1_kernel[bpp];

  // call kernel copy_00_helper_0_0_1
  err |= freia_op_call_kernel(kernel, 2, 4, 0, 0, o0, o1, i0, i1, i2, i3)

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function copy_00

// opencl for dag 0

// opencl function copy_00_helper_0_0_1
KERNEL void copy_00_helper_0_0_1(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  GLOBAL PIXEL * i2, // const?
  int ofs_i2,
  GLOBAL PIXEL * i3, // const?
  int ofs_i3,
  int width,
  int pitch)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * p1 = o1 + ofs_o1;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;
  GLOBAL PIXEL * j1 = i1 + ofs_i1;
  GLOBAL PIXEL * j2 = i2 + ofs_i2;
  GLOBAL PIXEL * j3 = i3 + ofs_i3;
  int gid = pitch*get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    PIXEL in2 = j2[i];
    PIXEL in3 = j3[i];
    // pixel computations
    PIXEL t22 = PIXEL_SUB(in1, in0);
    PIXEL t27 = PIXEL_SUB(in3, in2);
    // set output pixels
    p0[i] = t22;
    p1[i] = t27;
  }
}

! updated function

int copy_00(freia_data2d *o0, freia_data2d *o1, freia_data2d *in)
{
   freia_data2d *a, *b, *c, *d;                                         /*0005*/
   // hardware accelerator generated variable
   freia_data2d *c_1 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa), *d_1 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa), *a_1 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);
   a = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0006*/
   b = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0007*/
   c = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0008*/
   d = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0009*/
   
   // 1
   freia_aipo_dilate_8c(a_1, in, freia_morpho_kernel_8c);               /*0012*/
   freia_aipo_erode_8c(c, in, freia_morpho_kernel_8c);                  /*0013*/
   
   // 2
   freia_aipo_dilate_8c(d_1, c, freia_morpho_kernel_8c);                /*0016*/
   freia_aipo_erode_8c(c_1, c, freia_morpho_kernel_8c);                 /*0017*/
   freia_aipo_dilate_8c(b, a_1, freia_morpho_kernel_8c);                /*0018*/
   freia_aipo_erode_8c(a, a_1, freia_morpho_kernel_8c);                 /*0019*/
   
   // 3
   freia_aipo_dilate_8c(d, d_1, freia_morpho_kernel_8c);                /*0023*/
   freia_aipo_erode_8c(c, c_1, freia_morpho_kernel_8c);                 /*0024*/
   
   // 4
   copy_00_helper_0_0_1(o0, o1, a, b, c, d);                            /*0027*/
   freia_common_destruct_data(a_1);
   freia_common_destruct_data(d_1);
   freia_common_destruct_data(c_1);

   return 0;                                                            /*0029*/
}

