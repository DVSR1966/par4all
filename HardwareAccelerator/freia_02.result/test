  "16 ^" -> "o";
  "15 +" [shape=trapezium,orientation=270];
  "15 +" -> "16 ^" [label="t2"];
  "14 /" [shape=trapezium,orientation=270];
  "14 /" -> "15 +" [label="t1"];
  "i0" -> "16 ^";
  "i0" -> "15 +";
  "i0" -> "14 /";
  "i1" -> "14 /";
}
