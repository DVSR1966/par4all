// compile kernels for freia_07_opencl_helper_0_0_0
static freia_status freia_07_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_07_opencl_helper_0_0_0
  const char * freia_07_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_07_opencl_helper_0_0_0\n"
    "KERNEL void freia_07_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  GLOBAL PIXEL * i2, // const?\n"
    "  int ofs_i2,\n"
    "  GLOBAL PIXEL * i3, // const?\n"
    "  int ofs_i3,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int height = get_global_size(0);\n"
    "  int thrid = get_global_id(0) + height*get_global_id(1);\n"
    "  int shift = pitch*get_global_id(0);\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;\n"
    "  GLOBAL PIXEL * j3 = i3 + ofs_i3 + shift;\n"
    "\n"
    "  int workshare = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int last = (get_global_id(1)+1)*workshare;\n"
    "  if (last>width) last = width;\n"
    "  int i;\n"
    "  for (i=get_global_id(1)*workshare; i<last; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in2 = j2[i];\n"
    "    PIXEL in3 = j3[i];\n"
    "    // pixel computations\n"
    "    PIXEL t18 = PIXEL_ADD(in0, in1);\n"
    "    PIXEL t19 = PIXEL_DIV(in2, in3);\n"
    "    PIXEL t20 = PIXEL_INF(t18, in0);\n"
    "    PIXEL t21 = PIXEL_SUP(t19, in3);\n"
    "    PIXEL t22 = PIXEL_SUB(t20, in0);\n"
    "    PIXEL t23 = PIXEL_OR(t21, in3);\n"
    "    // set output pixels\n"
    "    p0[i] = t22;\n"
    "    p1[i] = t23;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_07_opencl_helper_0_0_0_source, "freia_07_opencl_helper_0_0_0", "-DPIXEL8", &freia_07_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_07_opencl_helper_0_0_0_source, "freia_07_opencl_helper_0_0_0", "-DPIXEL16", &freia_07_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_07_opencl_helper_0_0_0
freia_status freia_07_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const freia_data2d * i2,
  const freia_data2d * i3)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_07_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_07_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_07_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 4, 0, 0, o0, o1, i0, i1, i2, i3);

  return err;
}
