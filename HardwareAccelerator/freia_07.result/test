  "i2" -> "19 /";
  "18 +" [shape=trapezium,orientation=270];
  "18 +" -> "20 <" [label="t0"];
  "i1" -> "18 +";
  "i0" -> "22 -";
  "i0" -> "20 <";
  "i0" -> "18 +";
}

