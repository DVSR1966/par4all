   // o0 = inf(i0 + i1, i0) - i0
   // o1 = sup(i2 / i3, i3) | i3
   freia_aipo_add(t0, i0, i1);                                          /*0018*/
   freia_aipo_div(t2, i2, i3);                                          /*0019*/
   freia_aipo_inf(t1, t0, i0);                                          /*0020*/
