{
   
   
   
   
   
   // o0 = inf(i0 + i1, i0) - i0
   // o1 = sup(i2 / i3, i3) | i3
   freia_07_spoc_helper_0_0_0(o0, i0, i1);                              /*0018*/
   freia_07_spoc_helper_0_1_0(o1, i2, i3);                              /*0019*/
   
   
   return 0;                                                            /*0030*/
}
