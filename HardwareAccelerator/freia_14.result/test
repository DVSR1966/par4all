
! initial function

freia_status freia_14(freia_data2d *o, int32_t c)
{
   freia_data2d *t = freia_common_create_data(16, 128, 128);
   // 2 alu to alu paths
   // t = h()
   // o = t + t
   freia_aipo_set_constant(t, c);                                       /*0009*/
   freia_aipo_add(o, t, t);                                             /*0010*/

   freia_common_destruct_data(t);                                       /*0012*/
   return FREIA_OK;                                                     /*0013*/
}

! accelerated function

