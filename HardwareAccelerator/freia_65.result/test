! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// dag 0: 0 ops and 1 copies, optimized to 0 ops and 0+1 copies
// # SPOC calls: 0

// dag 1: 3 ops and 0 copies, optimized to 3 ops and 0+0 copies
// split 0: 1 cut
// # SPOC calls: 1

// FREIA-SPoC helper function for module freia_65
freia_status freia_65_helper_1_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t pi0,
  int32_t pi1)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`t [0 input 0] -> [0 alu] 11 sub
  // nope
  // o [0 input 1] -> [0 alu] 11 sub
  // nope
  // alu 11 sub stage 0 flipped
  si.alu[0][0].op = SPOC_ALU_SUB_IN1_IN0;


  // STAGE 1
  // 0`t [0 input 0] -> [1 alu] 12 add_const
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`t [1 input 0] -> [1 alu] 12 add_const
  // nope
  // alu 12 add_const stage 1
  si.alu[1][0].op = SPOC_ALU_ADD_IN0_CONST;
  sp.alu[1][0].constant = pi0;


  // STAGE 2
  // o_1 [0 alu] -> [2 alu] 13 xor_const
  si.mux[0][1].op = SPOC_MUX_IN1;
  si.mux[0][3].op = SPOC_MUX_IN0;
  // o_1 [1 input 1] -> [2 alu] 13 xor_const
  si.mux[1][3].op = SPOC_MUX_IN1;
  // o_1 [2 input 1] -> [2 alu] 13 xor_const
  // nope
  // alu 13 xor_const stage 2 flipped
  si.alu[2][0].op = SPOC_ALU_XOR_IN1_CONST;
  sp.alu[2][0].constant = pi1;


  // output image 0`t on 0 and image o on 1
  // 0`t [1 alu] -> [2 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN1;
  si.mux[1][2].op = SPOC_MUX_IN0;
  // 0`t [2 input 0] -> [2 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`t [2 output 0] -> [7 output 0] 0 null
  // 0`t [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`t [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`t [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`t [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // 0`t [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  // o [2 alu] -> [2 output 1] 0 null
  si.mux[2][1].op = SPOC_MUX_IN1;
  si.mux[2][3].op = SPOC_MUX_IN0;

  // fill in to the end...
  // o [2 output 1] -> [7 output 1] 0 null
  // o [3 input 1] -> [7 output 1] 0 null
  si.mux[3][3].op = SPOC_MUX_IN1;
  // o [4 input 1] -> [7 output 1] 0 null
  si.mux[4][3].op = SPOC_MUX_IN1;
  // o [5 input 1] -> [7 output 1] 0 null
  si.mux[5][3].op = SPOC_MUX_IN1;
  // o [6 input 1] -> [7 output 1] 0 null
  si.mux[6][3].op = SPOC_MUX_IN1;
  // o [7 input 1] -> [7 output 1] 0 null
  si.mux[7][3].op = SPOC_MUX_IN1;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  return ret;
}

! updated function

freia_status freia_65(freia_data2d *o, const freia_data2d *i, int n)
{
   freia_data2d *t = freia_common_create_data(16, 128, 128);            /*0006*/
   freia_aipo_copy(t, i);
   {
      int i;                                                            /*0008*/
      for(i = 0; i <= n-1; i += 1)                                      /*0008*/
         // loop carried dep on t
         freia_65_helper_1_0_0(t, o, t, o, 1, 17);                      /*0011*/
   }
   freia_common_destruct_data(t);                                       /*0015*/
   return FREIA_OK;                                                     /*0016*/
}

