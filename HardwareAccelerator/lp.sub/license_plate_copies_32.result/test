  "96 D8" [shape=box];
  "96 D8" -> "98 D8" [label="imopen_14"];
  "imopen_15" -> "96 D8";
}
// graph for dag "license_plate_copies_terapix_helper_0_0_9" of module "license_plate_copies" in dot format
