  // 0`imcloseth [25 input 1] -> [30 alu] 179 and
  si.mux[25][3].op = SPOC_MUX_IN1;
  // 0`imcloseth [26 input 1] -> [30 alu] 179 and
  si.mux[26][3].op = SPOC_MUX_IN1;
  // 0`imcloseth [27 input 1] -> [30 alu] 179 and
  si.mux[27][3].op = SPOC_MUX_IN1;
  // 0`imcloseth [28 input 1] -> [30 alu] 179 and
  si.mux[28][3].op = SPOC_MUX_IN1;
  // 0`imcloseth [29 input 1] -> [30 alu] 179 and
  si.mux[29][3].op = SPOC_MUX_IN1;
  // 0`imcloseth [30 input 1] -> [30 alu] 179 and
  // nope
  // alu 179 and stage 30
  si.alu[30][0].op = SPOC_ALU_AND_IN0_IN1;


  // STAGE 31
  // 0`imand [30 alu] -> [31 poc 0] 185 erode_8c
  si.mux[30][0].op = SPOC_MUX_IN1;
  si.mux[30][2].op = SPOC_MUX_IN0;
  // 0`imand [31 input 0] -> [31 poc 0] 185 erode_8c
  // nope
