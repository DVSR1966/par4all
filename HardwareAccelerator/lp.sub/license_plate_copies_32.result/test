   ret_1 |= freia_aipo_dilate_8c(imclose, imtmp_1, kernel1x3);          /*0146*/
   ret_1 |= freia_aipo_copy(imtmp_1, imclose);                          /*0147*/
   ret_1 |= freia_aipo_dilate_8c(imclose, imtmp_1, kernel1x3);          /*0148*/
