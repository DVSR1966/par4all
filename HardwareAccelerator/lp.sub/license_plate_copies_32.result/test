  si.mux[29][0].op = SPOC_MUX_IN0;
  // threshold 176 threshold stage 29 side 0
  si.th[29][0].op = pi3? SPOC_TH_BINARIZE : SPOC_TH_NO_BINARIZE;
  sp.th[29][0].boundmin = pi1;
  sp.th[29][0].boundmax = pi2;


  // STAGE 15
  // 0`imclose [15 poc 1] -> [15 threshold 1] 177 threshold
