  si.mux[2][0].op = SPOC_MUX_IN0;
  // 0`imfilt_6 [3 input 0] -> [3 poc 0] 189 erode_8c
  // nope
  // poc 189 erode_8c stage 3 side 0
  si.poc[3][0].op = SPOC_POC_ERODE;
  si.poc[3][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[3][0].kernel[i] = pi0[i];


  // STAGE 4
  // 0`imfilt_5 [3 poc 0] -> [4 poc 0] 191 erode_8c
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`imfilt_5 [4 input 0] -> [4 poc 0] 191 erode_8c
  // nope
  // poc 191 erode_8c stage 4 side 0
  si.poc[4][0].op = SPOC_POC_ERODE;
  si.poc[4][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[4][0].kernel[i] = pi0[i];


  // STAGE 5
