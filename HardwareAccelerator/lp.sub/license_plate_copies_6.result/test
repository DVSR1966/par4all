   freia_data2d *imtmp_0;                                               /*0028*/
   freia_status ret_0;                                                  /*0029*/
   int i_0;                                                             /*0030*/
   freia_data2d *imtmp_1;                                               /*0031*/
   freia_status ret_1;                                                  /*0032*/
   int i_1;                                                             /*0033*/
   freia_data2d *imtmp_2;                                               /*0034*/
