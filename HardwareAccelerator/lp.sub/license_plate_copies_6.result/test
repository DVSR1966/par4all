  mcu_macro[0][11].ymin3 = y_0;
  mcu_macro[1][11].ymin3 = y_0;
  mcu_macro[0][11].iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[1][11].iter1 = TERAPIX_PE_NUMBER;
