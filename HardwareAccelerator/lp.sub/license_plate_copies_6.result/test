   freia_aipo_erode_8c(imopen, imopen, kernel1x3);                      /*0082*/
   freia_aipo_erode_8c(imopen, imopen, kernel1x3);                      /*0084*/
   freia_aipo_erode_8c(imopen, imopen, kernel1x3);                      /*0086*/
   freia_aipo_erode_8c(imopen, imopen, kernel1x3);                      /*0088*/
