int license_plate_copies(void)
{
   freia_dataio fdin;                                                   /*0010*/
   freia_dataio fdout;                                                  /*0011*/

   freia_data2d *in;                                                    /*0013*/
   freia_data2d *imopen;                                                /*0015*/
   freia_data2d *imclose;                                               /*0016*/
   freia_data2d *imopenth;                                              /*0017*/
   freia_data2d *imcloseth;                                             /*0018*/
