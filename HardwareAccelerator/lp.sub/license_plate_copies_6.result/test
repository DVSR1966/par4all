   i_0 = 15;                                                            /*0126*/

   freia_common_destruct_data(imtmp_0);                                 /*0128*/

   _return1 = ret_0;                                                    /*0130*/

   imtmp_1 = freia_common_create_data(imclose->bpp, imclose->widthWa, imclose->heightWa);             /*0132*/

