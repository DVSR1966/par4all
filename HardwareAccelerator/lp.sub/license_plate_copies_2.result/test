  si.poc[1][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][1].kernel[i] = pi0[i];

  // 0`imopen [1 poc 1] -> [1 threshold 1] 176 threshold
  si.mux[1][3].op = SPOC_MUX_IN1;
  // threshold 176 threshold stage 1 side 1
  si.th[1][1].op = pi3? SPOC_TH_BINARIZE : SPOC_TH_NO_BINARIZE;
  sp.th[1][1].boundmin = pi1;
  sp.th[1][1].boundmax = pi2;


  // output image 0`imopenth on 1
  // 0`imopenth [1 threshold 1] -> [1 output 1] 0 null
  // nope

  // fill in to the end...
  // 0`imopenth [1 output 1] -> [1 output 1] 0 null
  // nope

