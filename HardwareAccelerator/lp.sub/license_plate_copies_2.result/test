   ret_1 |= freia_aipo_copy(imtmp_1, imclose);                          /*0151*/
   i_1 = 8;                                                             /*0152*/

   ret_1 |= freia_aipo_erode_8c(imclose, imtmp_1, kernel1x3);           /*0154*/
   ret_1 |= freia_aipo_copy(imtmp_1, imclose);                          /*0155*/
   ret_1 |= freia_aipo_erode_8c(imclose, imtmp_1, kernel1x3);           /*0156*/
