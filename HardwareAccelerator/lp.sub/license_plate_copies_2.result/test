  // copy of operation 65 parameters
  for(i=0; i<9; i++)
    p_65[i] = pi10[i];
  gram.xoffset = x_65;
  gram.yoffset = y_65;
  gram.width = 3;
  gram.height = 3;
  gram.params = p_65;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_macro[0][65].macrocode.xmin3 = x_65;
  mcu_macro[1][65].macrocode.xmin3 = x_65;
  mcu_macro[0][65].macrocode.ymin3 = y_65;
  mcu_macro[1][65].macrocode.ymin3 = y_65;
  mcu_macro[0][65].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[1][65].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[0][65].macrocode.iter2 = imagelet_size;
  mcu_macro[1][65].macrocode.iter2 = imagelet_size;
  mcu_macro[0][65].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  mcu_macro[1][65].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  // 66: D8(3) -> 4
  mcu_macro[0][66].macrocode.xmin1 = imagelet_3;
  mcu_macro[1][66].macrocode.xmin1 = imagelet_3;
  mcu_macro[0][66].macrocode.ymin1 = 0;
  mcu_macro[1][66].macrocode.ymin1 = 0;
  mcu_macro[0][66].macrocode.xmin2 = imagelet_4;
  mcu_macro[1][66].macrocode.xmin2 = imagelet_4;
  mcu_macro[0][66].macrocode.ymin2 = 0;
  mcu_macro[1][66].macrocode.ymin2 = 0;
  mcu_macro[0][66].macrocode.xmin3 = x_65;
  mcu_macro[1][66].macrocode.xmin3 = x_65;
  mcu_macro[0][66].macrocode.ymin3 = y_65;
  mcu_macro[1][66].macrocode.ymin3 = y_65;
  mcu_macro[0][66].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[1][66].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[0][66].macrocode.iter2 = imagelet_size;
  mcu_macro[1][66].macrocode.iter2 = imagelet_size;
  mcu_macro[0][66].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  mcu_macro[1][66].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  // 67: D8(4) -> 3
  mcu_macro[0][67].macrocode.xmin1 = imagelet_4;
  mcu_macro[1][67].macrocode.xmin1 = imagelet_4;
  mcu_macro[0][67].macrocode.ymin1 = 0;
  mcu_macro[1][67].macrocode.ymin1 = 0;
  mcu_macro[0][67].macrocode.xmin2 = imagelet_3;
  mcu_macro[1][67].macrocode.xmin2 = imagelet_3;
  mcu_macro[0][67].macrocode.ymin2 = 0;
  mcu_macro[1][67].macrocode.ymin2 = 0;
  mcu_macro[0][67].macrocode.xmin3 = x_65;
  mcu_macro[1][67].macrocode.xmin3 = x_65;
  mcu_macro[0][67].macrocode.ymin3 = y_65;
  mcu_macro[1][67].macrocode.ymin3 = y_65;
  mcu_macro[0][67].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[1][67].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[0][67].macrocode.iter2 = imagelet_size;
  mcu_macro[1][67].macrocode.iter2 = imagelet_size;
  mcu_macro[0][67].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  mcu_macro[1][67].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  // 68: &(3,1) -> 1
  mcu_macro[0][68].macrocode.xmin1 = imagelet_3;
  mcu_macro[1][68].macrocode.xmin1 = imagelet_3;
  mcu_macro[0][68].macrocode.ymin1 = 0;
  mcu_macro[1][68].macrocode.ymin1 = 0;
  mcu_macro[0][68].macrocode.xmin2 = imagelet_io_1_0;
  mcu_macro[1][68].macrocode.xmin2 = imagelet_io_1_1;
  mcu_macro[0][68].macrocode.ymin2 = 0;
  mcu_macro[1][68].macrocode.ymin2 = 0;
  mcu_macro[0][68].macrocode.xmin3 = imagelet_io_1_0;
  mcu_macro[1][68].macrocode.xmin3 = imagelet_io_1_1;
  mcu_macro[0][68].macrocode.ymin3 = 0;
  mcu_macro[1][68].macrocode.ymin3 = 0;
  mcu_macro[0][68].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[1][68].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_macro[0][68].macrocode.iter2 = imagelet_size;
  mcu_macro[1][68].macrocode.iter2 = imagelet_size;
  mcu_macro[0][68].macrocode.addrStart = TERAPIX_UCODE_AND3;
  mcu_macro[1][68].macrocode.addrStart = TERAPIX_UCODE_AND3;

  // inputs:
  // - imagelet 1 is i0 for in
  tile_in[0][0].x = imagelet_io_1_0;
  tile_in[0][0].y = 0;
  tile_in[1][0].x = imagelet_io_1_1;
  tile_in[1][0].y = 0;

  // outputs:
  // - imagelet 1 is o0 for out
  tile_out[0][0].x = imagelet_io_1_0;
  tile_out[0][0].y = 0;
  tile_out[1][0].x = imagelet_io_1_1;
  tile_out[1][0].y = 0;


  // imagelet erosion for the computation
  mcu_instr.borderTop    = 11;
  mcu_instr.borderBottom = 11;
  mcu_instr.borderLeft   = 41;
  mcu_instr.borderRight  = 41;

  // outputs
  mcu_instr.nbout = 1;
  mcu_instr.out = tile_out;

  // inputs
  mcu_instr.nbin = 1;
  mcu_instr.in = tile_in;

  // actual instructions
  mcu_instr.nbinstr = 69;
  mcu_instr.instr   = mcu_macro;

  // call terapix runtime
  ret = freia_cg_template_process(&param, o0, i0);

  // extract measures

  return ret;
}


