// FREIA-SPoC helper function for module license_plate_copies
freia_status license_plate_copies_helper_0_0_7(freia_data2d * o0, freia_data2d * o1, freia_data2d * i0, freia_data2d * i1,
  const int32_t * pi0,
  int32_t pi1,
  int32_t pi2,
  int32_t pi3)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`imopen [0 input 1] -> [0 poc 1] 92 erode_8c
  // nope
  // poc 92 erode_8c stage 0 side 1
  si.poc[0][1].op = SPOC_POC_ERODE;
  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi0[i];


  // STAGE 1
  // 0`imopen [0 poc 1] -> [1 poc 1] 96 dilate_8c
  si.mux[0][3].op = SPOC_MUX_IN1;
  // 0`imopen [1 input 1] -> [1 poc 1] 96 dilate_8c
  // nope
  // poc 96 dilate_8c stage 1 side 1
  si.poc[1][1].op = SPOC_POC_DILATE;
  si.poc[1][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][1].kernel[i] = pi0[i];


  // STAGE 0
  // 0`imclose [0 input 0] -> [0 poc 0] 166 erode_8c
  // nope
  // poc 166 erode_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_ERODE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`imclose [0 poc 0] -> [1 poc 0] 168 erode_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`imclose [1 input 0] -> [1 poc 0] 168 erode_8c
  // nope
  // poc 168 erode_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_ERODE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];

  // 0`imclose [1 poc 0] -> [1 threshold 0] 177 threshold
  si.mux[1][0].op = SPOC_MUX_IN0;
  // threshold 177 threshold stage 1 side 0
  si.th[1][0].op = pi3? SPOC_TH_BINARIZE : SPOC_TH_NO_BINARIZE;
  sp.th[1][0].boundmin = pi1;
  sp.th[1][0].boundmax = pi2;


  // output image 0`imcloseth on 0 and image 0`imopen on 1
  // 0`imcloseth [1 threshold 0] -> [1 output 0] 0 null
  // nope

  // fill in to the end...
  // 0`imcloseth [1 output 0] -> [1 output 0] 0 null
  // nope

  // 0`imopen [1 poc 1] -> [1 output 1] 0 null
  si.mux[1][3].op = SPOC_MUX_IN1;

  // fill in to the end...
  // 0`imopen [1 output 1] -> [1 output 1] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  return ret;
}

