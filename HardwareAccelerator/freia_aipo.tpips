# generate accelerated functions for all AIPO functions.
delete freia_aipo
#setenv PIPS_HWAC_DEBUG_LEVEL=5
create freia_aipo freia_aipo.c
activate C_PARSER
setproperty PRETTYPRINT_C_CODE TRUE
echo
echo ! initial functions
echo
display PRINTED_FILE[%ALL]
echo
echo ! accelerated functions
echo
apply HARDWARE_ACCELERATOR[%ALL]
shell cat ./freia_aipo.database/Src/*_helper_functions.c
echo
echo ! new functions
echo
display PRINTED_FILE[%ALL]
close
delete freia_aipo
quit
