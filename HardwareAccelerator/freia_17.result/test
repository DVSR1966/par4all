! acceleration function

#include <freiaCommon.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

! updated function

freia_status freia_17(freia_data2d *o, freia_data2d *i)
{
   
   
   
   
   
   
   freia_data2d *t3 = freia_common_create_data(16, 128, 128), *t4 = freia_common_create_data(16, 128, 128), *t5 = freia_common_create_data(16, 128, 128);         /*0011*/
   
   // useless copies
   // t0 = i
   // t1 = t0
   // t2 = t1
   // o = t2
   // t3 = t1
   // t4 = t0
   // t5 = o
   freia_aipo_copy(o, i);
   freia_aipo_copy(t3, i);
   freia_aipo_copy(t4, i);
   freia_aipo_copy(t5, i);

   freia_common_destruct_data(t3);                                      /*0032*/
   freia_common_destruct_data(t4);                                      /*0033*/
   freia_common_destruct_data(t5);                                      /*0034*/

   return FREIA_OK;                                                     /*0036*/
}
