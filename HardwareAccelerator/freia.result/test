freia_status freia_common_close_input(freia_dataio *n)
{
   n->stuff = 0;                                                        /*0059*/
   global_io_effect++;                                                  /*0060*/
   return FREIA_OK;                                                     /*0061*/
}
freia_status freia_common_close_output(freia_dataio *n)
{
   n->stuff = 0;                                                        /*0066*/
   global_io_effect++;                                                  /*0067*/
   return FREIA_OK;                                                     /*0068*/
}
freia_data2d * freia_common_create_data(uint32_t x, uint32_t y, uint32_t z)
{
   freia_data2d *img = (freia_data2d *) malloc(sizeof(freia_data2d));   /*0011*/
   img->stuff = (int32_t) (x&y&z);                                      /*0012*/
   return img;                                                          /*0013*/
}
freia_status freia_common_destruct_data(freia_data2d *img)
{
   free(img);                                                           /*0018*/
   return FREIA_OK;                                                     /*0019*/
}
freia_status freia_common_open_input(freia_dataio *in, uint32_t n)
{
   in->framebpp = 16;                                                   /*0024*/
   in->frameheight = 256;                                               /*0025*/
   in->framewidth = 512;                                                /*0026*/
   in->stuff = n;                                                       /*0027*/
   global_io_effect++;                                                  /*0028*/
   return FREIA_OK;                                                     /*0029*/
}
freia_status freia_common_open_output(freia_dataio *out, uint32_t n, uint32_t w, uint32_t h, uint32_t b)
{
   out->framebpp = b;                                                   /*0035*/
   out->frameheight = h;                                                /*0036*/
   out->framewidth = w;                                                 /*0037*/
   out->stuff = n;                                                      /*0038*/
   global_io_effect++;                                                  /*0039*/
   return FREIA_OK;                                                     /*0040*/
}
freia_status freia_common_rx_image(freia_data2d *img, freia_dataio *in)
{
   img->stuff = in->stuff;                                              /*0045*/
   global_io_effect++;                                                  /*0046*/
   return FREIA_OK;                                                     /*0047*/
}
freia_status freia_common_tx_image(const freia_data2d *img, freia_dataio *out)
{
   out->stuff = img->stuff;                                             /*0052*/
   global_io_effect++;                                                  /*0053*/
   return FREIA_OK;                                                     /*0054*/
}
