//               <must be written>: r[0]
   *r = i->stuff;                                                       /*0206*/
   return FREIA_OK;                                                     /*0206*/
}
//               <must be read   >: i0 i0[0][.stuff] i1 i1[0][.stuff] 
//                                  o
//               <must be written>: o[0][.stuff]
freia_status freia_aipo_inf(freia_data2d *o, const freia_data2d *i0, const freia_data2d *i1)
{
