! initial function

freia_status cst_00(freia_data2d *o, uint32_t k)
{
   // this means o := 0
   freia_aipo_xor(o, o, o);                                             /*0006*/
   freia_aipo_erode_8c(o, o, k);                                        /*0007*/
   freia_aipo_copy(o, o);                                               /*0008*/
   freia_aipo_dilate_6c(o, o, k);                                       /*0009*/
   freia_aipo_add_const(o, o, 0);                                       /*0010*/
   freia_aipo_sub_const(o, o, 0);                                       /*0011*/
   freia_aipo_mul_const(o, o, 1);                                       /*0012*/
   freia_aipo_div_const(o, o, 1);                                       /*0013*/
   return FREIA_OK;                                                     /*0014*/
}

