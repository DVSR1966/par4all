
! initial function

freia_status my_subsat(freia_data2d *o, freia_data2d *i0, freia_data2d *i1)
{
   return freia_aipo_subsat(o, i0, i1);                                 /*0005*/
}

! accelerated function

// graph for dag "dag_0" of module "my_subsat" in dot format
digraph "dag_0" {
  // inputs
  "i0" [shape=box];
  "i1" [shape=box];

  // outputs
  "o" [shape=box];

  // computation vertices
  "5 subsat" [shape=circle];
  "i0" -> "5 subsat";
  "i1" -> "5 subsat";
  "5 subsat" -> "o";
}
// graph for dag "my_subsat_helper_0" of module "my_subsat" in dot format
digraph "my_subsat_helper_0" {
  // inputs
  "i0" [shape=box];
  "i1" [shape=box];

  // outputs
  "o" [shape=box];

  // computation vertices
  "5 subsat" [shape=circle];
  "i0" -> "5 subsat";
  "i1" -> "5 subsat";
  "5 subsat" -> "o";
}
#include <freiaCommon.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// FREIA-SPoC helper function for module my_subsat
freia_status my_subsat_helper_0(freia_data2d * o0, freia_data2d * i0, freia_data2d * i1)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_sel op = 0;
  freia_op_param param;
  freia_status ret;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // i0 [0 input 0] -> [0 alu] o
  // nope
  // i1 [0 input 1] -> [0 alu] o
  // nope
  si.alu[0][0].op = SPOC_ALU_SUBSAT_IN0_IN1;


  // output image o on 0
  // o [0 alu] -> [0 output 0] o
  si.mux[0][0].op = SPOC_MUX_IN1;
  si.mux[0][2].op = SPOC_MUX_IN0;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(op, param, o0, NULL, i0, i1);

  return ret;
}

! new function

freia_status my_subsat(freia_data2d *o, freia_data2d *i0, freia_data2d *i1)
{
   return my_subsat_helper_0(o, i0, i1);                                /*0005*/
}
