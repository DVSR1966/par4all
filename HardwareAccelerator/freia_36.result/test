! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// FREIA-SPoC helper function for module freia_36
freia_status freia_36_helper_0_0_0(freia_data2d * o0, freia_data2d * i0, freia_data2d * i1,
  int32_t * po0)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // i0 [0 input 0] -> [0 alu] 9 add
  // nope
  // i1 [0 input 1] -> [0 alu] 9 add
  // nope
  // alu 9 add stage 0
  si.alu[0][0].op = SPOC_ALU_ADD_IN0_IN1;

  // 0`t [0 alu] -> [0 measure 0] 9 add
  si.mux[0][0].op = SPOC_MUX_IN1;
  si.mux[0][2].op = SPOC_MUX_IN0;


  // output image 0`t on 0
  // 0`t [0 measure 0] -> [0 output 0] 0 null
  // nope

  // fill in to the end...
  // 0`t [0 output 0] -> [7 output 0] 0 null
  // 0`t [1 input 0] -> [7 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`t [2 input 0] -> [7 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;
  // 0`t [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`t [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`t [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`t [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // 0`t [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i1);

  // get reductions
  freia_cg_read_reduction_results(&redres);

  // measure 10 global_min stage 0 side 0
  *po0 = (int32_t) reduc.measure[0][0].minimum;

  return ret;
}

! updated function

freia_status freia_36(freia_data2d *i0, freia_data2d *i1)
{
   freia_data2d *t = freia_common_create_data(16, 128, 128);            /*0006*/
   int32_t min;                                                         /*0007*/
   // should t be extracted?
   freia_36_helper_0_0_0(t, i0, i1, &min);                              /*0009*/
   freia_common_destruct_data(t);                                       /*0011*/
   return FREIA_OK;                                                     /*0012*/
}
