   freia_common_destruct_data(g2);                                      /*0035*/
   freia_08_helper_0_0_0(d1, g1, g2);                                   /*0029*/
   freia_08_helper_0_1_0(d0, g0, g1);                                   /*0030*/
   freia_08_helper_0_2_0(o, d0, d1);                                    /*0031*/
   freia_common_destruct_data(d0);                                      /*0036*/
