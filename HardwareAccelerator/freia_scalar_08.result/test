#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// dag 0: 5 ops (0 copies), optimized to 4 (0 copies)
// split 0: 1 cut
// # SPOC calls: 1

// FREIA-SPoC helper function for module freia_scalar_08
freia_status freia_scalar_08_helper_0_0_0(freia_data2d * o0, freia_data2d * i0,
  const int32_t * pi0,
  const int32_t * pi1,
  const int32_t * pi2)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`in [0 input 0] -> [0 poc 0] 35 dilate_8c
  // nope
  // poc 35 dilate_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_DILATE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 2
  // 0`imWork1_1 [0 poc 0] -> [2 poc 0] 36 erode_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`imWork1_1 [1 input 0] -> [2 poc 0] 36 erode_8c
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`imWork1_1 [2 input 0] -> [2 poc 0] 36 erode_8c
  // nope
  // poc 36 erode_8c stage 2 side 0
  si.poc[2][0].op = SPOC_POC_ERODE;
  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi1[i];

  // 0`imWork1_1 [1 poc 0] -> [2 poc 1] 39 erode_8c
  si.mux[1][1].op = SPOC_MUX_IN0;
  si.mux[1][3].op = SPOC_MUX_IN0;
  // 0`imWork1_1 [2 input 1] -> [2 poc 1] 39 erode_8c
  // nope
  // poc 39 erode_8c stage 2 side 1
  si.poc[2][1].op = SPOC_POC_ERODE;
  si.poc[2][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][1].kernel[i] = pi2[i];

  // 0`out [2 poc 1] -> [2 alu] 40 inf
  // nope
  // 0`imWork1 [2 poc 0] -> [2 alu] 40 inf
  // nope
  // alu 40 inf stage 2
  si.alu[2][0].op = SPOC_ALU_INF_IN0_IN1;


  // output image 0`imClose on 0
  // 0`imClose [2 alu] -> [2 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN1;
  si.mux[2][2].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`imClose [2 output 0] -> [7 output 0] 0 null
  // 0`imClose [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`imClose [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`imClose [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`imClose [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // 0`imClose [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, NULL);

  return ret;
}

