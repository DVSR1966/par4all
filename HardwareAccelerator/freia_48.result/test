  "15 =" -> "out";
  "14 /." [shape=trapezium,orientation=270];
  "14 /." -> "15 =" [label="t1"];
  "13 +" [shape=trapezium,orientation=270];
  "13 +" -> "14 /." [label="out"];
  "12 *." [shape=trapezium,orientation=270];
  "12 *." -> "13 +" [label="out"];
  "out" -> "12 *.";
  "11 *." [shape=trapezium,orientation=270];
  "11 *." -> "13 +" [label="t0"];
  "10 =" [shape=circle];
  "10 =" -> "11 *." [label="t0"];
  "in" -> "10 =";
}
