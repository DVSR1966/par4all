! initial function

void freia_skip_08(freia_data2d *o, const freia_data2d *i)
{
   int32_t max, seuil;                                                  /*0005*/
   freia_data2d *t = freia_common_create_data(16, 128, 128);            /*0006*/
   // use some control in an intermediate statement
   freia_aipo_xor_const(t, i, 111);                                     /*0008*/
   freia_aipo_add_const(t, t, 12);                                      /*0009*/
   freia_aipo_global_max(t, &max);                                      /*0010*/
   if (max>128)                                                         /*0011*/
      seuil = max-10;                                                   /*0012*/
   else
      seuil = max+15;                                                   /*0014*/
   freia_aipo_threshold(t, t, 8, seuil-4, true);                        /*0015*/
   freia_aipo_sub_const(o, t, 12);                                      /*0016*/
   freia_common_destruct_data(t);                                       /*0017*/
}

