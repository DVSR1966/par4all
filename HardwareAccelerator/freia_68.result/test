   freia_data2d *tmp = freia_common_create_data(16, 128, 128);          /*0008*/
   freia_68_helper_0_0_0(tmp, o1, in, 1);                               /*0009*/
   freia_68_helper_0_1_0(o0, tmp, in);                                  /*0010*/
   freia_common_destruct_data(tmp);                                     /*0012*/
}

