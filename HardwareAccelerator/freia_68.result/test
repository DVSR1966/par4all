void freia_68(freia_data2d *o0, freia_data2d *o1, const freia_data2d *in)
{
   freia_68_helper_0_0_0(o0, o1, in, 1);                                /*0009*/
}

! cleaned up function

void freia_68(freia_data2d *o0, freia_data2d *o1, const freia_data2d *in)
{
