#
# AIPO
#

! initial function

freia_status freia_25(freia_data2d *o0, freia_data2d *o1, freia_data2d *i)
{
   // parallel reuses
   // o0 = i + i
   // o1 = i * i
   freia_aipo_add(o0, i, i);                                            /*0009*/
   freia_aipo_mul(o1, i, i);                                            /*0010*/

   return FREIA_OK;                                                     /*0012*/
}

! graphs

// graph for dag "dag_0" of module "freia_25" in dot format
digraph "dag_0" {
  // inputs
  "i" [shape=circle];

  // outputs
  "o0" [shape=circle];
  "o1" [shape=circle];

  // computation vertices
  "10 *" [shape=trapezium,orientation=270];
  "10 *" -> "o1";
  "9 +" [shape=trapezium,orientation=270];
  "9 +" -> "o0";
  "i" -> "10 *";
  "i" -> "10 *";
  "i" -> "9 +";
  "i" -> "9 +";
}
