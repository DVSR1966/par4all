// freia terapix includes
#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <terapix.h>
#include <terapix_ucode.h>

/* FREIA terapix helper function for module anr999
 * 1 input image, 2 output images
 * 12 image operations in dag
 * dag length is 6, dag width is 2
 * costs in cycles per imagelet row:
 * - computation: 156
 * - communication: 96
 */
freia_status anr999_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  freia_data2d * i0,
  const int32_t * pi3,
  int32_t * po4,
  int32_t * po5)
{
  // declarations:
  freia_microcode mcode;
  freia_op_param param;
  freia_dynamic_param dyn_param;
  terapix_gram gram;
  int i;
  freia_status ret;
  // overall structure which describes the computation
  terapix_mcu_instr mcu_instr;
  // operation 0 parameters
  int16_t p_0[9];
  const int32_t x_0 = 0;
  const int32_t y_0 = 0;
  terapix_mcu_macrocode mcu_macro[2][12];
