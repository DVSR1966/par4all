
! initial function

void freia_skip_02(freia_data2d *o, freia_data2d *i)
{
   
   
   freia_data2d *t0 = freia_common_create_data(16, 128, 128), *t1 = freia_common_create_data(16, 128, 128);          /*0007*/
   int32_t min, seuil;                                                  /*0008*/
   // div_const must be reordered *before* seuil assignment...
   freia_aipo_global_min(i, &min);                                      /*0010*/
   freia_aipo_and_const(t0, i, 127);                                    /*0011*/
   seuil = min-10;                                                      /*0012*/
   freia_aipo_div_const(t1, t0, 3);                                     /*0013*/
   freia_aipo_threshold(o, t1, seuil, 210, false);                      /*0014*/
   // cleanup
   freia_common_destruct_data(t0);                                      /*0016*/
   freia_common_destruct_data(t1);                                      /*0017*/
}

