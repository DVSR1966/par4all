
TRANSFORMERS


//  T() {}

void run_int(void)
{

//  T(b) {}

   int b;

//  T(b) {b==0}

   b = 0;

//  T() {b==0}

   if (b==1)

//  T() {0==-1}

      "unreachable";
}

//  T() {}

void run_enum1(void)
{

//  T(b) {}

   enum mybool b;

//  T(b) {b==0}

   b = 0;

//  T() {b==0}

   if (b==1)

//  T() {0==-1}

      "unreachable";
}

//  T() {}

void run_enum2(void)
{

//  T(b) {}

   enum mybool b;

//  T(b) {b==0}

   b = mybool_false;

//  T() {b==0}

   if (b==mybool_true)

//  T() {0==-1}

      "unreachable";
}

PRECONDITIONS


//  P() {}

void run_int(void)
{

//  P() {}

   int b;

//  P(b) {}

   b = 0;

//  P(b) {b==0}

   if (b==1)

//  P() {0==-1}

      "unreachable";
}

//  P() {}

void run_enum1(void)
{

//  P() {}

   enum mybool b;

//  P(b) {}

   b = 0;

//  P(b) {b==0}

   if (b==1)

//  P() {0==-1}

      "unreachable";
}

//  P() {}

void run_enum2(void)
{

//  P() {}

   enum mybool b;

//  P(b) {}

   b = mybool_false;

//  P(b) {b==0}

   if (b==mybool_true)

//  P() {0==-1}

      "unreachable";
}
