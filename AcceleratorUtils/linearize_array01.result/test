      SUBROUTINE P4A_KERNEL_0(N, A, B, I, J)
      INTEGER N, I, J
      INTEGER A(1:N, 1:N), B(1:N, 1:N)
C Loop nest P4A end
      IF (J.LE.N-1.AND.I.LE.N-1) THEN                                   0008
         A(I+1,J+1) = 1                                                 0009
         B(I+1,J+1) = 1                                                 0010
      ENDIF
      END
      SUBROUTINE P4A_KERNEL_0(N, A, B, I, J)
      INTEGER N, I, J
      INTEGER A(1:N*N), B(1:N*N)
C Loop nest P4A end
      IF (J.LE.N-1.AND.I.LE.N-1) THEN                                   0008
         A(I+1+J*N) = 1                                                 0009
         B(I+1+J*N) = 1                                                 0010
      ENDIF
      END
/* C pretty print for module P4A_KERNEL_0. */
#include "math.h"
#include "stdlib.h"
void
p4a_kernel_0 (int n, int a[n * n - 0], int b[n * n - 0], int i, int j)
{

  if ((j <= (n - 1)) && (i <= (n - 1)))
    {
      a[((i + 1) + (j * n)) - 1] = 1;
      b[((i + 1) + (j * n)) - 1] = 1;
    }
  return;
}
