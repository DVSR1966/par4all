      PROGRAM FOO
      INTEGER I, J
      INTEGER A(10, 10)
      DO J = 1, 10                                                      0004
         DO I = 1, 10                                                   0005
            A(I,J) = I+J*10                                             0006
         ENDDO
      ENDDO
      END
      PROGRAM FOO
      INTEGER I, J
      INTEGER A(1:100)
      DO J = 1, 10                                                      0004
         DO I = 1, 10                                                   0005
            A(I+10*J) = I+J*10                                          0006
         ENDDO
      ENDDO
      END
/* C pretty print for module FOO. */
int
main (int argc, char *argv[])
{
  int i;
  int j;
  int a[100];

  for (j = 1; j <= 10; j++)
    {
      for (i = 1; i <= 10; i++)
	{
	  a[(i + (10 * j)) - 1] = (i + (j * 10));
	}
    }
  return 0;
}
