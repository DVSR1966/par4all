
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i
   do {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p[10];

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   do {

// Points To:
// p[*] -> a[*] , EXACT
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p[i] = &a[i];

// Points To:
// p[*] -> a[*] , EXACT
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      i++;
   }
   while (i<10);

// Points To:
// p[*] -> a[*] , EXACT
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i
   do {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

 *********************** Use-Def Chains *********************

	11 --> 11 with conflicts
		from <i-W-EXACT> to <i-W-EXACT>
		from <i-W-EXACT> to <i-R-EXACT>
		from <i-R-EXACT> to <i-W-EXACT>
	11 --> 10 with conflicts
		from <i-W-EXACT> to <i-R-EXACT>
	10 --> 11 with conflicts
		from <i-R-EXACT> to <i-W-EXACT>
	10 --> 10 with conflicts
		from <p[i]-W-EXACT> to <p[i]-W-EXACT>
	12 --> 11 with conflicts
		from <i-R-EXACT> to <i-W-EXACT>
	04 --> 11 with conflicts
		from <i-W-EXACT> to <i-W-EXACT>
		from <i-W-EXACT> to <i-R-EXACT>
	04 --> 10 with conflicts
		from <i-W-EXACT> to <i-R-EXACT>
	04 --> 12 with conflicts
		from <i-W-EXACT> to <i-R-EXACT>

