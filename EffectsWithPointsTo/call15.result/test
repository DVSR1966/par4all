
void call15(int *pi, tf_t *q)
{
//             <may be written>: _pi_1[0]
//             <    is read   >: pi
   *pi = 1;
//             <may be written>: _q_2[0].one
//             <    is read   >: q
   q->one = 1;
}
int main()
{
   int i;
   tf_t s;
//             <may be written>: i s.one

   call15(&i, &s);
}

