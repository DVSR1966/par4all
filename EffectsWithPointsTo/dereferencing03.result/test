
POINTS-TO

//  {}
void dereferencing03()
{
//  points to = {}
   double *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   *(p+1) = 0.;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   *(p+i) = 0.;
}

PROPER POINTER EFFECTS

void dereferencing03()
{
   double *p;
   int i;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *(p+1) = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
   *(p+i) = 0.;
}
