
PROPER  EFFECTS 

int main()
{
   int a[10];
   int *p;
//             <    is written>: b c
   int b = 0, c = 1;
//             <    is read   >: b c
   if (b==c)
//             <    is written>: p
      p = &a[0];
   else
//             <    is written>: p
      p = &a[1];
//             <    is read   >: p
//             <    is written>: p
   p = p+1;
//             <    is read   >: p
//             <    is written>: p
   p = p+3;
//             <    is read   >: p
//             <    is written>: p
   p = p-1;
//             <    is read   >: p
//             <    is written>: p
   p = p-2;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int a[10];
//  {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b = 0, c = 1;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   if (b==c)
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      p = &a[0];
   else
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      p = &a[1];
//  {(p,a[0],-MAY-);(p,a[1],-MAY-)}
   p = p+1;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   p = p+3;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   p = p-1;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   p = p-2;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int a[10];
   int *p;
//             <    is written>: b c
   int b = 0, c = 1;
//             <    is read   >: b c
   if (b==c)
//             <    is written>: p
      p = &a[0];
   else
//             <    is written>: p
      p = &a[1];
//             <    is read   >: p
//             <    is written>: p
   p = p+1;
//             <    is read   >: p
//             <    is written>: p
   p = p+3;
//             <    is read   >: p
//             <    is written>: p
   p = p-1;
//             <    is read   >: p
//             <    is written>: p
   p = p-2;
   return 0;
}

