
int main()
{
   int **c, **a, **b, *y;
//             <    is written>: p x z
   int x = 1, z = 2, p = 1;
//             <    is written>: a

   a = &x;
//             <    is written>: b
   b = &y;
//             <    is read   >: p
   if (p)
//             <    is written>: y
      y = &z;
   else
//             <    is written>: y
      y = &x;
//             <    is written>: c
   c = &y;
   return 0;
}

 *********************** Use-Def Chains *********************

	04 --> 08 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>
	09 --> 11 with conflicts
		from <y-W-EXACT> to <y-W-EXACT>

****************** End of Dependence Graph ******************
