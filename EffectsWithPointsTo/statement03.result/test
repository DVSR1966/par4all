
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *x, *y, k[5], h;
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   k = 1;
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   h = 2;
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   x = &k[2];
//  {(x,k[2],-Exact-);(y,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   y = &h;
//  {(x,k[2],-Exact-);(y,h,-Exact-)}

   x = y;
//  {(x,h,-Exact-);(y,h,-Exact-)}

   return 0;
}

PROPER POINTER EFFECTS

int main()
{
   int *x, *y, k[5], h;
   k = 1;
//             <    is written>: h
   h = 2;
//             <    is written>: x

   x = &k[2];
//             <    is written>: y
   y = &h;
//             <    is read   >: y
//             <    is written>: x

   x = y;

   return 0;
}
