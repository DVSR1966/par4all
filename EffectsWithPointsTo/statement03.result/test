
PROPER  EFFECTS 

int main()
{
   int *x, *y, k[5], h;
   k = 1;
//             <    is written>: h
   h = 2;
//             <    is written>: x

   x = &k[2];
//             <    is written>: y
   y = &h;
//             <    is read   >: y
//             <    is written>: x

   x = y;

   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int *x, *y, k[5], h;

// Points To:
// x -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   k = 1;

// Points To:
// x -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   h = 2;

// Points To:
// x -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   x = &k[2];

// Points To:
// x -> &k[2] , EXACT
// y -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   y = &h;

// Points To:
// x -> &k[2] , EXACT
// y -> &h , EXACT


   x = y;

// Points To:
// x -> &h , EXACT
// y -> &h , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *x, *y, k[5], h;
   k = 1;
//             <    is written>: h
   h = 2;
//             <    is written>: x

   x = &k[2];
//             <    is written>: y
   y = &h;
//             <    is read   >: y
//             <    is written>: x

   x = y;

   return 0;
}

 *********************** Use-Def Chains *********************

	08 --> 10 with conflicts
		from <y-W-EXACT> to <y-R-EXACT>
	07 --> 10 with conflicts
		from <x-W-EXACT> to <x-W-EXACT>

****************** End of Dependence Graph ******************
