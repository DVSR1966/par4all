
int main()
{
   int i;
   int *p;
   int **q;
//             <    is written>: p

   p = 0;
//             <    is read   >: q
   **q = 0;

   *p = 1;
   *q = &i;

   return 0;
}

 *********************** Use-Def Chains *********************


