
POINTS-TO

//  {}
int main()
{
//  {}
   int j = 0, k, tab[5];
//  {}
   int *p = &j;
//  {(p,j,-Exact-)}
   int *q = tab[1];
//  {(p,j,-Exact-);(q,tab[1][0],-Exact-)}

   k = 1;
//  {(p,j,-Exact-);(q,tab[1][0],-Exact-)}
   j = k;
//  {(p,j,-Exact-);(q,tab[1][0],-Exact-)}
   p = &k;
//  {(p,k,-Exact-);(q,tab[1][0],-Exact-)}

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: j
   int j = 0, k, tab[5];
//             <    is written>: p
   int *p = &j;
//             <    is read   >: tab[1]
//             <    is written>: q
   int *q = tab[1];
//             <    is written>: k

   k = 1;
//             <    is read   >: k
//             <    is written>: j
   j = k;
//             <    is written>: p
   p = &k;

   return 0;
}

 *********************** Use-Def Chains *********************

	07 --> 08 with conflicts
		from <k-W-EXACT> to <k-R-EXACT>
	04 --> 09 with conflicts
		from <p-W-EXACT> to <p-W-EXACT>
	03 --> 08 with conflicts
		from <j-W-EXACT> to <j-W-EXACT>

****************** End of Dependence Graph ******************
