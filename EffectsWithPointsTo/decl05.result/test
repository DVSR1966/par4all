
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int tab[3] = {0, 1, 2};
//  points to = {}

   tab[0] = tab[0]+2;
//  points to = {}
   return 0;
}

PROPER POINTER EFFECTS

int main()
{
//             <may be written>: tab[*]
   int tab[3] = {0, 1, 2};
//             <    is read   >: tab[0]
//             <    is written>: tab[0]

   tab[0] = tab[0]+2;
   return 0;
}
