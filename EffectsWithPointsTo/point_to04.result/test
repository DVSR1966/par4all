
POINTS-TO

//  {}
void point_to04()
{
//  {}
   struct s {
      int a;
      int b;
      int c;
      int d;
   } c, *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = &c;
//  {(p,c,-Exact-)}

   p->d = p->a;
//  {(p,c,-Exact-)}
   p->d = p->a = 4;
}

PROPER  EFFECTS WITH POINTS TO

void point_to04()
{
   struct s {
      int a;
      int b;
      int c;
      int d;
   } c, *p;
//             <    is written>: p

   p = &c;
//             <    is read   >: c.a p
//             <    is written>: c.d

   p->d = p->a;
//             <    is read   >: p
//             <    is written>: c.a c.d
   p->d = p->a = 4;
}

 *********************** Use-Def Chains *********************

	12 --> 13 with conflicts
		from <c.d-W-EXACT> to <c.d-W-EXACT>
		from <c.a-R-EXACT> to <c.a-W-EXACT>
	10 --> 13 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>
	10 --> 12 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>

****************** End of Dependence Graph ******************
