
int main()
{
//             <    is written>: i
   int i = 0;
//:   BEGIN UNSTRUCTURED
//             <    is read   >: i
   if (i>0) goto l;
//             <    is written>: i
   i = 1;
l:   ;
//:   END UNSTRUCTURED
   return 0;
}
int main();

