
PROPER  EFFECTS 

int main()
{
   float t;
   float *a;
   int i;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a _MALLOC_EFFECTS:_MALLOC_

   a = (float *) malloc(10*sizeof(float));
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is written>: i
      for(i = 0; i <= 9; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
         a[i] = a[i]+t*0, 5;
      t+0, 01;
   }
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   float t;

// Points To: none

   float *a;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   a = (float *) malloc(10*sizeof(float));

// Points To:
// a -> *HEAP*_l_9[0] , EXACT


   t = 1.0;

// Points To:
// a -> *HEAP*_l_9[0] , EXACT

   while (t<2.0) {

// Points To:
// a -> *HEAP*_l_9[0] , EXACT

      for(i = 0; i <= 9; i += 1)

// Points To:
// a -> *HEAP*_l_9[0] , EXACT

         a[i] = a[i]+t*0, 5;

// Points To:
// a -> *HEAP*_l_9[0] , EXACT

      t+0, 01;
   }

// Points To:
// a -> *HEAP*_l_9[0] , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   float t;
   float *a;
   int i;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a _MALLOC_EFFECTS:_MALLOC_

   a = (float *) malloc(10*sizeof(float));
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is written>: i
      for(i = 0; i <= 9; i += 1)
//             <    is read   >: *HEAP*_l_9[i] a i t
//             <    is written>: *HEAP*_l_9[i]
         a[i] = a[i]+t*0, 5;
      t+0, 01;
   }
   return 0;
}

 *********************** Use-Def Chains *********************

	-1 --> -1 with conflicts
		from <*HEAP*_l_9[i]-W-EXACT> to <*HEAP*_l_9[i]-W-EXACT>
		from <*HEAP*_l_9[i]-W-EXACT> to <*HEAP*_l_9[i]-R-EXACT>
		from <*HEAP*_l_9[i]-R-EXACT> to <*HEAP*_l_9[i]-W-EXACT>
	-1 --> 12 with conflicts
		from <i-R-EXACT> to <i-W-EXACT>
	-1 --> -1 with conflicts
		from <t-W-EXACT> to <t-R-EXACT>
	-1 --> -1 with conflicts
		from <t-W-EXACT> to <t-R-EXACT>
	09 --> -1 with conflicts
		from <a-W-EXACT> to <a-R-EXACT>
	12 --> 12 with conflicts
		from <i-W-EXACT> to <i-W-EXACT>

