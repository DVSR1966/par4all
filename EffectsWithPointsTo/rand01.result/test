
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int entier;
//  points to = {}
   int graine;
//  points to = {}
   double virgule;
//  points to = {}
   graine = 50;
//  points to = {}
   srandom(graine);
//  points to = {}
   /* generator initialization */
   virgule = (double) rand()/(2147483647+1);
//  points to = {}
   entier = 1+rand();
}

PROPER POINTER EFFECTS

int main()
{
   int entier;
   int graine;
   double virgule;
//             <    is written>: graine
   graine = 50;
//             <    is read   >: graine
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   srandom(graine);
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: virgule _RANDOM_EFFECTS:GEN_SEED
   /* generator initialization */
   virgule = (double) rand()/(2147483647+1);
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: entier _RANDOM_EFFECTS:GEN_SEED
   entier = 1+rand();
}
