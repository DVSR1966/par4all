
PROPER  EFFECTS 

int foo(int **dest, int *source)
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: dest source
   *dest = source;
}
int main()
{
   int *p, *q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   foo(&q, p);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = 2;
   return 0;
}

POINTS-TO


// Points To:
// dest -> &_dest_1[0] , EXACT
// source -> &_source_2[0] , EXACT

int foo(int **dest, int *source)
{

// Points To:
// dest -> &_dest_1[0] , EXACT
// source -> &_source_2[0] , EXACT

   *dest = source;
}

// Points To: none

int main()
{

// Points To: none

   int *p, *q;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   *p = 1;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   foo(&q, p);

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   *q = 2;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int foo(int **dest, int *source)
{
//             <    is read   >: dest source
//             <    is written>: _dest_1[0]
   *dest = source;
}
int main()
{
   int *p, *q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p = 1;
//             <    is read   >: p
//             <    is written>: q

   foo(&q, p);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = 2;
   return 0;
}

 *********************** Use-Def Chains *********************

	14 --> 15 with conflicts
		from <q-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <q-W-EXACT> to <q-R-EXACT>
		from <p-R-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
	12 --> 15 with conflicts
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <q-R-EXACT>
		from <p-R-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
	12 --> 14 with conflicts
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <q-W-EXACT>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <p-R-EXACT>

