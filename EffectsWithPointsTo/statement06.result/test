PROPER  EFFECTS WITH POINTS TO

int main()
{
   int a, b, c, d;
   int *pa, *pb, *pc, *pd;
   int **ppa, **ppb, **ppc, **ppd;
//             <    is written>: a
   a = 1;
//             <    is written>: b
   b = 2;
//             <    is written>: c
   c = 3;
//             <    is written>: d
   d = 4;
//             <    is written>: pa

   pa = &a;
//             <    is written>: ppa
   ppa = &pa;
//             <    is read   >: ppa
//             <    is written>: pa
   *ppa = &b;
//             <    is written>: pb

   pb = &b;
//             <    is written>: ppb
   ppb = &pb;
//             <    is read   >: ppb
//             <    is written>: pb
   *ppb = &a;
//             <    is written>: pc

   pc = &c;
//             <    is written>: ppc
   ppc = &pc;
//             <    is read   >: ppc
//             <    is written>: pc
   *ppc = &a;
//             <    is written>: pd

   pd = &d;
//             <    is written>: ppd
   ppd = &pd;
//             <    is read   >: ppd
//             <    is written>: pd
   *ppd = &c;
//             <    is read   >: pd
//             <    is written>: pc

   pc = pd;
//             <    is read   >: ppc
//             <    is written>: pc

   *ppc = &b;
//             <    is read   >: pc ppa
//             <    is written>: pa

   *ppa = pc;
//             <    is read   >: pd ppd
//             <    is written>: pa

   pa = *ppd;
//             <    is read   >: pd ppa ppd
//             <    is written>: pa

   *ppa = *ppd;

   return 0;
}

