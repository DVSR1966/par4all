
POINTS-TO

//  {}
int main(void)
{
//  {}
   int i1;
//  {}
   int *pi1 = &i1;
//  {(pi1,i1,-Exact-)}
   int **pp;
//  {(pi1,i1,-Exact-);(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   srand(time((void *) 0));
//  {(pi1,i1,-Exact-);(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   if (alea())
//  {(pi1,i1,-Exact-);(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      pp = &pi1;
//  {(pi1,i1,-Exact-);(pp,pi1,-MAY-);(pp,*ANY_MODULE*:*NOWHERE*_b0,
//   -MAY-)}

   *pp = (void *) 0;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,*NULL_POINTER*,-MAY-);(pi1,
//   *NULL_POINTER*,-MAY-);(pi1,i1,-MAY-);(pp,pi1,-MAY-);(pp,
//   *ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
   printf("%p %p \n", pp, pi1);
//  {(*ANY_MODULE*:*ANYWHERE*_b0,*NULL_POINTER*,-MAY-);(pi1,
//   *NULL_POINTER*,-MAY-);(pi1,i1,-MAY-);(pp,pi1,-MAY-);(pp,
//   *ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main(void)
{
   int i1;
//             <    is written>: pi1
   int *pi1 = &i1;
   int **pp;
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   srand(time((void *) 0));
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout _RANDOM_EFFECTS:GEN_SEED 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED 
//                               __IO_EFFECTS:LUNS[1]
   if (alea())
//             <    is written>: pp
      pp = &pi1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pp

   *pp = (void *) 0;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: pi1 pp stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%p %p \n", pp, pi1);
   return 0;
}

