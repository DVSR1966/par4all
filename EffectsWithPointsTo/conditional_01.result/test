
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *q;
//  {(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i = 0, j = 1;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = &i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,i,-Exact-)}
   p = i>0?&j:q;
//  {(p,i,-MAY-);(p,j,-MAY-);(q,i,-Exact-)}
   printf("address of p =%p\n", p);
//  {(p,i,-MAY-);(p,j,-MAY-);(q,i,-Exact-)}
   printf("address of q =%p\n", q);
//  {(p,i,-MAY-);(p,j,-MAY-);(q,i,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *q;
   int *p;
//             <    is written>: i j
   int i = 0, j = 1;
//             <    is written>: q
   q = &i;
//             <may be read   >: q
//             <    is read   >: i
//             <    is written>: p
   p = i>0?&j:q;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: p stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("address of p =%p\n", p);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: q stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("address of q =%p\n", q);
   return 0;
}
