
int bla(int *p)
{
//             <    is read   >: p
//             <    is written>: _p_1[0]
   *p = 13;
   return 17;
}
int call19(int *q)
{
   int i, k;
   something st;
//             <    is read   >: q
//             <    is written>: i
   // bla(int *)
   i = bla(q);
//             <    is read   >: _q_1[0] i q
//             <    is written>: i
   i += *q;
//             <    is read   >: i
//             <    is written>: i
   i += bla(&k);
//             <    is read   >: i k
//             <    is written>: i
   i += k;
//             <    is read   >: i
//             <    is written>: i
   i += bla(&st.data);
//             <    is read   >: i st.data
//             <    is written>: i
   i += st.data;
//             <    is read   >: _q_1[0] i q
//             <    is written>: i
   // foo(int)
   i += foo(*q);
//             <    is read   >: i k
//             <    is written>: i
   i += foo(k);
//             <    is read   >: i st.data
//             <    is written>: i
   i += foo(st.data);
//             <    is read   >: i
   return i;
}
int foo(int a)
{
//             <    is read   >: a
   return a*3;
}

