
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: i
   do {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
      p[i] = i;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = (int *) malloc(10*sizeof(int));

// Points To:
// p -> *HEAP*_l_7[0] , EXACT

   do {

// Points To:
// p -> *HEAP*_l_7[0] , EXACT

      p[i] = i;

// Points To:
// p -> *HEAP*_l_7[0] , EXACT

      i++;
   }
   while (i<10);

// Points To:
// p -> *HEAP*_l_7[0] , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: i
   do {
//             <    is read   >: i p
//             <    is written>: *HEAP*_l_7[i]
      p[i] = i;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

