
PROPER  EFFECTS 

int main()
{
   int a[10];
   int *p;
//             <    is written>: p
   p = &a[0];
//             <    is read   >: p
//             <    is written>: p
   p++;
//             <    is read   >: p
//             <    is written>: p
   ++p;
//             <    is read   >: p
//             <    is written>: p
   p--;
//             <    is read   >: p
//             <    is written>: p
   --p;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int a[10];
//  {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p = &a[0];
//  {(p,a[0],-Exact-)}
   p++;
//  {(p,a[*],-Exact-)}
   ++p;
//  {(p,a[0],-Exact-)}
   p--;
//  {(p,a[*],-Exact-)}
   --p;
//  {(p,a[0],-Exact-)}
   return 0;
}

