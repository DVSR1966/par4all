int main()
{
   int a[10];
   int *p;
//             <    is written>: p
   p = &a[0];
//             <    is read   >: p
//             <    is written>: p
   p++;
//             <    is read   >: p
//             <    is written>: p
   ++p;
//             <    is read   >: p
//             <    is written>: p
   p--;
//             <    is read   >: p
//             <    is written>: p
   --p;
   return 0;
}

