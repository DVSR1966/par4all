
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int i;
//  points to = {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int **q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}

   *p = 1;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   *q = &i;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,i,-Exact-);(p,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
   int *p;
   int **q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;

   return 0;
}
