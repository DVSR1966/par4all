
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int d1 = 10;
//  points to = {}
   int d2 = 10;
//  points to = {}
   int y[d1][d2];
//  points to = {}

   call04(d1, d2, y);
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: d1
   int d1 = 10;
//             <    is written>: d2
   int d2 = 10;
   int y[d1][d2];
//             <may be written>: y[*][*]
//             <    is read   >: d1 d2

   call04(d1, d2, y);
}
