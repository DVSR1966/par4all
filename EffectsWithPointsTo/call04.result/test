PROPER  EFFECTS WITH POINTS TO

void call04(int n, int m, int x[n][m])
{
   int i, j;
//             <    is read   >: n
//             <    is written>: i

   for(i = 0; i <= n-1; i += 1)
//             <    is read   >: m n
//             <    is written>: j
      for(j = 0; j <= m-1; j += 1)
//             <    is read   >: i j m n
//             <    is written>: x[i][j]
         x[i][j] = 0;
}
int main()
{
//             <    is written>: d1
   int d1 = 10;
//             <    is written>: d2
   int d2 = 10;
   int y[d1][d2];
//             <may be written>: y[*][*]
//             <    is read   >: d1 d2

   call04(d1, d2, y);
}

