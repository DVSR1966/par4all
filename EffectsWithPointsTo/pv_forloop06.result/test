
PROPER  EFFECTS 

int main()
{
   float f;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: f
   f = 0.0;
//             <    is read   >: f
   while (f<10.5) {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      *p++ = 2**q++;
//             <    is read   >: f
//             <    is written>: f
      f += 1.0;
   }
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   float f;

// Points To: none

   int *p, *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = &a[0];

// Points To:
// p -> a[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = &b[0];

// Points To:
// p -> a[0] , EXACT
// q -> b[0] , EXACT

   f = 0.0;

// Points To:
// p -> a[0] , EXACT
// q -> b[0] , EXACT

   while (f<10.5) {

// Points To:
// p -> a[0] , MAY
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// q -> b[0] , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

      *p++ = 2**q++;

// Points To:
// p -> a[0] , MAY
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// q -> b[0] , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

      f += 1.0;
   }

// Points To:
// p -> a[0] , MAY
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// q -> b[0] , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   float f;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: f
   f = 0.0;
//             <    is read   >: f
   while (f<10.5) {
//             <may be read   >: b[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: a[0] *ANY_MODULE*:*ANYWHERE*
      *p++ = 2**q++;
//             <    is read   >: f
//             <    is written>: f
      f += 1.0;
   }
   return 0;
}

