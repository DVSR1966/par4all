
int main()
{
   float f;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: f
   f = 0.0;
//             <    is read   >: f
   while (f<10.5) {
//             <may be read   >: b[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: a[0] *ANY_MODULE*:*ANYWHERE*
      *p++ = 2**q++;
//             <    is read   >: f
//             <    is written>: f
      f += 1.0;
   }
   return 0;
}

