
POINTS-TO

//  {}
int main(int argc, char *argv[])
{
//  points to = {}
   int *pi;
//  {(pi,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   float *px;
//  {(pi,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(px,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}

   pi = (int *) malloc(sizeof(int));
//  {(pi,*HEAP*_l_10,-Exact-);(px,*ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   px = (float *) malloc(sizeof(int));
//  {(pi,*HEAP*_l_10,-Exact-);(px,*HEAP*_l_11,-Exact-)}

   return 0;
}

PROPER POINTER EFFECTS

int main(int argc, char *argv[])
{
   int *pi;
   float *px;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pi _MALLOC_EFFECTS:_MALLOC_

   pi = (int *) malloc(sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: px _MALLOC_EFFECTS:_MALLOC_
   px = (float *) malloc(sizeof(int));

   return 0;
}
