
POINTS-TO

//  {}
int main(int argc, char *argv[])
{
//  points to = {}
   int *pi;
//  {(pi,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   float *px;
//  {(pi,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(px,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   float *pz;
//  {(pi,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(px,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(pz,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   int (*pt)[30];
//  {(pi,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(pt,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(px,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-);(pz,*ANY_MODULE*:*NOWHERE*_b1,-Exact-)}

   pi = (int *) malloc(sizeof(int));
//  {(pi,*HEAP*_l_12,-Exact-);(pt,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(px,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(pz,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   px = (float *) malloc(sizeof(float));
//  {(pi,*HEAP*_l_12,-Exact-);(pt,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(px,*HEAP*_l_13,-Exact-);(pz,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   pz = (float *) malloc(sizeof(float));
//  {(pi,*HEAP*_l_12,-Exact-);(pt,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(px,*HEAP*_l_13,-Exact-);(pz,*HEAP*_l_14,-Exact-)}
   pt = (int (*)[30]) malloc(sizeof(*pt));
//  {(pi,*HEAP*_l_12,-Exact-);(pt,*HEAP*_l_15,-Exact-);(px,
//   *HEAP*_l_13,-Exact-);(pz,*HEAP*_l_14,-Exact-)}

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main(int argc, char *argv[])
{
   int *pi;
   float *px;
   float *pz;
   int (*pt)[30];
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pi _MALLOC_EFFECTS:_MALLOC_

   pi = (int *) malloc(sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: px _MALLOC_EFFECTS:_MALLOC_
   px = (float *) malloc(sizeof(float));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pz _MALLOC_EFFECTS:_MALLOC_
   pz = (float *) malloc(sizeof(float));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pt _MALLOC_EFFECTS:_MALLOC_
   pt = (int (*)[30]) malloc(sizeof(*pt));

   return 0;
}
