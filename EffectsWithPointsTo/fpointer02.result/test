
Points-to for fpointer02


// Points To IN:
// col -> *NULL_POINTER* , MAY
// col -> _col_1[0] , MAY


// Points To OUT: none

void fpointer02(int *col, int indent)
{

// Points To:
// col -> *NULL_POINTER* , MAY
// col -> _col_1[0] , MAY

   *col = 0;

// Points To:
// col -> _col_1[0] , EXACT

   if (*col<indent)
      ;

// Points To:
// col -> _col_1[0] , EXACT

   while (*col<indent)
      ;
}

Proper effects with points-to for fpointer02

void fpointer02(int *col, int indent)
{
//             <    is read   >: col
//             <    is written>: _col_1[0]
   *col = 0;
//             <    is read   >: _col_1[0] col indent
   if (*col<indent)
      ;
//             <    is read   >: _col_1[0] col indent
   while (*col<indent)
      ;
}
