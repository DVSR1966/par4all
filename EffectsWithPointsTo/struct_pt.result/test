
PROPER  EFFECTS 

int main()
{
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
   my_str *m, *n;
   int j;
//             <    is written>: j
   j = 2;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: m _MALLOC_EFFECTS:_MALLOC_
   m = (my_str *) malloc(sizeof(my_str));
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: m
   m->i = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: m
   m->p = &j;
//             <    is read   >: m
//             <    is written>: n
   n = m;

   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
//  {}
   my_str *m, *n;
//  {(m,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(n,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int j;
//  {(m,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(n,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   j = 2;
//  {(m,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(n,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   m = (my_str *) malloc(sizeof(my_str));
//  {(m,*HEAP*_l_10,-Exact-);(n,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   m->i = 1;
//  {(m,*HEAP*_l_10,-Exact-);(n,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   m->p = &j;
//  {(*HEAP*_l_10[p],j,-Exact-);(m,*HEAP*_l_10,-Exact-);(n,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   n = m;
//  {(*HEAP*_l_10[p],j,-Exact-);(m,*HEAP*_l_10,-Exact-);(n,
//   *HEAP*_l_10,-Exact-)}

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
   my_str *m, *n;
   int j;
//             <    is written>: j
   j = 2;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: m _MALLOC_EFFECTS:_MALLOC_
   m = (my_str *) malloc(sizeof(my_str));
//             <    is read   >: m
//             <    is written>: *HEAP*_l_10.i
   m->i = 1;
//             <    is read   >: m
//             <    is written>: *HEAP*_l_10.p
   m->p = &j;
//             <    is read   >: m
//             <    is written>: n
   n = m;

   return 0;
}

