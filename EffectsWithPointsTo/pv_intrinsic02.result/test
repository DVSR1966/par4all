
PROPER  EFFECTS 

int main()
{
   char *end;
   long int li;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

   li = strtol("32445667 is a number", &end, 0);
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   char *end;

// Points To:
// end -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   long int li;

// Points To:
// end -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   li = strtol("32445667 is a number", &end, 0);

// Points To:
// end -> &*ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   char *end;
   long int li;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

   li = strtol("32445667 is a number", &end, 0);
   return 0;
}

