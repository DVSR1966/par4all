void lhs01()
{
//             <    is written>: i
   int i = 2;
//             <    is written>: j
   int j = 2;
//             <    is written>: k
   int k = 2;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i
   
   /* This is a deprecated construct which is accepted by PIPS parser */
   //i>2?i:j=3;
   //(i>2?i:j)=3;
   
   *(i>2?&i:&j) = 3;
//             <    is written>: i j
   /* printf("i=%d, j=%d\n", i, j); */
   
   /* This is a deprecated construct which is accepted by PIPS parser */
   //i>2?i=3:j=3;
   /* This is OK*/
   i = j = 2;
//             <may be written>: i j
//             <    is read   >: i
   i>2?(i = 3):(j = 3);
//             <    is written>: i j

   i = j = 2;
//             <    is read   >: i
   if (i>2)
//             <    is written>: i
      i = 3;
   else
//             <    is written>: j
      j = 3;
//             <    is written>: i j

   i = j = 2;
//             <    is read   >: i
//             <    is written>: j
   i>2?(j = 4):(j = 3);
//             <    is read   >: i

   if (i>2)
//             <    is written>: j
      j = 4;
   else
//             <    is written>: j
      j = 3;
}
