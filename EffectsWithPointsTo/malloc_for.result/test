
PROPER  EFFECTS 

int main(int argc, char *argv[])
{
   int *p, i, j;
//             <    is written>: i
   i = 0;
//             <    is written>: j
   j = 1;
//             <    is written>: i

   for(i = 1; i <= 4; i += 1)
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

      p = (int *) malloc(sizeof(int));
   
   
   return 0;
}

