
PROPER  EFFECTS 

void exit01()
{
   int i;
   int *j;
//             <    is written>: j

   j = &i;
   exit(0);
//             <    is written>: i
   i = 2;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void exit01()
{

// Points To: none

   int i;

// Points To: none

   int *j;

// Points To:
// j -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   j = &i;

// Points To:
// j -> i , EXACT

   exit(0);

// Points To:
// j -> i , EXACT

   i = 2;
}

PROPER  EFFECTS WITH POINTS TO

void exit01()
{
   int i;
   int *j;
//             <    is written>: j

   j = &i;
   exit(0);
//             <    is written>: i
   i = 2;
}

