
POINTS-TO

//  {}
void exit01()
{
//  {}
   int i;
//  {}
   int *j;
//  {(j,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   j = &i;
//  {(j,i,-Exact-)}
   exit(0);
//  {(j,i,-Exact-)}
   i = 2;
}

PROPER  EFFECTS WITH POINTS TO

void exit01()
{
   int i;
   int *j;
//             <    is written>: j

   j = &i;
   exit(0);
//             <    is written>: i
   i = 2;
}

