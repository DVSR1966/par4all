
POINTS-TO

//  {}
int main()
{
//  {}
   mystruct s1;
//  {(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   mystruct s2 = s1;
//  {(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   mystruct s1;
//             <may be read   >: s1.b[*]
//             <may be written>: s2.b[*]
//             <    is read   >: s1.a s1.c
//             <    is written>: s2.a s2.c
   mystruct s2 = s1;
   return 0;
}

