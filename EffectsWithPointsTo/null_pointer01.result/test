
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int i;                                                               /*0006*/
//  points to = {}
   int *p = 0;                                                          /*0007*/
//  {(p,*NULL_POINTER*,-Exact-)}
   int **q = 0;                                                         /*0008*/
//  {(p,*NULL_POINTER*,-Exact-);(q,*NULL_POINTER*,-Exact-)}

   *p = 1;                                                              /*0010*/
//  {(p,*NULL_POINTER*,-Exact-);(q,*NULL_POINTER*,-Exact-)}
   *q = &i;                                                             /*0011*/
//  {(*NULL_POINTER*,i,-Exact-);(p,*NULL_POINTER*,-Exact-);(q,
//   *NULL_POINTER*,-Exact-)}

   return 0;                                                            /*0013*/
}
int main()
{
   int i;                                                               /*0006*/
//             <    is written>: p
   int *p = 0;                                                          /*0007*/
//             <    is written>: q
   int **q = 0;                                                         /*0008*/
//             <    is read   >: p
//             <    is written>: *NULL_POINTER*

   *p = 1;                                                              /*0010*/
//             <    is read   >: q
//             <    is written>: *NULL_POINTER*
   *q = &i;                                                             /*0011*/

   return 0;                                                            /*0013*/
}

