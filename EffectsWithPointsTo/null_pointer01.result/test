
PROPER  EFFECTS 

int main()
{
   int i;
//             <    is written>: p
   int *p = 0;
//             <    is written>: q
   int **q = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;

   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i;
//  {}
   int *p = 0;
//  {(p,*NULL_POINTER*,-Exact-)}
   int **q = 0;
//  {(p,*NULL_POINTER*,-Exact-);(q,*NULL_POINTER*,-Exact-)}

   *p = 1;
//  {(p,*NULL_POINTER*,-Exact-);(q,*NULL_POINTER*,-Exact-)}
   *q = &i;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,i,-Exact-);(p,*NULL_POINTER*,
//   -Exact-);(q,*NULL_POINTER*,-Exact-)}

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
//             <    is written>: p
   int *p = 0;
//             <    is written>: q
   int **q = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;

   return 0;
}

 *********************** Use-Def Chains *********************

	10 --> 11 with conflicts
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <q-R-EXACT>
		from <p-R-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
	08 --> 11 with conflicts
		from <q-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <q-W-EXACT> to <q-R-EXACT>
	08 --> 10 with conflicts
		from <q-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
	07 --> 11 with conflicts
		from <p-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
	07 --> 10 with conflicts
		from <p-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <p-W-EXACT> to <p-R-EXACT>

