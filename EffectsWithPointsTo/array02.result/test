
POINTS-TO

//  {}
void array02()
{
//  points to = {}
   float x[11][2];
//  points to = {}
   int i;
//  points to = {}

   for(i = 1; i <= 9; i += 1)
//  points to = {}
      x[i][1] = 0.;
}

PROPER POINTER EFFECTS

void array02()
{
   float x[11][2];
   int i;
//             <    is written>: i

   for(i = 1; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: x[i][1]
      x[i][1] = 0.;
}
