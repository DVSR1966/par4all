
PROPER  EFFECTS 

FLGR_Data2D * flgr2d_create_pixmap_link(FLGR_Data2D *datain, int partsNumber, int partIndex, int overlapSize)
{
   FLGR_Data2D *dat;
   int i, k, nbrow, startin;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: dat _MALLOC_EFFECTS:_MALLOC_
   /* dat = (FLGR_Data2D*) flgr_malloc(sizeof(FLGR_Data2D)); */
   dat = (FLGR_Data2D *) malloc(sizeof(FLGR_Data2D));
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: dat overlapSize
   dat->link_overlap = overlapSize;
//             <    is read   >: nbrow overlapSize
//             <    is written>: i

   for(i = 0; i <= nbrow-overlapSize-1; i += 1) {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      (dat->row)[i] = (datain->row)[i];
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      (dat->row)[i]->dim = i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      (dat->array)[i] = (dat->row)[i]->array;
   }
}

POINTS-TO


// Points To IN:
// datain -> _datain_1 , EXACT


// Points To OUT: none

FLGR_Data2D * flgr2d_create_pixmap_link(FLGR_Data2D *datain, int partsNumber, int partIndex, int overlapSize)
{

// Points To:
// datain -> _datain_1 , EXACT

   FLGR_Data2D *dat;

// Points To:
// dat -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// datain -> _datain_1 , EXACT

   int i, k, nbrow, startin;

// Points To:
// dat -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// datain -> _datain_1 , EXACT

   /* dat = (FLGR_Data2D*) flgr_malloc(sizeof(FLGR_Data2D)); */
   dat = (FLGR_Data2D *) malloc(sizeof(FLGR_Data2D));

// Points To:
// dat -> *HEAP*_l_22 , EXACT
// datain -> _datain_1 , EXACT

   dat->link_overlap = overlapSize;

// Points To:
// dat -> *HEAP*_l_22 , EXACT
// datain -> _datain_1 , EXACT


   for(i = 0; i <= nbrow-overlapSize-1; i += 1) {

// Points To:
// dat[0].array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// dat[0].row[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// dat -> *HEAP*_l_22 , EXACT
// datain -> _datain_1 , EXACT

      (dat->row)[i] = (datain->row)[i];

// Points To:
// dat[0].array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// dat[0].row[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// dat -> *HEAP*_l_22 , EXACT
// datain -> _datain_1 , EXACT

      (dat->row)[i]->dim = i;

// Points To:
// dat[0].array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// dat[0].row[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// dat -> *HEAP*_l_22 , EXACT
// datain -> _datain_1 , EXACT

      (dat->array)[i] = (dat->row)[i]->array;
   }
}

PROPER  EFFECTS WITH POINTS TO

FLGR_Data2D * flgr2d_create_pixmap_link(FLGR_Data2D *datain, int partsNumber, int partIndex, int overlapSize)
{
   FLGR_Data2D *dat;
   int i, k, nbrow, startin;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: dat _MALLOC_EFFECTS:_MALLOC_
   /* dat = (FLGR_Data2D*) flgr_malloc(sizeof(FLGR_Data2D)); */
   dat = (FLGR_Data2D *) malloc(sizeof(FLGR_Data2D));
//             <    is read   >: dat overlapSize
//             <    is written>: *HEAP*_l_22.link_overlap
   dat->link_overlap = overlapSize;
//             <    is read   >: nbrow overlapSize
//             <    is written>: i

   for(i = 0; i <= nbrow-overlapSize-1; i += 1) {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: *HEAP*_l_22.row[i]
      (dat->row)[i] = (datain->row)[i];
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: *HEAP*_l_22.row *HEAP*_l_22.row[i] 
//                               dat i nbrow overlapSize
      (dat->row)[i]->dim = i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: *HEAP*_l_22.array[i]
      (dat->array)[i] = (dat->row)[i]->array;
   }
}

