PROPER  EFFECTS WITH POINTS TO

int * foo()
{
//             <    is written>: cond i j
   int cond = 0, i = 1, j = 2;
   int *p, *q;
//             <    is written>: p

   p = &i;
//             <    is written>: q
   q = &j;
//             <    is read   >: cond

   if (cond)
//             <    is read   >: p
      return p;
   else
//             <    is read   >: q
      return q;
//             <    is written>: q

   q = &cond;
//             <    is read   >: q
   return q;
}
int main()
{
   int *r;
//             <    is written>: r
   r = foo();

   return 0;
}
int *foo();

int main();

