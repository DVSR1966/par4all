
PROPER  EFFECTS 

void dereferencing02()
{
//             <    is written>: x
   double x = 1.;
//             <    is written>: p
   double *p = &x;
//             <    is written>: q
   double **q = &p;
   double **w;
//             <    is written>: i
   int i = 1;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   
   
   **q = 2.;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   **(q+(i = 0)) = 3.;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   **(w+(i = 2)) = 4.;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: x
   x = **q;
//             <    is read   >: q
//             <    is written>: q
   q++;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: q
   *q++;
}
int main()
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   dereferencing02();
   return 1;
}

POINTS-TO

//  {}
void dereferencing02()
{
//  {}
   double x = 1.;
//  {}
   double *p = &x;
//  {(p,x,-Exact-)}
   double **q = &p;
//  {(p,x,-Exact-);(q,p,-Exact-)}
   double **w;
//  {(p,x,-Exact-);(q,p,-Exact-);(w,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   int i = 1;
//  {(p,x,-Exact-);(q,p,-Exact-);(w,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   
   
   **q = 2.;
//  {(p,x,-Exact-);(q,p,-Exact-);(w,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   **(q+(i = 0)) = 3.;
//  {(p,x,-Exact-);(q,p,-Exact-);(w,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   **(w+(i = 2)) = 4.;
//  {(p,x,-Exact-);(q,p,-Exact-);(w,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   x = **q;
//  {(p,x,-Exact-);(q,p,-Exact-);(w,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   q++;
//  {(p,x,-Exact-);(q,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(w,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   *q++;
}
//  {}
int main()
{
//  {}
   dereferencing02();
//  {}
   return 1;
}

PROPER  EFFECTS WITH POINTS TO

void dereferencing02()
{
//             <    is written>: x
   double x = 1.;
//             <    is written>: p
   double *p = &x;
//             <    is written>: q
   double **q = &p;
   double **w;
//             <    is written>: i
   int i = 1;
//             <    is read   >: p q
//             <    is written>: x
   
   
   **q = 2.;
//             <    is read   >: p q
//             <    is written>: i x
   **(q+(i = 0)) = 3.;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   **(w+(i = 2)) = 4.;
//             <    is read   >: p q x
//             <    is written>: x
   x = **q;
//             <    is read   >: q
//             <    is written>: q
   q++;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: q
   *q++;
}
int main()
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   dereferencing02();
   return 1;
}

