
POINTS-TO

//  {}
int main()
{
//  {}
   int i;
//  {}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a[10];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b[10];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = &a[0];
//  {(p,a[0],-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = &b[0];
//  {(p,a[0],-Exact-);(q,b[0],-Exact-)}
   *p++ = 2**q++;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is read   >: b[0] p q
//             <    is written>: a[0] p q
   *p++ = 2**q++;
   return 0;
}

 *********************** Use-Def Chains *********************

	11 --> 12 with conflicts
		from <q-W-EXACT> to <q-W-EXACT>
		from <q-W-EXACT> to <q-R-EXACT>
	10 --> 12 with conflicts
		from <p-W-EXACT> to <p-W-EXACT>
		from <p-W-EXACT> to <p-R-EXACT>

****************** End of Dependence Graph ******************
