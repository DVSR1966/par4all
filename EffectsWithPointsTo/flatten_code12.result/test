
POINTS-TO

//  {}
int main()
{
//  points to = {}
   flatten_code12();
//  points to = {}

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <may be read   >: stdout stdout[0] __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
//             <    is read   >: foo_0
   flatten_code12();

   return 0;
}
