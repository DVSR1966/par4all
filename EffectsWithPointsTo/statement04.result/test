
POINTS-TO

//  {}
int main()
{
//  {}
   int **x, *y, k, h;
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   k = 1;
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   h = 2;
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   y = &h;
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,h,-Exact-)}
   *x = y;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,h,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,h,-Exact-)}
   printf("x = %p \n", x);
//  {(*ANY_MODULE*:*ANYWHERE*_b0,h,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,h,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int **x, *y, k, h;
//             <    is written>: k
   k = 1;
//             <    is written>: h
   h = 2;
//             <    is written>: y
   y = &h;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x y
   *x = y;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("x = %p \n", x);
   return 0;
}

 *********************** Use-Def Chains *********************

	07 --> 08 with conflicts
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <stdout[0]-W-MAY>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <__IO_EFFECTS:LUNS[1]-W-EXACT>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <x-R-EXACT>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <stdout[0]-R-MAY>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <stdout-R-EXACT>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <__IO_EFFECTS:LUNS[1]-R-EXACT>
	06 --> 07 with conflicts
		from <y-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <y-W-EXACT> to <y-R-EXACT>
	05 --> 07 with conflicts
		from <h-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
	04 --> 07 with conflicts
		from <k-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>

