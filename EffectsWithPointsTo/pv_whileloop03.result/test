
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: p
      p = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   while (i<10) {

// Points To:
// p -> &a[*] , MAY
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , MAY

      p = &a[i];

// Points To:
// p -> &a[*] , MAY
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , MAY

      i++;
   }

// Points To:
// p -> &a[*] , MAY
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , MAY

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: p
      p = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   return 0;
}

