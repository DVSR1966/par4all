
POINTS-TO

//  {}
int main(void)
{
//  {}
   int i, j;
//  {}
   int **pp;
   {
//  {(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      int *pi = &i;
//  {(pi,i,-Exact-);(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      pp = &pi;
//  {(pi,i,-Exact-);(pp,pi,-Exact-)}
      **pp = 1;
   }
//  {(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   
   // pp points to a dead value in the stack
   // should generate an error
   **pp = 0;
//  {(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   // segfault?
   *pp = &j;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,j,-Exact-);(pp,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main(void)
{
   int i, j;
   int **pp;
   {
//             <    is written>: pi
      int *pi = &i;
//             <    is written>: pp
      pp = &pi;
//             <    is read   >: pi pp
//             <    is written>: i
      **pp = 1;
   }
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   
   // pp points to a dead value in the stack
   // should generate an error
   **pp = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pp
   // segfault?
   *pp = &j;
   return 0;
}

 *********************** Use-Def Chains *********************

	17 --> 18 with conflicts
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <*ANY_MODULE*:*ANYWHERE*-W-MAY> to <pp-R-EXACT>
		from <*ANY_MODULE*:*ANYWHERE*-R-MAY> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
	12 --> 18 with conflicts
		from <i-W-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <pp-R-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
		from <pi-R-EXACT> to <*ANY_MODULE*:*ANYWHERE*-W-MAY>
