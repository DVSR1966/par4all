
POINTS-TO

//  {}
int main(int argc, char *argv[])
{
//  points to = {}
   int t;
//  points to = {}
   
   
   for(t = 0; t <= 63; t += 1)
//  points to = {}
      compute();
//  points to = {}
   
   
   return 0;
}

PROPER POINTER EFFECTS

int main(int argc, char *argv[])
{
   int t;
//             <    is written>: t
   
   
   for(t = 0; t <= 63; t += 1)
//             <may be written>: tab[*][*]
      compute();
   
   
   return 0;
}
