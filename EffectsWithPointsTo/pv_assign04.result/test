
int main()
{
   mystruct s1, s2;
   mystruct tab_s[2];
//             <may be read   >: s1.b[*]
//             <may be written>: tab_s[0].b[*]
//             <    is read   >: s1.a s1.c
//             <    is written>: tab_s[0].a tab_s[0].c
   tab_s[0] = s1;
   tab_s[1] = s1;
   tab_s[1] = s2;
   return 0;
}

