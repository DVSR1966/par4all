
PROPER  EFFECTS 

int main()
{
   int *p, i;
//             <    is written>: i
   i = 1;
//             <    is read   >: i

   if (i>0)
//             <    is written>: p
      p = &i;
   else
//             <    is written>: p
      p = &i;

   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int *p, i;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   i = 1;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   if (i>0)

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &i;
   else

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &i;

// Points To:
// p -> i , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *p, i;
//             <    is written>: i
   i = 1;
//             <    is read   >: i

   if (i>0)
//             <    is written>: p
      p = &i;
   else
//             <    is written>: p
      p = &i;

   return 0;
}

 *********************** Use-Def Chains *********************

	04 --> 06 with conflicts
		from <i-W-EXACT> to <i-R-EXACT>
	07 --> 10 with conflicts
		from <p-W-EXACT> to <p-W-EXACT>

****************** End of Dependence Graph ******************
