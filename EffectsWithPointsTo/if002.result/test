
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *p, i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   i = 1;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   if (i>0)
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      p = &i;
   else
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      p = &i;
//  {(p,i,-Exact-)}

   return 0;
}

PROPER POINTER EFFECTS

int main()
{
   int *p, i;
//             <    is written>: i
   i = 1;
//             <    is read   >: i

   if (i>0)
//             <    is written>: p
      p = &i;
   else
//             <    is written>: p
      p = &i;

   return 0;
}
