
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int a, aa, *b, *bb, **c;
//  {(b,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(bb,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(c,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}

   a = 1;
//  {(b,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(bb,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(c,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   b = &a;
//  {(b,a,-Exact-);(bb,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(c,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   c = &b;
//  {(b,a,-Exact-);(bb,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(c,b,
//   -Exact-)}

   bb = *c;
//  {(b,a,-Exact-);(bb,a,-Exact-);(c,b,-Exact-)}
   aa = **c;
//  {(b,a,-Exact-);(bb,a,-Exact-);(c,b,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int a, aa, *b, *bb, **c;
//             <    is written>: a

   a = 1;
//             <    is written>: b
   b = &a;
//             <    is written>: c
   c = &b;
//             <    is read   >: b c
//             <    is written>: bb

   bb = *c;
//             <    is read   >: a b c
//             <    is written>: aa
   aa = **c;
   return 0;
}
