
void dereferencing06()
{
//             <    is written>: x
   double x = 1.;
//             <    is written>: p
   double *p = &x;
//             <    is read   >: p x
//             <    is written>: p
   
   /* This read has no effect unless p == 0 and the compiler does not optimize? */
   *p++;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p++ = 1;
}
int main()
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   dereferencing06();
}

 *********************** Use-Def Chains *********************


****************** End of Dependence Graph ******************
