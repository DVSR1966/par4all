
POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *a, *b, *c;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(b,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(c,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   int i;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(b,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(c,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   i = 0;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(b,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(c,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   /* Allocation dynamiques des tableaux */
   a = (int *) malloc(20*sizeof(int));
//  {(a,*HEAP*_l_9,-Exact-);(b,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(c,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   b = (int *) malloc(20*sizeof(int));
//  {(a,*HEAP*_l_9,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   c = (int *) malloc(20*sizeof(int));
//  {(a,*HEAP*_l_9,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
   /* Creation d'alias entre a et b */
   a = b;
//  {(a,*HEAP*_l_10,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
   
   /* Suppression de l'ancien alias entre a et b, creation d'un nouveau
    * entre a et c */
   a = c;
//  {(a,*HEAP*_l_11,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
   
   /* Initialisation du tableau b */
   for(i = 0; i <= 19; i += 1)
//  {(a,*HEAP*_l_11,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
      b[i] = i;
//  {(a,*HEAP*_l_11,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
   
   /* Initialisation du tableau c */
   for(i = 0; i <= 19; i += 1)
//  {(a,*HEAP*_l_11,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
      c[i] = 1;
//  {(a,*HEAP*_l_11,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
   /* Initialisation du tableau a */
   for(i = 0; i <= 19; i += 1)
//  {(a,*HEAP*_l_11,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
      a[i] = b[i];
//  {(a,*HEAP*_l_11,-Exact-);(b,*HEAP*_l_10,-Exact-);(c,*HEAP*_l_11,
//   -Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *a, *b, *c;
   int i;
//             <    is written>: i
   i = 0;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a _MALLOC_EFFECTS:_MALLOC_
   /* Allocation dynamiques des tableaux */
   a = (int *) malloc(20*sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: b _MALLOC_EFFECTS:_MALLOC_
   b = (int *) malloc(20*sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: c _MALLOC_EFFECTS:_MALLOC_
   c = (int *) malloc(20*sizeof(int));
//             <    is read   >: b
//             <    is written>: a
   /* Creation d'alias entre a et b */
   a = b;
//             <    is read   >: c
//             <    is written>: a
   
   /* Suppression de l'ancien alias entre a et b, creation d'un nouveau
    * entre a et c */
   a = c;
//             <    is written>: i
   
   /* Initialisation du tableau b */
   for(i = 0; i <= 19; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: b i
      b[i] = i;
//             <    is written>: i
   
   /* Initialisation du tableau c */
   for(i = 0; i <= 19; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: c i
      c[i] = 1;
//             <    is written>: i
   /* Initialisation du tableau a */
   for(i = 0; i <= 19; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      a[i] = b[i];
   return 0;
}
