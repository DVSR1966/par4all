
Points-to for call17


// Points To IN:
// x -> *NULL_POINTER* , MAY
// x -> _x_1[0] , MAY


// Points To OUT: none

void call17(int *x)
{
   /* check aliases */

// Points To:
// x -> *NULL_POINTER* , MAY
// x -> _x_1[0] , MAY

   int *y = x;

// Points To:
// x -> *NULL_POINTER* , MAY
// x -> _x_1[0] , MAY
// y -> *NULL_POINTER* , MAY
// y -> _x_1[0] , MAY

   y[0] = 1;

// Points To:
// x -> *NULL_POINTER* , MAY
// x -> _x_1[0] , MAY
// y -> _x_1[0] , EXACT

   return;
}

Proper effects with points-to for call17

void call17(int *x)
{
   /* check aliases */
//             <    is read   >: x
//             <    is written>: y
   int *y = x;
//             <may be written>: _x_1[0]
//             <    is read   >: y
   y[0] = 1;
   return;
}

