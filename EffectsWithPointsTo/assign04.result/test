
PROPER  EFFECTS 

void assign04()
{
   int ***p;
   int **q;
   int *r;
   int i;
//             <    is written>: r

   r = &i;
//             <    is written>: p
   //q = &r;
   p = &q;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   **p = r;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   ***p = 0;
}
/* To be out of Emami's patterns */

void assign04();

void foo();
void foo()
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   assign04();
}

POINTS-TO

//  {}
void assign04()
{
//  {}
   int ***p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int **q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   int *r;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b2,
//   -Exact-)}
   int i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b2,
//   -Exact-)}

   r = &i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(r,i,-Exact-)}
   //q = &r;
   p = &q;
//  {(p,q,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(r,i,
//   -Exact-)}
   **p = r;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,i,-Exact-);(p,q,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(r,i,-Exact-)}
   ***p = 0;
}
//  {}
/* To be out of Emami's patterns */

//  {}
void assign04();

//  {}
void foo();
//  {}
void foo()
{
//  {}
   assign04();
}

PROPER  EFFECTS WITH POINTS TO

void assign04()
{
   int ***p;
   int **q;
   int *r;
   int i;
//             <    is written>: r

   r = &i;
//             <    is written>: p
   //q = &r;
   p = &q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p q r
   **p = r;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   ***p = 0;
}
/* To be out of Emami's patterns */

void assign04();

void foo();
void foo()
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   assign04();
