
PROPER  EFFECTS 

int main()
{
   int i;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: i
   i = 0;
//             <    is read   >: i
   do {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      *p++ = 2**q++;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p, *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = &a[0];

// Points To:
// p -> a[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = &b[0];

// Points To:
// p -> a[0] , EXACT
// q -> b[0] , EXACT

   i = 0;

// Points To:
// p -> a[0] , EXACT
// q -> b[0] , EXACT

   do {

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

      *p++ = 2**q++;

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

      i++;
   }
   while (i<10);

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: i
   i = 0;
//             <    is read   >: i
   do {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      *p++ = 2**q++;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

