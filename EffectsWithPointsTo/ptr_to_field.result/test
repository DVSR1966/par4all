int ptr_to_field()
{
   typedef struct {                                                     /*0008*/
      int *q;
      int *p;
   } my_str;

   my_str *m, *n;                                                       /*0010*/
//             <    is written>: i j
   int i = 0, j = 1;                                                    /*0011*/
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: m _MALLOC_EFFECTS:_MALLOC_
   m = (my_str *) malloc(sizeof(my_str));                               /*0012*/
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: n _MALLOC_EFFECTS:_MALLOC_
   n = (my_str *) malloc(sizeof(my_str));                               /*0013*/
//             <    is read   >: m
//             <    is written>: *HEAP*_l_12.q
   m->q = &i;                                                           /*0014*/
//             <    is read   >: m
//             <    is written>: *HEAP*_l_12.p
   m->p = &j;                                                           /*0015*/
//             <    is read   >: *HEAP*_l_12.p m n
//             <    is written>: *HEAP*_l_13.q
   n->q = m->p;                                                         /*0016*/
//             <    is read   >: *HEAP*_l_12.q m n
//             <    is written>: *HEAP*_l_13.p
   n->p = m->q;                                                         /*0017*/

   return 0;                                                            /*0019*/
}

