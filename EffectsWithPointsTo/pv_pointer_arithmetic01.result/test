
POINTS-TO

//  {}
int main()
{
//  {}
   int a[10];
//  {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p = &a[0];
//  {(p,a[0],-Exact-)}
   p = p+1;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   p = p+3;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   p = p-1;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   p = p-2;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int a[10];
   int *p;
//             <    is written>: p
   p = &a[0];
//             <    is read   >: p
//             <    is written>: p
   p = p+1;
//             <    is read   >: p
//             <    is written>: p
   p = p+3;
//             <    is read   >: p
//             <    is written>: p
   p = p-1;
//             <    is read   >: p
//             <    is written>: p
   p = p-2;
   return 0;
}

