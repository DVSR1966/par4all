
PROPER  EFFECTS 

int main()
{
   int **x, **y, *z, *t;
   int i, j;
//             <    is written>: i
   i = 1;
//             <    is written>: j
   j = 2;
//             <    is written>: z

   z = &i;
//             <    is written>: t
   t = &j;
//             <    is read   >: t
//             <    is written>: z

   z = t;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x z
   *x = z;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf(" x = %p\n", x);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int **x, **y, *z, *t;
//  {(t,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(z,*ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   int i, j;
//  {(t,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(z,*ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   i = 1;
//  {(t,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(z,*ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   j = 2;
//  {(t,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(z,*ANY_MODULE*:*NOWHERE*_b1,-Exact-)}

   z = &i;
//  {(t,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(z,i,-Exact-)}
   t = &j;
//  {(t,j,-Exact-);(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(z,i,-Exact-)}

   z = t;
//  {(t,j,-Exact-);(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(z,j,-Exact-)}
   *x = z;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,j,-Exact-);(t,j,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(z,j,-Exact-)}
   printf(" x = %p\n", x);
//  {(*ANY_MODULE*:*ANYWHERE*_b0,j,-Exact-);(t,j,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(y,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(z,j,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int **x, **y, *z, *t;
   int i, j;
//             <    is written>: i
   i = 1;
//             <    is written>: j
   j = 2;
//             <    is written>: z

   z = &i;
//             <    is written>: t
   t = &j;
//             <    is read   >: t
//             <    is written>: z

   z = t;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x z
   *x = z;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf(" x = %p\n", x);
   return 0;
}

