
POINTS-TO


// Points To IN:
// _bqq_2 -> *NULL_POINTER* , MAY
// _bqq_2 -> _bqq_2_2 , MAY
// bpp -> *NULL_POINTER* , MAY
// bpp -> _bpp_1 , MAY
// bqq -> *NULL_POINTER* , MAY
// bqq -> _bqq_2 , MAY


// Points To OUT:
// _bpp_1 -> *NULL_POINTER* , MAY
// _bpp_1 -> _bqq_2_2 , MAY
// _bqq_2 -> *NULL_POINTER* , MAY
// _bqq_2 -> _bqq_2_2 , MAY

void bar(int **bpp, int **bqq)
{

// Points To:
// _bqq_2 -> *NULL_POINTER* , MAY
// _bqq_2 -> _bqq_2_2 , MAY
// bpp -> *NULL_POINTER* , MAY
// bpp -> _bpp_1 , MAY
// bqq -> *NULL_POINTER* , MAY
// bqq -> _bqq_2 , MAY

   *bpp = *bqq;

// Points To:
// _bpp_1 -> *NULL_POINTER* , MAY
// _bpp_1 -> _bqq_2_2 , MAY
// _bqq_2 -> *NULL_POINTER* , MAY
// _bqq_2 -> _bqq_2_2 , MAY
// bpp -> _bpp_1 , EXACT
// bqq -> _bqq_2 , EXACT

   printf("pointers exchanged");

// Points To:
// _bpp_1 -> *NULL_POINTER* , MAY
// _bpp_1 -> _bqq_2_2 , MAY
// _bqq_2 -> *NULL_POINTER* , MAY
// _bqq_2 -> _bqq_2_2 , MAY
// bpp -> _bpp_1 , EXACT
// bqq -> _bqq_2 , EXACT

   return;
}

PROPER  EFFECTS WITH POINTS TO

void bar(int **bpp, int **bqq)
{
//             <may be read   >: _bqq_2
//             <may be written>: _bpp_1
//             <    is read   >: bpp bqq
   *bpp = *bqq;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("pointers exchanged");
   return;
}

