POINTS-TO


// Points To IN: none


// Points To OUT: none

void foo()
{

// Points To: none

   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;

// Points To:
// p -> i , EXACT
// pp -> p , EXACT
// q -> j , EXACT
// qq -> q , EXACT


   bar(pp, qq);

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> j , MAY
// pp -> p , EXACT
// q -> j , EXACT
// qq -> q , EXACT

   return;
}

PROPER  EFFECTS WITH POINTS TO

void foo()
{
//             <    is written>: i j p pp q qq
   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;
//             <may be read   >: q stdout[0]
//             <may be written>: p stdout[0]
//             <    is read   >: pp qq stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   bar(pp, qq);
   return;
}

CUMULATED  EFFECTS WITH POINTS TO

//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
void foo()
{
//             <    is written>: i j p pp q qq
   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;
//             <may be read   >: q stdout[0]
//             <may be written>: p stdout[0]
//             <    is read   >: pp qq stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   bar(pp, qq);
   return;
}
