// psrc1 -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT
// psrc2 -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   fgINT32 v1;

// Points To:
// data1 -> _data1_2[0] , EXACT
// data2 -> _data2_3[0] , EXACT
// datdest -> _datdest_1[0] , EXACT
// pdest -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT
// psrc1 -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT
// psrc2 -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   fgINT32 v2;

// Points To:
// data1 -> _data1_2[0] , EXACT
// data2 -> _data2_3[0] , EXACT
// datdest -> _datdest_1[0] , EXACT
// pdest -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT
