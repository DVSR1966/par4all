
POINTS-TO

//  {}
int global01()
{
//  points to = {}
   i = &k;
//  {(i,k,-Exact-)}
   j = i;
//  {(i,k,-Exact-);(j,k,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int global01()
{
//             <    is written>: i
   i = &k;
//             <    is read   >: i
//             <    is written>: j
   j = i;
   return 0;
}
