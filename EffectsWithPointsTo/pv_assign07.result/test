// Points To: none

int main()
{

// Points To: none

   mystruct s1, s2, *s1p, *s2p;

// Points To:
// s1.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s2.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int i = 1;

// Points To:
// s1.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s2.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int j = 2;

// Points To:
// s1.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s2.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int k = 3;

// Points To:
// s1.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s2.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   s1p = &s1;

// Points To:
// s1.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1p -> s1 , EXACT
// s2.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   s2p = &s1;

// Points To:
// s1.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1p -> s1 , EXACT
// s2.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2p -> s1 , EXACT


   s1.a = &i;

// Points To:
// s1.a -> i , EXACT
// s1p -> s1 , EXACT
// s2.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2p -> s1 , EXACT

   s2.a = &j;

// Points To:
// s1.a -> i , EXACT
// s1p -> s1 , EXACT
// s2.a -> j , EXACT
// s2p -> s1 , EXACT


   s1p->a = s2.a;

// Points To:
// s1.a -> j , EXACT
// s1p -> s1 , EXACT
// s2.a -> j , EXACT
// s2p -> s1 , EXACT

   s2p->a = &k;

// Points To:
// s1.a -> k , EXACT
// s1p -> s1 , EXACT
// s2.a -> j , EXACT
// s2p -> s1 , EXACT


   s2p = &s2;

// Points To:
// s1.a -> k , EXACT
// s1p -> s1 , EXACT
// s2.a -> j , EXACT
// s2p -> s2 , EXACT

   return 0;
}

