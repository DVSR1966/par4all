
POINTS-TO

//  {}
void point_to03()
{
//  {}
   typedef double a_t[10][20];
//  {}
   a_t a;
//  {}
   a_t *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = &a;
//  {(p,a,-Exact-)}
   ((*p)[2])[3] = 1.5;
}

PROPER  EFFECTS WITH POINTS TO

void point_to03()
{
   typedef double a_t[10][20];
   a_t a;
   a_t *p;
//             <    is written>: p

   p = &a;
//             <    is read   >: p
//             <    is written>: a[2][3]
   ((*p)[2])[3] = 1.5;
}

 *********************** Use-Def Chains *********************

	07 --> 08 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>

