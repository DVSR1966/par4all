   printf("Exit with %d", j);
   return i;
}

Controlizer output (return as a function)

int for_loop09()
{
   int i, j = 0;
   for(i = 0; i <= 6; i += 1) {
      if (i==3) {
         printf("%d", i);
         return i;
      }
      j++;
   }
   printf("Exit with %d", j);
   return i;
}

Preconditions (return as a function)


//  P() {}

int for_loop09()
{

//  P() {}

   int i, j = 0;

//  P(i,j) {j==0}

   for(i = 0; i <= 6; i += 1) {

//  P(for_loop09,i,j) {0<=i, i<=6}

      if (i==3) {

//  P(for_loop09,i,j) {i==3}

         printf("%d", i);

//  P(for_loop09,i,j) {i==3}

         return i;
      }

//  P(for_loop09,i,j) {0<=i, i<=6}

      j++;
   }

//  P(for_loop09,i,j) {i==7}

   printf("Exit with %d", j);

//  P(for_loop09,i,j) {i==7}

   return i;
}

Parser output

int for_loop09()
{
   int i, j = 0;
   //PIPS generated variable
   int for_loop09;
   for (i = 0;i!=5;i++) {
      if (i==3) {
         printf("%d", i);
         for_loop09 = i;
         return for_loop09;
      }
      j++;
   }
   printf("Exit with %d", j);
   for_loop09 = i;
   return for_loop09;
   return for_loop09;
}

Controlizer output (return as a goto)

int for_loop09()
{
   int i, j = 0;
   //PIPS generated variable
   int for_loop09;
   i = 0;
l99997:   ;
   if (i!=5) goto l99999;
   if (i==3) goto l99998;
   j++;
   i++;
   goto l99997;
l99998:   ;
   printf("%d", i);
   for_loop09 = i;
   return for_loop09;
l99999:   ;
   printf("Exit with %d", j);
   for_loop09 = i;
   return for_loop09;
}

Preconditions (return as a goto)


//  P() {}

int for_loop09()
{

//  P() {}

   int i, j = 0;
   //PIPS generated variable

//  P(i,j) {j==0}

   int for_loop09;

//  P(for_loop09,i,j) {j==0}

   i = 0;
l99997:   ;

//  P(for_loop09,i,j) {i==j, 0<=i, i<=6}

   if (i!=5) goto l99999;

//  P(for_loop09,i,j) {i==5, j==5}

   if (i==3) goto l99998;

//  P(for_loop09,i,j) {i==5, j==5}

   j++;

//  P(for_loop09,i,j) {i==5, j==6}

   i++;
   goto l99997;
l99998:   ;

//  P() {0==-1}

   printf("%d", i);

//  P() {0==-1}

   for_loop09 = i;
   return for_loop09;
l99999:   ;

//  P(for_loop09,i,j) {i==j, 0<=i, i<=6}

   printf("Exit with %d", j);

//  P(for_loop09,i,j) {i==j, 0<=i, i<=6}

   for_loop09 = i;

//  P(for_loop09,i,j) {for_loop09==i, for_loop09==j, 0<=for_loop09,
//    for_loop09<=6}

   return for_loop09;
}
