   printf("Begin\n");

//  P() {i==5}

   if ((rand()&1)==0) goto b;

//  P() {i==5}

   
   /* The a label */
   
a:   printf("a\n");

//  P() {i==5}

   /* The b label */
   ;

//  P() {i==5}

   /* The b label */
b:   printf("b\n");

//  P() {i==5}

   /* Oh, go to a... */
   ;
   goto a;
