int main()
{
   int i = 5;
   printf("Begin\n");
   if ((rand()&1)==0) goto l99999;
   
   /* The a label */
a:   printf("a\n");
   /* The b label */
   ;
   /* The b label */
b:   printf("b\n");
   /* Oh, go to a... */
   goto a;
l99999:   ;
   goto b;
}

//  P() {}

int main()
{
   int i = 5;

//  P() {}

   printf("Begin\n");

//  P() {}

   if ((rand()&1)==0) goto l99999;

//  P() {}

   
   /* The a label */
a:   printf("a\n");
   /* The b label */
   ;

//  P() {}

   /* The b label */
b:   printf("b\n");
   /* Oh, go to a... */
   goto a;
l99999:   ;
   goto b;
}

//  P() {}

int main()
{
   int i = 5;

//  P() {i==5}

   printf("Begin\n");

//  P() {i==5}

   if ((rand()&1)==0) goto b;

//  P() {i==5}

   
   /* The a label */
a:   printf("a\n");
   /* The b label */

//  P() {i==5}

   /* The b label */
b:   printf("b\n");
   /* Oh, go to a... */
   goto a;
}
