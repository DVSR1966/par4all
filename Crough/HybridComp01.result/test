      PROGRAM HYBRIDCOMP01
      INTEGER I
      INTEGER J
      INTEGER A(5, 5)
      CALL TOTO(A)                                                      0005
      PRINT *, A                                                        0010
      END
/* C pretty print for module TOTO. */
#include "math.h"
#include "stdlib.h"
void
toto_ (int a[5][5])
{
  int j;
  int i;

  for (i = 1; i <= 5; i++)
    {
      for (j = 1; j <= 5; j++)
	{
	  a[j - 1][i - 1] = (i * j);
	}
      continue;
    }
  return;
}

