Code after outlining

      PROGRAM HYBRIDCOMP13
      INTEGER I
      INTEGER J
      REAL*8 A(5, 5)
      REAL*8 CST(5)
      CST(1) = 1.0
      CST(2) = 2.0
      CST(3) = 3.0
      CST(4) = 4.0
      CST(5) = 5.0
      CALL TOTO(A, CST)
      PRINT *, A
      END
/* C pretty print for module TOTO. */
#include "math.h"
#include "stdlib.h"
void
toto_ (double a[5][5], double cst[5])
{
  int j;
  int i;

  for (i = 1; i <= 5; i++)
    {
      for (j = 1; j <= 5; j++)
	{
	  a[j - 1][i - 1] = (cst[i - 1] * 2.4e38);
	}
      continue;
    }
  return;
}

