/* C pretty print for module TOTO. */
#include "p4a_crough_types.h"

void
toto_ (p4a_int a[5][5])
{
  p4a_int j;
  p4a_int i;

  for (i = 1; i <= 5; i++)
    {
      for (j = 1; j <= 5; j++)
	{
	  a[j - 1][i - 1] = (i * 1147483648);
	}
      continue;
    }
  return;
}

