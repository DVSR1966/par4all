

extern void atp(float *cdens[2]);

extern int main(int argc, char **argv);
void atp(float *cdens[2])
{
   int i;
   for(i = 0; i <= 1073741823; i += 1) {
      cdens[i][0] = 1;
      cdens[i][1] = 0;
   }
}
int main(int argc, char **argv)
{
   float cdens[1024][2];
   atp(cdens);
}


extern void atp(float *cdens[2]);

extern int main(int argc, char **argv);
void atp(float *cdens[2])
{
   int i;
   for(i = 0; i <= 1073741823; i += 1) {
      *((float *)cdens+i) = 1;
      *((float *)cdens+1+i) = 0;
   }
}
int main(int argc, char **argv)
{
   float cdens[2048];
   atp(cdens);
}
