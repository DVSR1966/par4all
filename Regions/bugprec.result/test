
C  P() {}

!
! PRECISION BUG IN MUST_REGIONS
!
! first test case is tagged MAY
! bug second loop case is tagged EXACT, 
! although it should be the same.
!
      PROGRAM BUGPREC
      REAL A(10)
      INTEGER I, J, N, P

C  P() {}


      DO I = 1, 10                                                      0001

C  P(I) {1<=I, I<=10}

         A(I) = I                                                       0002
      ENDDO

C  P(I) {I==11}


      READ *, N, P, I, J                                                0003

C  P(I,J,N,P) {}


!ABC if (1.le.p.and.(n.lt.0.or.n.gt.7)) stop
      IF (1.LE.P) THEN                                                  0004

C  P(I,J,N,P) {1<=P}

         DO I = N, N+3                                                  0005

C  P(I,J,N,P) {N<=I, I<=N+3, 1<=P}

            A(I) = 10                                                   0006
         ENDDO
      ENDIF

C  P(I,J,N,P) {}


!ABC if (1.le.p.and.(n.lt.0.or.n.gt.7)) stop
      DO J = 1, P                                                       0007

C  P(I,J,N,P) {1<=J, J<=P}

         DO I = N, N+3                                                  0008

C  P(I,J,N,P) {N<=I, I<=N+3, 1<=J, J<=P}

            A(I) = 10                                                   0009
         ENDDO
      ENDDO

      END

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==5}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==5}>

!
! PRECISION BUG IN MUST_REGIONS
!
! first test case is tagged MAY
! bug second loop case is tagged EXACT, 
! although it should be the same.
!
      PROGRAM BUGPREC
      REAL A(10)
      INTEGER I, J, N, P

C  <A(PHI1)-W-EXACT-{1<=PHI1, PHI1<=10}>


      DO I = 1, 10                                                      0001

C  <A(PHI1)-W-EXACT-{PHI1==I, 1<=I, I<=10}>

         A(I) = I                                                       0002
      ENDDO

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==5}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==5}>


      READ *, N, P, I, J                                                0003

C  <A(PHI1)-W-MAY-{N<=PHI1, PHI1<=3+N, 1<=P}>


!ABC if (1.le.p.and.(n.lt.0.or.n.gt.7)) stop
      IF (1.LE.P) THEN                                                  0004

C  <A(PHI1)-W-EXACT-{N<=PHI1, PHI1<=3+N, 1<=P}>

         DO I = N, N+3                                                  0005

C  <A(PHI1)-W-EXACT-{PHI1==I, N<=I, I<=3+N, 1<=P}>

            A(I) = 10                                                   0006
         ENDDO
      ENDIF

C  <A(PHI1)-W-EXACT-{N<=PHI1, PHI1<=3+N, 1<=P}>


!ABC if (1.le.p.and.(n.lt.0.or.n.gt.7)) stop
      DO J = 1, P                                                       0007

C  <A(PHI1)-W-EXACT-{N<=PHI1, PHI1<=3+N, 1<=J, J<=P}>

         DO I = N, N+3                                                  0008

C  <A(PHI1)-W-EXACT-{PHI1==I, N<=I, I<=3+N, 1<=J, J<=P}>

            A(I) = 10                                                   0009
         ENDDO
      ENDDO

      END
