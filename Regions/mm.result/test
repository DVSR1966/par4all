--- Regions for MM

C  <A(PHI1,PHI2)-R-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2}>
C  <A(PHI1,PHI2)-W-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2}>
C  <B(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=N, 1<=PHI2, PHI2<=N}>
C  <C(PHI1,PHI2)-R-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2}>

C
      SUBROUTINE MM(N, A, B, C)
      INTEGER N
      REAL A(N, *), B(N, *), C(N, *)

C  <A(PHI1,PHI2)-R-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2}>
C  <A(PHI1,PHI2)-W-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2}>
C  <B(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=N, 1<=PHI2, PHI2<=N}>
C  <C(PHI1,PHI2)-R-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2}>

C
      DO I = 1, N                                                       0001

C  <A(PHI1,PHI2)-R-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2, 1<=I, I<=N}>
C  <A(PHI1,PHI2)-W-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2, 1<=I, I<=N}>
C  <B(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=N, 1<=PHI2, PHI2<=N, 1<=I,
C    I<=N}>
C  <C(PHI1,PHI2)-R-MAY-{1<=PHI1, PHI1<=N, 1<=PHI2, 1<=I, I<=N}>

         CALL SMXPY(N, A(1,I), C(1,I), B)                               0002
      ENDDO
C
      END
