
C  <A(PHI1,PHI2)-R-MAY-{PHI1==J, PHI2==I, 1+I<=J}>
C  <A(PHI1,PHI2)-W-MAY-{PHI1==I, PHI2==J, 1+I<=J}>


      SUBROUTINE TEST(A,I,J)
      INTEGER A(10,10), I, J

C  <A(PHI1,PHI2)-R-MAY-{PHI1==J, PHI2==I, 1+I<=J}>
C  <A(PHI1,PHI2)-W-MAY-{PHI1==I, PHI2==J, 1+I<=J}>


      IF (I.LT.J) THEN                                                  0001

C  <A(PHI1,PHI2)-R-EXACT-{PHI1==J, PHI2==I, 1+I<=J}>
C  <A(PHI1,PHI2)-W-EXACT-{PHI1==I, PHI2==J, 1+I<=J}>

         A(I,J) = A(J,I)                                                0002
      ENDIF
      END
      PROGRAM IF1
      INTEGER A(10,10), I, J 

C  <A(PHI1,PHI2)-R-MAY-{PHI1<=10, 1<=PHI2, PHI2<=10, PHI2+1<=PHI1}>
C  <A(PHI1,PHI2)-W-MAY-{1<=PHI1, 1<=PHI2, PHI2<=10, PHI1+1<=PHI2}>


      DO I = 1, 10                                                      0001

C  <A(PHI1,PHI2)-R-MAY-{I==PHI2, PHI1<=10, 1<=PHI2, PHI2<=10,
C    PHI2+1<=PHI1}>
C  <A(PHI1,PHI2)-W-MAY-{I==PHI1, 1<=PHI1, 1<=PHI2, PHI2<=10,
C    PHI1+1<=PHI2}>

         DO J = 1, 10                                                   0002

C  <A(PHI1,PHI2)-R-MAY-{J==PHI1, I==PHI2, PHI1<=10, 1<=PHI2, PHI2<=10,
C    PHI2+1<=PHI1}>
C  <A(PHI1,PHI2)-W-MAY-{I==PHI1, J==PHI2, 1<=PHI1, 1<=PHI2, PHI2<=10,
C    PHI1+1<=PHI2}>

            CALL TEST(A, I, J)                                          0003
         ENDDO
      ENDDO

      END
      PROGRAM IF1
      INTEGER A(10,10), I, J 

      DOALL I = 1, 10                                                   0001
         DOALL J = 1, 10                                                0002
            CALL TEST(A, I, J)                                          0003
         ENDDO
      ENDDO

      END
