
C  <__IO_EFFECTS:LUNS(PHI1)-R-MAY-{0==-1}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-MAY-{0==-1}>

      FUNCTION  LORDRE( L1 )
      INTEGER L1, LORDRE, I
      LORDRE = 0                                                        0003
      I = 1                                                             0004
99998 CONTINUE
C     DO loop 10 with exit had to be desugared
      IF (I.GT.L) GOTO 99999                                            0004
      IF (L1.EQ.I) GOTO 15                                              0005
10    CONTINUE                                                          0006
      I = I+1                                                           0004
      GOTO 99998
99999 CONTINUE

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==0, LORDRE==0, L+1<=I}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==0, LORDRE==0, L+1<=I}>

      STOP 100                                                          0007
15    CONTINUE                                                          0008
      LORDRE = I                                                        0008
      END

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6}>

! next line added by fsplit() in pips
      PROGRAM MAIN000

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6}>

      PRINT *, LORDRE(0)                                                0003
      END

