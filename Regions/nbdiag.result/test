
C  <A(PHI1,PHI2)-IN-EXACT-{1<=PHI1, PHI1<=3NP, 1<=PHI2, PHI2<=3NP}>
C  <B(PHI1,PHI2)-IN-EXACT-{1<=PHI1, PHI1<=3NP, 1<=PHI2, PHI2<=3NP}>
C  <RL(PHI1,PHI2)-IN-MAY-{PHI2==1, 1<=NP}>

C***********************************************************************
C Excerpt from Cr2cnf from ONERA
C Purpose: in regions of Z2
C***********************************************************************
      SUBROUTINE NBDIAG(A,B,RL,NP)
      IMPLICIT REAL*8(A-H,O-Z)
C  ROUTINE DE CALCUL DE A=A-TB*DIAG(RL)*B .
      COMMON/TOTO/Z1(640),Z2(640),Z3(640)
      DIMENSION A(N,N),B(N,N),RL(N,N)
C     added to simulate actual preconditions
      N = 3*NP                                                          0001

C  <RL(PHI1,PHI2)-IN-MAY-{PHI2==1, N==3NP, 1<=NP}>

      DO 1 K = 1, N                                                     0002

C  <RL(PHI1,PHI2)-IN-MAY-{PHI2==1, N==3NP, 1<=K, K<=3NP}>

1        Z1(K) = RL(K*(N+1)-N,1)                                        0003

C  <A(PHI1,PHI2)-IN-EXACT-{1<=PHI1, PHI1<=3NP, 1<=PHI2, PHI2<=3NP,
C    N==3NP}>
C  <B(PHI1,PHI2)-IN-EXACT-{1<=PHI1, PHI1<=3NP, 1<=PHI2, PHI2<=3NP,
C    N==3NP, 1<=NP}>
C  <Z1(PHI1)-IN-EXACT-{1<=PHI1, PHI1<=3NP, N==3NP, 1<=NP}>

      DO 2 J = 1, N                                                     0004

C  <B(PHI1,PHI2)-IN-EXACT-{PHI2==J, 1<=PHI1, PHI1<=3NP, N==3NP, 1<=J,
C    J<=3NP}>
C  <Z1(PHI1)-IN-EXACT-{1<=PHI1, PHI1<=3NP, N==3NP, 1<=J, J<=3NP}>

         DO 3 K = 1, N                                                  0005

C  <B(PHI1,PHI2)-IN-EXACT-{PHI1==K, PHI2==J, N==3NP, 1<=J, J<=3NP,
C    1<=K, K<=3NP}>
C  <Z1(PHI1)-IN-EXACT-{PHI1==K, N==3NP, 1<=J, J<=3NP, 1<=K, K<=3NP}>

3           Z2(K) = Z1(K)*B(K,J)                                        0006

C  <A(PHI1,PHI2)-IN-EXACT-{J==PHI2, 1<=PHI1, PHI1<=3NP, 3NP==N, 1<=J,
C    J<=3NP, 1<=NP}>
C  <B(PHI1,PHI2)-IN-EXACT-{1<=PHI1, PHI1<=3NP, 1<=PHI2, PHI2<=3NP,
C    N==3NP, 1<=J, J<=3NP, 1<=NP}>
C  <Z2(PHI1)-IN-EXACT-{1<=PHI1, PHI1<=3NP, N==3NP, 1<=J, J<=3NP,
C    1<=NP}>

         DO 4 K = 1, N, 3                                               0007

C  <A(PHI1,PHI2)-IN-EXACT-{J==PHI2, 1<=PHI1, PHI1<=3NP, 3NP==N, 1<=J,
C    J<=3NP, 1<=K, K<=3NP}>
C  <B(PHI1,PHI2)-IN-EXACT-{K<=PHI1, PHI1<=2+K, 1<=PHI2, PHI2<=3NP,
C    N==3NP, 1<=J, J<=3NP, 1<=K, K<=3NP}>
C  <Z2(PHI1)-IN-EXACT-{K<=PHI1, PHI1<=2+K, N==3NP, 1<=J, J<=3NP, 1<=K,
C    K<=3NP, 1<=NP}>

            DO 4 I = 1, N                                               0008

C  <A(PHI1,PHI2)-IN-EXACT-{PHI1==I, PHI2==J, N==3NP, 1<=I, I<=3NP,
C    1<=J, J<=3NP, 1<=K, K<=3NP}>
C  <B(PHI1,PHI2)-IN-EXACT-{PHI2==I, K<=PHI1, PHI1<=2+K, N==3NP, 1<=I,
C    I<=3NP, 1<=J, J<=3NP, 1<=K, K<=3NP}>
C  <Z2(PHI1)-IN-EXACT-{K<=PHI1, PHI1<=2+K, N==3NP, 1<=I, I<=3NP, 1<=J,
C    J<=3NP, 1<=K, K<=3NP}>

4              A(I,J) = A(I,J)-B(K,I)*Z2(K)-B(K+1,I)*Z2(K+1)-B(K+2,I)   0009
     &         *Z2(K+2)                                                 0009
2        CONTINUE                                                       0010
      END
