}

Printed file for "switch03"

int switch03(int predicate)
{
   int x = 0;
   if (predicate==0) goto switch_1_case_0;
   if (predicate==1) goto switch_1_case_1;
   if (predicate==2) goto switch_1_case_2;
   if (predicate==3) {
      goto switch_1_case_3;
   }
   else {
      goto switch_1_case_3;
   }
switch_1_case_2:   ;
   return x+3;
switch_1_case_1:   ;
   x = x+1;
   goto switch_1_case_2;
switch_1_case_0:   ;
   return 111;
switch_1_case_3:   ;
break_1:   ;
   return 333;
}
