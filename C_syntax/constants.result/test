
Parser output

main()
{
   int i;
   int j;
   int k;
   int l;
   i = 015;
   // octal constant
   j = 0x1ae;
   // hexadecimal
   k = 890L;
   // long constant
   l = i+j+k;
}

Controlizer output

main()
{
   int i;
   int j;
   int k;
   int l;
   i = 015;
   // octal constant
   j = 0x1ae;
   // hexadecimal
   k = 890L;
   // long constant
   l = i+j+k;
}

Preconditions


//  P() {}

main()
{
   int i;
   int j;
   int k;
   int l;

//  P() {}

   i = 015;

//  P(0`i) {0`i==13}

   // octal constant
   j = 0x1ae;

//  P(0`i,0`j) {0`i==13, 0`j==430}

   // hexadecimal
   k = 890L;

//  P(0`i,0`j,0`k) {0`i==13, 0`j==430, 0`k==890}

   // long constant
   l = i+j+k;
}

