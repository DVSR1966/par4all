
Printed file for main

main()
{
   char c1;
   unsigned char c2;
   signed char c3;
   int i1;
   int i2;
   int i3;
   int r;

   c1 = '\\';
   c2 = '\'';
   c3 = '\012';

   i1 = c1;
   i2 = c2;
   i3 = c3;

   r = i1+i2+i3;
}

Preconditions for main


//  P() {}

main()
{
   char c1;
   unsigned char c2;
   signed char c3;
   int i1;
   int i2;
   int i3;
   int r;

//  P() {}


   c1 = '\\';

//  P(0`c1) {0`c1==92}

   c2 = '\'';

//  P(0`c1,0`c2) {0`c1==92, 0`c2==39}

   c3 = '\012';

//  P(0`c1,0`c2,0`c3) {0`c1==92, 0`c2==39, 0`c3==10}


   i1 = c1;

//  P(0`c1,0`c2,0`c3,0`i1) {0`c1==92, 0`c2==39, 0`c3==10, 0`i1==92}

   i2 = c2;

//  P(0`c1,0`c2,0`c3,0`i1,0`i2) {0`c1==92, 0`c2==39, 0`c3==10,
//    0`i1==92, 0`i2==39}

   i3 = c3;

//  P(0`c1,0`c2,0`c3,0`i1,0`i2,0`i3) {0`c1==92, 0`c2==39, 0`c3==10,
//    0`i1==92, 0`i2==39, 0`i3==10}


   r = i1+i2+i3;
}
