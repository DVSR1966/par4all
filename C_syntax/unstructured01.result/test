void unstructured01()
{
   int a, b, c;

   if (a==1)
      goto switch_1_case_1;
   goto break_1;
switch_1_case_1:   ;

   if (b==1)
      goto switch_2_case_1;
   goto switch_2_default;
switch_2_case_1:   ;
   goto break_2;
switch_2_default:   ;
   printf("Error\n");
break_2:   ;
   goto break_1;
break_1:   ;

   if (c==1)
      goto switch_3_case_1;
   goto break_3;
switch_3_case_1:   ;
   goto break_3;
break_3:   ;
}

