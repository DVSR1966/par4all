
Parser output

main()
{
   unsigned int i = 4294967295U;
   unsigned int j;
   int k;
   int t;
   int u;

   j = i;
   k = j;
   t = i-j+k;
   i = u;
   j = u;
   k = u;
}

Controlizer output

main()
{
   unsigned int i = 4294967295U;
   unsigned int j;
   int k;
   int t;
   int u;

   j = i;
   k = j;
   t = i-j+k;
   i = u;
   j = u;
   k = u;
}

Preconditions


//  P() {}

main()
{
   unsigned int i = 4294967295U;
   unsigned int j;
   int k;
   int t;
   int u;

//  P() {0`i==-1}


   j = i;

//  P(0`j) {0`i==-1, 0`j==-1}

   k = j;

//  P(0`j,0`k) {0`i==-1, 0`j==-1, 0`k==-1}

   t = i-j+k;

//  P(0`j,0`k,0`t) {0`i==-1, 0`j==-1, 0`k==-1, 0`t==-1}

   i = u;

//  P(0`i,0`j,0`k,0`t) {0`i==0`u, 0`j==-1, 0`k==-1, 0`t==-1}

   j = u;

//  P(0`i,0`j,0`k,0`t) {0`i==0`j, 0`i==0`u, 0`k==-1, 0`t==-1}

   k = u;
}

