void constants03()
{
   int i = 0xff000000;
   int j = 0xffffffff;
   int t;

   i = 0xff000000;
   j = 0xffffffff;
   t = i+j;
}

Preconditions


//  P() {}

void constants03()
{
   int i = 0xff000000;
   int j = 0xffffffff;
   int t;

//  P() {0`i==-16777216, 0`j==-1}


   i = 0xff000000;

//  P(0`i) {0`i==-16777216, 0`j==-1}

   j = 0xffffffff;

//  P(0`i,0`j) {0`i==-16777216, 0`j==-1}

   t = i+j;
}

