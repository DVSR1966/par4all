main()
{
   enum fleurs {rose=1, marguerite=0, jasmin};
   enum legumes {carotte=51, haricot};
   enum fleurs ma1;
   enum fleurs ma2;
   enum fleurs ma3;
   enum legumes mon1;
   enum legumes mon2;
   int i;

   ma1 = rose;
   ma2 = marguerite;
   ma3 = jasmin;
   mon1 = carotte;
   mon2 = haricot;

   i = ma1+ma2+ma3+mon1+mon2;
}
