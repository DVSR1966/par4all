main()
{
   enum fleurs {rose=0x0001, marguerite=0, jasmin};
   enum legumes {carotte=rose+50, haricot};
   enum fleurs ma1;
   enum fleurs ma2;
   enum fleurs ma3;
   enum legumes mon1;
   enum legumes mon2;
   int i;
   int a1;
   int a2;
   int a3;
   int on1;
   int on2;
   int j;

   ma1 = rose;
   ma2 = marguerite;
   ma3 = jasmin;
   mon1 = carotte;
   mon2 = haricot;

   i = ma1+ma2+ma3+mon1+mon2;

   a1 = rose;
   a2 = marguerite;
   a3 = jasmin;
   on1 = carotte;
   on2 = haricot;

   j = a1+a2+a3+on1+on2;
}

//  P() {}

main()
{
   enum fleurs {rose=0x0001, marguerite=0, jasmin};
   enum legumes {carotte=rose+50, haricot};
   enum fleurs ma1;
   enum fleurs ma2;
   enum fleurs ma3;
   enum legumes mon1;
   enum legumes mon2;
   int i;
   int a1;
   int a2;
   int a3;
   int on1;
   int on2;
   int j;

//  P() {}


   ma1 = rose;

//  P() {}

   ma2 = marguerite;

//  P() {}

   ma3 = jasmin;

//  P() {}

   mon1 = carotte;

//  P() {}

   mon2 = haricot;

//  P() {}


   i = ma1+ma2+ma3+mon1+mon2;

//  P(0`i) {}


   a1 = rose;

//  P(0`a1,0`i) {0`a1==1}

   a2 = marguerite;

//  P(0`a1,0`a2,0`i) {0`a1==1, 0`a2==0}

   a3 = jasmin;

//  P(0`a1,0`a2,0`a3,0`i) {0`a1==1, 0`a2==0, 0`a3==1}

   on1 = carotte;

//  P(0`a1,0`a2,0`a3,0`i,0`on1) {0`a1==1, 0`a2==0, 0`a3==1, 0`on1==51}

   on2 = haricot;

//  P(0`a1,0`a2,0`a3,0`i,0`on1,0`on2) {0`a1==1, 0`a2==0, 0`a3==1,
//    0`on1==51, 0`on2==52}


   j = a1+a2+a3+on1+on2;
}
