void point_to03()
{
   struct s {
      int foo;
   };
   typedef struct s a_t;
   typedef a_t *p_t;
   a_t a;
   p_t p;

   (&a+2)->foo = 1;
   
   /* Floating point values are not allowed in pointer arithmetic */
   /* (&a+2.)->foo = 1; */

   (&a+(int )2.)->foo = 1;
   (2+&a)->foo = 1;
   (&a-2)->foo = 1;
}
