
PARSED PRINTED FILE

main()
{
   int a[5];
   int i;
   for (i = 0;i<5;i++)
   {
      if (a[i]<0) {
         goto found;
      }
   }
found:   printf("found\n");
}

PRINTED FILE (after controlization)

main()
{
   int a[5];
   int i;
   i = 0;
l99998:   ;
   if (!(i<5)) goto found;
   if (a[i]<0) goto found;
   i++;
   goto l99998;
found:   ;
   printf("found\n");
}
