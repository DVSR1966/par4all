{
   // BEGIN BLOCK
   // BEGIN BLOCK
   // A bara label
bara:   ;
   // A gwin label// This goto bara should not disappear:
gwin:   goto bara;
   // END BLOCK
   // END BLOCK
}
void miam(void)
{
   // BEGIN BLOCK
   // BEGIN BLOCK
bara:   ;
gwin:   goto bara;
   // END BLOCK
   // END BLOCK
}
extern void miam(void);
extern void comment(void);

