int foo(int predicate)
{
   int x = 0;
   if (predicate==0) goto foo_switch_1_case_0;
   if (predicate==1) goto foo_switch_1_case_1;
   if (predicate==2) goto foo_switch_1_case_2;
   if (predicate==3) goto foo_switch_1_case_3;
foo_switch_1_default:   ;
