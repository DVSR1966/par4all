setenv WSPC decl43
setenv MAIN $WSPC.c
delete $WSPC
create $WSPC $MAIN
activate C_PARSER
activate PRINT_CODE
setproperty PRETTYPRINT_C_CODE TRUE
setproperty PRETTYPRINT_STATEMENT_NUMBER TRUE
display PRINTED_FILE[safe_malloc]

apply UNSPLIT
shell cc -c decl43.database/Src/decl43.c

close
delete $WSPC
quit
