main()
{
   int i;
   int j;
   int a[5];
   i = 1;
   while (i<5) {
      a[i] = i;
      i = i+1;
   }
}
main()
{
   int i;
   int j;
   int a[5];
//               <must be written>: 0`i
   i = 1;
//               <must be read   >: 0`i
   while (i<5) {
//               <must be read   >: 0`i
//               <must be written>: 0`a[i]
      a[i] = i;
//               <must be read   >: 0`i
//               <must be written>: 0`i
      i = i+1;
   }
}

//  T() {}

main()
{
   int i;
   int j;
   int a[5];

//  T(0`i) {0`i==1}

   i = 1;

//  T(0`i) {i#init<=0`i}

   while (i<5) {

//  T() {0`i<=4}

      a[i] = i;

//  T(0`i) {0`i==i#init+1, 0`i<=5}

      i = i+1;
   }
}

//  P() {}

main()
{
   int i;
   int j;
   int a[5];

//  P() {}

   i = 1;

//  P(0`i) {0`i==1}

   while (i<5) {

//  P(0`i) {1<=0`i, 0`i<=4}

      a[i] = i;

//  P(0`i) {1<=0`i, 0`i<=4}

      i = i+1;
   }
}
main()
{
   int i;
   int j;
   int a[5];
   i = 1;

//  <0`a[PHI1]-W-MAY-{1<=PHI1, PHI1<=4}>

   while (i<5) {

//  <0`a[PHI1]-W-EXACT-{PHI1==0`i, 1<=0`i, 0`i<=4}>

      a[i] = i;
      i = i+1;
   }
}
