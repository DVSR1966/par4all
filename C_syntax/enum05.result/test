enum {a, b, c=b+3};
extern  main();

Preconditions derived from enum


//  P() {}

enum {a, b, c=b+3};
extern  main();


//  P() {}

main()
{
   int ia;
   int ib;
   int ic;
   int t;

//  P() {}


   ia = a;

//  P(0`ia) {0`ia==0}

   ib = b;

//  P(0`ia,0`ib) {0`ia==0, 0`ib==1}

   ic = c;

//  P(0`ia,0`ib,0`ic) {0`ia==0, 0`ib==1, 0`ic==4}


   t = ia+ib+ic;
}
