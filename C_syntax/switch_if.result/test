
PARSED PRINTED FILE

void pretty(int n)
{
   char *p;

   if (n==1)
      goto pretty_switch_1_case_1;
   if (n==2)
      goto pretty_switch_1_case_2;
   if (n==3)
      goto pretty_switch_1_case_3;
   goto pretty_break_1;
pretty_switch_1_case_1:   ;
   p = "one";
   if (0)
pretty_switch_1_case_2:      ;
   p = "two";
   if (0)
pretty_switch_1_case_3:      ;
   p = "three";
   printf("%s", p);
   goto pretty_break_1;
pretty_break_1:   ;
}

PRINTED FILE

void pretty(int n)
{
   char *p;
   if (n==1) goto pretty_switch_1_case_1;
   if (n==2) goto pretty_switch_1_case_2;
   if (n==3) {
   }
   else {
      goto pretty_break_1;
   }
pretty_switch_1_case_3:   ;
l99999:   ;
   p = "three";
   printf("%s", p);
   goto pretty_break_1;
pretty_switch_1_case_2:   ;
l99998:   ;
   p = "two";
   if (0) {
      goto pretty_switch_1_case_3;
   }
   else {
      goto l99999;
   }
pretty_switch_1_case_1:   ;
   p = "one";
   if (0) {
      goto pretty_switch_1_case_2;
   }
   else {
      goto l99998;
   }
pretty_break_1:   ;
}

printed file after RESTRUCTURE_CONTROL

void pretty(int n)
{
   char *p;
   if (n==1) goto pretty_switch_1_case_1;
   if (n==2) goto pretty_switch_1_case_2;
   if (n==3) {
   }
   else {
      goto pretty_break_1;
   }
pretty_switch_1_case_3:   ;
l99999:   ;
   p = "three";
   printf("%s", p);
   goto pretty_break_1;
pretty_switch_1_case_2:   ;
l99998:   ;
   p = "two";
   if (0) {
      goto pretty_switch_1_case_3;
   }
   else {
      goto l99999;
   }
pretty_switch_1_case_1:   ;
   p = "one";
   if (0) {
      goto pretty_switch_1_case_2;
   }
   else {
      goto l99998;
   }
pretty_break_1:   ;
}
