Transformers for "main"


//  T() {}

int main()
{
   int i;
   int j;

//  T(i,j) {i==2, j==1}


   i = (j = 1, 2);

//  T(i,j) {i==3, i#init==2, j==4, j#init==1}

   i = 3, j = 4;

//  T(i) {i==-11, i#init==3, j==4}


   i = foo(-5, -6);
}

Preconditions for "main"


//  P() {}

int main()
{
   int i;
   int j;

//  P() {}


   i = (j = 1, 2);

//  P(i,j) {i==2, j==1}

   i = 3, j = 4;

//  P(i,j) {i==3, j==4}


   i = foo(-5, -6);
}

