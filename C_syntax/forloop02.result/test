
PARSED PRINTED FILE for main

main()
{
   int i;
   int j;
   int a[5];
   for ((i = 1,j = 2);i<5;(i++,j += 2))
      a[i] = i;
}

PRINTED FILE for main

main()
{
   int i;
   int j;
   int a[5];
   (i = 1,j = 2);
   while (i<5) {
      a[i] = i;
      (i++,j += 2);
   }
}

PRINTED FILE with effects for main

main()
{
   int i;
   int j;
   int a[5];
//               <must be written>: i j
   (i = 1,j = 2);
//               <must be read   >: i
   while (i<5) {
//               <must be read   >: i
//               <must be written>: a[i]
      a[i] = i;
//               <must be read   >: i j
//               <must be written>: i j
      (i++,j += 2);
   }
}

PRINTED FILE with transformers for main


//  T() {}

main()
{
   int i;
   int j;
   int a[5];

//  T(i,j) {}

   (i = 1,j = 2);

//  T(i,j) {}

   while (i<5) {

//  T() {}

      a[i] = i;

//  T(i,j) {}

      (i++,j += 2);
   }
}
