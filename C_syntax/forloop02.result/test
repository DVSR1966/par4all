
PARSED PRINTED FILE for main

main()
{
   int i;
   int j;
   int a[5];
   for ((i = 1, j = 2);i<5;(i++, j += 2))
      a[i] = i;
}

PRINTED FILE for main

main()
{
   int i;
   int j;
   int a[5];
   (i = 1, j = 2);
   while (i<5) {
      a[i] = i;
      (i++, j += 2);
   }
}

PRINTED FILE with effects for main

main()
{
   int i;
   int j;
   int a[5];
//               <must be written>: 0`i 0`j
   (i = 1, j = 2);
//               <must be read   >: 0`i
   while (i<5) {
//               <must be read   >: 0`i
//               <must be written>: 0`a[i]
      a[i] = i;
//               <must be read   >: 0`i 0`j
//               <must be written>: 0`i 0`j
      (i++, j += 2);
   }
}

PRINTED FILE with transformers for main


//  T() {}

main()
{
   int i;
   int j;
   int a[5];

//  T(0`i,0`j) {}

   (i = 1, j = 2);

//  T(0`i,0`j) {}

   while (i<5) {

//  T() {}

      a[i] = i;

//  T(0`i,0`j) {}

      (i++, j += 2);
   }
}
