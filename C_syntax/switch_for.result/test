
PARSED PRINTED FILE

void gl_enable()
{
   int cap;
   int p;
   if (cap==1)
      goto switch_1_case_1;
   goto break_1;
switch_1_case_1:   ;
   for (p = 0;p<6;p++)
      goto break_2;
break_2:   ;
break_1:   ;
}

PRINTED FILE

void gl_enable()
{
   int cap;
   int p;
   if (cap==1) {
   }
   else {
      goto break_2;
   }
switch_1_case_1:   ;
   p = 0;
   if (!(p<6)) {
   }
break_2:   ;
break_1:   ;
}
