
PARSED PRINTED FILE

void gl_enable()
{
   int cap, p;

   if (cap==1)
      goto gl_enable_switch_1_case_1;
   goto gl_enable_break_1;
gl_enable_switch_1_case_1:   ;

   for (p = 0;p<6;p++)
      goto gl_enable_break_2;
gl_enable_break_2:   ;
gl_enable_break_1:   ;
}

