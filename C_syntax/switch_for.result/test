   if (cap==1) {
   }
   else {
      goto gl_enable_break_1;
   }
gl_enable_switch_1_case_1:   ;
   p = 0;
   if (!(p<6)) {
   }
gl_enable_break_2:   ;
gl_enable_break_1:   ;
}

printed file after RESTRUCTURE_CONTROL

void gl_enable()
{
   int cap, p;
   if (cap==1) {
gl_enable_switch_1_case_1:      ;
      p = 0;
      if (!(p<6))
         ;
gl_enable_break_2:      ;
   }
