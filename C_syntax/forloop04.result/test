main()
{
   int i;
   int j;
   int a[5];
   for(i = 1; i <= 4; i += 1)
      a[i] = i;
}
main()
{
   int i;
   int j;
   int a[5];
//               <must be written>: 0`i
   for(i = 1; i <= 4; i += 1)
//               <must be read   >: 0`i
//               <must be written>: 0`a[i]
      a[i] = i;
}

//  T() {}

main()
{
   int i;
   int j;
   int a[5];

//  T(0`i) {1<=0`i}

   for(i = 1; i <= 4; i += 1)

//  T() {1<=0`i, 0`i<=4}

      a[i] = i;
}

//  P() {}

main()
{
   int i;
   int j;
   int a[5];

//  P() {}

   for(i = 1; i <= 4; i += 1)

//  P(0`i) {1<=0`i, 0`i<=4}

      a[i] = i;
}
main()
{
   int i;
   int j;
   int a[5];

//  <0`a[PHI1]-W-MAY-{1<=PHI1, PHI1<=4}>

   for(i = 1; i <= 4; i += 1)

//  <0`a[PHI1]-W-EXACT-{PHI1==0`i, 1<=0`i, 0`i<=4}>

      a[i] = i;
}
