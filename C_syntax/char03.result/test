int main()
{
   char *s;
   wchar_t *ws;
   unsigned char c = '\023';
   wchar_t wc = '\076';
   c = '\0';                                                            /*0007*/
   c = '\01';                                                           /*0008*/
   c = '\xc';                                                           /*0009*/
   c = '\xef';                                                          /*0010*/
   wc = '\xde';                                                         /*0011*/
   wc = '\xdef';                                                        /*0012*/
   wc = '\xabcdef01';                                                   /*0013*/
   wc = '\U0001babe';                                                   /*0014*/
   wc = '\ubabe';                                                       /*0015*/

   s = "\023\076\0\01\xc\xef\xde";                                      /*0017*/

   ws = "\023\076\0\01\xc\xef\xde\xdef\xabcdef01\U0001babe\ubabe";      /*0019*/

   return 0;                                                            /*0021*/
}
