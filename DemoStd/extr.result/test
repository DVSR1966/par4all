--- Preconditions for EXTR

C  P() {}

      SUBROUTINE EXTR(NI,NC)
C********************************************************
C     CALCULE LES COEFFICIENTS D EXTRAPOLATION
C     SUR LA SURFACE DE L AILE  (K=K1)
C********************************************************
      DIMENSION T(52,21,60)
      COMMON/CT/T
      COMMON/CI/I1,I2,IMAX,I1P1,I1P2,I2M1,I2M2,IBF
      COMMON/CJ/J1,J2,JMAX,J1P1,J1P2,J2M1,J2M2,JA,JB,JAM1,JBP1
      COMMON/CK/K1,K2,KMAX,K1P1,K1P2,K2M1,K2M2
      COMMON/CNI/L

C  P() {}


      READ (NXYZ) I1, I2, J1, JA, K1, K2                                0001

C  P(I1,I2,J1,JA,K1,K2) {}

      REWIND (NXYZ)                                                     0002

C  P(I1,I2,J1,JA,K1,K2) {}

C     
      N4 = 4                                                            0003

C  P(I1,I2,J1,JA,K1,K2,N4) {N4==4}

      IF (J1.GE.1) THEN                                                 0004

C  P(I1,I2,J1,JA,K1,K2,N4) {N4==4, 1<=J1}

         J1 = J1+1                                                      0005

C  P(I1,I2,J1,JA,K1,K2,N4) {N4==4, 2<=J1}

         J2 = 2*JA+1                                                    0006

C  P(I1,I2,J1,J2,JA,K1,K2,N4) {J2==2JA+1, N4==4, 2<=J1}

         JA = JA+1                                                      0007

C  P(I1,I2,J1,J2,JA,K1,K2,N4) {J2==2JA-1, N4==4, 2<=J1}


         L = NI                                                         0008

C  P(I1,I2,J1,J2,JA,K1,K2,L,N4) {J2==2JA-1, L==NI, N4==4, 2<=J1}

         K = K1                                                         0009

C  P(I1,I2,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    2<=J1}

         DO 300 J = J1, JA                                              0010

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

            S1 = D(J, K, J, K+1)                                        0011

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

            S2 = D(J, K+1, J, K+2)+S1                                   0012

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

            S3 = D(J, K+2, J, K+3)+S2                                   0013

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

            T(J,1,NC+3) = S2*S3/((S1-S2)*(S1-S3))                       0014

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

            T(J,1,NC+4) = S3*S1/((S2-S3)*(S2-S1))                       0015

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

            T(J,1,NC+5) = S1*S2/((S3-S1)*(S3-S2))                       0016

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

C           JH=J1+J2-J
            T(J1+J2-J,1,NC+3) = T(J,1,NC+3)                             0017

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

            T(J1+J2-J,1,NC+4) = T(J,1,NC+4)                             0018

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

            T(J1+J2-J,1,NC+5) = T(J,1,NC+5)                             0019

C  P(I1,I2,J,J1,J2,JA,K,K1,K2,L,N4) {J2==2JA-1, K==K1, L==NI, N4==4,
C    J1<=J, J<=JA, 2<=J1}

300         CONTINUE                                                    0020
      ENDIF

      END
--- Parallel version
      SUBROUTINE EXTR(NI,NC)
C********************************************************
C     CALCULE LES COEFFICIENTS D EXTRAPOLATION
C     SUR LA SURFACE DE L AILE  (K=K1)
C********************************************************
      DIMENSION T(52,21,60)
      COMMON/CT/T
      COMMON/CI/I1,I2,IMAX,I1P1,I1P2,I2M1,I2M2,IBF
      COMMON/CJ/J1,J2,JMAX,J1P1,J1P2,J2M1,J2M2,JA,JB,JAM1,JBP1
      COMMON/CK/K1,K2,KMAX,K1P1,K1P2,K2M1,K2M2
      COMMON/CNI/L

      READ (NXYZ) I1, I2, J1, JA, K1, K2                                0001
      REWIND (NXYZ)                                                     0002
C     
      N4 = 4                                                            0003
      IF (J1.GE.1) THEN                                                 0004
         J1 = J1+1                                                      0005
         J2 = 2*JA+1                                                    0006
         JA = JA+1                                                      0007

         L = NI                                                         0008
         K = K1                                                         0009
         DOALL J = J1, JA                                               0010
            PRIVATE S3,S2,S1
            S1 = D(J, K, J, K+1)                                        0011
            S2 = D(J, K+1, J, K+2)+S1                                   0012
            S3 = D(J, K+2, J, K+3)+S2                                   0013
            T(J,1,NC+3) = S2*S3/((S1-S2)*(S1-S3))                       0014
            T(J,1,NC+4) = S3*S1/((S2-S3)*(S2-S1))                       0015
            T(J,1,NC+5) = S1*S2/((S3-S1)*(S3-S2))                       0016
         ENDDO
         DOALL J = J1, JA                                               0010
            T(J1+J2-J,1,NC+5) = T(J,1,NC+5)                             0019
         ENDDO
         DOALL J = J1, JA                                               0010
            T(J1+J2-J,1,NC+4) = T(J,1,NC+4)                             0018
         ENDDO
         DOALL J = J1, JA                                               0010
C           JH=J1+J2-J
            T(J1+J2-J,1,NC+3) = T(J,1,NC+3)                             0017
         ENDDO
      ENDIF

      END
