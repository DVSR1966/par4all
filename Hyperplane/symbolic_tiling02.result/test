char isme(int elligent)
{
   int N;
   int roverti;
   int repide[elligent];
donne:
   for(roverti = 0; roverti <= elligent-1; roverti += 2)
      repide[roverti] = repide[roverti+1]*2;
}
char isme(int elligent)
{
   int N;
   int roverti;
   int repide[elligent];
   //PIPS generated variable
   int rovertit;
#pragma omp parallel for private(roverti)
donne:
   for(rovertit = 0; rovertit <= elligent-1; rovertit += N)
#pragma omp parallel for 
      for(roverti = rovertit; roverti <= MIN(rovertit+N, elligent-1); roverti += 2)
         repide[roverti] = repide[roverti+1]*2;
}
char isme(int elligent)
{
   int N;
   int roverti;
   int repide[elligent];
   //PIPS generated variable
   int rovertit;

//  <repide[PHI1]-R-MAY-{}>
//  <repide[PHI1]-W-MAY-{}>

#pragma omp parallel for private(roverti)
donne:
   for(rovertit = 0; rovertit <= elligent-1; rovertit += N)

//  <repide[PHI1]-R-MAY-{rovertit+1<=PHI1}>
//  <repide[PHI1]-W-MAY-{rovertit<=PHI1}>

#pragma omp parallel for 
      for(roverti = rovertit; roverti <= MIN(rovertit+N, elligent-1); roverti += 2)

//  <repide[PHI1]-R-EXACT-{PHI1==roverti+1, rovertit<=roverti}>
//  <repide[PHI1]-W-EXACT-{PHI1==roverti, rovertit<=roverti}>

         repide[roverti] = repide[roverti+1]*2;
}

