C                                                                    1 (STMT) 
c
      K = 24                                                            0001
C                                                                    1 (STMT) 
      X = 1.1                                                           0002
C                                                                    1 (STMT) 
      Y = 82.2                                                          0003
C                                                                  441 (DO)   
      DO I = 1, 20                                                      0004
C                                                                   22 (BLOCK)
C        BEGIN BLOCK
C                                                                    6 (TEST) 
         IF (SIN(X).GT.1.) THEN                                         0005
C                                                                    8 (BLOCK)
C           BEGIN BLOCK
C                                                                    4 (STMT) 
            X = X*X                                                     0006
C                                                                    4 (STMT) 
            X = X+X                                                     0007
C           END BLOCK
         ENDIF
C                                                                    3 (STMT) 
         CALL P(X)                                                      0008
C                                                                    8 (TEST) 
         IF (X.GT.Y) THEN                                               0009
C                                                                    6 (BLOCK)
C           BEGIN BLOCK
C                                                                    3 (STMT) 
            X = X-0.5                                                   0010
C                                                                    3 (STMT) 
            Y = Y-0.1                                                   0011
C           END BLOCK
         ELSE
C                                                                    6 (BLOCK)
C           BEGIN BLOCK
C                                                                    3 (STMT) 
            X = X+0.5                                                   0012
C                                                                    3 (STMT) 
            CALL P(X)                                                   0013
C           END BLOCK
         ENDIF
C                                                                    2 (STMT) 
         A(I) = 1.                                                      0014
C                                                                    2 (STMT) 
         WRITE (*, '(2F10.5)') X, Y                                     0015
C        END BLOCK
      ENDDO
