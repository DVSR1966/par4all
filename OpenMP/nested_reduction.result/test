// OMP style
int main(void)
{
   int k = 0, i = 0;                                                    /*0004*/
   int sum = 0;                                                         /*0005*/


   for(k = 0; k <= 99; k += 1)
      for(i = 0; i <= 99; i += 1)
         sum += i+k;                                                    /*0009*/

   printf("sum: %d\n", sum);                                            /*0013*/

   return 0;                                                            /*0015*/
}
int main(void)
{
   int k = 0, i = 0;                                                    /*0004*/
   int sum = 0;                                                         /*0005*/


#pragma omp parallel for reduction(+:sum) private (i)
   for(k = 0; k <= 99; k += 1)
#pragma omp parallel for reduction(+:sum)
      for(i = 0; i <= 99; i += 1)
         sum += i+k;                                                    /*0009*/

   printf("sum: %d\n", sum);                                            /*0013*/

   return 0;                                                            /*0015*/
}
