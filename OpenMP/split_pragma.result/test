// OMP style
      PROGRAM SPLIT_PRAGMA

      INTEGER N, I
      INTEGER PARAM1, PARAM2, PARAM3, PARAM4, PARAM5
      INTEGER PARAM6, PARAM7, PARAM8, PARAM9, PARAM0
      INTEGER PARAM11, PARAM12, PARAM13, PARAM14
      INTEGER PARAM15, PARAM16, PARAM17, PARAM18
      INTEGER PARAM19, PARAM10
      PARAMETER (N=10)
      REAL A(N)


!$OMP PARALLEL DO PRIVATE(PARAM9,PARAM8,PARAM7,PARAM6,PARAM5,PARAM4,
!$OMPxPARAM3,PARAM2,PARAM19,PARAM18,PARAM17,PARAM16,PARAM15,PARAM14,
!$OMPxPARAM13,PARAM12,PARAM11,PARAM10,PARAM1,PARAM0)
      DO I = 1, N
         PARAM1 = I+1
         PARAM2 = I+PARAM1
         PARAM3 = I+PARAM2
         PARAM4 = I+PARAM3
         PARAM5 = I+PARAM4
         PARAM6 = I+PARAM5
         PARAM7 = I+PARAM6
         PARAM8 = I+PARAM7
         PARAM9 = I+PARAM8
         PARAM0 = I+PARAM9
         PARAM11 = I+PARAM0
         PARAM12 = I+PARAM11
         PARAM13 = I+PARAM12
         PARAM14 = I+PARAM13
         PARAM15 = I+PARAM14
         PARAM16 = I+PARAM15
         PARAM17 = I+PARAM16
         PARAM18 = I+PARAM17
         PARAM19 = I+PARAM18
         PARAM10 = I+PARAM19
         A(I) = PARAM14*1.0
      ENDDO
      PRINT *, A
      END
      PROGRAM SPLIT_PRAGMA

      INTEGER N, I
      INTEGER PARAM1, PARAM2, PARAM3, PARAM4, PARAM5
      INTEGER PARAM6, PARAM7, PARAM8, PARAM9, PARAM0
      INTEGER PARAM11, PARAM12, PARAM13, PARAM14
      INTEGER PARAM15, PARAM16, PARAM17, PARAM18
      INTEGER PARAM19, PARAM10
      PARAMETER (N=10)
      REAL A(N)

!$omp parallel do private(PARAM9, PARAM8, PARAM7, PARAM6, PARAM5
!$omp& , PARAM4, PARAM3, PARAM2, PARAM19, PARAM18, PARAM17, PARAM16, 
!$omp& PARAM15, PARAM14, PARAM13, PARAM12, PARAM11, PARAM10, PARAM1, 
!$omp& PARAM0)
      DO I = 1, N
         PARAM1 = I+1
         PARAM2 = I+PARAM1
         PARAM3 = I+PARAM2
         PARAM4 = I+PARAM3
         PARAM5 = I+PARAM4
         PARAM6 = I+PARAM5
         PARAM7 = I+PARAM6
         PARAM8 = I+PARAM7
         PARAM9 = I+PARAM8
         PARAM0 = I+PARAM9
         PARAM11 = I+PARAM0
         PARAM12 = I+PARAM11
         PARAM13 = I+PARAM12
         PARAM14 = I+PARAM13
         PARAM15 = I+PARAM14
         PARAM16 = I+PARAM15
         PARAM17 = I+PARAM16
         PARAM18 = I+PARAM17
         PARAM19 = I+PARAM18
         PARAM10 = I+PARAM19
         A(I) = PARAM14*1.0
      ENDDO
!$omp end parallel do
      PRINT *, A
      END
