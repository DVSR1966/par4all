// OMP style
int main()
{
   int b[size];
   int i;
   int j;
   i = 0;                                                               /*0006*/
   j = 0;                                                               /*0007*/



#pragma omp parallel for 
   for(i = 0; i <= size-1; i += 1)                                      /*0009*/
      b[i] = 0;                                                         /*0010*/



#pragma omp parallel for private(i)
   for(j = 0; j <= size-1; j += 1)                                      /*0013*/
      for(i = 0; i <= 99; i += 1)                                       /*0014*/
         b[j] = b[j]+i;                                                 /*0015*/



   for(i = 0; i <= 99; i += 1)                                          /*0019*/
#pragma omp parallel for 
      for(j = 0; j <= size-1; j += 1)                                   /*0020*/
         b[j] = b[j]+i;                                                 /*0021*/

   return 0;                                                            /*0025*/
}
int main()
{
   int b[size];
   int i;
   int j;
   i = 0;                                                               /*0006*/
   j = 0;                                                               /*0007*/



#pragma omp parallel for 
   for(i = 0; i <= size-1; i += 1)                                      /*0009*/
      b[i] = 0;                                                         /*0010*/



#pragma omp parallel for private(i)
   for(j = 0; j <= size-1; j += 1)                                      /*0013*/
      for(i = 0; i <= 99; i += 1)                                       /*0014*/
         b[j] = b[j]+i;                                                 /*0015*/



   for(i = 0; i <= 99; i += 1)                                          /*0019*/
#pragma omp parallel for 
      for(j = 0; j <= size-1; j += 1)                                   /*0020*/
         b[j] = b[j]+i;                                                 /*0021*/

   return 0;                                                            /*0025*/
}
