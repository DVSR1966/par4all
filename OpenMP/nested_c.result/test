// OMP style
int main(void)
{
   int k = 0;
   int i = 0;
   int sum = 0;

   for(k = 0; k <= 99; k += 1) {                                        /*0006*/
      for(i = 0; i <= 99; i += 1)                                       /*0007*/
         sum += i;                                                      /*0008*/
      sum += k;                                                         /*0010*/
   }

   for(i = 0; i <= 99; i += 1)                                          /*0013*/
      sum += i;                                                         /*0014*/

   printf("sum: %d\n", sum);                                            /*0017*/

   return 0;                                                            /*0019*/
}
int main(void)
{
   int k = 0;
   int i = 0;
   int sum = 0;

#pragma omp parallel for reduction(+:sum)
   for(k = 0; k <= 99; k += 1) {                                        /*0006*/
#pragma omp parallel for reduction(+:sum)
      for(i = 0; i <= 99; i += 1)                                       /*0007*/
         sum += i;                                                      /*0008*/
      sum += k;                                                         /*0010*/
   }

#pragma omp parallel for reduction(+:sum)
   for(i = 0; i <= 99; i += 1)                                          /*0013*/
      sum += i;                                                         /*0014*/

   printf("sum: %d\n", sum);                                            /*0017*/

   return 0;                                                            /*0019*/
}
