
TRANSFORMERS


//  T() {0==-1}

void run()
{

//  T(a,b) {a==0, b==0}

   int a = 0, b = 0;

//  T(a) {a==1, a#init==0, b==0}


   a = b+1;

//  T(a,b) {a#init==1, b#init==0}


   while (1) {

//  T(a) {}

      
      
      while (alea()) 

//  T(a) {a==b+1}

         if (1) {

//  T() {}

            if (!(a<b||b==0)) 

//  T() {0==-1}

               error();

//  T(a) {a==0}

            a = 0;

//  T(a) {a==b+1, a#init==0}

            a = b+1;
         }

//  T(a,b) {}

      
      
      
      while (alea()) 

//  T(a,b) {a==1, b==0}

         if (1) {

//  T(b) {a==b-1}

            if (alea()) {

//  T() {}

               if (!(a<b||b==0)) 

//  T() {0==-1}

                  error();

//  T(b) {a==b-1}

               b = a+1;
            }
            else {

//  T(b) {a==b-1}

               b = a+1;

//  T() {a==b-1}

               if (!!(b<a||a==0)) 

//  T() {0==-1}

                  error();

//  T() {a==b-1}

               if (!(a<b||b==0)) 

//  T() {0==-1}

                  error();
            }

//  T() {a==b-1}

            if (!!(b<a||a==0)) 

//  T() {0==-1}

               error();

//  T(a) {a==0, a#init==b-1}

            a = 0;

//  T() {a==0}

            if (!(b<a||a==0)) 

//  T() {0==-1}

               error();

//  T(b) {a==0, b==0}

            b = 0;

//  T(a) {a==1, a#init==0, b==0}

            a = b+1;
         }

//  T(a,b) {a#init<=a}

      
      
      
      while (alea()) 

//  T(a,b) {a==a#init+2, b==0}

         if (1) {

//  T(b) {a==b-1}

            if (alea()) {

//  T() {}

               if (!(a<b||b==0)) 

//  T() {0==-1}

                  error();

//  T(b) {a==b-1}

               b = a+1;
            }
            else {

//  T(b) {a==b-1}

               b = a+1;

//  T() {a==b-1}

               if (!!(b<a||a==0)) 

//  T() {0==-1}

                  error();

//  T() {a==b-1}

               if (!(a<b||b==0)) 

//  T() {0==-1}

                  error();
            }

//  T() {a==b-1}

            if (!!(b<a||a==0)) 

//  T() {0==-1}

               error();

//  T(a) {a==0, a#init==b-1}

            a = 0;

//  T(a) {a==b+1, a#init==0}

            if (alea()) {

//  T() {a==0}

               if (!(b<a||a==0)) 

//  T() {0==-1}

                  error();

//  T(a) {a==b+1, a#init==0}

               a = b+1;
            }
            else {

//  T(a) {a==b+1, a#init==0}

               a = b+1;

//  T() {a==b+1}

               if (!!(a<b||b==0)) 

//  T() {0==-1}

                  error();

//  T() {a==b+1}

               if (!(b<a||a==0)) 

//  T() {0==-1}

                  error();
            }

//  T(b) {a==b#init+1, b==0}

            b = 0;
         }
   }
}

PRECONDITIONS


//  P() {}

void run()
{

//  P() {}

   int a = 0, b = 0;

//  P(a,b) {a==0, b==0}


   a = b+1;

//  P(a,b) {a==1, b==0}


   while (1) {

//  P(a,b) {b==0, 1<=a}

      
      
      while (alea()) 

//  P(a,b) {b==0, 1<=a}

         if (1) {

//  P(a,b) {b==0, 1<=a}

            if (!(a<b||b==0)) 

//  P() {0==-1}

               error();

//  P(a,b) {b==0, 1<=a}

            a = 0;

//  P(a,b) {a==0, b==0}

            a = b+1;
         }

//  P(a,b) {b==0, 1<=a}

      
      
      
      while (alea()) 

//  P(a,b) {b==0, 1<=a}

         if (1) {

//  P(a,b) {b==0, 1<=a}

            if (alea()) {

//  P(a,b) {b==0, 1<=a}

               if (!(a<b||b==0)) 

//  P() {0==-1}

                  error();

//  P(a,b) {b==0, 1<=a}

               b = a+1;
            }
            else {

//  P(a,b) {b==0, 1<=a}

               b = a+1;

//  P(a,b) {a==b-1, 1<=a}

               if (!!(b<a||a==0)) 

//  P() {0==-1}

                  error();

//  P(a,b) {a==b-1, 1<=a}

               if (!(a<b||b==0)) 

//  P() {0==-1}

                  error();
            }

//  P(a,b) {a==b-1, 1<=a}

            if (!!(b<a||a==0)) 

//  P() {0==-1}

               error();

//  P(a,b) {a==b-1, 1<=a}

            a = 0;

//  P(a,b) {a==0, 2<=b}

            if (!(b<a||a==0)) 

//  P() {0==-1}

               error();

//  P(a,b) {a==0, 2<=b}

            b = 0;

//  P(a,b) {a==0, b==0}

            a = b+1;
         }

//  P(a,b) {b==0, 1<=a}

      
      
      
      while (alea()) 

//  P(a,b) {b==0, 1<=a}

         if (1) {

//  P(a,b) {b==0, 1<=a}

            if (alea()) {

//  P(a,b) {b==0, 1<=a}

               if (!(a<b||b==0)) 

//  P() {0==-1}

                  error();

//  P(a,b) {b==0, 1<=a}

               b = a+1;
            }
            else {

//  P(a,b) {b==0, 1<=a}

               b = a+1;

//  P(a,b) {a==b-1, 1<=a}

               if (!!(b<a||a==0)) 

//  P() {0==-1}

                  error();

//  P(a,b) {a==b-1, 1<=a}

               if (!(a<b||b==0)) 

//  P() {0==-1}

                  error();
            }

//  P(a,b) {a==b-1, 1<=a}

            if (!!(b<a||a==0)) 

//  P() {0==-1}

               error();

//  P(a,b) {a==b-1, 1<=a}

            a = 0;

//  P(a,b) {a==0, 2<=b}

            if (alea()) {

//  P(a,b) {a==0, 2<=b}

               if (!(b<a||a==0)) 

//  P() {0==-1}

                  error();

//  P(a,b) {a==0, 2<=b}

               a = b+1;
            }
            else {

//  P(a,b) {a==0, 2<=b}

               a = b+1;

//  P(a,b) {a==b+1, 3<=a}

               if (!!(a<b||b==0)) 

//  P() {0==-1}

                  error();

//  P(a,b) {a==b+1, 3<=a}

               if (!(b<a||a==0)) 

//  P() {0==-1}

                  error();
            }

//  P(a,b) {a==b+1, 3<=a}

            b = 0;
         }
   }
}
