
Transformers for slope10


//  T() {}

void slope10()
{

//  T(k,x,y) {}

   int x, y, k;

//  T() {0<=x, x<=50}

   if (!(0<=x&&x<=50))

//  T() {0==-1}

      abort();

//  T(k) {x+y==k}


   if (2*x-99*y<=0)

//  T(k) {x+y==k}

      k = x+y;
   else

//  T(k) {x+y==k}

      k = x+y;

//  T() {}


   return;
}

Preconditions for slope10


//  P() {}

void slope10()
{

//  P() {}

   int x, y, k;

//  P(k,x,y) {}

   if (!(0<=x&&x<=50))

//  P(k,x,y) {}

      abort();

//  P(k,x,y) {0<=x, x<=50}


   if (2*x-99*y<=0)

//  P(k,x,y) {0<=x, x<=50, x<=49y, x<=y+48}

      k = x+y;
   else

//  P(k,x,y) {0<=x, x<=50, y+1<=x, 49y+1<=x}

      k = x+y;

//  P(k,x,y) {x+y==k, 0<=x, x<=50}


   return;
}
