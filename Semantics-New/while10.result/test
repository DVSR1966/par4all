
Source code with transformers


//  T() {}

int main()
{

//  T(c) {c==0}

   int c = 0;

//  T(i) {c==0, i==0}

   int i = 0;

//  T(j) {c==0, i==0, j==0}

   int j = 0;

//  T() {0==-1}


   while (i++&&j<=10)
      ;

//  T() {c==0, i==1, j==0}

   return;
}

Source code with preconditions


//  P() {}

int main()
{

//  P() {}

   int c = 0;

//  P(c) {c==0}

   int i = 0;

//  P(c,i) {c==0, i==0}

   int j = 0;

//  P(c,i,j) {c==0, i==0, j==0}


   while (i++&&j<=10)
      ;

//  P(c,i,j) {c==0, i==1, j==0}

   return;
}
