            while (flip())

//  T(x,y) {s==2, x==x#init+1, y==y#init+1, 0<=x}

               if (s==2) {

//  T(x) {s==2, x==x#init+1}

                  x++;

//  T(y) {s==2, y==y#init+1}

                  y++;

//  T() {s==2, 0<=x}

