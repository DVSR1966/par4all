//  P() {0==-1}

                  if (x<0)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P() {0==-1}

                  while (1)

//  P() {0==-1}

                     ;

//  P() {0==-1}

            if ((s = 2)&&x-y<0&&x-y<y) {

//  P() {0==-1}

               x -= y;

//  P() {0==-1}

               s = 1;

//  P() {0==-1}

               if (x<0)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
         }
   }
}
