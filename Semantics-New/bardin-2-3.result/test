
TRANSFORMERS


//  T() {0==-1}

void run(void)
{

//  T(s,x,y) {}

   int s, x, y;

//  T(s) {s==1}

   s = 1;

//  T(x) {s==1, 0<=x}

   x = rand();

//  T(y) {s==1, 0<=x, 0<=y}

   y = rand();

//  T() {0==-1}

   if (x>=0&&y>=0) {

//  T(x) {s==1, x#init<=x, y<=x, 0<=x#init, 0<=y}

      if (x<y) {

//  T(x) {s==1, 0<=x, x#init<=x, x+1<=y}

         while (flip())

//  T(x) {s==1, x==x#init+2, 2<=x, x+1<=y, 1<=y}

            if (s==1&&x>=0&&x+2<y) {

//  T(x) {s==1, x==x#init+2, 2<=x, x+1<=y, 1<=y}

               x += 2;

//  T() {s==1, 2<=x, x+1<=y, 1<=y}

               if (x<0)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {s==1, 1<=y}

               while (1)

//  T() {s==1, 1<=y}

                  ;

//  T(x) {s==1, x==x#init+2, 2<=x, y<=x, 1<=y}

         if (s==1&&x>=0&&x+2>=y) {

//  T(x) {s==1, x==x#init+2, 2<=x, y<=x, 1<=y}

            x += 2;

//  T() {s==1, 2<=x, y<=x, 1<=y}

            if (x<0)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 0<=x, x+3<=y, 1<=y}

            while (1)

//  T() {s==1, 0<=x, x+3<=y, 1<=y}

               ;
      }

//  T(s,x,y) {s==1, s#init==1, 0<=x, 0<=x#init, y#init<=x#init, 0<=y,
//    y#init<=y, 0<=y#init}


      while (1)

//  T(s,x,y) {s==1, s#init==1, x#init+y<=2x+y#init, 0<=x#init,
//    y#init<=y}


         if (flip())

//  T(x) {s==1, x==x#init+2, 2<=x}

            if (s==1&&x>=0) {

//  T(x) {s==1, x==x#init+2, 2<=x}

               x += 2;

//  T() {s==1, 2<=x}

               if (x<0)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;
         else if (flip()) {

//  T(s) {s==2, s#init==1, 0<=x, y<=x}

            if (s==1&&x>=y) {

//  T(s) {s==2, s#init==1, y<=x}

               s = 2;

//  T() {s==2, 0<=x, y<=x}

               if (x<0)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;

//  T(x,y) {s==2, x+y#init==x#init+y, 0<=x, y<=x, y#init<=y}

            while (flip())

//  T(x,y) {s==2, x==x#init+1, y==y#init+1, 0<=x}

               if (s==2) {

//  T(x) {s==2, x==x#init+1}

                  x++;

//  T(y) {s==2, y==y#init+1}

                  y++;

//  T() {s==2, 0<=x}

                  if (x<0)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T(s,x) {s==1, s#init==2, x+y==x#init, 0<=x#init, y<=x#init,
//    2y<=x#init}

            if ((s = 2)&&x-y>=0&&x-y>=y) {

//  T(x) {s==2, x+y==x#init, 0<=x#init, y<=x#init, 2y<=x#init}

               x -= y;

//  T(s) {s==1, s#init==2, 0<=x, 0<=x+y, y<=x}

               s = 1;

//  T() {s==1, 0<=x, 0<=x+y, y<=x}

               if (x<0)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {s==2, 0<=x, y<=x}

               while (1)

//  T() {s==2, 0<=x, y<=x}

                  ;
         }
         else if (flip()) {

//  T(s) {s==2, s#init==1, 0<=x, y<=x}

            if (s==1&&x>=y) {

//  T(s) {s==2, s#init==1, y<=x}

               s = 2;

//  T() {s==2, 0<=x, y<=x}

               if (x<0)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;

//  T(x,y) {s==2, x+y#init==x#init+y, 0<=x, y<=x, y#init<=y}

            while (flip())

//  T(x,y) {s==2, x==x#init+1, y==y#init+1, 0<=x}

               if (s==2) {

//  T(x) {s==2, x==x#init+1}

                  x++;

//  T(y) {s==2, y==y#init+1}

                  y++;

//  T() {s==2, 0<=x}

                  if (x<0)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T() {0==-1}

            if ((s = 2)&&x-y<0&&x-y>=y) {

//  T() {0==-1}

               x -= y;

//  T() {0==-1}

               s = 1;

//  T() {0==-1}

               if (x<0)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {s==2, 0<=x, y<=x}

               while (1)

//  T() {s==2, 0<=x, y<=x}

                  ;

//  T() {0==-1}

            while (flip()) {

//  T() {0==-1}

               if (s==1&&x>=y) {

//  T() {0==-1}

                  s = 2;

//  T() {0==-1}

                  if (x<0)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T() {0==-1}

               while (flip())

//  T() {0==-1}

                  if (s==2) {

//  T() {0==-1}

                     x++;

//  T() {0==-1}

                     y++;

//  T() {0==-1}

                     if (x<0)

