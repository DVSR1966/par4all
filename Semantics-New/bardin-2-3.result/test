PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int s, x, y;

//  P(s,x,y) {}

   s = 1;

//  P(s,x,y) {s==1}

   x = rand();

//  P(s,x,y) {s==1, 0<=x}

