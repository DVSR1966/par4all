PROPER  EFFECTS

void memcopy01(int size, char src[size], char dst[size])
{
   int i;
//             <    is read   >: size
//             <    is written>: i
   for(i = 0; i <= size-1; i += 1)
//             <    is read   >: i size src[i]
//             <    is written>: dst[i]
      dst[i] = src[i];
}

