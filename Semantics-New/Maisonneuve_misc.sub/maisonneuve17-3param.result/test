//    2y<=2s1+x, 2x+3<=2s1+y, y<=s1+x+1, x+1<=s1+y, y<=s1+1, 0<=s2,
//    y<=s2+x+2, x+1<=s2+y, 0<=x, y<=x+6, x<=y+4, 1<=y}

               y--;

//  P(o,s1,s2,x,y) {0<=o, o+3<=s1, s2<=o+s1+2, o+1<=s1+s2,
