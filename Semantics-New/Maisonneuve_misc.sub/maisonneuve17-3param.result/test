            if (x>=s1-o&&x<=s1+s2-o&&y>=s1-o&&y<=s1+s2-o&&x-1>=0&&x-1<=s1&&y-1>=0&&y-1<=s1&&y-1<s1+s2-o-x+1) {

//  P(o,s1,s2,x,y) {0<=o, o+3<=s1, o+2x+4<=3s1+s2, o+x+2<=s1+s2,
//    s1+s2<=3o+x+y, s2+y<=o+s1+x+2, s2+x<=o+s1+y+2, s1+y<=o+s2+x+1,
