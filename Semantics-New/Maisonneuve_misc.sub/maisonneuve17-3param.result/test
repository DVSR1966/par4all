                  if (x==0&&y==s1+s2-o)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(o,s1,s2,x,y) {0<=o, o+3<=s1, o+2x+4<=3s1+s2, o+x+2<=s1+s2,
