//    o+y+3<=s1+s2, s1<=2o+x+1, s1<=o+x+1, 2s1<=2o+x+y+2, s1<=2o+y+1,
//    s1<=o+y+1, o+3<=s2, 2s2<=2o+x+y+4, y<=o+x+1, x<=o+y+1, 0<=s1,
