//    o+y+2<=s1+s2, s1<=2o+x, s1<=o+x, 2s1<=2o+x+y, s1<=2o+y, s1<=o+y,
//    o+3<=s2, 2s2<=2o+x+y+2, y<=o+x+1, x<=o+y+1, x<=s1+1,
//    2y+1<=2s1+x, 2x+1<=2s1+y, y<=s1+1, 1<=x, y<=x+5, x<=y+5, 1<=y}

               x--;

