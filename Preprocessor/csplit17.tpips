delete csplit17
create csplit17 csplit17.c

# check signatures with parameter names are parsed (about csplit16.c)

shell for i in csplit17.database/[#a-z_]*[a-z_0-9!]/*.c; do echo;echo '/*' Module `basename $i .c` '*/';echo; \
	cat $i; done

# quit

# This has nothing to do with the preprocessing

close
open csplit17

activate C_PARSER
setproperty PRETTYPRINT_C_CODE TRUE
setproperty PRETTYPRINT_STATEMENT_NUMBER FALSE

display PRINTED_FILE[csplit17]
close
delete csplit17
quit
