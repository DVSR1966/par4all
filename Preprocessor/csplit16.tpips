delete csplit16
create csplit16 csplit16.c

# check that the compilation unit can be parser specially

shell for i in csplit16.database/[#a-z_]*[a-z_0-9!]/*.c; do echo;echo '/*' Module `basename $i .c` '*/';echo; \
	cat $i; done

# quit

# This has nothing to do with the preprocessing

close
open csplit16

activate C_PARSER
setproperty PRETTYPRINT_C_CODE TRUE
setproperty PRETTYPRINT_STATEMENT_NUMBER FALSE

display PRINTED_FILE[csplit16]
close
delete csplit16
quit
