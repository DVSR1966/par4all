delete ${WSPACE}

setproperty ABORT_ON_USER_ERROR TRUE
setenv SHELL /bin/bash

create ${WSPACE} ${FILE}

# Display result of csplit, except for the compilation unit(s) Because of
# static functions, do not forget the COMPILATION_UNIT_PREFIX, but no
# static functions in validation files using default_tpips?

# display C_SOURCE_FILE[%ALL]

# Get rid of preprocessed files
shell rm -f ${WSPACE}.database/Tmp/*.c
shell cat ${WSPACE}.database/*/*.c|sed -e '/^# 1[^0-9][^0-9]*/d'

close
delete ${WSPACE}
quit
