
Initial code

void a()
{
   int i, j;
   j = 0;
kernel:
   for(i = 0; i <= 255; i += 1)
      j += b(i);
}
int b(int i)
{
   return i*2;
}

Code after outlining

void a()
{
   int i, j;
   j = 0;
kernel:   kernel(&j);
}
int b(int i)
{
   return i*2;
}
void kernel(int *j)
{
   //PIPS generated variable
   int i;
kernel:
   for(i = 0; i <= 255; i += 1)
      *j += b(i);
}

