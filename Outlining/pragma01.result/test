
Initial code

int pof()
{
   int a;
#pragma  pips outline
   a = 1;
}
int pof();

Code after outlining

void kernel(int a_0[1])
{
   //PIPS generated variable
   int a;
   a = *a_0;
#pragma   pips outline
   a = 1;
   *a_0 = a;
}
int pof()
{
   int a;
   kernel(&a);
}
int pof();
//PIPS generated variable
void kernel(int a_0[1]);

