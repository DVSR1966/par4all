Code after outlining

void assign(int *b, int i)
{
   coca cola = {b};
   do_assign(cola.pepsi, i);
}
void ding(int b[100], int i_0[1])
{
   //PIPS generated variable
   int i;
   i = *i_0;
here:   if (1)
      assign(b, i);
   *i_0 = i;
}
static void assign(int *b, int i)
{
   coca cola = {b};
   do_assign(cola.pepsi, i);
}
static void do_assign(int b[1], int i)
{
   b[i] = i;
}
float ing(int i)
{
   int b[100];
here:   ding(b, &i);
}
static void do_assign(int b[1], int i)
{
   b[i] = i;
}
