Code after outlining

void foo(int bar[256][256])
{
   int i, j;
   for(i = 0; i <= 255; i += 1) {
      //PIPS generated variable
      void kernel(int bar[256][256], int i);

kernel:      kernel(bar, i);
   }
}
void kernel(int bar[256][256], int i)
{
   //PIPS generated variable
   extern int j, K;
kernel:
   for(j = 0; j <= 255; j += 1)
      bar[i][j] = K*i*j;
}
