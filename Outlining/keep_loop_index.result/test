before
int main()
{
   int i, j;
   j = 1;
there:
   for(i = 0; i <= 2; i += 1)
      j *= i;
   return j;
}
after
int main()
{
   int i, j;
   j = 1;
there:   outlined(2+1, &j);
   return j;
}
void outlined(int I_0, int *j)
{
   //PIPS generated variable
   int i;
there:
   for(i = 0; i <= I_0-1; i += 1)
      *j *= i;
}

