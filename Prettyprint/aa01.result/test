      PROGRAM AA01

      REAL X(10), Y(10), Z(10), U(10,10)
      INTEGER IX(10)

      X(1:10) = 0.                                                      0002

      X(1:10) = 0.                                                      0004

C     Reset array X
      X(1:10) = 0.                                                      0006

      X(1:10) = 0.                                                      0008

      X(2:10) = 0.                                                      0010

      X(1:9) = 0.                                                       0012

      X(1:9) = 0.                                                       0014

      X(10:1:-1) = 0.                                                   0016

      X(3:9:2) = 0.                                                     0018

C     Cannot be parallelized by PIPS. Hence, the array assignment cannot be prettyprinted
      DO I = 1, 3                                                       0019
         X(I**2+1) = 0.                                                 0020
      ENDDO

      X(1:10) = Y(1:10)                                                 0022

C     Detect a scalar in the right hand side
      X(1:10) = Y(K)                                                    0024

C     Use an array contructor with an implicit DO for a non-affine expression
      X(1:10) = Y((/ (I,I=1,10) /)/2)                                   0026

      X(1:10) = Y(1:10)+Z(1:10)                                         0028

      DO I = 1, 10                                                      0029
         U(I,I) = 0.                                                    0030
      ENDDO

      DO I = 1, 10                                                      0031
         U(I,I**2) = 0.                                                 0032
      ENDDO

      DO I = 1, 10                                                      0033
         U(I,IX(I)) = 0.                                                0034
      ENDDO

      DO I = 1, 10                                                      0035
         CALL INC(X(I))                                                 0036
      ENDDO

      END

      SUBROUTINE INC(Y)
      Y = Y+1.                                                          0001
      END
