      PROGRAM AA08

C     The vector expression is not prettyprinted because PIPS does not manage to
C     parallelize such a loop. Strict monotonicity of power over positive numbers
C     is not taken into account.

      REAL U(10,10)

      DO I = 1, 10                                                      0001
         U(I,1:10) = 0.                                                 0003
      ENDDO

      END
