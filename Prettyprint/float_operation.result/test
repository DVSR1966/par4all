a + c - b = 0.000000e+00
a + (c - b)= 1.000000e+00
"prettyprint with PRETTYPRINT_ALL_PARENTHESES = FALSE"
""
int main()
{
   float a, b, c, d, e;
   a = 1.0;
   b = 100.0E+25;
   c = b;
   e = a+c-b;
   d = a+c-b;
   printf("a + c - b = %e\na + (c - b)= %e\n", e, d);
   return (int) d;
}
a + c - b = 0.000000e+00
a + (c - b)= 0.000000e+00
""
"prettyprint with PRETTYPRINT_ALL_PARENTHESES = TRUE"
""
int main()
{
   float a, b, c, d, e;
   a = 1.0;
   b = 100.0E+25;
   c = b;
   e = ((a+c)-b);
   d = (a+(c-b));
   printf("a + c - b = %e\na + (c - b)= %e\n", e, d);
   return ((int) d);
}
a + c - b = 0.000000e+00
a + (c - b)= 1.000000e+00
