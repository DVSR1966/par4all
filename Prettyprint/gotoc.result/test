
Userview

      SUBROUTINE GOTOC(I)

      IF (I.EQ.1) THEN                                                  0001
         GOTO 100
      ENDIF
      IF (I.EQ.2) THEN                                                  0001
         GOTO 200
      ENDIF
      IF (I.EQ.3) THEN                                                  0001
         GOTO 300
      ENDIF

      PRINT *, I                                                        0002

100   CONTINUE                                                          0003
      PRINT *, I                                                        0004

200   CONTINUE                                                          0005
      PRINT *, I                                                        0006

300   CONTINUE                                                          0007
      PRINT *, I                                                        0008

400   CONTINUE                                                          0009

      END

Userview with proper effects

      SUBROUTINE GOTOC(I)

      IF (I.EQ.1) THEN                                                  0001
         GOTO 100
      ENDIF
      IF (I.EQ.2) THEN                                                  0001
         GOTO 200
      ENDIF
      IF (I.EQ.3) THEN                                                  0001
         GOTO 300
      ENDIF
C               <must be read   >: I __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)

      PRINT *, I                                                        0002

100   CONTINUE                                                          0003
C               <must be read   >: I __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)
      PRINT *, I                                                        0004

200   CONTINUE                                                          0005
C               <must be read   >: I __IO_EFFECTS:LUNS(6)
