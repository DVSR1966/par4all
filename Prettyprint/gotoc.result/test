      END

Userview with cumulated effects

C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
C               <must be read   >: I
      SUBROUTINE GOTOC(I)

      IF (I.EQ.1) THEN                                                  0001
         GOTO 100
      ENDIF
      IF (I.EQ.2) THEN                                                  0001
         GOTO 200
      ENDIF
      IF (I.EQ.3) THEN                                                  0001
         GOTO 300
      ENDIF
C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
C               <must be read   >: I

      PRINT *, I                                                        0002

100   CONTINUE                                                          0003
C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
C               <must be read   >: I
      PRINT *, I                                                        0004

200   CONTINUE                                                          0005
C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
C               <must be read   >: I
      PRINT *, I                                                        0006

300   CONTINUE                                                          0007
C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
C               <must be read   >: I
      PRINT *, I                                                        0008

400   CONTINUE                                                          0009

      END

Userview with transformers


C  T() {}

      SUBROUTINE GOTOC(I)



      IF (I.EQ.1) THEN                                                  0001


         GOTO 100
      ENDIF


      IF (I.EQ.2) THEN                                                  0001


         GOTO 200
      ENDIF


      IF (I.EQ.3) THEN                                                  0001


         GOTO 300
      ENDIF

C  T() {}


      PRINT *, I                                                        0002

C  T() {}


100   CONTINUE                                                          0003

C  T() {}

      PRINT *, I                                                        0004

C  T() {}


200   CONTINUE                                                          0005

C  T() {}

      PRINT *, I                                                        0006

C  T() {}


300   CONTINUE                                                          0007

C  T() {}

      PRINT *, I                                                        0008

C  T() {}


400   CONTINUE                                                          0009

      END

