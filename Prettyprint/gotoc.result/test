Userview with regions


C  <__IO_EFFECTS:LUNS(PHI1)-R-MAY-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-MAY-{PHI1==6}>

      SUBROUTINE GOTOC(I)

      IF (I.EQ.1) THEN                                                  0001
         GOTO 100
      ENDIF
      IF (I.EQ.2) THEN                                                  0001
         GOTO 200
      ENDIF
      IF (I.EQ.3) THEN                                                  0001
         GOTO 300
      ENDIF

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6}>


      PRINT *, I                                                        0002

100   CONTINUE                                                          0003

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6}>

      PRINT *, I                                                        0004

200   CONTINUE                                                          0005

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6}>

      PRINT *, I                                                        0006

300   CONTINUE                                                          0007

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6}>

      PRINT *, I                                                        0008

400   CONTINUE                                                          0009

      END

Userview with complexities

C                                                                 4.38 (SUMMARY)
      SUBROUTINE GOTOC(I)

      IF (I.EQ.1) THEN                                                  0001
         GOTO 100
      ENDIF
      IF (I.EQ.2) THEN                                                  0001
         GOTO 200
      ENDIF
      IF (I.EQ.3) THEN                                                  0001
         GOTO 300
      ENDIF
C                                                                  1/8 (STMT) 

      PRINT *, I                                                        0002
C                                                                    0 (STMT) 

100   CONTINUE                                                          0003
C                                                                    1 (STMT) 
      PRINT *, I                                                        0004
C                                                                    0 (STMT) 

200   CONTINUE                                                          0005
C                                                                    1 (STMT) 
      PRINT *, I                                                        0006
C                                                                    0 (STMT) 

300   CONTINUE                                                          0007
C                                                                    1 (STMT) 
      PRINT *, I                                                        0008
C                                                                    0 (STMT) 

