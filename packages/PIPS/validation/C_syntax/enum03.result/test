enum {a=sizeof(int), b};

extern int main();

Preconditions derived from enum


//  P() {}


//  P() {}

enum {a=sizeof(int), b};


//  P() {}

extern int main();


//  P() {}

int main()
{

//  P() {}

   int ia;

//  P(ia) {}

   int ib;

//  P(ia,ib) {}

   int t;

//  P(ia,ib,t) {}


   ia = a;

//  P(ia,ib,t) {}

   ib = b;

//  P(ia,ib,t) {ib==1}


   t = ia+ib;
}
