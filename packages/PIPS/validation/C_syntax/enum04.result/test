enum {a, b, c=b};

extern int main();

Preconditions derived from enum


//  P() {}


//  P() {}

enum {a, b, c=b};


//  P() {}

extern int main();


//  P() {}

int main()
{

//  P() {}

   int ia;

//  P(ia) {}

   int ib;

//  P(ia,ib) {}

   int ic;

//  P(ia,ib,ic) {}

   int t;

//  P(ia,ib,ic,t) {}


   ia = a;

//  P(ia,ib,ic,t) {ia==0}

   ib = b;

//  P(ia,ib,ic,t) {ia==0, ib==1}

   ic = c;

//  P(ia,ib,ic,t) {ia==0, ib==1, ic==1}


   t = ia+ib+ic;
}
