setenv WSPC decl40
setenv MAIN $WSPC.c
delete $WSPC
setproperty CHECK_C_SYNTAX_BEFORE_RUNNING_PIPS FALSE
create $WSPC $MAIN
activate C_PARSER
activate PRINT_CODE
setproperty PRETTYPRINT_LANGUAGE "C"
setproperty PRETTYPRINT_STATEMENT_NUMBER TRUE
display PRINTED_FILE[foo]

apply UNSPLIT
shell cc -c decl40.database/Src/decl40.c

close
delete $WSPC
quit
