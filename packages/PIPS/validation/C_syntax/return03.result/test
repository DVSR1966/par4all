Printed file

void return03()
{
   int i, j;
   if (i) goto l99999;
   j = 2;
   goto l0000;
l99999:   ;
   j = 1;
l0000:   return;
}

Printed file after restructure_control

void return03()
{
   int i, j;
   if (i)
      j = 1;
   else
      j = 2;
l0000:   return;
