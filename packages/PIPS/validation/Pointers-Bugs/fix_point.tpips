echo
module main
activate PROPER_EFFECTS_WITH_POINTS_TO
#setenv PROPER_EFFECTS_DEBUG_LEVEL 8
activate PRINT_CODE_PROPER_EFFECTS
display PRINTED_FILE
 

close
delete fix_point
