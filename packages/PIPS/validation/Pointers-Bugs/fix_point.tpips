activate PRINT_CODE_PROPER_EFFECTS


echo
echo Proper effects for "main"
echo
display PRINTED_FILE[count]

activate PRINT_CODE_POINTS_TO_LIST

echo
echo POINTS TO for "count"
echo
display PRINTED_FILE[count]

echo 
