  // output image 0`imfilt on 0
  // 0`imfilt [15 poc 0] -> [15 output 0] 0 null
  si.mux[15][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`imfilt [15 output 0] -> [15 output 0] 0 null
  // nope

