  si.poc[1][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][1].kernel[i] = pi0[i];

  // 0`imopen [1 poc 1] -> [1 threshold 1] 176 threshold
  si.mux[1][3].op = SPOC_MUX_IN1;
  // threshold 176 threshold stage 1 side 1
  si.th[1][1].op = pi3? SPOC_TH_BINARIZE : SPOC_TH_NO_BINARIZE;
  sp.th[1][1].boundmin = pi1;
  sp.th[1][1].boundmax = pi2;


  // STAGE 2
  // 0`imcloseth [0 input 0] -> [2 alu] 179 and
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`imcloseth [1 input 0] -> [2 alu] 179 and
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`imcloseth [2 input 0] -> [2 alu] 179 and
  // nope
  // 0`imopenth [1 threshold 1] -> [2 alu] 179 and
  // 0`imopenth [2 input 1] -> [2 alu] 179 and
  // nope
  // alu 179 and stage 2
  si.alu[2][0].op = SPOC_ALU_AND_IN0_IN1;


  // STAGE 3
  // 0`imand [2 alu] -> [3 poc 0] 185 erode_8c
  si.mux[2][0].op = SPOC_MUX_IN1;
  si.mux[2][2].op = SPOC_MUX_IN0;
