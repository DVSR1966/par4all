    sp.poc[4][0].kernel[i] = pi1[i];


  // STAGE 5
  // 0`imout [4 poc 0] -> [5 poc 0] 215 erode_8c
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`imout [5 input 0] -> [5 poc 0] 215 erode_8c
  // nope
  // poc 215 erode_8c stage 5 side 0
  si.poc[5][0].op = SPOC_POC_ERODE;
  si.poc[5][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[5][0].kernel[i] = pi1[i];


  // STAGE 6
  // 0`imout [5 poc 0] -> [6 poc 0] 217 erode_8c
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`imout [6 input 0] -> [6 poc 0] 217 erode_8c
  // nope
  // poc 217 erode_8c stage 6 side 0
  si.poc[6][0].op = SPOC_POC_ERODE;
  si.poc[6][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[6][0].kernel[i] = pi1[i];


  // output image 0`imout on 0
  // 0`imout [6 poc 0] -> [6 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`imout [6 output 0] -> [6 output 0] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, NULL);

  return ret;
}

// FREIA-SPoC helper function for module license_plate_copies
freia_status license_plate_copies_helper_0_0_6(freia_data2d * o0, freia_data2d * i0,
  const int32_t * pi0,
  const int32_t * pi1)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`imout [0 input 0] -> [0 poc 0] 219 erode_8c
  // nope
  // poc 219 erode_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_ERODE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`imout [0 poc 0] -> [1 poc 0] 223 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`imout [1 input 0] -> [1 poc 0] 223 dilate_8c
  // nope
  // poc 223 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];


  // STAGE 2
  // 0`imout [1 poc 0] -> [2 poc 0] 225 dilate_8c
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`imout [2 input 0] -> [2 poc 0] 225 dilate_8c
  // nope
  // poc 225 dilate_8c stage 2 side 0
  si.poc[2][0].op = SPOC_POC_DILATE;
  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi0[i];


  // STAGE 3
  // 0`imout [2 poc 0] -> [3 poc 0] 227 dilate_8c
  si.mux[2][0].op = SPOC_MUX_IN0;
  // 0`imout [3 input 0] -> [3 poc 0] 227 dilate_8c
  // nope
  // poc 227 dilate_8c stage 3 side 0
  si.poc[3][0].op = SPOC_POC_DILATE;
  si.poc[3][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[3][0].kernel[i] = pi0[i];


  // STAGE 4
  // 0`imout [3 poc 0] -> [4 poc 0] 229 dilate_8c
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`imout [4 input 0] -> [4 poc 0] 229 dilate_8c
  // nope
  // poc 229 dilate_8c stage 4 side 0
  si.poc[4][0].op = SPOC_POC_DILATE;
  si.poc[4][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[4][0].kernel[i] = pi0[i];


  // STAGE 5
  // 0`imout [4 poc 0] -> [5 poc 0] 237 dilate_8c
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`imout [5 input 0] -> [5 poc 0] 237 dilate_8c
  // nope
  // poc 237 dilate_8c stage 5 side 0
  si.poc[5][0].op = SPOC_POC_DILATE;
  si.poc[5][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[5][0].kernel[i] = pi1[i];


  // STAGE 6
  // 0`out [5 poc 0] -> [6 poc 0] 238 dilate_8c
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`out [6 input 0] -> [6 poc 0] 238 dilate_8c
  // nope
  // poc 238 dilate_8c stage 6 side 0
  si.poc[6][0].op = SPOC_POC_DILATE;
  si.poc[6][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[6][0].kernel[i] = pi1[i];


  // output image 0`out on 0
  // 0`out [6 poc 0] -> [6 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`out [6 output 0] -> [6 output 0] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, NULL);

  return ret;
}

