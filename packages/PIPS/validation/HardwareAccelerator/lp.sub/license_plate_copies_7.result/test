// FREIA-SPoC helper function for module license_plate_copies
freia_status license_plate_copies_helper_0_0_4(freia_data2d * o0, freia_data2d * i0, freia_data2d * i1,
  const int32_t * pi0,
  int32_t pi1,
  int32_t pi2,
  int32_t pi3,
  const int32_t * pi4)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`imopen [0 input 1] -> [0 poc 1] 122 dilate_8c
  // nope
  // poc 122 dilate_8c stage 0 side 1
  si.poc[0][1].op = SPOC_POC_DILATE;
  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi0[i];


  // STAGE 1
  // 0`imopen [0 poc 1] -> [1 poc 1] 124 dilate_8c
  si.mux[0][3].op = SPOC_MUX_IN1;
  // 0`imopen [1 input 1] -> [1 poc 1] 124 dilate_8c
  // nope
  // poc 124 dilate_8c stage 1 side 1
