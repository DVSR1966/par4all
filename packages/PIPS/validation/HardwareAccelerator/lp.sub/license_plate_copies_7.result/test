   ret_0 |= freia_aipo_copy(imtmp_0, imopen);                           /*0081*/
   ret_0 |= freia_aipo_erode_8c(imopen, imtmp_0, kernel1x3);            /*0082*/
   ret_0 |= freia_aipo_copy(imtmp_0, imopen);                           /*0083*/
