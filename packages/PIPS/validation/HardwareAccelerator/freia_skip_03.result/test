! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 4 ops and 0 copies, optimized to 4 ops and 0+0 copies

// hold kernels for freia_skip_03_helper_0_0_0
static cl_kernel freia_skip_03_helper_0_0_0_kernel[2];

// compile kernels for freia_skip_03_helper_0_0_0
static freia_status freia_skip_03_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_skip_03_helper_0_0_0
  const char * freia_skip_03_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_skip_03_helper_0_0_0\n"
    "KERNEL void freia_skip_03_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0;\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  int2 mmin = { PIXEL_MAX, 0 };\n"
    "  int2 mmax = { PIXEL_MIN, 0 };\n"
    "  int idy = get_global_id(0);\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int gid = pitch*get_global_id(0);\n"
    "  int i;\n"
    "  for (i=gid; i < (gid+width); i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL_VOLUME(red10, in0);\n"
    "    PIXEL t11 = PIXEL_ADD(in0, c0);\n"
    "    PIXEL t13 = PIXEL_ADD(t11, c1);\n"
    "    // set output pixels\n"
    "    p0[i] = t13;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  redX[idy].vol = vol;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_skip_03_helper_0_0_0_source, "freia_skip_03_helper_0_0_0", "-DPIXEL8", &freia_skip_03_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_skip_03_helper_0_0_0_source, "freia_skip_03_helper_0_0_0", "-DPIXEL16", &freia_skip_03_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_skip_03_helper_0_0_0
freia_status freia_skip_03_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  int32_t * po2,
  int32_t c0,
  int32_t c1)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_skip_03_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_skip_03_helper_0_0_0_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel freia_skip_03_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 1, 1, 2, 1, o0, i0, c0, c1, &redres);

  // return reduction results
  *po2 = redres.volume;

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_skip_03

// opencl for dag 0

// opencl function freia_skip_03_helper_0_0_0
KERNEL void freia_skip_03_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width,
  int pitch,
  int c0,
  int c1,
  GLOBAL TMeasure * redX)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;

  // reduction stuff is currently hardcoded...
  int vol = 0;
  int2 mmin = { PIXEL_MAX, 0 };
  int2 mmax = { PIXEL_MIN, 0 };
  int idy = get_global_id(0);

  // thread's pixel loop
  int gid = pitch*get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL_VOLUME(red10, in0);
    PIXEL t11 = PIXEL_ADD(in0, c0);
    PIXEL t13 = PIXEL_ADD(t11, c1);
    // set output pixels
    p0[i] = t13;
  }

  // reduction copy out
  redX[idy].vol = vol;
}

