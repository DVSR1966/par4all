//             <    is read   >: i0 i0[0].stuff i1 i1[0].stuff o
//             <    is written>: o[0].stuff
freia_status freia_aipo_sup(freia_data2d *o, const freia_data2d *i0, const freia_data2d *i1)
{
//             <    is read   >: i0 i0[0].stuff i1 i1[0].stuff o
//             <    is written>: o[0].stuff
   o->stuff = i0->stuff|i1->stuff;
   return FREIA_OK;
}
//             <    is read   >: c i i[0].stuff o
//             <    is written>: o[0].stuff
freia_status freia_aipo_sup_const(freia_data2d *o, const freia_data2d *i, int32_t c)
{
//             <    is read   >: c i i[0].stuff o
//             <    is written>: o[0].stuff
   o->stuff = i->stuff|c;
   return FREIA_OK;
}
//             <may be read   >: c1 c2
//             <    is read   >: b i i[0].stuff o
//             <    is written>: o[0].stuff
freia_status freia_aipo_threshold(freia_data2d *o, const freia_data2d *i, int32_t c1, int32_t c2, bool b)
{
//             <may be read   >: c1 c2
//             <    is read   >: b i i[0].stuff o
//             <    is written>: o[0].stuff
   o->stuff = i->stuff|b?c1:c2;
   return FREIA_OK;
}
//             <    is read   >: i0 i0[0].stuff i1 i1[0].stuff o
//             <    is written>: o[0].stuff
freia_status freia_aipo_xor(freia_data2d *o, const freia_data2d *i0, const freia_data2d *i1)
{
//             <    is read   >: i0 i0[0].stuff i1 i1[0].stuff o
//             <    is written>: o[0].stuff
   o->stuff = i0->stuff|i1->stuff;
   return FREIA_OK;
}
//             <    is read   >: c i i[0].stuff o
//             <    is written>: o[0].stuff
freia_status freia_aipo_xor_const(freia_data2d *o, const freia_data2d *i, int32_t c)
{
//             <    is read   >: c i i[0].stuff o
//             <    is written>: o[0].stuff
   o->stuff = i->stuff|c;
   return FREIA_OK;
}
//             <    is read   >: c1 c2 i i[0].stuff o
//             <    is written>: o[0].stuff
freia_status freia_cipo_gradient(freia_data2d *o, const freia_data2d *i, int32_t c1, uint32_t c2)
{
//             <    is read   >: c1 c2 i i[0].stuff o
//             <    is written>: o[0].stuff
   o->stuff = i->stuff|c1+c2;
   return FREIA_OK;
}
//             <    is read   >: c1 c2 i i[0].stuff o
//             <    is written>: o[0].stuff
freia_status freia_cipo_inner_gradient(freia_data2d *o, const freia_data2d *i, int32_t c1, uint32_t c2)
{
//             <    is read   >: c1 c2 i i[0].stuff o
//             <    is written>: o[0].stuff
   o->stuff = i->stuff|c1+c2;
   return FREIA_OK;
}
//             <    is read   >: n global_io_effect
//             <    is written>: n[0].stuff global_io_effect
freia_status freia_common_close_input(freia_dataio *n)
{
//             <    is read   >: n
//             <    is written>: n[0].stuff
   n->stuff = 0;
//             <    is read   >: global_io_effect
//             <    is written>: global_io_effect
   global_io_effect++;
   return FREIA_OK;
}
//             <    is read   >: n global_io_effect
//             <    is written>: n[0].stuff global_io_effect
freia_status freia_common_close_output(freia_dataio *n)
{
//             <    is read   >: n
//             <    is written>: n[0].stuff
   n->stuff = 0;
//             <    is read   >: global_io_effect
//             <    is written>: global_io_effect
   global_io_effect++;
   return FREIA_OK;
}
//             <    is read   >: x y z _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
freia_data2d * freia_common_create_data(uint32_t x, uint32_t y, uint32_t z)
{
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: img _MALLOC_EFFECTS:_MALLOC_
   freia_data2d *img = (freia_data2d *) malloc(sizeof(freia_data2d));
//             <    is read   >: img x y z
//             <    is written>: img[0].stuff
   img->stuff = (int32_t) (x&y&z);
//             <    is read   >: img
   return img;
}
//             <    is read   >: img _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
freia_status freia_common_destruct_data(freia_data2d *img)
{
//             <    is read   >: img _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   free(img);
   return FREIA_OK;
}
//             <    is read   >: color image image[0].stuff x1 x2 y1 
//                               y2
//             <    is written>: image[0].stuff
freia_status freia_common_draw_line(freia_data2d *image, int32_t x1, int32_t y1, int32_t x2, int32_t y2, int32_t color)
{
//             <    is read   >: color image image[0].stuff x1 x2 y1 
//                               y2
//             <    is written>: image[0].stuff
   image->stuff += color+x1+y1+x2+y2;
}
//             <    is read   >: color image image[0].stuff x1 x2 y1 
//                               y2
//             <    is written>: image[0].stuff
freia_status freia_common_draw_rect(freia_data2d *image, int32_t x1, int32_t y1, int32_t x2, int32_t y2, int32_t color)
{
//             <    is read   >: color image image[0].stuff x1 x2 y1 
//                               y2
//             <    is written>: image[0].stuff
   image->stuff += color+x1+y1+x2+y2;
}
//             <    is read   >: in n global_io_effect
//             <    is written>: in[0].framebpp in[0].frameheight 
//                               in[0].framewidth in[0].stuff 
//                               global_io_effect
freia_status freia_common_open_input(freia_dataio *in, uint32_t n)
{
//             <    is read   >: in
//             <    is written>: in[0].framebpp
   in->framebpp = 16;
//             <    is read   >: in
//             <    is written>: in[0].frameheight
   in->frameheight = 256;
//             <    is read   >: in
//             <    is written>: in[0].framewidth
   in->framewidth = 512;
//             <    is read   >: in n
//             <    is written>: in[0].stuff
   in->stuff = n;
//             <    is read   >: global_io_effect
//             <    is written>: global_io_effect
   global_io_effect++;
   return FREIA_OK;
}
//             <    is read   >: b h n out w global_io_effect
//             <    is written>: out[0].framebpp out[0].frameheight 
//                               out[0].framewidth out[0].stuff 
//                               global_io_effect
freia_status freia_common_open_output(freia_dataio *out, uint32_t n, uint32_t w, uint32_t h, uint32_t b)
{
//             <    is read   >: b out
//             <    is written>: out[0].framebpp
   out->framebpp = b;
//             <    is read   >: h out
//             <    is written>: out[0].frameheight
   out->frameheight = h;
//             <    is read   >: out w
//             <    is written>: out[0].framewidth
   out->framewidth = w;
//             <    is read   >: n out
//             <    is written>: out[0].stuff
   out->stuff = n;
//             <    is read   >: global_io_effect
//             <    is written>: global_io_effect
   global_io_effect++;
   return FREIA_OK;
}
//             <    is read   >: img in in[0].stuff global_io_effect
//             <    is written>: img[0].stuff global_io_effect
freia_status freia_common_rx_image(freia_data2d *img, freia_dataio *in)
{
//             <    is read   >: img in in[0].stuff
//             <    is written>: img[0].stuff
   img->stuff = in->stuff;
//             <    is read   >: global_io_effect
//             <    is written>: global_io_effect
   global_io_effect++;
   return FREIA_OK;
}
//             <    is read   >: img img[0].stuff out out[0].stuff 
//                               global_io_effect
//             <    is written>: out[0].stuff global_io_effect
freia_status freia_common_tx_image(const freia_data2d *img, freia_dataio *out)
{
//             <    is read   >: img img[0].stuff out out[0].stuff
//             <    is written>: out[0].stuff
   out->stuff += img->stuff;
//             <    is read   >: global_io_effect
//             <    is written>: global_io_effect
   global_io_effect++;
   return FREIA_OK;
}

