! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// FREIA-SPoC helper function for module freia_28
freia_status freia_28_helper_0_0_0(freia_data2d * o0, freia_data2d * i0,
  int32_t * po0,
  const int32_t * pi1,
  int32_t * po2)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // i [0 input 0] -> [0 measure 0] 0 undefined
  si.mux[0][0].op = SPOC_MUX_IN0;


  // STAGE 1
  // i [0 measure 0] -> [1 poc 0] 18 erode_8c
  // i [1 input 0] -> [1 poc 0] 18 erode_8c
  // nope
  // poc 18 erode_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_ERODE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi1[i];

  // 0`X0 [1 poc 0] -> [1 measure 0] 18 erode_8c
  si.mux[1][0].op = SPOC_MUX_IN0;


  // output image 0`X0 on 0
  // 0`X0 [1 measure 0] -> [1 output 0] 0 null
  // nope

  // fill in to the end...
  // 0`X0 [1 output 0] -> [7 output 0] 0 null
  // 0`X0 [2 input 0] -> [7 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;
  // 0`X0 [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`X0 [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`X0 [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`X0 [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // 0`X0 [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i0);

  // get reductions
  freia_cg_read_reduction_results(&redres);

  // measure 16 global_max stage 0 side 0
  *po0 = (int32_t) reduc.measure[0][0].maximum;

  // measure 19 global_min stage 1 side 0
  *po2 = (int32_t) reduc.measure[1][0].minimum;

  return ret;
}

// FREIA-SPoC helper function for module freia_28
freia_status freia_28_helper_0_1_0(freia_data2d * o0, freia_data2d * o1, freia_data2d * i0, freia_data2d * i1,
  int32_t pi0,
  int32_t pi1,
  int32_t pi2,
  const int32_t * pi3,
  int32_t * po4,
  int32_t pi5,
  int32_t pi6,
  int32_t pi7)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // i [0 input 1] -> [0 threshold 1] 17 threshold
  si.mux[0][3].op = SPOC_MUX_IN1;
  // threshold 17 threshold stage 0 side 1
  si.th[0][1].op = pi2? SPOC_TH_BINARIZE : SPOC_TH_NO_BINARIZE;
  sp.th[0][1].boundmin = pi0;
  sp.th[0][1].boundmax = pi1;


  // STAGE 1
  // 0`tB [0 threshold 1] -> [1 poc 1] 22 dilate_8c
  // 0`tB [1 input 1] -> [1 poc 1] 22 dilate_8c
  // nope
  // poc 22 dilate_8c stage 1 side 1
  si.poc[1][1].op = SPOC_POC_DILATE;
  si.poc[1][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][1].kernel[i] = pi3[i];

  // 0`X1 [1 poc 1] -> [1 measure 1] 22 dilate_8c
  si.mux[1][3].op = SPOC_MUX_IN1;


  // STAGE 0
  // 0`X0 [0 input 0] -> [0 threshold 0] 20 threshold
  si.mux[0][0].op = SPOC_MUX_IN0;
  // threshold 20 threshold stage 0 side 0
  si.th[0][0].op = pi7? SPOC_TH_BINARIZE : SPOC_TH_NO_BINARIZE;
  sp.th[0][0].boundmin = pi5;
  sp.th[0][0].boundmax = pi6;


  // output image 0`tC on 0 and image 0`X1 on 1
  // 0`tC [0 threshold 0] -> [1 output 0] 0 null
  // 0`tC [1 input 0] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`tC [1 output 0] -> [7 output 0] 0 null
  // 0`tC [2 input 0] -> [7 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;
  // 0`tC [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`tC [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`tC [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`tC [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // 0`tC [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  // 0`X1 [1 measure 1] -> [1 output 1] 0 null
  // nope

  // fill in to the end...
  // 0`X1 [1 output 1] -> [7 output 1] 0 null
  // 0`X1 [2 input 1] -> [7 output 1] 0 null
  si.mux[2][3].op = SPOC_MUX_IN1;
  // 0`X1 [3 input 1] -> [7 output 1] 0 null
  si.mux[3][3].op = SPOC_MUX_IN1;
  // 0`X1 [4 input 1] -> [7 output 1] 0 null
  si.mux[4][3].op = SPOC_MUX_IN1;
  // 0`X1 [5 input 1] -> [7 output 1] 0 null
  si.mux[5][3].op = SPOC_MUX_IN1;
  // 0`X1 [6 input 1] -> [7 output 1] 0 null
  si.mux[6][3].op = SPOC_MUX_IN1;
  // 0`X1 [7 input 1] -> [7 output 1] 0 null
  si.mux[7][3].op = SPOC_MUX_IN1;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  // get reductions
  freia_cg_read_reduction_results(&redres);

  // measure 23 global_vol stage 1 side 1
  *po4 = (int32_t) reduc.measure[1][1].volume;

  return ret;
}

// FREIA-SPoC helper function for module freia_28
freia_status freia_28_helper_0_2_0(freia_data2d * o0, freia_data2d * i0)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`tC [0 input 0] -> [0 alu] 21 add
  // nope
  // 0`tC [0 input 1] -> [0 alu] 21 add
  // nope
  // alu 21 add stage 0
  si.alu[0][0].op = SPOC_ALU_ADD_IN0_IN1;


  // output image 0`tC2 on 0
  // 0`tC2 [0 alu] -> [0 output 0] 0 null
  si.mux[0][0].op = SPOC_MUX_IN1;
  si.mux[0][2].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`tC2 [0 output 0] -> [7 output 0] 0 null
  // 0`tC2 [1 input 0] -> [7 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`tC2 [2 input 0] -> [7 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;
  // 0`tC2 [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`tC2 [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`tC2 [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`tC2 [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // 0`tC2 [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i0);

  return ret;
}

// FREIA-SPoC helper function for module freia_28
freia_status freia_28_helper_0_3_0(freia_data2d * o0, freia_data2d * i0, freia_data2d * i1,
  int32_t pi0,
  int32_t pi1,
  int32_t pi2)
{
  spoc_instr si;
  spoc_param sp;
  spoc_reduction reduc;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_reduction_results redres;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`X1 [0 input 0] -> [0 threshold 0] 24 threshold
  si.mux[0][0].op = SPOC_MUX_IN0;
  // threshold 24 threshold stage 0 side 0
  si.th[0][0].op = pi2? SPOC_TH_BINARIZE : SPOC_TH_NO_BINARIZE;
  sp.th[0][0].boundmin = pi0;
  sp.th[0][0].boundmax = pi1;


  // STAGE 1
  // 0`tD [0 threshold 0] -> [1 alu] 25 add
  // 0`tD [1 input 0] -> [1 alu] 25 add
  // nope
  // 0`tC2 [0 input 1] -> [1 alu] 25 add
  si.mux[0][3].op = SPOC_MUX_IN1;
  // 0`tC2 [1 input 1] -> [1 alu] 25 add
  // nope
  // alu 25 add stage 1
  si.alu[1][0].op = SPOC_ALU_ADD_IN0_IN1;


  // output image o on 0
  // o [1 alu] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN1;
  si.mux[1][2].op = SPOC_MUX_IN0;

  // fill in to the end...
  // o [1 output 0] -> [7 output 0] 0 null
  // o [2 input 0] -> [7 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;
  // o [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // o [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // o [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // o [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // o [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i1);

  return ret;
}

! updated function

freia_status freia_28(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tC = freia_common_create_data(16, 128, 128), *tC2 = freia_common_create_data(16, 128, 128);       /*0012*/
   int sA, sB, sC;                                                      /*0013*/
   
   // complex dag for split
   freia_28_helper_0_0_0(X0, i, &sA, k, &sB);                           /*0016*/
   freia_28_helper_0_1_0(tC, X1, X0, i, sA-10, sA-1, false, k, &sC, sB-10, sB-1, false);              /*0017*/
   freia_28_helper_0_2_0(tC2, tC);                                      /*0021*/
   freia_28_helper_0_3_0(o, X1, tC2, sC-10, sC-1, false);               /*0024*/

   freia_common_destruct_data(X0);                                      /*0027*/
   freia_common_destruct_data(X1);                                      /*0028*/
   freia_common_destruct_data(tC);                                      /*0030*/
   return FREIA_OK;                                                     /*0032*/
}

! cleaned up function

freia_status freia_28(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tC = freia_common_create_data(16, 128, 128), *tC2 = freia_common_create_data(16, 128, 128);       /*0012*/
   int sA, sB, sC;                                                      /*0013*/
   
   // complex dag for split
   freia_28_helper_0_0_0(X0, i, &sA, k, &sB);                           /*0016*/
   freia_28_helper_0_1_0(tC, X1, X0, i, sA-10, sA-1, 0, k, &sC, sB-10, sB-1, 0);       /*0017*/
   freia_28_helper_0_2_0(tC2, tC);                                      /*0021*/
   freia_28_helper_0_3_0(o, X1, tC2, sC-10, sC-1, 0);                   /*0024*/

   freia_common_destruct_data(X0);                                      /*0027*/
   freia_common_destruct_data(X1);                                      /*0028*/
   freia_common_destruct_data(tC);                                      /*0030*/
   return 0;                                                            /*0032*/
}
#
# TERAPIX
#

! initial function

freia_status freia_28(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tB = freia_common_create_data(16, 128, 128), *tC = freia_common_create_data(16, 128, 128), *tC2 = freia_common_create_data(16, 128, 128), *tD = freia_common_create_data(16, 128, 128);     /*0012*/
   int sA, sB, sC;                                                      /*0013*/
   
   // complex dag for split
   freia_aipo_global_max(i, &sA);                                       /*0016*/
   freia_aipo_threshold(tB, i, sA-10, sA-1, false);                     /*0017*/
   freia_aipo_erode_8c(X0, i, k);                                       /*0018*/
   freia_aipo_global_min(X0, &sB);                                      /*0019*/
   freia_aipo_threshold(tC, X0, sB-10, sB-1, false);                    /*0020*/
   freia_aipo_add(tC2, tC, tC);                                         /*0021*/
   freia_aipo_dilate_8c(X1, tB, k);                                     /*0022*/
   freia_aipo_global_vol(X1, &sC);                                      /*0023*/
   freia_aipo_threshold(tD, X1, sC-10, sC-1, false);                    /*0024*/
   freia_aipo_add(o, tC2, tD);                                          /*0025*/

   freia_common_destruct_data(X0);                                      /*0027*/
   freia_common_destruct_data(X1);                                      /*0028*/
   freia_common_destruct_data(tB);                                      /*0029*/
   freia_common_destruct_data(tC);                                      /*0030*/
   freia_common_destruct_data(tD);                                      /*0031*/
   return FREIA_OK;                                                     /*0032*/
}

