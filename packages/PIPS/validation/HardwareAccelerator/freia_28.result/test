! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <terapix.h>
#include <terapix_ucode.h>

/* FREIA terapix helper function for module freia_28
 * 2 input images, 1 output image
 * 4 image operations in dag
 * dag length is 3, dag width is 2
 * cost is 9 cycles per imagelet row
 */
freia_status freia_28_helper_0_0(
  freia_data2d * o0,
  freia_data2d * i0,
  freia_data2d * i1,
  int32_t pi3,
  int32_t pi4,
  int32_t pi5)
{
  // declarations:
  freia_microcode mcode;
  freia_op_param param;
  freia_dynamic_param dyn_param;
  terapix_gram gram;
  int i;
  freia_status ret;
  // operation 0 parameters
  int16_t p_0[3];
  const int32_t x_0 = 0;
  const int32_t y_0 = 0;
  terapix_mcu_instr mcu_instr[2][2];

  // imagelets definitions:
  // - 2 computation imagelets
  // - 2 double buffer imagelets
  int imagelet_size = 256;
  int imagelet_1 = 0;
  int imagelet_2 = 256;
  int imagelet_3 = 512;
  int imagelet_4 = 768;

  // double buffer assignment
  int imagelet_io_1_0 = imagelet_1;
  int imagelet_io_1_1 = imagelet_3;
  int imagelet_io_2_0 = imagelet_2;
  int imagelet_io_2_1 = imagelet_4;

  // body:
  // mcode param
  mcode.raw = (void*) terapix_ucode_array;
  mcode.size = TERAPIX_UCODE_SIZE_T;
  freia_mg_write_microcode(&mcode);

  // subimage operation
  param.size = sizeof(mcu_instr);
  param.raw = (void*) mcu_instr;

  // dyn_param contents  dyn_param.raw = &gram;
  dyn_param.size = sizeof(terapix_gram);
  // 0: thr(1) -> 1
  mcu_instr[0][0].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][0].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][0].macrocode.ymin1 = 0;
  mcu_instr[1][0].macrocode.ymin1 = 0;
  mcu_instr[0][0].macrocode.xmin2 = imagelet_1;
  mcu_instr[1][0].macrocode.xmin2 = imagelet_1;
  mcu_instr[0][0].macrocode.ymin2 = 0;
  mcu_instr[1][0].macrocode.ymin2 = 0;
  // copy of operation 0 parameters
  p_0[0] = pi3;
  p_0[1] = pi4;
  p_0[2] = pi5;
  gram.xoffset = x_0;
  gram.yoffset = y_0;
  gram.width = 3;
  gram.height = 1;
  gram.params = p_0;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_instr[0][0].macrocode.xmin3 = x_0;
  mcu_instr[1][0].macrocode.xmin3 = x_0;
  mcu_instr[0][0].macrocode.ymin3 = y_0;
  mcu_instr[1][0].macrocode.ymin3 = y_0;
  mcu_instr[0][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][0].macrocode.iter2 = imagelet_size;
  mcu_instr[1][0].macrocode.iter2 = imagelet_size;
  mcu_instr[0][0].macrocode.addrStart = TERAPIX_UCODE_THRESHOLD;
  mcu_instr[1][0].macrocode.addrStart = TERAPIX_UCODE_THRESHOLD;
  // 1: +(2,1) -> 1
  mcu_instr[0][1].macrocode.xmin1 = imagelet_io_2_0;
  mcu_instr[1][1].macrocode.xmin1 = imagelet_io_2_1;
  mcu_instr[0][1].macrocode.ymin1 = 0;
  mcu_instr[1][1].macrocode.ymin1 = 0;
  mcu_instr[0][1].macrocode.xmin2 = imagelet_1;
  mcu_instr[1][1].macrocode.xmin2 = imagelet_1;
  mcu_instr[0][1].macrocode.ymin2 = 0;
  mcu_instr[1][1].macrocode.ymin2 = 0;
  mcu_instr[0][1].macrocode.xmin3 = imagelet_io_1_0;
  mcu_instr[1][1].macrocode.xmin3 = imagelet_io_1_1;
  mcu_instr[0][1].macrocode.ymin3 = 0;
  mcu_instr[1][1].macrocode.ymin3 = 0;
  mcu_instr[0][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][1].macrocode.iter2 = imagelet_size;
  mcu_instr[1][1].macrocode.iter2 = imagelet_size;
  mcu_instr[0][1].macrocode.addrStart = TERAPIX_UCODE_ADD3;
  mcu_instr[1][1].macrocode.addrStart = TERAPIX_UCODE_ADD3;

  // inputs:
  // - imagelet 1 is i0 for X1
  // - imagelet 2 is i1 for tC2

  // outputs:
  // - imagelet 1 is o0 for o

  // double buffer management:
  // - buffer 1/3
  // - buffer 2/4

  // output imagelet erosion:
  //   north: 0
  //   south: 0
  //    west: 0
  //    east: 0

  ret = freia_cg_template_process_2i_1o(&param, o0, i0, i1);

  // extract measures

  return ret;
}


/* FREIA terapix helper function for module freia_28
 * 2 input images, 2 output images
 * 7 image operations in dag
 * dag length is 4, dag width is 2
 * cost is 32 cycles per imagelet row
 */
freia_status freia_28_helper_0_1(
  freia_data2d * o0,
  freia_data2d * o1,
  freia_data2d * i0,
  freia_data2d * i1,
  int32_t pi4,
  int32_t pi5,
  int32_t pi6,
  const int32_t * pi7,
  int32_t * po8,
  int32_t pi9,
  int32_t pi10,
  int32_t pi11)
{
  // declarations:
  freia_microcode mcode;
  freia_op_param param;
  freia_dynamic_param dyn_param;
  terapix_gram gram;
  int i;
  freia_status ret;
  // operation 0 parameters
  int16_t p_0[3];
  const int32_t x_0 = 0;
  const int32_t y_0 = 0;
  // operation 1 parameters
  int16_t p_1[9];
  const int32_t x_1 = 3;
  const int32_t y_1 = 0;
  // operation 3 parameters
  int16_t p_3[3];
  const int32_t x_3 = 6;
  const int32_t y_3 = 0;
  terapix_mcu_instr mcu_instr[2][6];

  // imagelets definitions:
  // - 3 computation imagelets
  // - 2 double buffer imagelets
  int imagelet_size = 204;
  int imagelet_1 = 0;
  int imagelet_2 = 204;
  int imagelet_3 = 408;
  int imagelet_4 = 612;
  int imagelet_5 = 816;

  // double buffer assignment
  int imagelet_io_1_0 = imagelet_1;
  int imagelet_io_1_1 = imagelet_4;
  int imagelet_io_2_0 = imagelet_2;
  int imagelet_io_2_1 = imagelet_5;

  // body:
  // mcode param
  mcode.raw = (void*) terapix_ucode_array;
  mcode.size = TERAPIX_UCODE_SIZE_T;
  freia_mg_write_microcode(&mcode);

  // subimage operation
  param.size = sizeof(mcu_instr);
  param.raw = (void*) mcu_instr;

  // dyn_param contents  dyn_param.raw = &gram;
  dyn_param.size = sizeof(terapix_gram);
  // 0: thr(2) -> 2
  mcu_instr[0][0].macrocode.xmin1 = imagelet_io_2_0;
  mcu_instr[1][0].macrocode.xmin1 = imagelet_io_2_1;
  mcu_instr[0][0].macrocode.ymin1 = 0;
  mcu_instr[1][0].macrocode.ymin1 = 0;
  mcu_instr[0][0].macrocode.xmin2 = imagelet_2;
  mcu_instr[1][0].macrocode.xmin2 = imagelet_2;
  mcu_instr[0][0].macrocode.ymin2 = 0;
  mcu_instr[1][0].macrocode.ymin2 = 0;
  // copy of operation 0 parameters
  p_0[0] = pi4;
  p_0[1] = pi5;
  p_0[2] = pi6;
  gram.xoffset = x_0;
  gram.yoffset = y_0;
  gram.width = 3;
  gram.height = 1;
  gram.params = p_0;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_instr[0][0].macrocode.xmin3 = x_0;
  mcu_instr[1][0].macrocode.xmin3 = x_0;
  mcu_instr[0][0].macrocode.ymin3 = y_0;
  mcu_instr[1][0].macrocode.ymin3 = y_0;
  mcu_instr[0][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][0].macrocode.iter2 = imagelet_size;
  mcu_instr[1][0].macrocode.iter2 = imagelet_size;
  mcu_instr[0][0].macrocode.addrStart = TERAPIX_UCODE_THRESHOLD;
  mcu_instr[1][0].macrocode.addrStart = TERAPIX_UCODE_THRESHOLD;
  // 1: D8(2) -> 3
  mcu_instr[0][1].macrocode.xmin1 = imagelet_2;
  mcu_instr[1][1].macrocode.xmin1 = imagelet_2;
  mcu_instr[0][1].macrocode.ymin1 = 0;
  mcu_instr[1][1].macrocode.ymin1 = 0;
  mcu_instr[0][1].macrocode.xmin2 = imagelet_io_3_0;
  mcu_instr[1][1].macrocode.xmin2 = imagelet_io_3_1;
  mcu_instr[0][1].macrocode.ymin2 = 0;
  mcu_instr[1][1].macrocode.ymin2 = 0;
  // copy of operation 1 parameters
  for(i=0; i<9; i++)
    p_1[i] = pi7[i];
  gram.xoffset = x_1;
  gram.yoffset = y_1;
  gram.width = 3;
  gram.height = 3;
  gram.params = p_1;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_instr[0][1].macrocode.xmin3 = x_1;
  mcu_instr[1][1].macrocode.xmin3 = x_1;
  mcu_instr[0][1].macrocode.ymin3 = y_1;
  mcu_instr[1][1].macrocode.ymin3 = y_1;
  mcu_instr[0][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][1].macrocode.iter2 = imagelet_size;
  mcu_instr[1][1].macrocode.iter2 = imagelet_size;
  mcu_instr[0][1].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  mcu_instr[1][1].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  // set measure vol at 1022
  // 2: vol(3)
  mcu_instr[0][2].macrocode.xmin1 = imagelet_io_3_0;
  mcu_instr[1][2].macrocode.xmin1 = imagelet_io_3_1;
  mcu_instr[0][2].macrocode.ymin1 = 0;
  mcu_instr[1][2].macrocode.ymin1 = 0;
  mcu_instr[0][2].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][2].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][2].macrocode.iter2 = imagelet_size;
  mcu_instr[1][2].macrocode.iter2 = imagelet_size;
  mcu_instr[0][2].macrocode.addrStart = TERAPIX_UCODE_VOL?;
  mcu_instr[1][2].macrocode.addrStart = TERAPIX_UCODE_VOL?;
  // 3: thr(1) -> 3
  mcu_instr[0][3].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][3].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][3].macrocode.ymin1 = 0;
  mcu_instr[1][3].macrocode.ymin1 = 0;
  mcu_instr[0][3].macrocode.xmin2 = imagelet_3;
  mcu_instr[1][3].macrocode.xmin2 = imagelet_3;
  mcu_instr[0][3].macrocode.ymin2 = 0;
  mcu_instr[1][3].macrocode.ymin2 = 0;
  // copy of operation 3 parameters
  p_3[0] = pi9;
  p_3[1] = pi10;
  p_3[2] = pi11;
  gram.xoffset = x_3;
  gram.yoffset = y_3;
  gram.width = 3;
  gram.height = 1;
  gram.params = p_3;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_instr[0][3].macrocode.xmin3 = x_3;
  mcu_instr[1][3].macrocode.xmin3 = x_3;
  mcu_instr[0][3].macrocode.ymin3 = y_3;
  mcu_instr[1][3].macrocode.ymin3 = y_3;
  mcu_instr[0][3].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][3].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][3].macrocode.iter2 = imagelet_size;
  mcu_instr[1][3].macrocode.iter2 = imagelet_size;
  mcu_instr[0][3].macrocode.addrStart = TERAPIX_UCODE_THRESHOLD;
  mcu_instr[1][3].macrocode.addrStart = TERAPIX_UCODE_THRESHOLD;
  // 4: +(3,3) -> 1
  mcu_instr[0][4].macrocode.xmin1 = imagelet_3;
  mcu_instr[1][4].macrocode.xmin1 = imagelet_3;
  mcu_instr[0][4].macrocode.ymin1 = 0;
  mcu_instr[1][4].macrocode.ymin1 = 0;
  mcu_instr[0][4].macrocode.xmin2 = imagelet_3;
  mcu_instr[1][4].macrocode.xmin2 = imagelet_3;
  mcu_instr[0][4].macrocode.ymin2 = 0;
  mcu_instr[1][4].macrocode.ymin2 = 0;
  mcu_instr[0][4].macrocode.xmin3 = imagelet_io_1_0;
  mcu_instr[1][4].macrocode.xmin3 = imagelet_io_1_1;
  mcu_instr[0][4].macrocode.ymin3 = 0;
  mcu_instr[1][4].macrocode.ymin3 = 0;
  mcu_instr[0][4].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][4].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][4].macrocode.iter2 = imagelet_size;
  mcu_instr[1][4].macrocode.iter2 = imagelet_size;
  mcu_instr[0][4].macrocode.addrStart = TERAPIX_UCODE_ADD3;
  mcu_instr[1][4].macrocode.addrStart = TERAPIX_UCODE_ADD3;
  // copy 3 -> 2
  mcu_instr[0][5].macrocode.xmin1 = imagelet_3;
  mcu_instr[1][5].macrocode.xmin1 = imagelet_3;
  mcu_instr[0][5].macrocode.ymin1 = 0;
  mcu_instr[1][5].macrocode.ymin1 = 0;
  mcu_instr[0][5].macrocode.xmin2 = imagelet_2;
  mcu_instr[1][5].macrocode.xmin2 = imagelet_2;
  mcu_instr[0][5].macrocode.ymin2 = 0;
  mcu_instr[1][5].macrocode.ymin2 = 0;
  mcu_instr[0][5].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][5].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][5].macrocode.iter2 = imagelet_size;
  mcu_instr[1][5].macrocode.iter2 = imagelet_size;
  mcu_instr[0][5].macrocode.addrStart = TERAPIX_UCODE_COPY;
  mcu_instr[1][5].macrocode.addrStart = TERAPIX_UCODE_COPY;

  // inputs:
  // - imagelet 1 is i0 for X0
  // - imagelet 2 is i1 for i

  // outputs:
  // - imagelet 1 is o0 for tC2
  // - imagelet 2 is o1 for X1

  // double buffer management:
  // - buffer 1/4
  // - buffer 2/5

  // output imagelet erosion:
  //   north: 1
  //   south: 1
  //    west: 1
  //    east: 1

  ret = freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  // extract measures
  // get measure vol result from 1022

  return ret;
}


/* FREIA terapix helper function for module freia_28
 * 1 input image, 1 output image
 * 4 image operations in dag
 * dag length is 3, dag width is 1
 * cost is 21 cycles per imagelet row
 */
freia_status freia_28_helper_0_2(
  freia_data2d * o0,
  freia_data2d * i0,
  int32_t * po2,
  const int32_t * pi3,
  int32_t * po4)
{
  // declarations:
  freia_microcode mcode;
  freia_op_param param;
  freia_dynamic_param dyn_param;
  terapix_gram gram;
  int i;
  freia_status ret;
  // operation 1 parameters
  int16_t p_1[9];
  const int32_t x_1 = 0;
  const int32_t y_1 = 0;
  terapix_mcu_instr mcu_instr[2][4];

  // imagelets definitions:
  // - 2 computation imagelets
  // - 1 double buffer imagelets
  int imagelet_size = 340;
  int imagelet_1 = 0;
  int imagelet_2 = 340;
  int imagelet_3 = 680;

  // double buffer assignment
  int imagelet_io_1_0 = imagelet_1;
  int imagelet_io_1_1 = imagelet_3;

  // body:
  // mcode param
  mcode.raw = (void*) terapix_ucode_array;
  mcode.size = TERAPIX_UCODE_SIZE_T;
  freia_mg_write_microcode(&mcode);

  // subimage operation
  param.size = sizeof(mcu_instr);
  param.raw = (void*) mcu_instr;

  // dyn_param contents  dyn_param.raw = &gram;
  dyn_param.size = sizeof(terapix_gram);
  // set measure max at 1023
  // 0: max(1)
  mcu_instr[0][0].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][0].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][0].macrocode.ymin1 = 0;
  mcu_instr[1][0].macrocode.ymin1 = 0;
  mcu_instr[0][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][0].macrocode.iter2 = imagelet_size;
  mcu_instr[1][0].macrocode.iter2 = imagelet_size;
  mcu_instr[0][0].macrocode.addrStart = TERAPIX_UCODE_MAX?;
  mcu_instr[1][0].macrocode.addrStart = TERAPIX_UCODE_MAX?;
  // 1: E8(1) -> 2
  mcu_instr[0][1].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][1].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][1].macrocode.ymin1 = 0;
  mcu_instr[1][1].macrocode.ymin1 = 0;
  mcu_instr[0][1].macrocode.xmin2 = imagelet_io_2_0;
  mcu_instr[1][1].macrocode.xmin2 = imagelet_io_2_1;
  mcu_instr[0][1].macrocode.ymin2 = 0;
  mcu_instr[1][1].macrocode.ymin2 = 0;
  // copy of operation 1 parameters
  for(i=0; i<9; i++)
    p_1[i] = pi3[i];
  gram.xoffset = x_1;
  gram.yoffset = y_1;
  gram.width = 3;
  gram.height = 3;
  gram.params = p_1;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_instr[0][1].macrocode.xmin3 = x_1;
  mcu_instr[1][1].macrocode.xmin3 = x_1;
  mcu_instr[0][1].macrocode.ymin3 = y_1;
  mcu_instr[1][1].macrocode.ymin3 = y_1;
  mcu_instr[0][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][1].macrocode.iter2 = imagelet_size;
  mcu_instr[1][1].macrocode.iter2 = imagelet_size;
  mcu_instr[0][1].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  mcu_instr[1][1].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  // set measure min at 1022
  // 2: min(2)
  mcu_instr[0][2].macrocode.xmin1 = imagelet_io_2_0;
  mcu_instr[1][2].macrocode.xmin1 = imagelet_io_2_1;
  mcu_instr[0][2].macrocode.ymin1 = 0;
  mcu_instr[1][2].macrocode.ymin1 = 0;
  mcu_instr[0][2].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][2].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][2].macrocode.iter2 = imagelet_size;
  mcu_instr[1][2].macrocode.iter2 = imagelet_size;
  mcu_instr[0][2].macrocode.addrStart = TERAPIX_UCODE_MIN?;
  mcu_instr[1][2].macrocode.addrStart = TERAPIX_UCODE_MIN?;
  // copy 2 -> 1
  mcu_instr[0][3].macrocode.xmin1 = imagelet_2;
  mcu_instr[1][3].macrocode.xmin1 = imagelet_2;
  mcu_instr[0][3].macrocode.ymin1 = 0;
  mcu_instr[1][3].macrocode.ymin1 = 0;
  mcu_instr[0][3].macrocode.xmin2 = imagelet_1;
  mcu_instr[1][3].macrocode.xmin2 = imagelet_1;
  mcu_instr[0][3].macrocode.ymin2 = 0;
  mcu_instr[1][3].macrocode.ymin2 = 0;
  mcu_instr[0][3].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][3].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][3].macrocode.iter2 = imagelet_size;
  mcu_instr[1][3].macrocode.iter2 = imagelet_size;
  mcu_instr[0][3].macrocode.addrStart = TERAPIX_UCODE_COPY;
  mcu_instr[1][3].macrocode.addrStart = TERAPIX_UCODE_COPY;

  // inputs:
  // - imagelet 1 is i0 for i

  // outputs:
  // - imagelet 1 is o0 for X0

  // double buffer management:
  // - buffer 1/3

  // output imagelet erosion:
  //   north: 1
  //   south: 1
  //    west: 1
  //    east: 1

  ret = freia_cg_template_process_1i_1o(&param, o0, i0);

  // extract measures
  // get measure max result from 1023
  // get measure min result from 1022

  return ret;
}


! updated function

freia_status freia_28(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tC2 = freia_common_create_data(16, 128, 128);        /*0012*/
   int sA, sB, sC;                                                      /*0013*/
   
   // complex dag for split
   freia_28_helper_0_2(X0, i, &sA, k, &sB);                             /*0016*/
   freia_28_helper_0_1(tC2, X1, X0, i, sA-10, sA-1, false, k, &sC, sB-10, sB-1, false);               /*0017*/
   freia_28_helper_0_0(o, X1, tC2, sC-10, sC-1, false);                 /*0024*/

   freia_common_destruct_data(X0);                                      /*0027*/
   freia_common_destruct_data(X1);                                      /*0028*/
   return FREIA_OK;                                                     /*0032*/
}

! cleaned up function

freia_status freia_28(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tC2 = freia_common_create_data(16, 128, 128);        /*0012*/
   int sA, sB, sC;                                                      /*0013*/
   
   // complex dag for split
   freia_28_helper_0_2(X0, i, &sA, k, &sB);                             /*0016*/
   freia_28_helper_0_1(tC2, X1, X0, i, sA-10, sA-1, 0, k, &sC, sB-10, sB-1, 0);        /*0017*/
   freia_28_helper_0_0(o, X1, tC2, sC-10, sC-1, 0);                     /*0024*/

   freia_common_destruct_data(X0);                                      /*0027*/
   freia_common_destruct_data(X1);                                      /*0028*/
   return 0;                                                            /*0032*/
}
