digraph "dag_0" {
  // inputs
  "i0" [shape=circle];
  "i1" [shape=circle];

  // outputs
  "o" [shape=circle];

  // computation vertices
  "13 -" [shape=trapezium,orientation=270];
  "13 -" -> "o";
  "12 <" [shape=trapezium,orientation=270];
  "12 <" -> "13 -" [label="tmp2"];
  "11 +" [shape=trapezium,orientation=270];
  "11 +" -> "12 <" [label="tmp1"];
  "i1" -> "11 +";
  "i0" -> "13 -";
  "i0" -> "12 <";
