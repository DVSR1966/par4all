   freia_data2d *i2;                                                    /*0011*/
   freia_data2d *i3;                                                    /*0012*/
   freia_data2d *i4;                                                    /*0013*/
   freia_data2d *i5;                                                    /*0014*/
   freia_data2d *i6;                                                    /*0015*/
   freia_data2d *i7;                                                    /*0016*/
   freia_data2d *i8;                                                    /*0017*/
