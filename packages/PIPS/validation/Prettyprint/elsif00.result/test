int elsif00(int i)
{
   // direct
   if (i==1)                                                            /*0005*/
      i++;                                                              /*0006*/
   else if (i==2)                                                       /*0005*/
      i--;                                                              /*0008*/
   else
      i -= 3;                                                           /*0010*/
   // blocks
   if (i==4)                                                            /*0012*/
      i++;                                                              /*0013*/
   else if (i==5)                                                       /*0012*/
      i--;                                                              /*0015*/
   else
      i -= 6;                                                           /*0017*/
   // sub blocks
   if (i==2)                                                            /*0020*/
      i++;                                                              /*0021*/
   else if (i==5)                                                       /*0020*/
      i--;                                                              /*0024*/
   else
      i -= 3;                                                           /*0026*/
   // block sub blocks
   if (i==2)                                                            /*0029*/
      i++;                                                              /*0030*/
   else if (i==5)                                                       /*0029*/
      i--;                                                              /*0033*/
   else
      i -= 3;                                                           /*0035*/
}
