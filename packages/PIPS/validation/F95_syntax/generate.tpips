delete generate

setproperty ABORT_ON_USER_ERROR TRUE
setproperty PREPROCESSOR_MISSING_FILE_HANDLING "generate"

create generate generate.f90

apply SUMMARY_PRECONDITION(GENERATE)

display PRINTED_FILE[%ALL]

apply UNSPLIT[%ALL]

echo
echo "compile sources produced by pips"
echo
shell gcc -c -o generate.database/Src/generate.o  generate.database/Src/generate.f90

close
quit
