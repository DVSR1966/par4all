
PROPER  EFFECTS 

int main()
{
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
   my_str *m, *n;
   int j;
//             <    is written>: j
   j = 2;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: m _MALLOC_EFFECTS:_MALLOC_
   m = (my_str *) malloc(sizeof(my_str));
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: m
   m->i = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: m
   m->p = &j;
//             <    is read   >: m
//             <    is written>: n
   n = m;

   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   typedef struct my_struct {
      int i;
      int *p;
   } my_str;

// Points To: none

   my_str *m, *n;

// Points To:
// m -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// n -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int j;

// Points To:
// m -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// n -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   j = 2;

// Points To:
// m -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// n -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   m = (my_str *) malloc(sizeof(my_str));

// Points To:
// m -> *HEAP*_l_10 , EXACT
// n -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   m->i = 1;

// Points To:
// m -> *HEAP*_l_10 , EXACT
// n -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   m->p = &j;

// Points To:
// *HEAP*_l_10.p -> j , EXACT
// m -> *HEAP*_l_10 , EXACT
// n -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   n = m;

// Points To:
// *HEAP*_l_10.p -> j , EXACT
// m -> *HEAP*_l_10 , EXACT
// n -> *HEAP*_l_10 , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
   my_str *m, *n;
   int j;
//             <    is written>: j
   j = 2;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: m _MALLOC_EFFECTS:_MALLOC_
   m = (my_str *) malloc(sizeof(my_str));
//             <    is read   >: m
//             <    is written>: *HEAP*_l_10.i
   m->i = 1;
//             <    is read   >: m
//             <    is written>: *HEAP*_l_10.p
   m->p = &j;
//             <    is read   >: m
//             <    is written>: n
   n = m;

   return 0;
}

 *********************** Use-Def Chains *********************

	10 --> 11 with conflicts
		from <m-W-EXACT> to <m-R-EXACT>
	10 --> 12 with conflicts
		from <m-W-EXACT> to <m-R-EXACT>
	10 --> 13 with conflicts
		from <m-W-EXACT> to <m-R-EXACT>

