
PROPER  EFFECTS 

void call15(int *pi, tf_t *q)
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pi
   *pi = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   q->one = 1;
}
int main()
{
   int i;
   tf_t s;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

   call15(&i, &s);
}

