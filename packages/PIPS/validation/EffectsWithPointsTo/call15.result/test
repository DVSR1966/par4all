PROPER  EFFECTS WITH POINTS TO

void call15(int *pi, tf_t *q)
{
//             <    is read   >: pi
//             <    is written>: _pi_1[0]
   *pi = 1;
//             <    is read   >: q
//             <    is written>: _q_2[0].one
   q->one = 1;
}
int main()
{
   int i;
   tf_t s;
//             <    is written>: i s.one

   call15(&i, &s);
}

