
PROPER  EFFECTS 

int main()
{
   int a[10];
   int *p;
//             <    is written>: b c
   int b = 0, c = 1;
//             <    is read   >: b c
   if (b==c)
//             <    is written>: p
      p = &a[0];
   else
//             <    is written>: p
      p = &a[1];
//             <    is read   >: p
//             <    is written>: p
   p++;
//             <    is read   >: p
//             <    is written>: p
   ++p;
//             <    is read   >: p
//             <    is written>: p
   p--;
//             <    is read   >: p
//             <    is written>: p
   --p;
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int a[10];

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b = 0, c = 1;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (b==c)

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &a[0];
   else

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &a[1];

// Points To:
// p -> a[0] , MAY
// p -> a[1] , MAY

   p++;

// Points To:
// p -> a[*] , EXACT

   ++p;

// Points To:
// p -> a[*] , EXACT

   p--;

// Points To:
// p -> a[*] , EXACT

   --p;

// Points To:
// p -> a[*] , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int a[10];
   int *p;
//             <    is written>: b c
   int b = 0, c = 1;
//             <    is read   >: b c
   if (b==c)
//             <    is written>: p
      p = &a[0];
   else
//             <    is written>: p
      p = &a[1];
//             <    is read   >: p
//             <    is written>: p
   p++;
//             <    is read   >: p
//             <    is written>: p
   ++p;
//             <    is read   >: p
//             <    is written>: p
   p--;
//             <    is read   >: p
//             <    is written>: p
   --p;
   return 0;
}

