
PROPER  EFFECTS 

void point_to03()
{
   typedef double a_t[10][20];
   a_t a;
   a_t *p;
//             <    is written>: p

   p = &a;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   ((*p)[2])[3] = 1.5;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void point_to03()
{

// Points To: none

   typedef double a_t[10][20];

// Points To: none

   a_t a;

// Points To: none

   a_t *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = &a;

// Points To:
// p -> a , EXACT

   ((*p)[2])[3] = 1.5;
}

PROPER  EFFECTS WITH POINTS TO

void point_to03()
{
   typedef double a_t[10][20];
   a_t a;
   a_t *p;
//             <    is written>: p

   p = &a;
//             <    is read   >: p
//             <    is written>: a[2][3]
   ((*p)[2])[3] = 1.5;
}

 *********************** Use-Def Chains *********************

	07 --> 08 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>

