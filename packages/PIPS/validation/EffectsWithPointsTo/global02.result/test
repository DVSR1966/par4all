
PROPER  EFFECTS 

int main()
{
   int **q, *r, i;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: r
   r = &**q;

   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int **q, *r, i;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   q = p;

// Points To:
// _p_1[0] -> __p_1_2[0] , EXACT
// p -> _p_1[0] , EXACT
// q -> _p_1[0] , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   *q = &i;

// Points To:
// _p_1[0] -> i , EXACT
// p -> _p_1[0] , EXACT
// q -> _p_1[0] , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   r = &**q;

// Points To:
// _p_1[0] -> i , EXACT
// p -> _p_1[0] , EXACT
// q -> _p_1[0] , EXACT
// r -> i , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int **q, *r, i;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <    is read   >: q
//             <    is written>: _p_1[0]
   *q = &i;
//             <    is read   >: _p_1[0] q
//             <    is written>: r
   r = &**q;

   return 0;
}

 *********************** Use-Def Chains *********************

	10 --> 11 with conflicts
		from <_p_1[0]-W-EXACT> to <_p_1[0]-R-EXACT>
	09 --> 11 with conflicts
		from <q-W-EXACT> to <q-R-EXACT>
	09 --> 10 with conflicts
		from <q-W-EXACT> to <q-R-EXACT>

****************** End of Dependence Graph ******************
