
PROPER  EFFECTS 

void call03()
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pi
   *pi = 1;
//             <    is read   >: pi
//             <    is written>: pi
   pi++;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   q->one = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   (q->two)[4] = 2;
}
int main()
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pi q
   call03();
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void call03()
{

// Points To: none

   *pi = 1;

// Points To: none

   pi++;

// Points To:
// pi -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   q->one = 1;

// Points To:
// pi -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   (q->two)[4] = 2;
}

// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   call03();
}

PROPER  EFFECTS WITH POINTS TO

void call03()
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pi
   *pi = 1;
//             <    is read   >: pi
//             <    is written>: pi
   pi++;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   q->one = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   (q->two)[4] = 2;
}
int main()
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pi q
   call03();
}

