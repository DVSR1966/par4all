int * foo()
{
//             <    is written>: cond i j
   int cond = 0, i = 1, j = 2;
   int *p, *q;
   //PIPS generated variable
   int *foo;
//             <    is written>: p

   p = &i;
//             <    is written>: q
   q = &j;
//:   BEGIN UNSTRUCTURED
//             <    is read   >: cond

   if (cond) goto l99999;
//             <    is read   >: q
//             <    is written>: foo
   foo = q;
   return foo;
l99999:   ;
//             <    is read   >: p
//             <    is written>: foo
   foo = p;
//             <    is read   >: foo
   return foo;
//:   END UNSTRUCTURED
}
int main()
{
   int *r;
//             <    is written>: r
   r = foo();

   return 0;
}
int *foo();

