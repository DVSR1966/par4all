PROPER  EFFECTS WITH POINTS TO

void call16(int _i, int *_a, int *_b, int *_c)
{
//             <    is read   >: __b_2[0+_i] __c_3[0+_i] _a _b _c _i
//             <    is written>: __a_1[0+_i]
   _a[_i] = _b[_i]+_c[_i];
}
void call16(int _i, int *_a, int *_b, int *_c);

void dotprod(int *a, int *b, int *c);
void dotprod(int *a, int *b, int *c)
{
   int i;
//             <    is written>: i
   for(i = 0; i <= 999999; i += 1)
//             <may be read   >: _a_4[*] _b_5[*]
//             <    is read   >: a b c i
      call16(i, a, b, c);
   
   
   return;
}

