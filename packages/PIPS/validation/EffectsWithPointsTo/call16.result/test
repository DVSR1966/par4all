   for(i = 0; i <= 999999; i += 1)
//  {(a,_a_4[0],-Exact-);(b,_b_5[0],-Exact-);(c,_c_6[0],-Exact-)}
      call16(i, a, b, c);
//  {(a,_a_4[0],-Exact-);(b,_b_5[0],-Exact-);(c,_c_6[0],-Exact-)}
   
   
   return;
