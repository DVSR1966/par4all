   *p++ = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p-- = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *++p = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *--p = 0.;
}

