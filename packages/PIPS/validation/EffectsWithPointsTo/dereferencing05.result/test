
PROPER  EFFECTS 

void dereferencing05()
{
   double *p;
   int i;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p++ = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p-- = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *++p = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *--p = 0.;
}

POINTS-TO

//  {}
void dereferencing05()
{
//  {}
   double *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   *p++ = 0.;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   *p-- = 0.;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   *++p = 0.;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   *--p = 0.;
}

