
PROPER  EFFECTS 

int main()
{
   int i;
   int *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
      p[i] = i;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i;
//  {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = (int *) malloc(10*sizeof(int));
//  {(p,*HEAP*_l_7[0],-Exact-)}

   for(i = 0; i <= 9; i += 1)
//  {(p,*HEAP*_l_7[0],-Exact-)}
      p[i] = i;
//  {(p,*HEAP*_l_7[0],-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
   int *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i p
//             <    is written>: *HEAP*_l_7[i]
      p[i] = i;
   return 0;
}

 *********************** Use-Def Chains *********************

	10 --> 10 with conflicts
		from <*HEAP*_l_7[i]-W-EXACT> to <*HEAP*_l_7[i]-W-EXACT>
	07 --> 10 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>

****************** End of Dependence Graph ******************
