
PROPER  EFFECTS 

void dereferencing02()
{
//             <    is written>: x
   double x = 1.;
//             <    is written>: p
   double *p = &x;
//             <    is written>: q
   double **q = &p;
   double **w;
//             <    is written>: i
   int i = 1;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   
   
   **q = 2.;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   **(q+(i = 0)) = 3.;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   **(w+(i = 2)) = 4.;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: x
   x = **q;
//             <    is read   >: q
//             <    is written>: q
   q++;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: q
   *q++;
}
int main()
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   dereferencing02();
   return 1;
}

POINTS-TO


// Points To: none

void dereferencing02()
{

// Points To: none

   double x = 1.;

// Points To: none

   double *p = &x;

// Points To:
// p -> &x , EXACT

   double **q = &p;

// Points To:
// p -> &x , EXACT
// q -> &p , EXACT

   double **w;

// Points To:
// p -> &x , EXACT
// q -> &p , EXACT
// w -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i = 1;

// Points To:
// p -> &x , EXACT
// q -> &p , EXACT
// w -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   
   
   **q = 2.;

// Points To:
// p -> &x , EXACT
// q -> &p , EXACT
// w -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   **(q+(i = 0)) = 3.;

// Points To:
// p -> &x , EXACT
// q -> &p , EXACT
// w -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   **(w+(i = 2)) = 4.;

// Points To:
// p -> &x , EXACT
// q -> &p , EXACT
// w -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   x = **q;

// Points To:
// p -> &x , EXACT
// q -> &p , EXACT
// w -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q++;

// Points To:
// p -> &x , EXACT
// q -> &*ANY_MODULE*:*ANYWHERE*_b0 , EXACT
// w -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   *q++;
}

// Points To: none

int main()
{

// Points To: none

   dereferencing02();

// Points To: none

   return 1;
}

PROPER  EFFECTS WITH POINTS TO

void dereferencing02()
{
//             <    is written>: x
   double x = 1.;
//             <    is written>: p
   double *p = &x;
//             <    is written>: q
   double **q = &p;
   double **w;
//             <    is written>: i
   int i = 1;
//             <    is read   >: p q
//             <    is written>: x
   
   
   **q = 2.;
//             <    is read   >: p q
//             <    is written>: i x
   **(q+(i = 0)) = 3.;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   **(w+(i = 2)) = 4.;
//             <    is read   >: p q x
//             <    is written>: x
   x = **q;
//             <    is read   >: q
//             <    is written>: q
   q++;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: q
   *q++;
}
int main()
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   dereferencing02();
   return 1;
}

