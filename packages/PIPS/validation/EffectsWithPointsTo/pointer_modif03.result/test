int main()
{
   s_t s;
   int j;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: s.entier s.reel 
//                               _MALLOC_EFFECTS:_MALLOC_

   s = make_s(1, 2.);
//             <    is written>: s.entier
   s.entier = 4;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: j _MALLOC_EFFECTS:_MALLOC_

   j = make_s(1, 2.).entier;
//             <    is read   >: j

   return j;
}
s_t make_s(int i, float x)
{
   s_t *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (s_t *) malloc(sizeof(s_t));
//             <    is read   >: i p
//             <    is written>: *HEAP*_l_9.entier
   p->entier = i;
//             <    is read   >: p x
//             <    is written>: *HEAP*_l_9.reel
   p->reel = x;
//             <    is read   >: *HEAP*_l_9.entier *HEAP*_l_9.reel p
   return *p;
}

