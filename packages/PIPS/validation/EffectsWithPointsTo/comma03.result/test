
PROPER  EFFECTS 

int main()
{
//             <    is written>: i j k l
   int i = 0, j = 1, k = 2, l = 3;
   int *p, *q, *r, *s, *t;
//             <    is written>: r

   r = &i;
//             <    is written>: q
   q = &j;
//             <    is written>: s
   s = &k;
//             <    is read   >: q r s
//             <    is written>: p s
   /* we trat this instruction like :
        s = r;
        q;
        s;
        p = s;
     */
   p = (s = r, q, s);
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int i = 0, j = 1, k = 2, l = 3;

// Points To: none

   int *p, *q, *r, *s, *t;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// t -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   r = &i;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> &i , EXACT
// s -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// t -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = &j;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &j , EXACT
// r -> &i , EXACT
// s -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// t -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   s = &k;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &j , EXACT
// r -> &i , EXACT
// s -> &k , EXACT
// t -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   /* we trat this instruction like :
        s = r;
        q;
        s;
        p = s;
     */
   p = (s = r, q, s);

// Points To:
// p -> &i , EXACT
// q -> &j , EXACT
// r -> &i , EXACT
// s -> &i , EXACT
// t -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i j k l
   int i = 0, j = 1, k = 2, l = 3;
   int *p, *q, *r, *s, *t;
//             <    is written>: r

   r = &i;
//             <    is written>: q
   q = &j;
//             <    is written>: s
   s = &k;
//             <    is read   >: q r s
//             <    is written>: p s
   /* we trat this instruction like :
        s = r;
        q;
        s;
        p = s;
     */
   p = (s = r, q, s);
   return 0;
}

 *********************** Use-Def Chains *********************

	10 --> 17 with conflicts
		from <s-W-EXACT> to <s-W-EXACT>
		from <s-W-EXACT> to <s-R-EXACT>
	09 --> 17 with conflicts
		from <q-W-EXACT> to <q-R-EXACT>
	08 --> 17 with conflicts
		from <r-W-EXACT> to <r-R-EXACT>

****************** End of Dependence Graph ******************
