
PROPER  EFFECTS 

int address_of01()
{
   int *p, i[10];
//             <    is written>: p
   p = 1+&i[0];
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
   return *p;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int address_of01()
{

// Points To: none

   int *p, i[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = 1+&i[0];

// Points To:
// p -> i[1] , EXACT

   return *p;
}

PROPER  EFFECTS WITH POINTS TO

int address_of01()
{
   int *p, i[10];
//             <    is written>: p
   p = 1+&i[0];
//             <    is read   >: i[1] p
   return *p;
}

