
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 5;
//             <    is written>: j
   int j = 6;
   float x;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i j stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("i=%d, %d, %f\n", i, i+j, x);
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 5;

// Points To: none

   int j = 6;

// Points To: none

   float x;

// Points To: none

   printf("i=%d, %d, %f\n", i, i+j, x);
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 5;
//             <    is written>: j
   int j = 6;
   float x;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i j stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("i=%d, %d, %f\n", i, i+j, x);
}

