
PROPER  EFFECTS 

int main()
{
   float *a, *p;
   int i;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a _MALLOC_EFFECTS:_MALLOC_

   a = (float *) malloc(10*sizeof(float));
//             <    is read   >: a
//             <    is written>: i p

   p = a, i = 0;
//             <    is read   >: i
   while (i<10) {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
      *p = 1.0;
//             <    is read   >: p
//             <    is written>: p
      p++;
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   float *a, *p;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(p,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(p,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   a = (float *) malloc(10*sizeof(float));
//  {(a,*HEAP*_l_8[0],-Exact-);(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = a, i = 0;
//  {(a,*HEAP*_l_8[0],-Exact-);(p,*HEAP*_l_8[0],-Exact-)}
   while (i<10) {
//  {(a,*HEAP*_l_8[0],-Exact-);(p,*HEAP*_l_8[0],-MAY-);(p,
//   *HEAP*_l_8[*],-MAY-)}
      *p = 1.0;
//  {(a,*HEAP*_l_8[0],-Exact-);(p,*HEAP*_l_8[0],-MAY-);(p,
//   *HEAP*_l_8[*],-MAY-)}
      p++;
   }
//  {(a,*HEAP*_l_8[0],-Exact-);(p,*HEAP*_l_8[0],-MAY-);(p,
//   *HEAP*_l_8[*],-MAY-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   float *a, *p;
   int i;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a _MALLOC_EFFECTS:_MALLOC_

   a = (float *) malloc(10*sizeof(float));
//             <    is read   >: a
//             <    is written>: i p

   p = a, i = 0;
//             <    is read   >: i
   while (i<10) {
//             <may be written>: *HEAP*_l_8[0] *HEAP*_l_8[*]
//             <    is read   >: p
      *p = 1.0;
//             <    is read   >: p
//             <    is written>: p
      p++;
   }
   return 0;
}

