// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = (int *) malloc(2*sizeof(int));
   else

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = (int *) malloc(3*sizeof(int));

// Points To:
// p -> &*HEAP*_l_8[0] , MAY
// p -> &*HEAP*_l_10[0] , MAY

   p[0] = 0;

// Points To:
// p -> &*HEAP*_l_8[0] , MAY
// p -> &*HEAP*_l_10[0] , MAY

   p[1] = 1;

// Points To:
// p -> &*HEAP*_l_8[0] , MAY
// p -> &*HEAP*_l_10[0] , MAY

   free(p);

// Points To:
// p -> &*HEAP*_l_8[0] , MAY
// p -> &*HEAP*_l_10[0] , MAY

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *p;
   int a;
//             <    is read   >: a
   if (a==1)
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
      p = (int *) malloc(2*sizeof(int));
   else
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
      p = (int *) malloc(3*sizeof(int));
//             <may be written>: *HEAP*_l_10[0] *HEAP*_l_8[0]
//             <    is read   >: p
   p[0] = 0;
//             <may be written>: *HEAP*_l_10[1] *HEAP*_l_8[1]
//             <    is read   >: p
   p[1] = 1;
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   free(p);
   return 0;
}

