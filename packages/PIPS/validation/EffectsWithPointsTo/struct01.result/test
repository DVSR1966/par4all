
PROPER  EFFECTS 

int struct01()
{
   struct one {
      int first;
      int second;
   } x;
   int y[10];
//             <    is written>: x.first

   x.first = 1;
//             <    is written>: x.second
   x.second = 2;
//             <    is written>: y[0]

   y[0] = 1;
}

POINTS-TO

//  {}
int struct01()
{
//  {}
   struct one {
      int first;
      int second;
   } x;
//  {}
   int y[10];
//  {}

   x.first = 1;
//  {}
   x.second = 2;
//  {}

   y[0] = 1;
}

PROPER  EFFECTS WITH POINTS TO

int struct01()
{
   struct one {
      int first;
      int second;
   } x;
   int y[10];
//             <    is written>: x.first

   x.first = 1;
//             <    is written>: x.second
   x.second = 2;
//             <    is written>: y[0]

   y[0] = 1;
}

