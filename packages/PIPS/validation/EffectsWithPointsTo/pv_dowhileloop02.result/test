
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: i
   do {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
      p[i] = i;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i = 0;
//  {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = (int *) malloc(10*sizeof(int));
//  {(p,*HEAP*_l_7[0],-Exact-)}
   do {
//  {(p,*HEAP*_l_7[0],-Exact-)}
      p[i] = i;
//  {(p,*HEAP*_l_7[0],-Exact-)}
      i++;
   }
   while (i<10);
//  {(p,*HEAP*_l_7[0],-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: i
   do {
//             <    is read   >: i p
//             <    is written>: *HEAP*_l_7[0+i]
      p[i] = i;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

