
PROPER  EFFECTS 

int struct02()
{
   struct two {
      int first;
      int second;
   } x[10];
//             <    is written>: i
   int i = 0;
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
      if (1) {
//             <    is read   >: i
//             <    is written>: x[i].first
         x[i].first = 1;
//             <    is read   >: i
//             <    is written>: x[i].second
         x[i].second = 0;
      }
      else {
//             <    is read   >: i
//             <    is written>: x[i].first
         x[i].first = 0;
//             <    is read   >: i
//             <    is written>: x[i].second
         x[i].second = 1;
      }
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int struct02()
{

// Points To: none

   struct two {
      int first;
      int second;
   } x[10];

// Points To: none

   int i = 0;

// Points To: none


   for(i = 0; i <= 9; i += 1)

// Points To: none

      if (1) {

// Points To: none

         x[i].first = 1;

// Points To: none

         x[i].second = 0;
      }
      else {

// Points To: none

         x[i].first = 0;

// Points To: none

         x[i].second = 1;
      }
}

PROPER  EFFECTS WITH POINTS TO

int struct02()
{
   struct two {
      int first;
      int second;
   } x[10];
//             <    is written>: i
   int i = 0;
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
      if (1) {
//             <    is read   >: i
//             <    is written>: x[i].first
         x[i].first = 1;
//             <    is read   >: i
//             <    is written>: x[i].second
         x[i].second = 0;
      }
      else {
//             <    is read   >: i
//             <    is written>: x[i].first
         x[i].first = 0;
//             <    is read   >: i
//             <    is written>: x[i].second
         x[i].second = 1;
      }
}

