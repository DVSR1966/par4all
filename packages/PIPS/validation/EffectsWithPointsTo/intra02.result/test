
POINTS-TO


// Points To IN:
// _pp_1 -> __pp_1_1 , EXACT
// _qq_2 -> __qq_2_2 , EXACT
// pp -> _pp_1 , EXACT
// qq -> _qq_2 , EXACT


// Points To OUT:
// _pp_1 -> __qq_2_2 , EXACT
// _qq_2 -> __qq_2_2 , EXACT
// pp -> _pp_1 , EXACT
// qq -> _qq_2 , EXACT

void bar(int **pp, int **qq)
{

// Points To:
// _pp_1 -> __pp_1_1 , EXACT
// _qq_2 -> __qq_2_2 , EXACT
// pp -> _pp_1 , EXACT
// qq -> _qq_2 , EXACT

   *pp = *qq;

// Points To:
// _pp_1 -> __qq_2_2 , EXACT
// _qq_2 -> __qq_2_2 , EXACT
// pp -> _pp_1 , EXACT
// qq -> _qq_2 , EXACT

   printf("pointers exchanged");
}

// Points To IN: none


// Points To OUT: none

void foo()
{

// Points To: none

   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;

// Points To:
// p -> i , EXACT
// pp -> p , EXACT
// q -> j , EXACT
// qq -> q , EXACT


   bar(pp, qq);
}

PROPER  EFFECTS WITH POINTS TO

void bar(int **pp, int **qq)
{
//             <    is read   >: _qq_2 pp qq
//             <    is written>: _pp_1
   *pp = *qq;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("pointers exchanged");
}
void foo()
{
//             <    is written>: i j p pp q qq
   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: pp qq stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   bar(pp, qq);
}

CUMULATED  EFFECTS WITH POINTS TO

//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: _qq_2 pp qq stdout 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
void bar(int **pp, int **qq)
{
//             <    is read   >: _qq_2 pp qq
//             <    is written>: _pp_1
   *pp = *qq;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("pointers exchanged");
}
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
void foo()
{
//             <    is written>: i j p pp q qq
   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: pp qq stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   bar(pp, qq);
}
