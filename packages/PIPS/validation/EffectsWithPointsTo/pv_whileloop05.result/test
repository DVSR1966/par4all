
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 0;
   int *p, *q;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <    is read   >: i

   while (i<5) {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i q
      *q = i;
//             <    is read   >: q
//             <    is written>: q
      q++;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   free(p);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i = 0;
//  {}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = (int *) malloc(10*sizeof(int));
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = p;
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*HEAP*_l_7[0],-Exact-)}

   while (i<5) {
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*HEAP*_l_7[0],-MAY-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-MAY-)}
      *q = i;
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*HEAP*_l_7[0],-MAY-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-MAY-)}
      q++;
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*HEAP*_l_7[0],-MAY-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-MAY-)}
      i++;
   }
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*HEAP*_l_7[0],-MAY-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-MAY-)}

   free(p);
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*HEAP*_l_7[0],-MAY-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-MAY-)}
   return 0;
}

