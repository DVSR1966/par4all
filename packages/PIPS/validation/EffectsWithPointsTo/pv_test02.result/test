// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a[2];

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b = 0;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int c = 1;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (1)

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      a[0] = &b;
   else

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      a[0] = &c;

// Points To:
// a[0] -> b , MAY
// a[0] -> c , MAY


   if (1)

// Points To:
// a[0] -> b , MAY
// a[0] -> c , MAY

      a[0] = &b;
   else

// Points To:
// a[0] -> b , MAY
// a[0] -> c , MAY

      a[0] = &b;

// Points To:
// a[0] -> b , EXACT

   return 0;
}

