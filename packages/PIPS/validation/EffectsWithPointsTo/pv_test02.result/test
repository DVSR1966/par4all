
PROPER  EFFECTS 

int main()
{
   int *a[2];
//             <    is written>: b
   int b = 0;
//             <    is written>: c
   int c = 1;
   if (1)
//             <    is written>: a[0]
      a[0] = &b;
   else
//             <    is written>: a[0]
      a[0] = &c;

   if (1)
//             <    is written>: a[0]
      a[0] = &b;
   else
//             <    is written>: a[0]
      a[0] = &b;
   return 0;
}

