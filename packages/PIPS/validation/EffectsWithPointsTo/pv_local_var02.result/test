
PROPER  EFFECTS 

int main()
{
   int i;
   int *p;
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1) {
      int *q;
//             <    is read   >: i
//             <    is written>: q
      q = &a[i];
//             <    is read   >: q
//             <    is written>: p
      p = q;
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i;
//  {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a[10];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   for(i = 0; i <= 9; i += 1) {
//  {(p,a[*],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-);(q,a[*],
//   -MAY-)}
      int *q;
//  {(p,a[*],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-);(q,a[*],
//   -MAY-)}
      q = &a[i];
//  {(p,a[*],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-);(q,a[*],
//   -MAY-)}
      p = q;
   }
//  {(p,a[*],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
   int *p;
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1) {
      int *q;
//             <    is read   >: i
//             <    is written>: q
      q = &a[i];
//             <    is read   >: q
//             <    is written>: p
      p = q;
   }
   return 0;
}

 *********************** Use-Def Chains *********************

	12 --> 12 with conflicts
		from <p-W-EXACT> to <p-W-EXACT>
	12 --> 11 with conflicts
		from <q-R-EXACT> to <q-W-EXACT>
	11 --> 12 with conflicts
		from <q-W-EXACT> to <q-R-EXACT>
	11 --> 11 with conflicts
		from <q-W-EXACT> to <q-W-EXACT>

****************** End of Dependence Graph ******************
