   q = (int *) malloc(2*sizeof(int));
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*HEAP*_l_8[0],-Exact-)}
   free(a==0?p:q);
//  {(p,*HEAP*_l_7[0],-Exact-);(q,*HEAP*_l_8[0],-Exact-)}
   return 0;
}

