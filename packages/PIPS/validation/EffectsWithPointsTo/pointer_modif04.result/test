{
   s_t s;
   int j;
//             <    is read   >: s.max s.tab _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   init_s(s, 10);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: s.max s.tab

   compute_s(s, 10);
   
   
   return 0;
}

