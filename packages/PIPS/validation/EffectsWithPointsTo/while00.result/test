
PROPER  EFFECTS 

int main()
{
   int *a, *b, c, d;
//             <    is written>: c
   c = 0;
//             <    is written>: d
   d = 1;
//             <    is written>: b
   b = &c;
//             <    is read   >: c

   while (c>0) {
//             <    is read   >: b
//             <    is written>: a
      a = b;
//             <    is written>: b
      b = &d;
   }
//             <    is written>: b

   b = &c;

   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int *a, *b, c, d;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   c = 0;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   d = 1;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   b = &c;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> c , EXACT


   while (c>0) {

// Points To:
// a -> d , MAY
// a -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a -> c , MAY
// b -> d , MAY
// b -> c , MAY

      a = b;

// Points To:
// a -> d , MAY
// a -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a -> c , MAY
// b -> d , MAY
// b -> c , MAY

      b = &d;
   }

// Points To:
// a -> d , MAY
// a -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a -> c , MAY
// b -> d , MAY
// b -> c , MAY


   b = &c;

// Points To:
// a -> d , MAY
// a -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a -> c , MAY
// b -> c , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *a, *b, c, d;
//             <    is written>: c
   c = 0;
//             <    is written>: d
   d = 1;
//             <    is written>: b
   b = &c;
//             <    is read   >: c

   while (c>0) {
//             <    is read   >: b
//             <    is written>: a
      a = b;
//             <    is written>: b
      b = &d;
   }
//             <    is written>: b

   b = &c;

   return 0;
}

