
PROPER  EFFECTS 

int main()
{
   int i;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: i
   i = 0;
//             <    is read   >: i
   do {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      *p++ = 2**q++;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i;
//  {}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a[10];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b[10];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = &a[0];
//  {(p,a[0],-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = &b[0];
//  {(p,a[0],-Exact-);(q,b[0],-Exact-)}
   i = 0;
//  {(p,a[0],-Exact-);(q,b[0],-Exact-)}
   do {
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
      *p++ = 2**q++;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
      i++;
   }
   while (i<10);
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: i
   i = 0;
//             <    is read   >: i
   do {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      *p++ = 2**q++;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

