// Points To: none

int main(void)
{

// Points To: none

   int i1;

// Points To: none

   int *pi1 = &i1;

// Points To:
// pi1 -> i1 , EXACT

   int **pp;

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   srand(time((void *) 0));

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (alea())

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      pp = &pi1;

// Points To:
// pi1 -> i1 , EXACT
// pp -> pi1 , MAY
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY


   *pp = (void *) 0;

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> *NULL_POINTER* , MAY
// pi1 -> *NULL_POINTER* , MAY
// pi1 -> i1 , MAY
// pp -> pi1 , MAY
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY

   printf("%p %p \n", pp, pi1);

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> *NULL_POINTER* , MAY
// pi1 -> *NULL_POINTER* , MAY
// pi1 -> i1 , MAY
// pp -> pi1 , MAY
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY

   return 0;
}

