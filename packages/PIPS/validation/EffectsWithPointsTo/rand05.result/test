int main(void)
{
   int i1;
//             <    is written>: pi1
   int *pi1 = &i1;
   int **pp;
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   srand(time((void *) 0));
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout _RANDOM_EFFECTS:GEN_SEED 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED 
//                               __IO_EFFECTS:LUNS[1]
   if (alea())
//             <    is written>: pp
      pp = &pi1;
//             <may be written>: pi1 *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pp

   *pp = (void *) 0;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: pi1 pp stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%p %p \n", pp, pi1);
   return 0;
}
static int alea(void)
{
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: r _RANDOM_EFFECTS:GEN_SEED
   int r = rand();
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: r stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", r);
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   return rand()%2==1;
}

