
PROPER  EFFECTS 

void dereferencing04()
{
   double *p;
   double *q;
   double *r;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p q r
   *(p+(q-r)) = 0.;
}

POINTS-TO


// Points To: none

void dereferencing04()
{

// Points To: none

   double *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   double *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   double *r;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   *(p+(q-r)) = 0.;
}

PROPER  EFFECTS WITH POINTS TO

void dereferencing04()
{
   double *p;
   double *q;
   double *r;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p q r
   *(p+(q-r)) = 0.;
}

