
PROPER  EFFECTS 

int main()
{
   int *p, *q;
   int i, j;
//             <    is written>: p
   p = &i;
//             <    is written>: q
   q = &j;
//             <    is read   >: q
//             <    is written>: p
   p = q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p = 1;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i, j;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p = &i;
//  {(p,i,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = &j;
//  {(p,i,-Exact-);(q,j,-Exact-)}
   p = q;
//  {(p,j,-Exact-);(q,j,-Exact-)}
   *p = 1;
//  {(p,j,-Exact-);(q,j,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *p, *q;
   int i, j;
//             <    is written>: p
   p = &i;
//             <    is written>: q
   q = &j;
//             <    is read   >: q
//             <    is written>: p
   p = q;
//             <    is read   >: p
//             <    is written>: j
   *p = 1;
   return 0;
}

 *********************** Use-Def Chains *********************

	09 --> 10 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>
	08 --> 09 with conflicts
		from <q-W-EXACT> to <q-R-EXACT>
	07 --> 09 with conflicts
		from <p-W-EXACT> to <p-W-EXACT>

****************** End of Dependence Graph ******************
