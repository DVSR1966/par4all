int main()
{

// Points To: none

   int *pdeb, *pfin, *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pdeb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pfin -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   pdeb = &t[0];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pdeb -> t[0] , EXACT
// pfin -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   /*   repère le premier élément de t   */
   pfin = &t[10-1];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pdeb -> t[0] , EXACT
// pfin -> t[10-1] , EXACT

   /*   repère le dernier élément de t   */
   
   p = pdeb;

// Points To:
// p -> t[0] , EXACT
// pdeb -> t[0] , EXACT
// pfin -> t[10-1] , EXACT

   while (p<=pfin) {

// Points To:
// p -> t[0] , MAY
// p -> t[*] , MAY
// pdeb -> t[0] , EXACT
// pfin -> t[10-1] , EXACT

      if (*p==0)

// Points To:
// p -> t[0] , MAY
// p -> t[*] , MAY
// pdeb -> t[0] , EXACT
// pfin -> t[10-1] , EXACT

         printf("%d ", p-pdeb);

// Points To:
// p -> t[0] , MAY
// p -> t[*] , MAY
// pdeb -> t[0] , EXACT
// pfin -> t[10-1] , EXACT

      p++;
   }

// Points To:
// p -> t[0] , MAY
// p -> t[*] , MAY
// pdeb -> t[0] , EXACT
// pfin -> t[10-1] , EXACT

   printf("\n");

// Points To:
// p -> t[0] , MAY
// p -> t[*] , MAY
// pdeb -> t[0] , EXACT
// pfin -> t[10-1] , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *pdeb, *pfin, *p;
//             <    is written>: pdeb

   pdeb = &t[0];
//             <    is written>: pfin
   /*   repère le premier élément de t   */
   pfin = &t[10-1];
//             <    is read   >: pdeb
//             <    is written>: p
   /*   repère le dernier élément de t   */
   
   p = pdeb;
//             <    is read   >: p pfin
   while (p<=pfin) {
//             <may be read   >: t[0] t[*]
//             <    is read   >: p
      if (*p==0)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: p pdeb stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
         printf("%d ", p-pdeb);
//             <    is read   >: p
//             <    is written>: p
      p++;
   }
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("\n");
   return 0;
}

