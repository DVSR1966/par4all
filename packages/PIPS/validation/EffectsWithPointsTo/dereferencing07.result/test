
PROPER  EFFECTS 

void foo(int *col)
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: col
   *col++;

   return;
}
int main()
{
   int x, tab1[10], tab2[10], *p;
//             <    is read   >: x
//             <    is written>: x
   x++;
//             <    is read   >: x
//             <    is written>: x
   x += 3;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p tab1[tab2[x]] tab2[x] x
   *(p+tab1[tab2[x]]) = 0;
   return 1;
}

POINTS-TO


// Points To:
// col -> &_col_1[0] , EXACT

void foo(int *col)
{

// Points To:
// col -> &_col_1[0] , EXACT

   *col++;

// Points To:
// col -> &*ANY_MODULE*:*ANYWHERE*_b0 , EXACT


   return;
}

// Points To: none

int main()
{

// Points To: none

   int x, tab1[10], tab2[10], *p;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   x++;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   x += 3;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   *(p+tab1[tab2[x]]) = 0;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return 1;
}

PROPER  EFFECTS WITH POINTS TO

void foo(int *col)
{
//             <    is read   >: _col_1[0] col
//             <    is written>: col
   *col++;

   return;
}
int main()
{
   int x, tab1[10], tab2[10], *p;
//             <    is read   >: x
//             <    is written>: x
   x++;
//             <    is read   >: x
//             <    is written>: x
   x += 3;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p tab1[tab2[x]] tab2[x] x
   *(p+tab1[tab2[x]]) = 0;
   return 1;
}

