PROPER  EFFECTS WITH POINTS TO

void call06(int r, int m, int *x)
{
   int j;
//             <    is read   >: m
//             <    is written>: j

   for(j = 0; j <= m-1; j += 1)
//             <    is read   >: j m r x
//             <    is written>: _x_1[j]
      x[j] = 10*r+j;
   return;
}
int main()
{
//             <    is written>: d1
   int d1 = 4;
//             <    is written>: d2
   int d2 = 4;
   int y[d1][d2];
   int i, j;
//             <    is read   >: d1
//             <    is written>: i

   for(i = 0; i <= d1-1; i += 1)
//             <    is read   >: d1 d2 i
      call06(i, d2, &y[i][0]);
   exit(0);
}

