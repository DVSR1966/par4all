
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i = 0;
//  {}
   int *p[10];
//  {(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a[10];
//  {(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   while (i<10) {
//  {(p[*],a[*],-MAY-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      p[i] = &a[i];
//  {(p[*],a[*],-MAY-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      i++;
   }
//  {(p[*],a[*],-MAY-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   return 0;
}

