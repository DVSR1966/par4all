
int main()
{
   mystruct s1, s2;
   mystruct tab_s[2];
//             <    is read   >: s1.a
//             <    is written>: s2.a
   s2.a = s1.a;
   (s2.b)[0] = (s1.b)[0];
   tab_s[0].a = s1.a;
   (tab_s[0].b)[0] = (s1.b)[0];
   return 0;
}

