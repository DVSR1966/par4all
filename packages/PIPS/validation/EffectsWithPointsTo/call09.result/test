
PROPER  EFFECTS 

void call09(int i, int j)
{
//             <    is read   >: i j
//             <    is written>: i
   i = i+j;
}
int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*

   call09(s.one, sp->one);
}

POINTS-TO

//  {}
void call09(int i, int j)
{
//  {}
   i = i+j;
}
//  {}
int main()
{
//  {}
   tf_t s;
//  {}
   tf_t *sp = &s;
//  {(sp,s,-Exact-)}

   call09(s.one, sp->one);
}

PROPER  EFFECTS WITH POINTS TO

void call09(int i, int j)
{
//             <    is read   >: i j
//             <    is written>: i
   i = i+j;
}
int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <    is read   >: s.one sp

   call09(s.one, sp->one);
}

