
PROPER  EFFECTS 

void call09(int i, int j)
{
//             <    is read   >: i j
//             <    is written>: i
   i = i+j;
}
int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*

   call09(s.one, sp->one);
}

POINTS-TO


// Points To: none

void call09(int i, int j)
{

// Points To: none

   i = i+j;
}

// Points To: none

int main()
{

// Points To: none

   tf_t s;

// Points To: none

   tf_t *sp = &s;

// Points To:
// sp -> &s , EXACT


   call09(s.one, sp->one);
}

PROPER  EFFECTS WITH POINTS TO

void call09(int i, int j)
{
//             <    is read   >: i j
//             <    is written>: i
   i = i+j;
}
int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <    is read   >: s.one sp

   call09(s.one, sp->one);
}

 *********************** Use-Def Chains *********************

	15 --> 17 with conflicts
		from <sp-W-EXACT> to <sp-R-EXACT>

****************** End of Dependence Graph ******************
