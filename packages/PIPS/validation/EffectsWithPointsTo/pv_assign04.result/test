// Points To: none

int main()
{

// Points To: none

   mystruct s1, s2;

// Points To:
// s1.b[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1.c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2.b[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2.c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   mystruct tab_s[2];

// Points To:
// s1.b[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1.a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s1.c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
