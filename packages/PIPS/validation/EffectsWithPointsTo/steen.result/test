
PROPER  EFFECTS 

int main()
{
   int **c, **a, **b, *y;
//             <    is written>: p x z
   int x = 1, z = 2, p = 1;
//             <    is written>: a

   a = &x;
//             <    is written>: b
   b = &y;
//             <    is read   >: p
   if (p)
//             <    is written>: y
      y = &z;
   else
//             <    is written>: y
      y = &x;
//             <    is written>: c
   c = &y;
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int **c, **a, **b, *y;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int x = 1, z = 2, p = 1;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   a = &x;

// Points To:
// a -> x , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   b = &y;

// Points To:
// a -> x , EXACT
// b -> y , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   if (p)

// Points To:
// a -> x , EXACT
// b -> y , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

      y = &z;
   else

// Points To:
// a -> x , EXACT
// b -> y , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

      y = &x;

// Points To:
// a -> x , EXACT
// b -> y , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> z , MAY
// y -> x , MAY

   c = &y;

// Points To:
// a -> x , EXACT
// b -> y , EXACT
// c -> y , EXACT
// y -> z , MAY
// y -> x , MAY

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int **c, **a, **b, *y;
//             <    is written>: p x z
   int x = 1, z = 2, p = 1;
//             <    is written>: a

   a = &x;
//             <    is written>: b
   b = &y;
//             <    is read   >: p
   if (p)
//             <    is written>: y
      y = &z;
   else
//             <    is written>: y
      y = &x;
//             <    is written>: c
   c = &y;
   return 0;
}

