
POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int **c, **a, **b, *y;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int x = 1, z = 2, p = 1;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   
   //a = &x;
   b = &y;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> y , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   if (p)

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> y , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

      y = &z;
   else

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> y , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

      y = &x;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> y , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> x , MAY
// y -> z , MAY

   c = &y;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> y , EXACT
// c -> y , EXACT
// y -> x , MAY
// y -> z , MAY

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int **c, **a, **b, *y;
//             <    is written>: p x z
   int x = 1, z = 2, p = 1;
//             <    is written>: b
   
   //a = &x;
   b = &y;
//             <    is read   >: p
   if (p)
//             <    is written>: y
      y = &z;
   else
//             <    is written>: y
      y = &x;
//             <    is written>: c
   c = &y;
   return 0;
}

 *********************** Use-Def Chains *********************

	06 --> 10 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>
	11 --> 13 with conflicts
		from <y-W-EXACT> to <y-W-EXACT>

