
PROPER  EFFECTS 

int main()
{
   int *a, b, c, d;
//             <    is written>: c
   c = 3;
//             <    is written>: a
   /* S1 */
   a = &c;
//             <    is read   >: d __IO_EFFECTS:LUNS[d]
//             <    is written>: __IO_EFFECTS:LUNS[d]
   /* S2 */
   read(d);
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: b
   /* S3 */
   b = *a;
   /* S4 */
   
   
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int *a, b, c, d;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   c = 3;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   /* S1 */
   a = &c;

// Points To:
// a -> c , EXACT

   /* S2 */
   read(d);

// Points To:
// a -> c , EXACT

   /* S3 */
   b = *a;

// Points To:
// a -> c , EXACT

   /* S4 */
   
   
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *a, b, c, d;
//             <    is written>: c
   c = 3;
//             <    is written>: a
   /* S1 */
   a = &c;
//             <    is read   >: d __IO_EFFECTS:LUNS[d]
//             <    is written>: __IO_EFFECTS:LUNS[d]
   /* S2 */
   read(d);
//             <    is read   >: a c
//             <    is written>: b
   /* S3 */
   b = *a;
   /* S4 */
   
   
   return 0;
}

 *********************** Use-Def Chains *********************

	04 --> 07 with conflicts
		from <c-W-EXACT> to <c-R-EXACT>
	05 --> 07 with conflicts
		from <a-W-EXACT> to <a-R-EXACT>

****************** End of Dependence Graph ******************
