
PROPER  EFFECTS 

int main()
{
//             <may be read   >: stderr stderr[0] stdout stdout[0] 
//                               *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[1] 
//                               __IO_EFFECTS:LUNS[2]
//             <may be written>: stderr[0] stdout[0] 
//                               *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[1] 
//                               __IO_EFFECTS:LUNS[2]
   f3(6, 4, "ch1", "ch2", "ch3", "ch4", "ch5", "ch6");
   return 0;
}

