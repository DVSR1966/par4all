
PROPER  EFFECTS 

int main()
{
   int t[100][10][3], i;

   int (*p)[3];
//             <    is written>: i
   for(i = 0; i <= 2; i += 1)
//             <    is read   >: i
//             <    is written>: p
      p = t[i];

   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int t[100][10][3], i;


// Points To: none

   int (*p)[3];

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   for(i = 0; i <= 2; i += 1)

// Points To:
// p -> &t[*] , MAY
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , MAY

      p = t[i];

// Points To:
// p -> &t[*] , MAY
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , MAY


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int t[100][10][3], i;

   int (*p)[3];
//             <    is written>: i
   for(i = 0; i <= 2; i += 1)
//             <    is read   >: i
//             <    is written>: p
      p = t[i];

   return 0;
}

