      static int foo = 0;
//             <    is read   >: foo
//             <    is written>: foo

      foo++;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: foo stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

      fprintf(stdout, "foo=%d\n", foo);
   }
}
int main()
{
//             <may be read   >: stdout[0]
