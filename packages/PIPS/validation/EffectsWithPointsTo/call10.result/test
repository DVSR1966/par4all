
PROPER  EFFECTS 

void call10(int i, int x[10], int j, int y[10])
{
   int k, l;
//             <    is read   >: i
//             <    is written>: k

   for(k = 0; k <= i-1; k += 1)
//             <    is read   >: i
//             <    is written>: x[i]
      x[i] = 0;
//             <    is read   >: j
//             <    is written>: l
   for(l = 0; l <= j-1; l += 1)
//             <    is read   >: i j
//             <    is written>: y[i]
      y[i] = 0;
}
int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

   call10(s.one, s.two, sp->one, sp->two);
}

POINTS-TO


// Points To: none

void call10(int i, int x[10], int j, int y[10])
{

// Points To: none

   int k, l;

// Points To: none


   for(k = 0; k <= i-1; k += 1)

// Points To: none

      x[i] = 0;

// Points To: none

   for(l = 0; l <= j-1; l += 1)

// Points To: none

      y[i] = 0;
}

// Points To: none

int main()
{

// Points To: none

   tf_t s;

// Points To: none

   tf_t *sp = &s;

// Points To:
// sp -> &s , EXACT


   call10(s.one, s.two, sp->one, sp->two);
}

PROPER  EFFECTS WITH POINTS TO

void call10(int i, int x[10], int j, int y[10])
{
   int k, l;
//             <    is read   >: i
//             <    is written>: k

   for(k = 0; k <= i-1; k += 1)
//             <    is read   >: i
//             <    is written>: x[i]
      x[i] = 0;
//             <    is read   >: j
//             <    is written>: l
   for(l = 0; l <= j-1; l += 1)
//             <    is read   >: i j
//             <    is written>: y[i]
      y[i] = 0;
}
int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <may be written>: s.two[*]
//             <    is read   >: s.one sp

   call10(s.one, s.two, sp->one, sp->two);
}

 *********************** Use-Def Chains *********************

	20 --> 22 with conflicts
		from <sp-W-EXACT> to <sp-R-EXACT>

****************** End of Dependence Graph ******************
