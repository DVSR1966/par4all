
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 3;
   int *p;
   int a[10];
//             <    is read   >: i

   if (i>1) {
      int *q;
//             <    is read   >: i
//             <    is written>: q
      q = &a[i];
//             <    is read   >: q
//             <    is written>: p
      p = q;
//             <    is written>: i
      i = 0;
   }
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int i = 3;

// Points To: none

   int *p;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   if (i>1) {

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

      int *q;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

      q = &a[i];

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &a[*] , EXACT

      p = q;

// Points To:
// p -> &a[*] , EXACT
// q -> &a[*] , EXACT

      i = 0;
   }

// Points To:
// p -> &a[*] , MAY
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , MAY

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 3;
   int *p;
   int a[10];
//             <    is read   >: i

   if (i>1) {
      int *q;
//             <    is read   >: i
//             <    is written>: q
      q = &a[i];
//             <    is read   >: q
//             <    is written>: p
      p = q;
//             <    is written>: i
      i = 0;
   }
   return 0;
}

 *********************** Use-Def Chains *********************

	11 --> 13 with conflicts
		from <i-R-EXACT> to <i-W-EXACT>
	11 --> 12 with conflicts
		from <q-W-EXACT> to <q-R-EXACT>
	04 --> 13 with conflicts
		from <i-W-EXACT> to <i-W-EXACT>
	04 --> 11 with conflicts
		from <i-W-EXACT> to <i-R-EXACT>
	04 --> 08 with conflicts
		from <i-W-EXACT> to <i-R-EXACT>

