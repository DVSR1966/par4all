
PROPER  EFFECTS 

int decl01()
{
//             <    is written>: i
   int i = 2;
//             <    is read   >: i
//             <    is written>: j
   int j = i;
   int a[sizeof(i)];
//             <    is written>: i

   i = 2;
//             <    is read   >: i
//             <    is written>: j
   j = i;
//             <    is written>: j
   j = sizeof(i);
   return 0;
}

POINTS-TO


// Points To: none

int decl01()
{

// Points To: none

   int i = 2;

// Points To: none

   int j = i;

// Points To: none

   int a[sizeof(i)];

// Points To: none


   i = 2;

// Points To: none

   j = i;

// Points To: none

   j = sizeof(i);

// Points To: none

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int decl01()
{
//             <    is written>: i
   int i = 2;
//             <    is read   >: i
//             <    is written>: j
   int j = i;
   int a[sizeof(i)];
//             <    is written>: i

   i = 2;
//             <    is read   >: i
//             <    is written>: j
   j = i;
//             <    is written>: j
   j = sizeof(i);
   return 0;
}

