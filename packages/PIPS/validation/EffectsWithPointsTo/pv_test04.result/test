
PROPER  EFFECTS 

int main()
{
   int *a;
//             <    is written>: b
   int b = 0;
//             <    is written>: c
   int c = 1;
//             <    is read   >: b c
   if (b==c||b!=0)
//             <    is written>: a
      a = &b;
   else
//             <    is written>: a
      a = &c;
//             <    is read   >: b c

   if (b!=c)
//             <    is written>: a
      a = &b;
   else
//             <    is written>: a
      a = &b;
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int *a;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b = 0;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int c = 1;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (b==c||b!=0)

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      a = &b;
   else

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      a = &c;

// Points To:
// a -> b , MAY
// a -> c , MAY


   if (b!=c)

// Points To:
// a -> b , MAY
// a -> c , MAY

      a = &b;
   else

// Points To:
// a -> b , MAY
// a -> c , MAY

      a = &b;

// Points To:
// a -> b , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int *a;
//             <    is written>: b
   int b = 0;
//             <    is written>: c
   int c = 1;
//             <    is read   >: b c
   if (b==c||b!=0)
//             <    is written>: a
      a = &b;
   else
//             <    is written>: a
      a = &c;
//             <    is read   >: b c

   if (b!=c)
//             <    is written>: a
      a = &b;
   else
//             <    is written>: a
      a = &b;
   return 0;
}

