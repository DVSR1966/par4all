
PROPER  EFFECTS 

int main()
{
   FILE *pFile;
   int n;
   char name[100];
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: __IO_EFFECTS:LUNS[*]
//             <    is written>: pFile

   pFile = fopen("myfile.txt", "w");
//             <    is written>: n
   for(n = 0; n <= 2; n += 1) {
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      puts("please, enter a name: ");
//             <may be read   >: stdin[0]
//             <may be written>: name[*] stdin[0]
//             <    is read   >: stdin __IO_EFFECTS:LUNS[0]
//             <    is written>: __IO_EFFECTS:LUNS[0]
      gets(name);
//             <may be read   >: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
      fprintf(pFile, "Name %d %s\n", n, name);
   }
//             <may be read   >: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
   fclose(pFile);
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   FILE *pFile;

// Points To:
// pFile -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int n;

// Points To:
// pFile -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   char name[100];

// Points To:
// pFile -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   pFile = fopen("myfile.txt", "w");

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   for(n = 0; n <= 2; n += 1) {

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

      puts("please, enter a name: ");

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

      gets(name);

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

      fprintf(pFile, "Name %d %s\n", n, name);
   }

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   fclose(pFile);
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   FILE *pFile;
   int n;
   char name[100];
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: __IO_EFFECTS:LUNS[*]
//             <    is written>: pFile

   pFile = fopen("myfile.txt", "w");
//             <    is written>: n
   for(n = 0; n <= 2; n += 1) {
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      puts("please, enter a name: ");
//             <may be read   >: stdin[0]
//             <may be written>: name[*] stdin[0]
//             <    is read   >: stdin __IO_EFFECTS:LUNS[0]
//             <    is written>: __IO_EFFECTS:LUNS[0]
      gets(name);
//             <may be read   >: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
      fprintf(pFile, "Name %d %s\n", n, name);
   }
//             <may be read   >: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
   fclose(pFile);
}

