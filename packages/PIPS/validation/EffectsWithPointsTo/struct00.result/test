
POINTS-TO

//  {}
int main()
{
//  points to = {}
   struct one {
      int *first;
   } x;
//  {(x[first],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int y[10];
//  {(x[first],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   x.first = &y[5];
//  {(x[first],y[5],-Exact-)}
   
   
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   struct one {
      int *first;
   } x;
   int y[10];
//             <    is written>: x.first

   x.first = &y[5];
   
   
   return 0;
}

