
PROPER  EFFECTS 

int main()
{
   struct one {
      int *first;
   } x;
   int y[10];
//             <    is written>: x.first

   x.first = &y[5];
   
   
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   struct one {
      int *first;
   } x;

// Points To:
// x.first -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int y[10];

// Points To:
// x.first -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   x.first = &y[5];

// Points To:
// x.first -> &y[5] , EXACT

   
   
   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   struct one {
      int *first;
   } x;
   int y[10];
//             <    is written>: x.first

   x.first = &y[5];
   
   
   return 0;
}

