
PROPER  EFFECTS 

int main()
{
   int t[100][10][3];
   int *pu;
//             <    is written>: pu

   pu = t[1][2];
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int t[100][10][3];

// Points To: none

   int *pu;

// Points To:
// pu -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   pu = t[1][2];

// Points To:
// pu -> &t[1][2] , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int t[100][10][3];
   int *pu;
//             <    is written>: pu

   pu = t[1][2];
   return 0;
}

