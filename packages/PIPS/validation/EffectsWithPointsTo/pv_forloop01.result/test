
PROPER  EFFECTS 

int main()
{
   float t;
   float a;
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is read   >: a t
//             <    is written>: a
      a = a+t*0, 5;
//             <    is read   >: t
//             <    is written>: t
      t += 0, 01;
   }
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   float t;

// Points To: none

   float a;

// Points To: none


   t = 1.0;

// Points To: none

   while (t<2.0) {

// Points To: none

      a = a+t*0, 5;

// Points To: none

      t += 0, 01;
   }

// Points To: none

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   float t;
   float a;
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is read   >: a t
//             <    is written>: a
      a = a+t*0, 5;
//             <    is read   >: t
//             <    is written>: t
      t += 0, 01;
   }
   return 0;
}

 *********************** Use-Def Chains *********************

	-1 --> -1 with conflicts
		from <t-W-EXACT> to <t-W-EXACT>
		from <t-W-EXACT> to <t-R-EXACT>
		from <t-R-EXACT> to <t-W-EXACT>
	-1 --> -1 with conflicts
		from <t-W-EXACT> to <t-R-EXACT>
	-1 --> -1 with conflicts
		from <t-R-EXACT> to <t-W-EXACT>
	-1 --> -1 with conflicts
		from <a-W-EXACT> to <a-W-EXACT>
		from <a-W-EXACT> to <a-R-EXACT>
		from <a-R-EXACT> to <a-W-EXACT>
	-1 --> -1 with conflicts
		from <t-R-EXACT> to <t-W-EXACT>
	-1 --> -1 with conflicts
		from <t-W-EXACT> to <t-W-EXACT>
		from <t-W-EXACT> to <t-R-EXACT>
	-1 --> -1 with conflicts
		from <t-W-EXACT> to <t-R-EXACT>
	-1 --> -1 with conflicts
		from <t-W-EXACT> to <t-R-EXACT>

