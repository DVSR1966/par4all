
PROPER  EFFECTS 

int main()
{
//             <    is written>: x
   double x = 1.;
//             <    is written>: p
   double *p = &x;
//             <    is written>: q
   double *q = 0;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: q stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("q = %p", q);
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p = 2.;
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   double x = 1.;

// Points To: none

   double *p = &x;

// Points To:
// p -> &x , EXACT

   double *q = 0;

// Points To:
// p -> &x , EXACT
// q -> &*NULL_POINTER* , EXACT

   printf("q = %p", q);

// Points To:
// p -> &x , EXACT
// q -> &*NULL_POINTER* , EXACT

   q = p;

// Points To:
// p -> &x , EXACT
// q -> &x , EXACT

   *p = 2.;

// Points To:
// p -> &x , EXACT
// q -> &x , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: x
   double x = 1.;
//             <    is written>: p
   double *p = &x;
//             <    is written>: q
   double *q = 0;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: q stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("q = %p", q);
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <    is read   >: p
//             <    is written>: x
   *p = 2.;
   return 0;
}

 *********************** Use-Def Chains *********************

	08 --> 09 with conflicts
		from <q-R-EXACT> to <q-W-EXACT>
	07 --> 09 with conflicts
		from <q-W-EXACT> to <q-W-EXACT>
	07 --> 08 with conflicts
		from <q-W-EXACT> to <q-R-EXACT>
	06 --> 10 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>
	06 --> 09 with conflicts
		from <p-W-EXACT> to <p-R-EXACT>
	05 --> 10 with conflicts
		from <x-W-EXACT> to <x-W-EXACT>

****************** End of Dependence Graph ******************
