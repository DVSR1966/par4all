
PROPER  EFFECTS 

int main()
{
   int i;
   tf_t s;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

   call01(&i, &s);
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i;
//  {}
   tf_t s;
//  {}

   call01(&i, &s);
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
   tf_t s;
//             <    is written>: i s.one s.two[4]

   call01(&i, &s);
}

