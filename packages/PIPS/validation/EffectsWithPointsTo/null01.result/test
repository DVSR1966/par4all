
PROPER  EFFECTS 

int main()
{
   int *p;
//             <    is written>: p
   p = (void *) 0;

   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = (void *) 0;

// Points To:
// p -> *NULL_POINTER* , EXACT


   return 0;
}

