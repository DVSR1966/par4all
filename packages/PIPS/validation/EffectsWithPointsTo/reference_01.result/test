
PROPER  EFFECTS 

int main(void)
{
//             <    is written>: k
   int k = reference_01();
//             <    is read   >: k

   return k;
}
int reference_01(void)
{
//             <    is written>: i
   int i = 0;
//             <    is written>: j
   int j = 1;
   
   // effects on useless expression
   0;
   // effects on empty references...
   j/i;
   (void) j/i;
//             <    is read   >: i j
   return i/j;
}

