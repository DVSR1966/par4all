
PROPER  EFFECTS 

int main()
{
   int i;
   int *p[10];
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p[10];

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE* , EXACT

   int a[10];

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE* , EXACT


   for(i = 0; i <= 9; i += 1)

// Points To:
// p[*] -> a[*] , MAY
// p[*] -> *ANY_MODULE*:*NOWHERE* , MAY

      p[i] = &a[i];

// Points To:
// p[*] -> a[*] , MAY
// p[*] -> *ANY_MODULE*:*NOWHERE* , MAY

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i;
   int *p[10];
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
   return 0;
}

 *********************** Use-Def Chains *********************

	09 --> 09 with conflicts
		from <p[i]-W-EXACT> to <p[i]-W-EXACT>

****************** End of Dependence Graph ******************
