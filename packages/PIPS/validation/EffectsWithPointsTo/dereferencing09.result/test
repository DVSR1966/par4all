
PROPER  EFFECTS 

int main()
{
   int a, aa, *b, *bb, **c;
//             <    is written>: a

   a = 1;
//             <    is written>: b
   b = &a;
//             <    is written>: c
   c = &b;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: bb

   bb = *c;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: aa
   aa = **c;
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int a, aa, *b, *bb, **c;

// Points To:
// b -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// bb -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> &*ANY_MODULE*:*NOWHERE*_b1 , EXACT


   a = 1;

// Points To:
// b -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// bb -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> &*ANY_MODULE*:*NOWHERE*_b1 , EXACT

   b = &a;

// Points To:
// b -> &a , EXACT
// bb -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> &*ANY_MODULE*:*NOWHERE*_b1 , EXACT

   c = &b;

// Points To:
// b -> &a , EXACT
// bb -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> &b , EXACT


   bb = *c;

// Points To:
// b -> &a , EXACT
// bb -> &a , EXACT
// c -> &b , EXACT

   aa = **c;

// Points To:
// b -> &a , EXACT
// bb -> &a , EXACT
// c -> &b , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int a, aa, *b, *bb, **c;
//             <    is written>: a

   a = 1;
//             <    is written>: b
   b = &a;
//             <    is written>: c
   c = &b;
//             <    is read   >: b c
//             <    is written>: bb

   bb = *c;
//             <    is read   >: a b c
//             <    is written>: aa
   aa = **c;
   return 0;
}

 *********************** Use-Def Chains *********************

	07 --> 10 with conflicts
		from <c-W-EXACT> to <c-R-EXACT>
	07 --> 09 with conflicts
		from <c-W-EXACT> to <c-R-EXACT>
	06 --> 10 with conflicts
		from <b-W-EXACT> to <b-R-EXACT>
	06 --> 09 with conflicts
		from <b-W-EXACT> to <b-R-EXACT>
	05 --> 10 with conflicts
		from <a-W-EXACT> to <a-R-EXACT>

****************** End of Dependence Graph ******************
