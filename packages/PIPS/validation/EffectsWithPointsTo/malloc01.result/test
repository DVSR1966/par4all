
PROPER  EFFECTS 

int main(int argc, char *argv[])
{
   int i, j;
   int *a[64];
//             <    is written>: i

   for(i = 0; i <= 63; i += 1) {
//             <    is read   >: i _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a[i] _MALLOC_EFFECTS:_MALLOC_
      a[i] = (float) malloc(64*sizeof(float));
//             <    is written>: j
      for(j = 0; j <= 63; j += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: a[i] i j
         a[i][j] = 0.;
   }

   return 0;
}

