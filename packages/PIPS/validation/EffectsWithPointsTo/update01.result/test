
PROPER  EFFECTS 

void update01()
{
   int i;
   int *p;
//             <    is written>: p

   p = &i;
//             <    is read   >: p
//             <    is written>: p
   p++;
//             <    is written>: i
   i = 2;
}

POINTS-TO

//  {}
void update01()
{
//  {}
   int i;
//  {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = &i;
//  {(p,i,-Exact-)}
   p++;
//  {(p,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   i = 2;
}

PROPER  EFFECTS WITH POINTS TO

void update01()
{
   int i;
   int *p;
//             <    is written>: p

   p = &i;
//             <    is read   >: p
//             <    is written>: p
   p++;
//             <    is written>: i
   i = 2;
}

