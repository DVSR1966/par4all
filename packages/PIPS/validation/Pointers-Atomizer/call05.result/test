atomized code
int main()
{
   int d1 = 4;
   int d2 = 4;
   int y[d1][d2];
   int i, j;

   for(i = 0; i <= d1-1; i += 1)
      call05(i, d2, y[i]);

   for(i = 0; i <= d1-1; i += 1) {
      for(j = 0; j <= d2-1; j += 1)
         printf("y[%d][%d] = %d\t", i, j, y[i][j]);
      printf("\n");
   }
}
