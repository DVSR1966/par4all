### initial code
      PROGRAM DC
      DOUBLE COMPLEX A, B, C, D, E, F, G, H
      DOUBLE PRECISION X
      COMPLEX W, Z

      A = (1.0, 1.0)                                                    0001
      B = 1                                                             0002
      C = 1.0E0                                                         0003
      D = 1.0D0                                                         0004

      E = A+B                                                           0005
      F = COS(E)                                                        0006
      X = ABS(F)                                                        0007
      F = DCMPLX(X)                                                     0008
      H = LOG(G)+SQRT(F)+SIN(E)+EXP(A)                                  0009

      PRINT *, H                                                        0010

      W = (1.0, 1.0)                                                    0011
      Z = W+1                                                           0012

      W = CMPLX(1.0, 1.0)                                               0013
      Z = W+1                                                           0014

      END
