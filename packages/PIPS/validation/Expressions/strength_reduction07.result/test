
//  T(main) {main==0}

int main()
{

//  T(i) {i==0}

   int i = 0, *a = malloc(10*sizeof(int)), *b;

//  T(main) {i==0}

   if (!a)

//  T(main) {i==0, main==1}

      return 1;

//  T() {i==0}

   b = a;

//  T(i) {i#init==0, 1<=i}

   for(i = 1; i <= 8; i += 1)

//  T(i) {1<=i#init, i#init<=8}

      *(a+i-1) = 1;

//  T() {i==9}

   printf("%d", *(a+2));

//  T(main) {i==9, main==0}

   return 0;
}
int main()
{
   int i = 0, *a = malloc(10*sizeof(int)), *b;
   //PIPS generated variable
   int *a0;
   if (!a)
      return 1;
   b = a;
   a0 = a+1;
   for(i = 1; i <= 8; i += 1) {
      *(a0-1) = 1;
      a0 += 1;
   }
   printf("%d", *(a+2));
   return 0;
}
int main()
{
   int i = 0, *a = malloc(10*sizeof(int)), *b;
   //PIPS generated variable
   int *a0;
   if (!a)
      return 1;
   b = a;
   a0 = a+1;
   for(i = 1; i <= 8; i += 1) {
      *(a0-1) = 1;
      a0 += 1;
   }
   printf("%d", *(a+2));
   return 0;
}

