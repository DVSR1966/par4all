delete $WSPACE
setproperty ABORT_ON_USER_ERROR=TRUE
create $WSPACE $FILE
setproperty PRETTYPRINT_ALL_PRIVATE_VARIABLES TRUE

display PRINTED_FILE[%ALLFUNC]

apply PRIVATIZE_MODULE

display PRINTED_FILE[%ALLFUNC]

apply UNSPLIT
shell cc -fopenmp -c $WSPACE.database/Src/`basename $FILE` -o $WSPACE.database/Src/`basename $FILE`.o

close
 delete $WSPACE

quit
