C               <must be read   >: X Y __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)

      SUBROUTINE B(X, Y)
C               <must be read   >: X Y __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)

      PRINT *, X+Y                                                      0004

      END
