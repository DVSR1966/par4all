int main()
{
   float a[501];
   float b[501][501];
   float c[501][501][501];
   int i, j, k;

   for(i = 0; i <= 123; i += 2)
      for(j = 0; j <= 498; j += 1)
         if (i==0)
            a[i] = (float) i;
         else
            a[i] = a[i]+1.0;
   
   
   return 0;
}
int main()
{
   float a[501];
   float b[501][501];
   float c[501][501][501];
   int i, j, k;

   for(i = 0; i <= 123; i += 2)
      for(j = 0; j <= 498; j += 1)
         if (i==0)
            a[i] = (float) i;
         else
            a[i] = a[i]+1.0;
   
   
   return 0;
}
int main()
{
   float a[501];
   float b[501][501];
   float c[501][501][501];
   int i, j, k;
   // Loop nest P4A begin,2D(499, 62)
   for(i = 1; i <= 62; i += 1)
      for(j = 0; j <= 498; j += 1)
         // Loop nest P4A end
         if (i<=62&&j<=498)
            if (2*i-2==0)
               a[2*i-2] = (float) (2*i-2);
            else
               a[2*i-2] = a[2*i-2]+1.0;
   i = 124;
   
   
   return 0;
}
