// OMP style
int main()
{
   float a[10];                                                         /*0003*/
   float b[10][10][10][10][10];                                         /*0004*/
   int m = 0;                                                           /*0005*/
   int n = 0;                                                           /*0006*/
   int o = 2;                                                           /*0007*/


   for(m = 0; m <= 9; m += 1) {
      n = n+1;                                                          /*0010*/
      a[m] = n;                                                         /*0011*/
   }


   for(m = 0; m <= 9; m += 1) {
      o = 2+o;                                                          /*0017*/
      o = 2+o;                                                          /*0018*/
   }
   for(m = 0; m <= 9; m += 1) {
      // comments 4
      n = n+1;                                                          /*0016*/
      n = n+5;                                                          /*0019*/
      b[0][0][1][m][0] = n;                                             /*0020*/
   }

   return o;                                                            /*0023*/
}
int main()
{
   float a[10];                                                         /*0003*/
   float b[10][10][10][10][10];                                         /*0004*/
   int m = 0;                                                           /*0005*/
   int n = 0;                                                           /*0006*/
   int o = 2;                                                           /*0007*/


   for(m = 0; m <= 9; m += 1) {
      n = n+1;                                                          /*0010*/
      a[m] = n;                                                         /*0011*/
   }


#pragma omp parallel for reduction(+:o)
   for(m = 0; m <= 9; m += 1) {
      o = 2+o;                                                          /*0017*/
      o = 2+o;                                                          /*0018*/
   }
   for(m = 0; m <= 9; m += 1) {
      // comments 4
      n = n+1;                                                          /*0016*/
      n = n+5;                                                          /*0019*/
      b[0][0][1][m][0] = n;                                             /*0020*/
   }

   return o;                                                            /*0023*/
}
