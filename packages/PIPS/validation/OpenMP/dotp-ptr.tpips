delete      dotp-ptr
create      dotp-ptr dotp-ptr.c

setproperty PRETTYPRINT_SEQUENTIAL_STYLE "do"

apply       PRIVATIZE_MODULE(%ALL)
apply       COARSE_GRAIN_PARALLELIZATION(dotp_ptr)
