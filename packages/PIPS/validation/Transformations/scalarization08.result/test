
OUT regions

int scalarization08(int x[10], int y[10][10])
{
   // BEGIN BLOCK
   int i, j, k;

//  <x[PHI1]-OUT-EXACT-{PHI1==1}>

   for(i = 0; i <= 9; i += 1)

//  <x[PHI1]-OUT-EXACT-{PHI1==i, PHI1==1}>

      for(j = 0; j <= 9; j += 1)

//  <x[PHI1]-OUT-EXACT-{PHI1==i, PHI1==1, j==9, 0<=j}>

         x[i] = y[i][j];
   //printf("%d", x[1]);
   k = x[1];
   return k;
   // END BLOCK
}

Code after scalarization

int scalarization08(int x[10], int y[10][10])
{
   // BEGIN BLOCK
   int i, j, k;
   //PIPS generated variable
   int __scalar__0;

//  <x[PHI1]-OUT-EXACT-{PHI1==1}>

   for(i = 0; i <= 9; i += 1) {

//  <x[PHI1]-OUT-EXACT-{PHI1==i, PHI1==1}>

      // BEGIN BLOCK
      for(j = 0; j <= 9; j += 1)
         __scalar__0 = y[i][j];

//  <x[PHI1]-OUT-EXACT-{PHI1==i, PHI1==1, j==10}>

      x[i] = __scalar__0;
      // END BLOCK
   }
   //printf("%d", x[1]);
   k = x[1];
   return k;
   // END BLOCK
}
