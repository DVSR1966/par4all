                                          fgUINT16 *P_40;
                                          P_40 = pdat[i];
                                          _return0 = P_40[I_3];
                                          }
                                          _return2 = _return0;
                                          }
                                       }
                                       data_val = _return2;
                                       list_coord_x[m] = l;
                                       list_coord_y[m] = k;
                                       list_data[m] = data_val;
                                       list_nhb[m] = nhb_val;
                                       m++;
                                    }
                                    l++;
                                 }
                                 k++;
                              }
                              size[n] = m;
                           }
                        }
                     }
                     (*computeNhb0)(result,extr);
                     {
                        //PIPS generated variable
                        fgUINT16 *data_array0, *vector_array0;
                        //PIPS generated variable
                        int spp0, pos0;
                        data_array0 = data_array;
                        vector_array0 = vector_array;
                        spp0 = spp;
                        pos0 = j;
                        {
                           register fgUINT16 val;
                           register int i, k;
                           i = pos0*spp0;

                           for(k = 0; k <= spp0-1; k += 1) {
                              _return0 = vector_array0[k];
                              val = _return0;
                              {
                                 //PIPS generated variable
                                 fgUINT16 *array0;
                                 //PIPS generated variable
                                 int pos0;
                                 //PIPS generated variable
                                 fgUINT16 value0;
                                 array0 = data_array0;
                                 pos0 = i;
                                 value0 = val;
                                 array0[pos0] = value0;
                              }
                              i++;
                           }
                        }
                     }
                  }
               }
               flgr2d_destroy(nhbOdd);
               flgr2d_destroy(nhbEven);
               flgr2d_destroy_neighbor_box(extr);
               flgr_vector_destroy(result);
            }
         }
      }
   }
}
