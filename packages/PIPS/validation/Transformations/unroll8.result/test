      PROGRAM UNROLL8
C
      PARAMETER (N=4)
      REAL T(N,N)

C     Outer unrolling with different labels

Cxxx
      DO 10 I = 1, N                                                    0009
C
         DO 20 J = 1, N                                                 0011
            T(I,J) = -N*I+J                                             0012
20          CONTINUE                                                    0013
10       CONTINUE                                                       0014

C     Inner unrolling with different labels

      DO 30 I = 1, N                                                    0018
Cxxx
         DO 40 J = 1, N                                                 0020
            T(I,J) = -N*I+J                                             0021
40          CONTINUE                                                    0022
30       CONTINUE                                                       0023

C     Outer unrolling with same labels

Cxxx
      DO 11 I = 1, N                                                    0028
C
         DO 11 J = 1, N                                                 0030
            T(I,J) = -N*I+J                                             0031
11          CONTINUE                                                    0032

C     Inner unrolling with same labels

      DO 31 I = 1, N                                                    0036
Cxxx
         DO 31 J = 1, N                                                 0038
            T(I,J) = -N*I+J                                             0039
31          CONTINUE                                                    0040
C
      END
      PROGRAM UNROLL8
C
      PARAMETER (N=4)
      REAL T(N,N)

C     Outer unrolling with different labels

Cxxx
      DO I_0 = 1, N
         T(1,I_0) = -N*1+I_0
      ENDDO
      DO I_1 = 1, N
         T(2,I_1) = -N*2+I_1
      ENDDO
      DO I_2 = 1, N
         T(3,I_2) = -N*3+I_2
      ENDDO
      DO I_3 = 1, N
         T(4,I_3) = -N*4+I_3
      ENDDO
10    CONTINUE
      I = 5

C     Inner unrolling with different labels

      DO 30 I = 1, N                                                    0018
Cxxx
         T(I,1) = -N*I+1
         T(I,2) = -N*I+2
         T(I,3) = -N*I+3
         T(I,4) = -N*I+4
40       CONTINUE
         J = 5
30       CONTINUE                                                       0023

C     Outer unrolling with same labels

Cxxx
      DO I_4 = 1, N
         T(1,I_4) = -N*1+I_4
      ENDDO
      DO I_5 = 1, N
         T(2,I_5) = -N*2+I_5
      ENDDO
      DO I_6 = 1, N
         T(3,I_6) = -N*3+I_6
      ENDDO
      DO I_7 = 1, N
         T(4,I_7) = -N*4+I_7
      ENDDO
11    CONTINUE
      I = 5

C     Inner unrolling with same labels

      DO 31 I = 1, N                                                    0036
Cxxx
         T(I,1) = -N*I+1
         T(I,2) = -N*I+2
         T(I,3) = -N*I+3
         T(I,4) = -N*I+4
31       CONTINUE
         J = 5
C
      END
