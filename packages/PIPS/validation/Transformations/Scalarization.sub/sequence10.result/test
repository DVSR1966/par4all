
Complexity of "sequence10" before scalarization

//                                                                   21 (SUMMARY)
int sequence10(int x[10], int y[10][10])
{
//                                                                    0 (STMT)
   int k;
//                                                                    3 (STMT)
   x[1] = y[1][1];
//                                                                    5 (STMT)
   x[1] = x[1]+y[1][2];
//                                                                    5 (STMT)
   x[1] = x[1]+y[1][1];
//                                                                    5 (STMT)
   x[1] = x[1]+y[1][1];
//                                                                    2 (STMT)
   k = x[1];
//                                                                    1 (STMT)
   return k;
}

Code after scalarization

int sequence10(int x[10], int y[10][10])
{
   int k;
   //PIPS generated variable
   register int x_0;
   x_0 = y[1][1];
   x_0 = x_0+y[1][2];
   x_0 = x_0+y[1][1];
   x_0 = x_0+y[1][1];
   k = x_0;
   return k;
}

Complexity of "sequence10" after scalarization

//                                                                   13 (SUMMARY)
int sequence10(int x[10], int y[10][10])
{
//                                                                    0 (STMT)
   int k;
   //PIPS generated variable
//                                                                    0 (STMT)
   register int x_0;
//                                                                    2 (STMT)
   x_0 = y[1][1];
//                                                                    3 (STMT)
   x_0 = x_0+y[1][2];
//                                                                    3 (STMT)
   x_0 = x_0+y[1][1];
//                                                                    3 (STMT)
   x_0 = x_0+y[1][1];
//                                                                    1 (STMT)
   k = x_0;
//                                                                    1 (STMT)
   return k;
}
