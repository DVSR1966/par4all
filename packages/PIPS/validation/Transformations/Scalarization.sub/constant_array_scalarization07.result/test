void constant_array_scalarization07()
{
   int a[3];

   a[0] = 1;
   a[1] = 1;
   a[2] = 1;
   {
      int a[3];

      a[0] = 1;
      a[1] = 1;
      a[2] = 1;
   }
}
void constant_array_scalarization07()
{
   int a[3];
   //PIPS generated variable
   int a0, a1, a2, a3, a4, a5;

   a3 = 1;
   a4 = 1;
   a5 = 1;
   {
      int a[3];

      a0 = 1;
      a1 = 1;
      a2 = 1;
   }
}
