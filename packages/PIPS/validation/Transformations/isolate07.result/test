void lonesome_cowboy()
{
   int i, j, n = 12;
   int a[n][n];
   //PIPS generated variable
   int i0, i1;
   for(i = 0; i <= n-1; i += 1)
      for(j = 0; j <= n-1; j += 1)
         a[i][j] = 0;
   for(i = 1; i <= n-1-1; i += 1) {
      //PIPS generated variable
      int a0[1][8];
      ;

isolate:
