after loop expansion
int pop(int pop)
{
   int i = 0;
   //PIPS generated variable
   int I_0;
   // PIPS test to adjust loop size to required loop expansion parameter
   if (MOD(-i+op, 128)==0)
      I_0 = op-1;
   else
      I_0 = op-1+128-MOD(-i+op, 128);
l99999:
   for(i = i; i <= I_0; i += 1)
      if (i>=i&&i<=op-1) 
         printf("%d", i);
}
