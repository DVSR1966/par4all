
Initial code

void scalarization19(double B[SIZE][SIZE])
{
   // BEGIN BLOCK
   int i, j;
   for(i = 0; i <= SIZE-1; i += 1)
      for(j = 0; j <= SIZE-1; j += 1)
         B[i][j] = 0.;
   // END BLOCK
}

Initial code with preconditions


//  P() {TOP-LEVEL:SIZE==10}

void scalarization19(double B[SIZE][SIZE])
{

//  P() {TOP-LEVEL:SIZE==10}

   // BEGIN BLOCK
   int i, j;

//  P() {TOP-LEVEL:SIZE==10}

   for(i = 0; i <= SIZE-1; i += 1)

//  P(i,j) {TOP-LEVEL:SIZE==10, 0<=i, i<=9}

      for(j = 0; j <= SIZE-1; j += 1)

//  P(i,j) {TOP-LEVEL:SIZE==10, 0<=i, i<=9, 0<=j, j<=9}

         B[i][j] = 0.;
   // END BLOCK
}

IN regions

void scalarization19(double B[SIZE][SIZE])
{
   // BEGIN BLOCK
   int i, j;
   for(i = 0; i <= SIZE-1; i += 1)
      for(j = 0; j <= SIZE-1; j += 1)
         B[i][j] = 0.;
   // END BLOCK
}

OUT regions


//  <B[PHI1][PHI2]-OUT-EXACT-{PHI1==0, PHI2==0, TOP-LEVEL:SIZE==10}>

void scalarization19(double B[SIZE][SIZE])
{

//  <B[PHI1][PHI2]-OUT-EXACT-{PHI1==0, PHI2==0, TOP-LEVEL:SIZE==10}>

   // BEGIN BLOCK
   int i, j;

//  <B[PHI1][PHI2]-OUT-EXACT-{PHI1==0, PHI2==0, TOP-LEVEL:SIZE==10}>

   for(i = 0; i <= SIZE-1; i += 1)

//  <B[PHI1][PHI2]-OUT-EXACT-{PHI1==i, PHI1==0, PHI2==0,
//    TOP-LEVEL:SIZE==10}>

      for(j = 0; j <= SIZE-1; j += 1)

//  <B[PHI1][PHI2]-OUT-EXACT-{PHI1==i, PHI1==0, j==PHI2, PHI2==0,
//    TOP-LEVEL:SIZE==10}>

         B[i][j] = 0.;
   // END BLOCK
}

Code after scalarization

void scalarization19(double B[SIZE][SIZE])
{
   // BEGIN BLOCK
   int i, j;
   for(i = 0; i <= SIZE-1; i += 1)
      for(j = 0; j <= SIZE-1; j += 1)
         B[i][j] = 0.;
   // END BLOCK
}
