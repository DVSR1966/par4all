      PROGRAM UNROLL10
      INTEGER*1 LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 J
      INTEGER K
      INTEGER*8 L
      INTEGER*1 A(0:199)

      LU_NUB0 = 200
      LU_IB0 = 0
      LU_IND0 = 0
99999 CONTINUE
      DO 99998 LU_IND0 = 0, 199, 8
         A(LU_IND0) = LU_IND0
         A(LU_IND0+1) = LU_IND0+1
         A(LU_IND0+2) = LU_IND0+2
         A(LU_IND0+3) = LU_IND0+3
         A(LU_IND0+4) = LU_IND0+4
         A(LU_IND0+5) = LU_IND0+5
         A(LU_IND0+6) = LU_IND0+6
         A(LU_IND0+7) = LU_IND0+7
99998    CONTINUE
      I = 200
      PRINT *, A                                                        0015
      END
