int main()
{
   int a[10][10];
   {
      //PIPS generated variable
      int i;
      
      // tmp must be declared as private in the two loops
      for(i = 0; i <= 9; i += 1) {
         //PIPS generated variable
         int j1, tmp1;
         for(j1 = 0; j1 <= 9; j1 += 1) {
            tmp1 = a[i][j1];
            a[i][j1] = tmp1+j1;
         }
         tmp1 = i*2;
         a[i][i] = a[i][i]+tmp1;
      }
   }
   return 0;
}
