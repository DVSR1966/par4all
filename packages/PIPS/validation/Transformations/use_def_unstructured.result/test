      PROGRAM USE_DEF_UNSTRUCTURED

C  P() {}

C     BEGIN UNSTRUCTURED

C  P() {}


1     IF (I.EQ.0) GOTO 100                                              0003

C  P() {}

      A = 0                                                             0004

C  P() {}

      IF (I.EQ.2) GOTO 1                                                0005

C  P() {}

      B = 0                                                             0006

C  P() {}

      WRITE (*, B)                                                      0007

C  P() {}

100   CONTINUE                                                          0008
      GOTO 1
C     END UNSTRUCTURED
      END

