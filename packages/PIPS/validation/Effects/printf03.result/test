int main()
{
   int i = 5;
   int j = 6;
   float x;
   double y[10];
   char *fmt = "i=%d, %d, %f\n";

   y[0] = 0.;
   printf(fmt, i, i+j, x);
}
//               <must be read   >: __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
int main()
{
//               <must be written>: i
   int i = 5;
//               <must be written>: j
   int j = 6;
   float x;
   double y[10];
//               <must be written>: fmt
   char *fmt = "i=%d, %d, %f\n";
//               <must be written>: y[0]

   y[0] = 0.;
//               <may be read    >: fmt[*]
//               <must be read   >: __IO_EFFECTS:LUNS[1] fmt i j x
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf(fmt, i, i+j, x);
}
