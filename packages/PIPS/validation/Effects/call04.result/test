void call04(int n, int m, int x[n][m])
{
   int i,  j;
//               <must be read   >: n
//               <must be written>: i

   for(i = 0; i <= n-1; i += 1)
//               <must be read   >: m n
//               <must be written>: j
      for(j = 0; j <= m-1; j += 1)
//               <must be read   >: i j m n
//               <must be written>: x[i][j]
         x[i][j] = 0;
}

