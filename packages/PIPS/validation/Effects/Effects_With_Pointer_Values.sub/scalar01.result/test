int main()
{

// Pointer values: none

   int a, b, *p;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   a = 0;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   p = &a;

// Pointer values:
// p == &a (exact);

   b = *p;

// Pointer values:
// p == &a (exact);

   return b;
}
int main()
{
   int a, b, *p;
//             <    is written>: a
   a = 0;
//             <    is written>: p
   p = &a;
//             <    is read   >: a p
//             <    is written>: b
   b = *p;
//             <    is read   >: b
   return b;
}
