out regions for "foo"


//  <p[PHI1]-OUT-EXACT-{0<=PHI1, PHI1<=9}>

void foo(int *p)
{
   int i;                                                               /*0011*/

//  <p[PHI1]-OUT-EXACT-{0<=PHI1, PHI1<=9}>


   for(i = 0; i <= 9; i += 1)

//  <p[PHI1]-OUT-EXACT-{i==PHI1, 0<=i, i<=9}>

      p[i] = i;                                                         /*0014*/
   return;                                                              /*0015*/
}
