
Proper effects for "foo"

int foo(float (*b)[3])
{
   float c;                                                             /*0011*/
//               <must be read   >: b
//               <must be written>: b[0][3]
   (*b)[3] = 2.0;                                                       /*0012*/
//               <must be read   >: b b[0][3]
//               <must be written>: c
   c = (*b)[3];                                                         /*0013*/
//               <must be read   >: b
//               <must be written>: b[1][3]
   b[1][3] = 2.0;                                                       /*0014*/
//               <must be read   >: b b[1][3]
//               <must be written>: c
   c = b[1][3];                                                         /*0015*/
//               <must be read   >: b b[0][3]
//               <must be written>: b[0][3]
   (*b)[3]++;                                                           /*0017*/
//               <must be read   >: b b[0][3]
//               <must be written>: b[0][3]
   (*b)[3] += 5.0;                                                      /*0018*/
//               <must be read   >: b b[1][3]
//               <must be written>: b[1][3]
   b[1][3]++;                                                           /*0019*/
//               <must be read   >: b b[1][3]
//               <must be written>: b[1][3]
   b[1][3] += 5.0;                                                      /*0020*/
   return 1;                                                            /*0022*/
}

Proper effects for "foo2"

int foo2(float b[5][3])
{
   float c;                                                             /*0027*/
//               <must be written>: b[0][3]
   (*b)[3] = 2.0;                                                       /*0028*/
//               <must be read   >: b[0][3]
//               <must be written>: c
   c = (*b)[3];                                                         /*0029*/
//               <must be written>: b[1][3]
   b[1][3] = 2.0;                                                       /*0030*/
//               <must be read   >: b[1][3]
//               <must be written>: c
   c = b[1][3];                                                         /*0031*/
//               <must be read   >: b[0][3]
//               <must be written>: b[0][3]
   (*b)[3]++;                                                           /*0033*/
//               <must be read   >: b[0][3]
//               <must be written>: b[0][3]
   (*b)[3] += 5.0;                                                      /*0034*/
//               <must be read   >: b[1][3]
//               <must be written>: b[1][3]
   b[1][3]++;                                                           /*0035*/
//               <must be read   >: b[1][3]
//               <must be written>: b[1][3]
   b[1][3] += 5.0;                                                      /*0036*/

   return 1;                                                            /*0038*/
}

Proper effects for "foo3"

int foo3()
{
   float c;                                                             /*0043*/
//               <must be written>: d[0][3]
   (*d)[3] = 2.0;                                                       /*0044*/
//               <must be read   >: d[0][3]
//               <must be written>: c
   c = (*d)[3];                                                         /*0045*/
//               <must be written>: d[1][3]
   d[1][3] = 2.0;                                                       /*0046*/
//               <must be read   >: d[1][3]
//               <must be written>: c
   c = d[1][3];                                                         /*0047*/
//               <must be read   >: d[0][3]
//               <must be written>: d[0][3]
   (*d)[3]++;                                                           /*0049*/
//               <must be read   >: d[0][3]
//               <must be written>: d[0][3]
   (*d)[3] += 5.0;                                                      /*0050*/
//               <must be read   >: d[1][3]
//               <must be written>: d[1][3]
   d[1][3]++;                                                           /*0051*/
//               <must be read   >: d[1][3]
//               <must be written>: d[1][3]
   d[1][3] += 5.0;                                                      /*0052*/

   return 1;                                                            /*0054*/
}

Proper regions for "foo"

int foo(float (*b)[3])
{
   float c;                                                             /*0011*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3] = 2.0;                                                       /*0012*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b-R-EXACT-{}>
//  <c-W-EXACT-{}>

   c = (*b)[3];                                                         /*0013*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3] = 2.0;                                                       /*0014*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <c-W-EXACT-{}>

   c = b[1][3];                                                         /*0015*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3]++;                                                           /*0017*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3] += 5.0;                                                      /*0018*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3]++;                                                           /*0019*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3] += 5.0;                                                      /*0020*/
   return 1;                                                            /*0022*/
}

Proper regions for "foo2"

int foo2(float b[5][3])
{
   float c;                                                             /*0027*/

//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3] = 2.0;                                                       /*0028*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <c-W-EXACT-{}>

   c = (*b)[3];                                                         /*0029*/

//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3] = 2.0;                                                       /*0030*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <c-W-EXACT-{}>

   c = b[1][3];                                                         /*0031*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3]++;                                                           /*0033*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3] += 5.0;                                                      /*0034*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3]++;                                                           /*0035*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3] += 5.0;                                                      /*0036*/

   return 1;                                                            /*0038*/
}

