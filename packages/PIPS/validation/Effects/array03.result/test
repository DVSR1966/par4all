int foo3()
{
   float c;                                                             /*0043*/

//  <d[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*d)[3] = 2.0;                                                       /*0044*/

//  <c-W-EXACT-{}>
//  <d[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>

   c = (*d)[3];                                                         /*0045*/

//  <d[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   d[1][3] = 2.0;                                                       /*0046*/

//  <c-W-EXACT-{}>
//  <d[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>

   c = d[1][3];                                                         /*0047*/

//  <d[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <d[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*d)[3]++;                                                           /*0049*/

//  <d[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <d[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*d)[3] += 5.0;                                                      /*0050*/

//  <d[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <d[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   d[1][3]++;                                                           /*0051*/

//  <d[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <d[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   d[1][3] += 5.0;                                                      /*0052*/

   return 1;                                                            /*0054*/
}
