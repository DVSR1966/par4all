
Proper effects for "make_s"

s_t make_s(int i, float x)
{
   // BEGIN BLOCK
   s_t *p;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ p

   p = (s_t *) malloc(sizeof(s_t));
//               <must be read   >: i p
//               <must be written>: p[0][1]
   p->entier = i;
//               <must be read   >: p x
//               <must be written>: p[0][2]
   p->reel = x;
//               <must be read   >: p p[0]
   return *p;
   // END BLOCK
}

