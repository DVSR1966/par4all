Proper effects for "main"

int main()
{
   // BEGIN BLOCK
   s_t s;
   int j;
//               <may be read    >: TOP-LEVEL:*MEMORY*
//               <may be written >: TOP-LEVEL:*MEMORY*
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ s

   s = make_s(1, 2.);
//               <must be written>: s[1]
   s.entier = 4;
//               <may be read    >: TOP-LEVEL:*MEMORY*
//               <may be written >: TOP-LEVEL:*MEMORY*
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ j

   j = make_s(1, 2.).entier;
//               <must be read   >: j

   return j;
   // END BLOCK
}

