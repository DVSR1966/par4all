
Proper effects

void foo()
{
//               <must be read   >: i
//               <must be written>: j
   int j = i;
//               <must be read   >: __IO_EFFECTS:LUNS[1] j
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d", j);
}

Cumulated effects 

//               <must be read   >: i __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
void foo()
{
//               <must be read   >: i
//               <must be written>: j
   int j = i;
//               <must be read   >: __IO_EFFECTS:LUNS[1] j
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d", j);
}

Proper effects

int main()
{
//               <must be written>: i
   i = 3;
//               <must be read   >: i __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   foo();
   return 0;
}

Cumulated effects 

//               <must be read   >: i __IO_EFFECTS:LUNS[1]
//               <must be written>: i __IO_EFFECTS:LUNS[1]
int main()
{
//               <must be written>: i
   i = 3;
//               <must be read   >: i __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   foo();
   return 0;
}
