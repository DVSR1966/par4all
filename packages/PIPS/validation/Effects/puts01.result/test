int main()
{
   char *s = "sortie de puts";
   puts(s);
}
//               <may be read    >: stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: stdout __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
int main()
{
//               <must be written>: s
   char *s = "sortie de puts";
//               <may be read    >: stdout[0] s[*]
//               <may be written >: stdout[0]
//               <must be read   >: stdout __IO_EFFECTS:LUNS[1] s
//               <must be written>: __IO_EFFECTS:LUNS[1]
   puts(s);
}
