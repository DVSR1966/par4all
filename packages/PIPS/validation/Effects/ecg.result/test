
Proper effects for "FFT1"



      SUBROUTINE FFT1(S, F)
      INTEGER I, N
      PARAMETER(N=32768)
      REAL S(N), F(N)
C               <must be written>: I

      DO I = 1, N                                                       0008
C               <must be read   >: I I S(I)
C               <must be written>: F(I)
         F(I) = S(I)                                                    0009
      ENDDO

      END

Cumulated effects for "FFT1"



      SUBROUTINE FFT1(S, F)
      INTEGER I, N
      PARAMETER(N=32768)
      REAL S(N), F(N)
C               <must be read   >: I I S(I)
C               <must be written>: F(I) I

      DO I = 1, N                                                       0008
C               <must be read   >: I I S(I)
C               <must be written>: F(I)
         F(I) = S(I)                                                    0009
      ENDDO

      END

Proper effects for "PROCESS"

      PROGRAM PROCESS(SIG)
      INTEGER F, N
      PARAMETER(N=32768)
      REAL SIG(N)
      REAL SPECTRE(N)
C               <may be read    >: SIG(*)
C               <may be written >: SPECTRE(*)

      CALL FFT1(SIG, SPECTRE)                                           0007
C               <must be written>: P

      P = 0.                                                            0009
C               <must be written>: F
      DO F = 1, N                                                       0010
C               <must be read   >: F P SPECTRE(F)
C               <must be written>: P
         P = P+SPECTRE(F)**2                                            0011
      ENDDO
      END

Cumulated effects for "PROCESS"

      PROGRAM PROCESS(SIG)
      INTEGER F, N
      PARAMETER(N=32768)
      REAL SIG(N)
      REAL SPECTRE(N)
C               <may be read    >: SIG(*)
C               <may be written >: SPECTRE(*)

      CALL FFT1(SIG, SPECTRE)                                           0007
C               <must be written>: P

      P = 0.                                                            0009
C               <must be read   >: F P SPECTRE(F)
C               <must be written>: F P
      DO F = 1, N                                                       0010
C               <must be read   >: F P SPECTRE(F)
C               <must be written>: P
         P = P+SPECTRE(F)**2                                            0011
      ENDDO
      END
