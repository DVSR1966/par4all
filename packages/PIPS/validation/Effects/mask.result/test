      SUBROUTINE MASK(N)
C               <must be read   >: N

c     Check that MUST effects mask MAY effects

      IF (N.GT.2) THEN                                                  0005
C               <must be read   >: N
C               <must be written>: N
         N = N+1                                                        0006
      ENDIF

      END
C               <may be written >: N
C               <must be read   >: N
      SUBROUTINE MASK(N)
C               <may be written >: N
C               <must be read   >: N

c     Check that MUST effects mask MAY effects

      IF (N.GT.2) THEN                                                  0005
C               <must be read   >: N
C               <must be written>: N
         N = N+1                                                        0006
      ENDIF

      END
