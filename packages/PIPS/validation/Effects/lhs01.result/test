//               <must be written>: j
   int j = 2;
//               <must be written>: k
   int k = 2;
//               <may be written >: TOP-LEVEL:*MEMORY*
//               <must be read   >: i
   
   /* This is a deprecated construct which is accepted by PIPS parser */
   //i>2?i:j=3;
   //(i>2?i:j)=3;
   
   *(i>2?&i:&j) = 3;
//               <must be written>: i j
   /* printf("i=%d, j=%d\n", i, j); */
   
   /* This is a deprecated construct which is accepted by PIPS parser */
   //i>2?i=3:j=3;
   /* This is OK*/
   i = j = 2;
//               <may be written >: i j
//               <must be read   >: i
   i>2?(i = 3):(j = 3);
//               <must be written>: i j

   i = j = 2;
//               <must be read   >: i
   if (i>2)
//               <must be written>: i
      i = 3;
   else
//               <must be written>: j
      j = 3;
//               <must be written>: i j

   i = j = 2;
//               <must be read   >: i
//               <must be written>: j
   i>2?(j = 4):(j = 3);
//               <must be read   >: i

   if (i>2)
//               <must be written>: j
      j = 4;
   else
//               <must be written>: j
      j = 3;
}

