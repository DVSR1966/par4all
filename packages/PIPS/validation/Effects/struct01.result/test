Printed files with regions

int struct01()
{
   struct one {
      int first;
      int second;
   } x;
   int y[10];

//  <x[PHI1]-W-EXACT-{PHI1==1}>


   x.first = 1;

//  <x[PHI1]-W-EXACT-{PHI1==2}>

   x.second = 2;

//  <y[PHI1]-W-EXACT-{PHI1==0}>


   y[0] = 1;
}
