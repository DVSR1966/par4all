! out simple effects...
      PROGRAM OUT1
      INTEGER I, K
C               <must be exported>: I
      READ *, I, K                                                      0004
C               <must be exported>: __IO_EFFECTS:LUNS(6)
      PRINT *, I                                                        0005
C               <must be exported>: K
      K = 3                                                             0006
      PRINT *, K                                                        0007
      END
