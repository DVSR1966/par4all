
PROPER  EFFECTS

int main()
{
   int *a[2];
//             <    is written>: b
   int b = 0;
//             <    is written>: c
   int c = 1;
   if (1)
//             <    is written>: a[0]
      a[0] = &b;
   else
//             <    is written>: a[0]
      a[0] = &c;

   if (1)
//             <    is written>: a[0]
      a[0] = &b;
   else
//             <    is written>: a[0]
      a[0] = &b;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int *a[2];
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b = 0;
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int c = 1;
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   if (1)
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      a[0] = &b;
   else
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      a[0] = &c;
//  {(a[0],b,-MAY-);(a[0],c,-MAY-)}

   if (1)
//  {(a[0],b,-MAY-);(a[0],c,-MAY-)}
      a[0] = &b;
   else
//  {(a[0],b,-MAY-);(a[0],c,-MAY-)}
      a[0] = &b;
//  {(a[0],b,-Exact-)}
   return 0;
}
