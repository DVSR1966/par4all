
PROPER  EFFECTS

int main()
{
   float t;
   float *a;
   int i;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a _MALLOC_EFFECTS:_MALLOC_

   a = (float *) malloc(10*sizeof(float));
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is written>: i
      for(i = 0; i <= 9; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
         a[i] = a[i]+t*0, 5;
      t+0, 01;
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   float t;
//  points to = {}
   float *a;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   a = (float *) malloc(10*sizeof(float));
//  {(a,*HEAP*_l_9,-Exact-)}

   t = 1.0;
//  {(a,*HEAP*_l_9,-Exact-)}
   while (t<2.0) {
//  points to = {}
      for(i = 0; i <= 9; i += 1)
//  {(a,*HEAP*_l_9,-Exact-)}
         a[i] = a[i]+t*0, 5;
//  points to = {}
      t+0, 01;
   }
//  {(a,*HEAP*_l_9,-Exact-)}
   return 0;
}
