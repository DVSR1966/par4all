
PROPER  EFFECTS

int main()
{
   int *p, *q, *r;
//             <    is written>: a
   int a = 0;
//             <    is written>: b
   int b = 1;
//             <    is written>: c
   int c = 2;
//             <    is written>: q
   q = &a;
//             <    is written>: r
   r = &b;
   if (1)
//             <    is read   >: q
//             <    is written>: p
      p = q;
   else
//             <    is read   >: r
//             <    is written>: p
      p = r;
//             <    is written>: p

   p = &c;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int *p, *q, *r;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   int a = 0;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   int b = 1;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   int c = 2;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   q = &a;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,a,-Exact-);(r,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   r = &b;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,a,-Exact-);(r,b,
//   -Exact-)}
   if (1)
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,a,-Exact-);(r,b,
//   -Exact-)}
      p = q;
   else
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,a,-Exact-);(r,b,
//   -Exact-)}
      p = r;
//  {(p,a,-MAY-);(p,b,-MAY-);(q,a,-Exact-);(r,b,-Exact-)}

   p = &c;
//  {(p,c,-Exact-);(q,a,-Exact-);(r,b,-Exact-)}
   return 0;
}
