
PROPER  EFFECTS

int main()
{
   float *p[10];
   float a[10], t;
   int i;
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is written>: i
      for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: p[i]
         p[i] = &a[i];
//             <    is read   >: t
//             <    is written>: t
      t += 0, 01;
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   float *p[10];
//  {(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   float a[10], t;
//  {(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i;
//  {(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   t = 1.0;
//  {(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   while (t<2.0) {
//  {(p[*],a[*],-MAY-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      for(i = 0; i <= 9; i += 1)
//  {(p[*],a[*],-MAY-);(p[*],a[*],-MAY-);(p[*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
         p[i] = &a[i];
//  {(p[*],a[*],-MAY-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      t += 0, 01;
   }
//  {(p[*],a[*],-MAY-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 0;
}
