
PROPER POINTER EFFECTS

int main()
{
   float *p[10];
   float a[10], t;
   int i;
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is written>: i
      for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: p[i]
         p[i] = &a[i];
//             <    is read   >: t
//             <    is written>: t
      t += 0, 01;
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   float *p[10];
//  points to = {}
   float a[10], t;
//  points to = {}
   int i;
//  points to = {}

   t = 1.0;
//  points to = {}
   while (t<2.0) {
//  points to = {}
      for(i = 0; i <= 9; i += 1)
//  {(p[i],a[i],-MAY-)}
         p[i] = &a[i];
//  points to = {}
      t += 0, 01;
   }
//  {(p[i],a[i],-MAY-)}
   return 0;
}
