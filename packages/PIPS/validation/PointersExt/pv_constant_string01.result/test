
PROPER POINTER EFFECTS

int main()
{
   char *s;
//             <    is written>: s
   s = "hello\n";
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   char *s;
//  {(s,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   s = "hello\n";
//  {(s,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   return 0;
}
