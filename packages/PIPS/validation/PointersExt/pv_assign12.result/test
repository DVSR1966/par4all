
PROPER  EFFECTS

int main()
{
   int *a[2];
   int b[2];
//             <    is written>: c
   int c = 0;

   if (1)
//             <    is written>: a[0]
      a[0] = &b[0];
   else
//             <    is written>: a[0]
      a[0] = &b[1];
//             <    is written>: a[1]

   a[1] = &c;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int *a[2];
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b[2];
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int c = 0;
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   if (1)
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      a[0] = &b[0];
   else
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      a[0] = &b[1];
//  {(a[0],b[1],-MAY-);(a[0],b[0],-MAY-)}

   a[1] = &c;
//  {(a[0],b[1],-MAY-);(a[0],b[0],-MAY-);(a[1],c,-Exact-)}
   return 0;
}
