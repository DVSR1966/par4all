
PROPER POINTER EFFECTS

int main()
{
   int *p, *q;
//             <    is written>: a
   int a = 1;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int *) malloc(2*sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: q _MALLOC_EFFECTS:_MALLOC_
   q = (int *) malloc(2*sizeof(int));
//             <may be read   >: p q
//             <    is read   >: a _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   free(a==0?p:q);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a = 1;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p = (int *) malloc(2*sizeof(int));
//  {(p,*HEAP*_l_7,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = (int *) malloc(2*sizeof(int));
//  {(p,*HEAP*_l_7,-Exact-);(q,*HEAP*_l_8,-Exact-)}
   free(a==0?p:q);
//  {(p,*HEAP*_l_7,-Exact-);(q,*HEAP*_l_8,-Exact-)}
   return 0;
}
