
PROPER  EFFECTS

int main()
{
   int i;
   mystruct s1, s2;
//             <    is written>: i

   i = 0;
//             <    is read   >: i
   do {
      mystruct tab_s[2];
//             <may be read   >: s1.b[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: s1.a s1.c
      tab_s[0] = s1;
//             <may be read   >: s1.b[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: s1.a s1.c
      tab_s[1] = s1;
//             <may be read   >: s2.b[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: s2.a s2.c
      tab_s[1] = s2;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i;
//  {}
   mystruct s1, s2;
//  {(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   i = 0;
//  {(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   do {
//  {(*ANY_MODULE*:*NOWHERE*_b0,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      mystruct tab_s[2];
//  {(*ANY_MODULE*:*NOWHERE*_b0,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      tab_s[0] = s1;
//  {(*ANY_MODULE*:*NOWHERE*_b0,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      tab_s[1] = s1;
//  {(*ANY_MODULE*:*NOWHERE*_b0,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      tab_s[1] = s2;
//  {(*ANY_MODULE*:*NOWHERE*_b0,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      i++;
   }
   while (i<10);
//  {(*ANY_MODULE*:*NOWHERE*_b0,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s1[b][*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][*],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 0;
}
