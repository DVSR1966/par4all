int main()
{
   int i;
   int *p;
   int a[10];
//             <    is read   >: i
//             <    is written>: i

   for (i = 0;i<10;i++) {
      int *q;
//             <    is read   >: i
//             <    is written>: q
      q = &a[i];
//             <    is read   >: q
//             <    is written>: p
      p = q;
   }
   return 0;
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   int i;
//  points to = {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a[10];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   for (i = 0;i<10;i++) {
//  points to = {}
      int *q;
//  points to = {}
      q = &a[i];
//  points to = {}
      p = q;
   }
//  {(p,a[i],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
   return 0;
}
