
PROPER  EFFECTS

int main()
{
   int *p, *q, *r;
//             <    is written>: n
   int a, n = 10;
   void *s;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int *) malloc(2*sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: q _MALLOC_EFFECTS:_MALLOC_
   q = (int *) malloc(2*sizeof(a));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: r _MALLOC_EFFECTS:_MALLOC_
   r = (int *) malloc(sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: s _MALLOC_EFFECTS:_MALLOC_
   s = malloc(15);
//             <    is read   >: n _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int *) malloc(n*sizeof(int));
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int *p, *q, *r;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   int a, n = 10;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   void *s;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s,*ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   p = (int *) malloc(2*sizeof(int));
//  {(p,*HEAP*_l_8[0],-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   q = (int *) malloc(2*sizeof(a));
//  {(p,*HEAP*_l_8[0],-Exact-);(q,*HEAP*_l_9[0],-Exact-);(r,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   r = (int *) malloc(sizeof(int));
//  {(p,*HEAP*_l_8[0],-Exact-);(q,*HEAP*_l_9[0],-Exact-);(r,
//   *HEAP*_l_10,-Exact-);(s,*ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   s = malloc(15);
//  {(p,*HEAP*_l_8[0],-Exact-);(q,*HEAP*_l_9[0],-Exact-);(r,
//   *HEAP*_l_10,-Exact-);(s,*HEAP*_l_11[0],-Exact-)}
   p = (int *) malloc(n*sizeof(int));
//  {(p,*HEAP*_l_12[0],-Exact-);(q,*HEAP*_l_9[0],-Exact-);(r,
//   *HEAP*_l_10,-Exact-);(s,*HEAP*_l_11[0],-Exact-)}
   return 0;
}
