
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int a[10];
//             <    is read   >: i
   do {
//             <    is read   >: i
//             <    is written>: a[i]
      a[i] = i;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i = 0;
//  {}
   int a[10];
//  {}
   do {
//  {}
      a[i] = i;
//  {}
      i++;
   }
   while (i<10);
//  {}
   return 0;
}
