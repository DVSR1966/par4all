
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
   int a[10];
//             <    is read   >: i
   do {
//             <    is read   >: i
//             <    is written>: p
      p = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i = 0;
//  {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a[10];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   do {
//  {(p,a[i],-Exact-)}
      p = &a[i];
//  {(p,a[i],-Exact-)}
      i++;
   }
   while (i<10);
//  {(p,a[i],-Exact-)}
   return 0;
}
