
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i
   do {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i = 0;
//  {}
   int *p[10];
//  {(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a[10];
//  {(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   do {
//  {(p[*],a[*],-Exact-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      p[i] = &a[i];
//  {(p[*],a[*],-Exact-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      i++;
   }
   while (i<10);
//  {(p[*],a[*],-Exact-);(p[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 0;
}
