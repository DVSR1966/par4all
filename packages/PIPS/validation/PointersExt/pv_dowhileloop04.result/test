
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i
   do {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int i = 0;
//  {}
   int *p[10];
//  {}
   int a[10];
//  {}
   do {
//  {(p[i],a[i],-Exact-)}
      p[i] = &a[i];
//  {(p[i],a[i],-Exact-)}
      i++;
   }
   while (i<10);
//  {(p[i],a[i],-Exact-)}
   return 0;
}
