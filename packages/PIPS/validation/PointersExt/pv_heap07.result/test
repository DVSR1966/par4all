
PROPER POINTER EFFECTS

int main()
{
   int *p, *q;
   int a;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int *) malloc(2*sizeof(int));
//             <    is read   >: p
//             <    is written>: p[0]
   p[0] = 0;
//             <    is read   >: p
//             <    is written>: p[1]
   p[1] = 1;
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <    is read   >: q _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   free(q);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int a;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p = (int *) malloc(2*sizeof(int));
//  {(p,*HEAP*_l_8,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p[0] = 0;
//  {(p,*HEAP*_l_8,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p[1] = 1;
//  {(p,*HEAP*_l_8,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = p;
//  {(p,*HEAP*_l_8,-Exact-);(q,*HEAP*_l_8,-Exact-)}
   free(q);
//  {(p,*HEAP*_l_8,-Exact-);(q,*HEAP*_l_8,-Exact-)}
   return 0;
}
