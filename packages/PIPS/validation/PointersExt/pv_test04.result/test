
PROPER  EFFECTS

int main()
{
   int *a;
//             <    is written>: b
   int b = 0;
//             <    is written>: c
   int c = 1;
//             <    is read   >: b c
   if (b==c||b!=0)
//             <    is written>: a
      a = &b;
   else
//             <    is written>: a
      a = &c;
//             <    is read   >: b c

   if (b!=c)
//             <    is written>: a
      a = &b;
   else
//             <    is written>: a
      a = &b;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *a;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b = 0;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int c = 1;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   if (b==c||b!=0)
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      a = &b;
   else
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      a = &c;
//  {(a,b,-MAY-);(a,c,-MAY-)}

   if (b!=c)
//  {(a,b,-MAY-);(a,c,-MAY-)}
      a = &b;
   else
//  {(a,b,-MAY-);(a,c,-MAY-)}
      a = &b;
//  {(a,b,-Exact-)}
   return 0;
}
