int main()
{
   float *p;
   float a[10];
   int i;
//             <may be written>: p
//             <    is read   >: i
//             <    is written>: i

   for (i = 0;i<10;p = &a[i])
      ;
   return 0;
}
POINTS TO
//  {}
int main()
{
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   float *p;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   float a[10];
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int i;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-)}

   for (i = 0;i<10;p = &a[i])
//  {(p,a[i],-Exact-)}
      ;
//  {(p,a[i],-Exact-)}
   return 0;
}
