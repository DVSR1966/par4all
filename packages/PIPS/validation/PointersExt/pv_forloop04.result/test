
PROPER  EFFECTS

int main()
{
   float *p;
   float a[10];
   int i;
//             <    is written>: i

   i = 0;
//             <    is read   >: i
   while (i<10) {
//             <    is read   >: i
//             <    is written>: p
      p = &a[i];
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   float *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   float a[10];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   i = 0;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   while (i<10) {
//  {(p,a[i],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
      p = &a[i];
   }
//  {(p,a[i],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
   return 0;
}
