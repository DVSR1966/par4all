
PROPER  EFFECTS

int main()
{
   int i;
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: a[i]
      a[i] = i;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int i;
//  points to = {}
   int a[10];
//  points to = {}

   for(i = 0; i <= 9; i += 1)
//  points to = {}
      a[i] = i;
//  points to = {}
   return 0;
}
