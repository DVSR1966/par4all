
PROPER  EFFECTS

int main()
{
   int i;
   int *p, *q;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <    is written>: i

   for(i = 0; i <= 4; i += 1) {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i q
      *q = i;
//             <    is read   >: q
//             <    is written>: q
      q++;
   }
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   free(p);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int i;
//  points to = {}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = (int *) malloc(10*sizeof(int));
//  {(p,*HEAP*_l_8,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = p;
//  {(p,*HEAP*_l_8,-Exact-);(q,*HEAP*_l_8,-Exact-)}

   for(i = 0; i <= 4; i += 1) {
//  points to = {}
      *q = i;
//  points to = {}
      q++;
   }
//  {(p,*HEAP*_l_8,-Exact-);(q,*HEAP*_l_8,-MAY-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-MAY-)}

   free(p);
//  {(p,*HEAP*_l_8,-Exact-);(q,*HEAP*_l_8,-MAY-);(q,
//   *ANY_MODULE*:*ANYWHERE*_b0,-MAY-)}
   return 0;
}
