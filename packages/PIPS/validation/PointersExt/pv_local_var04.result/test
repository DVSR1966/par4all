//  {}
int main()
{
//  points to = {}

   if (a>1) {
//  points to = {}
      static int *p = &a;
//  {(p,a,-Exact-)}
      static int *q;
//  {(p,a,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}
      q = &b;
//  {(p,a,-Exact-);(q,b,-Exact-)}
      a = a-1;
   }
   else {
//  points to = {}
      static int *r = &a;
//  {(r,a,-Exact-)}
      static int *s;
//  {(r,a,-Exact-);(s,*ANY_MODULE*:*NOWHERE*,-Exact-)}
      s = &b;
//  {(r,a,-Exact-);(s,b,-Exact-)}
      a = a+2;
   }
//  {(p,a,-MAY-);(q,b,-MAY-);(r,a,-MAY-);(s,b,-MAY-)}
   return 0;
}
