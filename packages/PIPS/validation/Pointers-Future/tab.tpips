shell gcc -Wall -std=c99 -o tab tab.c

delete tab
create tab tab.c

setproperty PRETTYPRINT_SEQUENTIAL_STYLE "do"
setproperty ABORT_ON_USER_ERROR          TRUE

apply  PRIVATIZE_MODULE(%ALL)
apply  INTERNALIZE_PARALLEL_CODE(%ALL)
apply  OMPIFY_CODE(%ALL)
apply  UNSPLIT(%ALL)

shell gcc -Wall -std=c99 -fopenmp -o tab-omp tab.database/Src/tab.c

close
quit
