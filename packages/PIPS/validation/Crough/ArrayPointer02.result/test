
Initial code

	  SUBROUTINE ARRAYPOINTER02 (A)
      INTEGER I
      INTEGER A(5)
      DO 10 I = 1, 5
         A(I) = I
10       CONTINUE
      END
/* C pretty print for module ARRAYPOINTER02. */
void
arraypointer02 (int *a)
{
  int i;

  for (i = 1; i <= 5; i++)
    {
      a[i - 1] = i;
      continue;
    }
  return;
}

compiles C output

