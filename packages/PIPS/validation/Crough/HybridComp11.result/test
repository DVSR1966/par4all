      PROGRAM HYBRIDCOMP11
      INTEGER I
      INTEGER J
      INTEGER*4 A(5, 5)
      CALL TOTO(A)
      PRINT *, A
      END
/* C pretty print for module TOTO. */
void
toto_ (int a[5][5])
{
  int j;
  int i;

  for (i = 1; i <= 5; i++)
    {
      for (j = 1; j <= 5; j++)
	{
	  a[j - 1][i - 1] = (i * 1147483648);
	}
      continue;
    }
  return;
}

