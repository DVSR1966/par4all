
Initial code

      PROGRAM HYBRIDCOMP14
      INTEGER I
      INTEGER J
      REAL*4 A(5, 5)
      REAL*4 CST(5)
      CST(1) = 1.0
      CST(2) = 2.0
      CST(3) = 3.0
      CST(4) = 4.0
      CST(5) = 5.0
      DO 10 I = 1, 5
         DO J = 1, 5
            A(I,J) = CST(I)*2.4E5
         ENDDO
10       CONTINUE
      PRINT *, A
      END

Code after outlining

      PROGRAM HYBRIDCOMP14
      INTEGER I
      INTEGER J
      REAL*4 A(5, 5)
      REAL*4 CST(5)
      CST(1) = 1.0
      CST(2) = 2.0
      CST(3) = 3.0
      CST(4) = 4.0
      CST(5) = 5.0
      CALL TOTO(A, CST)
      PRINT *, A
      END
/* C pretty print for module TOTO. */
//needed include to compile the C output
#include "math.h"
#include "stdlib.h"
#include "complex.h"

// The macros to support some fortran intrinsics
// and complex declaration
#define crough_max(a,b) (((a)>(b))?(a):(b))
#define crough_min(a,b) (((a)<(b))?(a):(b))
#define crough_powi(a,b) ((a)^(b))
#define init_complex(a,b) (a + b*I)

void
toto_ (float a[5][5], float cst[5])
{
  int j;
  int i;

  for (i = 1; i <= 5; i++)
    {
      for (j = 1; j <= 5; j++)
	{
	  a[j - 1][i - 1] = (cst[i - 1] * 2.4e5);
	}
      continue;
    }
  return;
}

