Source code with transformers


//  T(pointer03) {}

int pointer03()
{

//  T(i) {i==3}

   int i = 3;                                                           /*0005*/

//  T() {}

   int *ip = &i;                                                        /*0006*/

//  T(i) {}

   *ip = 4;                                                             /*0007*/

//  T(pointer03) {i==pointer03}

   return i;                                                            /*0008*/
}

