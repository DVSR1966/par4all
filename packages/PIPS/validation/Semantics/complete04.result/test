void complete04()
{
   // BEGIN BLOCK
   int i;                                                               /*0010*/
   int n;                                                               /*0011*/
   {
      // BEGIN BLOCK
//               <must be written>: j
      int j = 0;                                                        /*0015*/
//               <must be read   >: i n
//               <must be written>: i
      for (i = 0;i<n;i++)                                               /*0016*/
//               <must be read   >: j
//               <must be written>: j
         j++;                                                           /*0017*/
      // END BLOCK
   }
//               <must be read   >: i

   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with cumulated effects

void complete04()
{
   // BEGIN BLOCK
   int i;                                                               /*0010*/
   int n;                                                               /*0011*/
   {
//               <must be read   >: i n
//               <must be written>: i
      // BEGIN BLOCK
//               <must be written>: j
      int j = 0;                                                        /*0015*/
//               <may be read    >: j
//               <may be written >: j
//               <must be read   >: i n
//               <must be written>: i
      for (i = 0;i<n;i++)                                               /*0016*/
//               <must be read   >: j
//               <must be written>: j
         j++;                                                           /*0017*/
      // END BLOCK
   }
//               <must be read   >: i

   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with transformers


//  T() {}

void complete04()
{

//  T() {}

   // BEGIN BLOCK

//  T(i) {}

   int i;                                                               /*0010*/

//  T(n) {}

   int n;                                                               /*0011*/
   {

//  T(i) {0<=i, n<=i}

      // BEGIN BLOCK

//  T(j) {j==0}

      int j = 0;                                                        /*0015*/

//  T(i,j) {i+j#init==j, i+j#init+1<=j+n, i+1<=n, j#init<=j}

      for (i = 0;i<n;i++)                                               /*0016*/

//  T(j) {j==j#init+1}

         j++;                                                           /*0017*/
      // END BLOCK
   }

//  T() {}


   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with preconditions


//  P() {}

void complete04()
{

//  P() {}

   // BEGIN BLOCK

//  P() {}

   int i;                                                               /*0010*/

//  P(i) {}

   int n;                                                               /*0011*/
   {

//  P(i,n) {}

      // BEGIN BLOCK

//  P(i,n) {}

      int j = 0;                                                        /*0015*/

//  P(i,j,n) {j==0}

      for (i = 0;i<n;i++)                                               /*0016*/

//  P(i,j,n) {i==j, 0<=i, i+1<=n}

         j++;                                                           /*0017*/
      // END BLOCK
   }

//  P(i,n) {0<=i, n<=i}


   foo(i);                                                              /*0021*/
   // END BLOCK
}

