
Source code with proper effects

void call01(int i)
{
//               <must be read   >: i
//               <must be written>: i
   i++;                                                                 /*0007*/
//               <must be read   >: j
//               <must be written>: j
   j++;                                                                 /*0008*/
}

Source code with cumulated effects

//               <must be read   >: j i
//               <must be written>: j
void call01(int i)
{
//               <must be read   >: i
//               <must be written>: i
   i++;                                                                 /*0007*/
//               <must be read   >: j
//               <must be written>: j
   j++;                                                                 /*0008*/
}

Source code with transformers


//  T(j) {j==j#init+1}

void call01(int i)
{

//  T(i) {i==i#init+1}

   i++;                                                                 /*0007*/

//  T(j) {j==j#init+1}

   j++;                                                                 /*0008*/
}

