
Source code with proper effects

void call02(int i, float x)
{
//               <must be read   >: i
//               <must be written>: i
   i++;                                                                 /*0007*/
//               <must be read   >: TOP-LEVEL:j
//               <must be written>: TOP-LEVEL:j
   j++;                                                                 /*0008*/
//               <must be read   >: x
//               <must be written>: x
   x++;                                                                 /*0009*/
}

Source code with cumulated effects

//               <must be read   >: TOP-LEVEL:j i x
//               <must be written>: TOP-LEVEL:j
void call02(int i, float x)
{
//               <must be read   >: i
//               <must be written>: i
   i++;                                                                 /*0007*/
//               <must be read   >: TOP-LEVEL:j
//               <must be written>: TOP-LEVEL:j
   j++;                                                                 /*0008*/
//               <must be read   >: x
//               <must be written>: x
   x++;                                                                 /*0009*/
}

Source code with transformers


//  T(TOP-LEVEL:j) {TOP-LEVEL:j==j#init+1}

void call02(int i, float x)
{

//  T(i) {i==i#init+1}

   i++;                                                                 /*0007*/

//  T(TOP-LEVEL:j) {TOP-LEVEL:j==j#init+1}

   j++;                                                                 /*0008*/

//  T() {}

   x++;                                                                 /*0009*/
}

