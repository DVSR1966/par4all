   int _return0, _return1;

   while (x<=4) {
      // BEGIN BLOCK
      // BEGIN BLOCK
      y++;
      {
         // BEGIN BLOCK
         // BEGIN BLOCK
         int t = x+2;

         if (t>6)
            t -= 3;
         else
            t--;
         // BEGIN BLOCK
         
         
         // BEGIN BLOCK
         _return0 = t;
         // END BLOCK
         // END BLOCK
