
Source code with proper effects

void complete02()
{
   // BEGIN BLOCK
   int i;                                                               /*0010*/
   {
      // BEGIN BLOCK
//               <must be written>: j
      int j = 0;                                                        /*0014*/
//               <must be read   >: i
//               <must be written>: i
      for (i = 0;i<10;i++) {                                            /*0015*/
         // BEGIN BLOCK
//               <must be read   >: j
//               <must be written>: j
         j++;                                                           /*0016*/
//               <must be read   >: i
//               <must be written>: i
         i--;                                                           /*0017*/
         // END BLOCK
      }
      // END BLOCK
   }
//               <must be read   >: i

   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with cumulated effects

void complete02()
{
   // BEGIN BLOCK
   int i;                                                               /*0010*/
   {
//               <must be read   >: i
//               <must be written>: i
      // BEGIN BLOCK
//               <must be written>: j
      int j = 0;                                                        /*0014*/
//               <must be read   >: i j
//               <must be written>: i j
      for (i = 0;i<10;i++) {                                            /*0015*/
//               <must be read   >: i j
//               <must be written>: i j
         // BEGIN BLOCK
//               <must be read   >: j
//               <must be written>: j
         j++;                                                           /*0016*/
//               <must be read   >: i
//               <must be written>: i
         i--;                                                           /*0017*/
         // END BLOCK
      }
      // END BLOCK
   }
//               <must be read   >: i

   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with transformers


//  T() {0==-1}

void complete02()
{

//  T() {0==-1}

   // BEGIN BLOCK

//  T() {}

   int i;                                                               /*0010*/
   {

//  T() {0==-1}

      // BEGIN BLOCK

//  T(j) {j==0}

      int j = 0;                                                        /*0014*/

//  T(i,j) {i==0, j#init<=j}

      for (i = 0;i<10;i++) {                                            /*0015*/

//  T(i,j) {i==i#init-1, j==j#init+1}

         // BEGIN BLOCK

//  T(j) {j==j#init+1}

         j++;                                                           /*0016*/

//  T(i) {i==i#init-1}

         i--;                                                           /*0017*/
         // END BLOCK
      }
      // END BLOCK
   }

//  T() {}


   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with preconditions


//  P() {}

void complete02()
{

//  P() {}

   // BEGIN BLOCK

//  P() {}

   int i;                                                               /*0010*/
   {

//  P() {}

      // BEGIN BLOCK

//  P() {}

      int j = 0;                                                        /*0014*/

//  P() {j==0}

      for (i = 0;i<10;i++) {                                            /*0015*/

//  P(i,j) {i==0, 0<=j}

         // BEGIN BLOCK

//  P(i,j) {i==0, 0<=j}

         j++;                                                           /*0016*/

//  P(i,j) {i==0, 1<=j}

         i--;                                                           /*0017*/
         // END BLOCK
      }
      // END BLOCK
   }

//  P() {0==-1}


   foo(i);                                                              /*0021*/
   // END BLOCK
}

