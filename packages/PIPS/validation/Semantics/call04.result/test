Source code with transformers for "call04"


//  T(TOP-LEVEL:j,call04) {TOP-LEVEL:j==j#init+1, call04==i+10}

int call04(int i)
{

//  T(i) {i==i#init+10}

   i += 10;                                                             /*0004*/

//  T(TOP-LEVEL:j) {TOP-LEVEL:j==j#init+1}

   j++;                                                                 /*0005*/

//  T(call04) {call04==i}


   return i;                                                            /*0007*/
}

Source code with transformers for "main"


//  T(TOP-LEVEL:j) {TOP-LEVEL:j==j#init+1}

main()
{
   int ai = 3;                                                          /*0015*/

//  T(TOP-LEVEL:j,ai) {TOP-LEVEL:j==j#init+1, ai==ai#init+10}


   ai = call04(ai++);                                                   /*0017*/

//  T(ai) {ai==0}


   ai = 0;                                                              /*0019*/
}

