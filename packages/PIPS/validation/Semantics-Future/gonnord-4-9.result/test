
TRANSFORMERS


//  T() {}

void run(void)
{

//  T(i,j,s) {}

   int s, i, j;

//  T(s) {s==1}

   s = 1;

//  T(i) {i==0, s==1}

   i = 0;

//  T(j) {i==0, j==-100, s==1}

   j = -100;

//  T(i,j,s) {i==101, i#init==0, j#init==-100, s==3, s#init==1,
//    0<=j+100}

   if (flip()) {

//  T(i,j,s) {s==1, s#init==1, 0<=i, i<=99, i#init<=i, 0<=j+100,
//    j#init<=j}

      while (flip()) {

//  T(i,s) {i==i#init+1, s==2, s#init==1, 0<=i, i<=99, i<=j+201,
//    0<=j+100}

         if (s==1&&i<99) {

//  T(s) {s==2, s#init==1, i<=98}

            s = 2;

//  T(i) {i==i#init+1, s==2, i<=99}

            i++;

//  T() {s==2, 0<=i, i<=99, i<=j+201, 0<=j+100}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;

//  T(j) {s==2, 0<=i, i<=99, 0<=j+100, j#init<=j}

         while (flip())

//  T(j) {i+j#init==j, s==2, 0<=i, i<=99, i<=j+201, i+j#init<=19,
//    0<=j+100, j#init<=19}

            if (s==2&&j<=19&&j+i<=19) {

//  T(j) {i+j#init==j, s==2, 0<=i, i<=99, i+j#init<=19, j#init<=19}

               j += i;

//  T() {s==2, 0<=i, i<=99, j<=i+19, i<=j+201, 0<=j+100, j<=19}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {s==2, 0<=i, i<=99, 20<=i+j}

               while (1)

//  T() {s==2, 0<=i, i<=99, 20<=i+j}

                  ;

//  T(s) {s==1, s#init==2, 0<=i, i<=99, i<=j+201, 0<=j+100}

         if (s==2) {

//  T(s) {s==1, s#init==2, 0<=i, i<=99, i<=j+201, 0<=j+100}

            s = 1;

//  T() {s==1, 0<=i, i<=99, i<=j+201, 0<=j+100}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;
      }

//  T(i,s) {i==101, i#init==100, s==2, s#init==1, 0<=j+100}

      if (s==1&&i==100) {

//  T(s) {i==100, s==2, s#init==1, 0<=j+100}

         s = 2;

//  T(i) {i==101, i#init==100, s==2, 0<=j+100}

         i++;

//  T() {i==101, s==2, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {s==1, 0<=i, 0<=j+100}

         while (1)

//  T() {s==1, 0<=i, 0<=j+100}

            ;

//  T(j) {i==101, s==2, 0<=j+100, j#init<=j}

      while (flip())

//  T(j) {i==101, j==j#init+101, s==2, 0<=j+100, j<=19}

         if (s==2&&j<=19&&j+i<=19) {

//  T(j) {i==101, j==j#init+101, s==2, j#init+82<=0}

            j += i;

//  T() {i==101, s==2, 0<=j+100, j<=19}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {i==101, s==2, 0<=j+81}

            while (1)

//  T() {i==101, s==2, 0<=j+81}

               ;

//  T(s) {i==101, s==1, s#init==2, 0<=j+100}

      if (s==2) {

//  T(s) {i==101, s==1, s#init==2, 0<=j+100}

         s = 1;

//  T() {i==101, s==1, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0==-1}

         while (1)

//  T() {0==-1}

            ;

//  T(s) {i==101, s==3, s#init==1, 0<=j+100}

      if (s==1&&i>100) {

//  T(s) {i==101, s==3, s#init==1, 0<=j+100}

         s = 3;

//  T() {i==101, s==3, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0==-1}

         while (1)

//  T() {0==-1}

            ;
   }
   else {

//  T(i,j,s) {i==101, i#init==0, j#init==-100, s==2, s#init==1, 20<=j,
//    j<=119}

      if (flip()) {

//  T(i,s) {i==1, i#init==0, j==-100, s==2, s#init==1}

         if (s==1&&i<99) {

//  T(s) {i==0, j==-100, s==2, s#init==1}

            s = 2;

//  T(i) {i==1, i#init==0, j==-100, s==2}

            i++;

//  T() {i==1, j==-100, s==2}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T(i,j,s) {s==2, s#init==2, 1<=i, i<=220, i#init<=i, 20i<=11j+3080,
//    0<=j+100, j#init<=j}

         while (flip())

//  T(i,j,s) {s==2, s#init==2, 99i#init+j#init<=98i+j,
//    220i+j<=220i#init+j#init+220, 101i<=101i#init+j#init+201,
//    j<=i#init+j#init, 0<=j+100, j#init<=j}

            if (flip())

//  T(j) {i+j#init==j, s==2, 0<=i, i<=j+201, i+j#init<=19, 0<=j+100,
//    j#init<=19}

               if (s==2&&j<=19&&j+i<=19) {

//  T(j) {i+j#init==j, s==2, i+j#init<=19, j#init<=19}

                  j += i;

//  T() {s==2, 0<=i, j<=i+19, i<=j+201, 0<=j+100, j<=19}

                  if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {}

                  while (1)

//  T() {}

                     ;
            else {

//  T(s) {s==1, s#init==2, 0<=i, i<=j+201, 0<=j+100}

               if (s==2) {

//  T(s) {s==1, s#init==2}

                  s = 1;

//  T() {s==1, 0<=i, i<=j+201, 0<=j+100}

                  if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {}

                  while (1)

//  T() {}

                     ;

//  T(i,s) {i==i#init+1, s==2, s#init==1, 1<=i, i<=99, i<=j+201,
//    0<=j+100}

               if (s==1&&i<99) {

//  T(s) {s==2, s#init==1, 0<=i, i<=98, i<=j+201, 0<=j+100}

                  s = 2;

//  T(i) {i==i#init+1, s==2, 1<=i, i<=99, i<=j+202, 0<=j+100}

                  i++;

//  T() {s==2, 1<=i, i<=99, i<=j+201, 0<=j+100}

                  if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {s==1, 99<=i, i<=j+201, 0<=j+100}

                  while (1)

//  T() {s==1, 99<=i, i<=j+201, 0<=j+100}

                     ;
            }

//  T(j) {i+j#init==j, s==2, 1<=i, 20<=i+j#init, 0<=j#init+100,
//    j#init<=19}

         if (s==2&&j<=19&&j+i>19) {

//  T(j) {i+j#init==j, s==2, 1<=i, 20<=i+j#init, 0<=j#init+100,
//    j#init<=19}

            j += i;

//  T() {s==2, 1<=i, j<=i+19, i<=j+100, 20<=j}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==2, 1<=i, 0<=j+100}

            while (1)

//  T() {s==2, 1<=i, 0<=j+100}

               ;

//  T(i,s) {s==2, s#init==2, i#init<=i, j<=i+19, i<=j+100, 20<=j}

         while (flip()) {

//  T(s) {s==1, s#init==2, 0<=i, i<=j+201, 20<=j}

            if (s==2) {

//  T(s) {s==1, s#init==2, 20<=j}

               s = 1;

//  T() {s==1, 0<=i, i<=j+201, 20<=j}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {20<=j}

               while (1)

//  T() {20<=j}

                  ;

//  T(i,s) {i==i#init+1, s==2, s#init==1, 1<=i, i<=99, i<=j+201,
//    20<=j}

            if (s==1&&i<99) {

//  T(s) {s==2, s#init==1, 0<=i, i<=98, i<=j+201, 20<=j}

               s = 2;

//  T(i) {i==i#init+1, s==2, 1<=i, i<=99, i<=j+202, 20<=j}

               i++;

//  T() {s==2, 1<=i, i<=99, i<=j+201, 20<=j}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {s==1, 99<=i, i<=j+201, 20<=j}

               while (1)

//  T() {s==1, 99<=i, i<=j+201, 20<=j}

                  ;
         }

//  T(s) {s==1, s#init==2, 1<=i, j<=i+19, i<=j+201, 20<=j}

         if (s==2) {

//  T(s) {s==1, s#init==2, 1<=i, j<=i+19, 20<=j}

            s = 1;

//  T() {s==1, 1<=i, j<=i+19, i<=j+201, 20<=j}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T(i,s) {i==101, i#init==100, s==2, s#init==1, 20<=j, j<=119}

         if (s==1&&i==100) {

//  T(s) {i==100, s==2, s#init==1, 20<=j, j<=119}

            s = 2;

//  T(i) {i==101, i#init==100, s==2, 20<=j, j<=119}

            i++;

//  T() {i==101, s==2, 20<=j, j<=119}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 1<=i, j<=i+19, i<=j+201, 20<=j}

            while (1)

//  T() {s==1, 1<=i, j<=i+19, i<=j+201, 20<=j}

               ;
      }
      else {

//  T() {0==-1}

         if (s==1&&i==100) {

//  T() {0==-1}

            s = 2;

//  T() {0==-1}

            i++;

//  T() {0==-1}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {i==0, j==-100, s==1}

            while (1)

//  T() {i==0, j==-100, s==1}

               ;

//  T() {0==-1}

         while (flip())

//  T() {0==-1}

            if (s==2&&j<=19&&j+i<=19) {

//  T() {0==-1}

               j += i;

//  T() {0==-1}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T() {0==-1}

         if (s==2&&j<=19&&j+i>19) {

//  T() {0==-1}

            j += i;

//  T() {0==-1}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;
      }

//  T(s) {i==101, s==1, s#init==2, 20<=j, j<=119}

      if (s==2) {

//  T(s) {i==101, s==1, s#init==2, 20<=j, j<=119}

         s = 1;

//  T() {i==101, s==1, 20<=j, j<=119}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0==-1}

         while (1)

//  T() {0==-1}

            ;

//  T(s) {i==101, s==3, s#init==1, 20<=j, j<=119}

      if (s==1&&i>100) {

//  T(s) {i==101, s==3, s#init==1, 20<=j, j<=119}

         s = 3;

//  T() {i==101, s==3, 20<=j, j<=119}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0==-1}

         while (1)

//  T() {0==-1}

            ;
   }
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int s, i, j;

//  P(i,j,s) {}

   s = 1;

//  P(i,j,s) {s==1}

   i = 0;

//  P(i,j,s) {i==0, s==1}

   j = -100;

//  P(i,j,s) {i==0, j==-100, s==1}

   if (flip()) {

//  P(i,j,s) {i==0, j==-100, s==1}

      while (flip()) {

//  P(i,j,s) {s==1, 0<=i, i<=99, 0<=j+100}

         if (s==1&&i<99) {

//  P(i,j,s) {s==1, 0<=i, i<=98, 0<=j+100}

            s = 2;

//  P(i,j,s) {s==2, 0<=i, i<=98, 0<=j+100}

            i++;

//  P(i,j,s) {s==2, 1<=i, i<=99, 0<=j+100}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,j,s) {i==99, s==1, 0<=j+100}

            while (1)

//  P(i,j,s) {i==99, s==1, 0<=j+100}

               ;

//  P(i,j,s) {s==2, 1<=i, i<=99, i<=j+201, 0<=j+100}

         while (flip())

//  P(i,j,s) {s==2, 1<=i, i<=99, 0<=j+100}

            if (s==2&&j<=19&&j+i<=19) {

//  P(i,j,s) {s==2, 1<=i, i<=99, i+j<=19, 0<=j+100, j<=19}

               j += i;

//  P(i,j,s) {s==2, 1<=i, i<=99, j<=i+19, i<=j+100, j<=19}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(i,j,s) {s==2, 1<=i, i<=99, 20<=i+j, 0<=j+100}

               while (1)

//  P(i,j,s) {s==2, 1<=i, i<=99, 20<=i+j, 0<=j+100}

                  ;

//  P(i,j,s) {s==2, 1<=i, i<=99, 0<=j+100}

         if (s==2) {

//  P(i,j,s) {s==2, 1<=i, i<=99, 0<=j+100}

            s = 1;

//  P(i,j,s) {s==1, 1<=i, i<=99, i<=j+201, 0<=j+100}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
      }

//  P(i,j,s) {s==1, 0<=i, i<=99, 0<=j+100}

      if (s==1&&i==100) {

//  P() {0==-1}

         s = 2;

//  P() {0==-1}

         i++;

//  P() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }
      else

//  P(i,j,s) {s==1, 0<=i, i<=99, 0<=j+100}

         while (1)

//  P(i,j,s) {s==1, 0<=i, i<=99, 0<=j+100}

            ;

//  P() {0==-1}

      while (flip())

//  P() {0==-1}

         if (s==2&&j<=19&&j+i<=19) {

//  P() {0==-1}

            j += i;

//  P() {0==-1}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P() {0==-1}

      if (s==2) {

//  P() {0==-1}

         s = 1;

//  P() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;

//  P() {0==-1}

      if (s==1&&i>100) {

//  P() {0==-1}

         s = 3;

//  P() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;
   }
   else {

//  P(i,j,s) {i==0, j==-100, s==1}

      if (flip()) {

//  P(i,j,s) {i==0, j==-100, s==1}

         if (s==1&&i<99) {

//  P(i,j,s) {i==0, j==-100, s==1}

            s = 2;

//  P(i,j,s) {i==0, j==-100, s==2}

            i++;

//  P(i,j,s) {i==1, j==-100, s==2}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P(i,j,s) {i==1, j==-100, s==2}

         while (flip())

//  P(i,j,s) {20i+1960s<=11j+5040, 197<=i+98s, i+219s<=439, 1<=s}

            if (flip())

//  P(i,j,s) {20i+1960s<=11j+5040, 197<=i+98s, i+219s<=439, 1<=s}

               if (s==2&&j<=19&&j+i<=19) {

//  P(i,j,s) {i==1, s==2, 0<=j+100, j<=18}

                  j += i;

//  P(i,j,s) {i==1, s==2, 0<=j+99, j<=19}

                  if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(i,j,s) {4599s<=100i+121j+6799, 20i+1960s<=11j+5040, 197<=i+98s,
//    i+219s<=439, 1<=s}

                  while (1)

//  P(i,j,s) {4599s<=100i+121j+6799, 20i+1960s<=11j+5040, 197<=i+98s,
//    i+219s<=439, 1<=s}

                     ;
            else {

//  P(i,j,s) {20i+1960s<=11j+5040, 197<=i+98s, i+219s<=439, 1<=s}

               if (s==2) {

//  P(i,j,s) {i==1, s==2, 0<=j+100}

                  s = 1;

//  P(i,j,s) {i==1, s==1, 0<=j+100}

                  if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(i,j,s) {s==1, 99<=i, i<=220, 20i<=11j+3080}

                  while (1)

//  P(i,j,s) {s==1, 99<=i, i<=220, 20i<=11j+3080}

                     ;

//  P(i,j,s) {i==1, s==1, 0<=j+100}

               if (s==1&&i<99) {

//  P(i,j,s) {i==1, s==1, 0<=j+100}

                  s = 2;

//  P(i,j,s) {i==1, s==2, 0<=j+100}

                  i++;

//  P(i,j,s) {i==2, s==2, 0<=j+100}

                  if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                     checking_error();
               }
               else

//  P() {0==-1}

                  while (1)

//  P() {0==-1}

                     ;
            }

//  P(i,j,s) {s==2, 1<=i, i<=2, 0<=j+100}

         if (s==2&&j<=19&&j+i>19) {

//  P(i,j,s) {s==2, 1<=i, i<=2, 20<=i+j, 0<=j+100, j<=19}

            j += i;

//  P(i,j,s) {s==2, 1<=i, i<=2, j<=i+19, i<=j+100, 20<=j}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,j,s) {s==2, 1<=i, i<=2, 0<=j+100}

            while (1)

//  P(i,j,s) {s==2, 1<=i, i<=2, 0<=j+100}

               ;

//  P(i,j,s) {s==2, 1<=i, i<=2, j<=i+19, i<=j+100, 20<=j}

         while (flip()) {

//  P(i,j,s) {j+175<=i+97s, i+21s<=j+121, 197<=i+98s, i+22s<=143,
//    20<=j, j<=21, 1<=s, s<=2}

            if (s==2) {

//  P(i,j,s) {s==2, 1<=i, i<=99, j<=i+19, i<=j+79, 20<=j, j<=21}

               s = 1;

//  P(i,j,s) {s==1, 1<=i, i<=99, j<=i+19, i<=j+79, 20<=j, j<=21}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(i,j,s) {s==1, 99<=i, i<=121, j+78<=i, i<=j+100, 20<=j, j<=21}

               while (1)

//  P(i,j,s) {s==1, 99<=i, i<=121, j+78<=i, i<=j+100, 20<=j, j<=21}

                  ;

//  P(i,j,s) {s==1, 1<=i, i<=99, j<=i+19, i<=j+79, 20<=j, j<=21}

            if (s==1&&i<99) {

//  P(i,j,s) {s==1, 1<=i, i<=98, j<=i+19, i<=j+79, 20<=j, j<=21}

               s = 2;

//  P(i,j,s) {s==2, 1<=i, i<=98, j<=i+19, i<=j+79, 20<=j, j<=21}

               i++;

//  P(i,j,s) {s==2, 2<=i, i<=99, j<=i+18, i<=j+80, 20<=j, j<=21}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(i,j,s) {i==99, s==1, 20<=j, j<=21}

               while (1)

//  P(i,j,s) {i==99, s==1, 20<=j, j<=21}

                  ;
         }

//  P(i,j,s) {s==2, i<=99, j<=i+19, 20<=j, j<=21}

         if (s==2) {

//  P(i,j,s) {s==2, i<=99, j<=i+19, 20<=j, j<=21}

            s = 1;

//  P(i,j,s) {s==1, 1<=i, i<=99, j<=i+19, 20<=j, j<=21}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P(i,j,s) {s==1, 1<=i, i<=99, j<=i+19, i<=j+201, 20<=j, j<=21}

         if (s==1&&i==100) {

//  P() {0==-1}

            s = 2;

//  P() {0==-1}

            i++;

//  P() {0==-1}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,j,s) {s==1, 1<=i, i<=99, j<=i+19, i<=j+201, 20<=j, j<=21}

            while (1)

//  P(i,j,s) {s==1, 1<=i, i<=99, j<=i+19, i<=j+201, 20<=j, j<=21}

               ;
      }
      else {

//  P(i,j,s) {i==0, j==-100, s==1}

         if (s==1&&i==100) {

//  P() {0==-1}

            s = 2;

//  P() {0==-1}

            i++;

//  P() {0==-1}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,j,s) {i==0, j==-100, s==1}

            while (1)

//  P(i,j,s) {i==0, j==-100, s==1}

               ;

//  P() {0==-1}

         while (flip())

//  P() {0==-1}

            if (s==2&&j<=19&&j+i<=19) {

//  P() {0==-1}

               j += i;

//  P() {0==-1}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;

//  P() {0==-1}

         if (s==2&&j<=19&&j+i>19) {

//  P() {0==-1}

            j += i;

//  P() {0==-1}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
      }

//  P() {0==-1}

      if (s==2) {

//  P() {0==-1}

         s = 1;

//  P() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;

//  P() {0==-1}

      if (s==1&&i>100) {

//  P() {0==-1}

         s = 3;

//  P() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;
   }
}
