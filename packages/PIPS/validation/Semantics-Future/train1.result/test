//  T(s) {b==s-9, b==s#init-8, t==1, d<=8}

            s++;

//  T() {b==s-9, t==1, d<=8}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {t==1, d<=8}

            while (1)

//  T() {t==1, d<=8}

               ;

//  T(s,t) {b==s-10, b==s#init-9, t==2, t#init==1, d<=8}

         if (t==1&&b==s-9) {

//  T(t) {b==s-9, t==2, t#init==1, d<=8}

            t = 2;

//  T(s) {b==s-10, b==s#init-9, t==2, d<=8}

            s++;

//  T() {b==s-10, t==2, d<=8}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();
         }
