//  T(b,s) {t==1, b<=b#init+1, 22b<=21b#init+s+19,
//    b#init+s#init+1<=b+s, 3s<=b+2s#init+10, d<=8, s<=s#init+1}

            if (rand()%2)

//  T(b,s) {t==1, b<=b#init+1, b#init+s#init+1<=b+s,
//    28b+5s<=27b#init+6s#init+25, 3s<=b+2s#init+10, d<=8,
//    s<=s#init+1}

               if (rand()%2)

//  T(b,s) {b+s==b#init+s#init+1, t==1, b#init<=b, b<=b#init+1,
