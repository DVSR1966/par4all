                     ;

//  T() {0==-1}

               if (t==4&&b>s+1) {

//  T() {0==-1}

                  s++;

//  T() {0==-1}

                  if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;
            }
         }

//  T() {0==-1}

         while (flip())

//  T() {0==-1}

            if (t==4&&b>s+1&&b>s+2) {

//  T() {0==-1}

               s++;

//  T() {0==-1}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T() {0==-1}

         if (t==4&&b>s+1&&b==s+2) {

//  T() {0==-1}

            s++;

//  T() {0==-1}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T() {0==-1}

         if (t==4&&b==s+1) {

//  T() {0==-1}

            t = 1;

//  T() {0==-1}

            s++;

//  T() {0==-1}

            d = 0;

//  T() {0==-1}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;
      }
   }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int b, s, d, t;

//  P(b,d,s,t) {}

   b = s = d = 0;

//  P(b,d,s,t) {b==0, d==0, s==0}

   t = 1;

//  P(b,d,s,t) {b==0, d==0, s==0, t==1}


   while (1) {

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}

      if (!(t==1&&b==s&&d<9))

//  P() {0==-1}

         assert_error();

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}


      if (flip())

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}

         if (rand()%2) {

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}

            if (t==1&&b<s+9) {

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}

               b++;

//  P(b,d,s,t) {b==s+1, d==0, t==1, 1<=b}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;

//  P(b,d,s,t) {b==s+1, d==0, t==1, 1<=b}

            if (t==1&&b<s+9) {

//  P(b,d,s,t) {b==s+1, d==0, t==1, 1<=b}

               b++;

//  P(b,d,s,t) {b==s+2, d==0, t==1, 2<=b}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;

//  P(b,d,s,t) {b==s+2, d==0, t==1, 2<=b}

            while (rand()%2)

//  P(b,d,s,t) {d==0, t==1, s+26<=13b, s<=b+9, b<=12s+8, b<=s+18,
//    0<=s}

               if (rand()%2) {

//  P(b,d,s,t) {d==0, t==1, s+26<=13b, s<=b+9, b<=12s+8, b<=s+18,
//    0<=s}

                  if (t==1&&b>s-9&&b==s+2) {

//  P(b,d,s,t) {b==s+2, d==0, t==1, 2<=b}

                     s++;

//  P(b,d,s,t) {b==s+1, d==0, t==1, 2<=b}

                     if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(b,d,s,t) {d==0, t==1, 39<=13b+11s, s+26<=13b, s<=b+9, b<=12s+8,
//    b<=s+18, 0<=s}

                     while (1)

//  P(b,d,s,t) {d==0, t==1, 39<=13b+11s, s+26<=13b, s<=b+9, b<=12s+8,
//    b<=s+18, 0<=s}

                        ;

//  P(b,d,s,t) {b==s+1, d==0, t==1, 2<=b}

                  if (t==1&&b<s+9) {

//  P(b,d,s,t) {b==s+1, d==0, t==1, 2<=b}

                     b++;

//  P(b,d,s,t) {b==s+2, d==0, t==1, 3<=b}

                     if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P() {0==-1}

                     while (1)

//  P() {0==-1}

                        ;
               }
               else if (rand()%2) {

//  P(b,d,s,t) {d==0, t==1, s+26<=13b, s<=b+9, b<=12s+8, b<=s+18,
//    0<=s}

                  if (t==1&&b<s+9&&b==s+8) {

//  P(b,d,s,t) {b==s+8, d==0, t==1, 8<=b}

                     b++;

//  P(b,d,s,t) {b==s+9, d==0, t==1, 9<=b}

                     if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(b,d,s,t) {d==0, t==1, s+26<=13b, s<=b+9, b<=23s+7, b<=12s+8,
//    b<=s+18, 0<=s}

                     while (1)

//  P(b,d,s,t) {d==0, t==1, s+26<=13b, s<=b+9, b<=23s+7, b<=12s+8,
//    b<=s+18, 0<=s}

