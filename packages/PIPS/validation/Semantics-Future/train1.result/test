//  T(b) {b==b#init+1, t==2, s<=b+10, b+1<=s, d<=8}

            if (t==2&&b<s-1&&b<s-1) {

//  T(b) {b==b#init+1, t==2, b+1<=s, d<=8}

               b++;

//  T() {t==2, s<=b+10, b+1<=s, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {t==2, s<=b+1, d<=8}

               while (1)

//  T() {t==2, s<=b+1, d<=8}

                  ;

//  T() {0==-1}

         if (t==2&&b<s-1&&b==s-1) {

//  T() {0==-1}

            b++;

//  T() {0==-1}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {t==2, s<=b+10, d<=8}

            while (1)

//  T() {t==2, s<=b+10, d<=8}

