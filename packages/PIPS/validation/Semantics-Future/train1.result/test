//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T(b) {b==b#init+1, b==s, t==1, d<=8}

            if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, b==s, t==1, d<=8}

               b++;

//  T() {b==s, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;
         }
         else {

//  T(s) {b==s-1, b==s#init, t==1, d<=8}

            if (t==1&&b>s-9) {

//  T(s) {b==s-1, b==s#init, t==1, d<=8}

               s++;

//  T() {b==s-1, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T(s) {b==s-2, b==s#init-1, t==1, d<=8}

            if (t==1&&b>s-9) {

//  T(s) {b==s-2, b==s#init-1, t==1, d<=8}

               s++;

//  T() {b==s-2, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T(b,s) {t==1, b#init<=b, 8b#init+27s#init<=8b+27s, s<=b+10,
//    b<=s+19, d<=8}

            while (rand()%2)

//  T(b,s) {t==1, b<=b#init+1, 22b<=21b#init+s+19,
//    b#init+s#init+1<=b+s, 3s<=b+2s#init+10, d<=8, s<=s#init+1}

               if (rand()%2)

//  T(b,s) {t==1, b<=b#init+1, b#init+s#init+1<=b+s,
//    28b+5s<=27b#init+6s#init+25, 3s<=b+2s#init+10, d<=8,
//    s<=s#init+1}

                  if (rand()%2)

//  T(b,s) {b+s==b#init+s#init+1, t==1, b#init<=b, b<=b#init+1,
//    2b#init+s<=3b+8, 22b<=21b#init+s+19, d<=8}

                     if (rand()%2)

//  T(b) {b==b#init+1, t==1, s<=b+10, b+2<=s, d<=8}

                        if (t==1&&b<s+9&&b<s-2) {

//  T(b) {b==b#init+1, t==1, b+2<=s, d<=8}

                           b++;

//  T() {t==1, s<=b+10, b+2<=s, d<=8}

                           if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                              checking_error();
                        }
                        else

//  T() {t==1, s<=b+2, d<=8}

                           while (1)

//  T() {t==1, s<=b+2, d<=8}

                              ;
                     else if (t==1&&b>s-9&&b>s-8) {

//  T(s) {s==s#init+1, t==1, s<=b+8, d<=8}

                        s++;

//  T() {t==1, s<=b+8, b<=s+19, d<=8}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {t==1, b+8<=s, d<=8}

                        while (1)

//  T() {t==1, b+8<=s, d<=8}

                           ;
                  else {

//  T(s) {b==s-9, b==s#init-8, t==1, d<=8}

                     if (t==1&&b>s-9&&b==s-8) {

//  T(s) {b==s-9, b==s#init-8, t==1, d<=8}

                        s++;

//  T() {b==s-9, t==1, d<=8}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {t==1, d<=8}

                        while (1)

//  T() {t==1, d<=8}

                           ;

//  T(b) {b==b#init+1, b==s-8, t==1, d<=8}

                     if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, b==s-8, t==1, d<=8}

                        b++;

//  T() {b==s-8, t==1, d<=8}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {0==-1}

                        while (1)

//  T() {0==-1}

                           ;
                  }
               else {

//  T(b) {b==b#init+1, b==s-1, t==1, d<=8}

                  if (t==1&&b<s+9&&b==s-2) {

//  T(b) {b==b#init+1, b==s-1, t==1, d<=8}

                     b++;

//  T() {b==s-1, t==1, d<=8}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {t==1, d<=8}

                     while (1)

//  T() {t==1, d<=8}

                        ;

//  T(s) {b==s-2, b==s#init-1, t==1, d<=8}

                  if (t==1&&b>s-9) {

//  T(s) {b==s-2, b==s#init-1, t==1, d<=8}

                     s++;

//  T() {b==s-2, t==1, d<=8}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
