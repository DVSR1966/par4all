
TRANSFORMERS


//  T() {0==-1}

void run(void)
{


//  T(b,d,s,t) {}

   int b, s, d, t;

//  T(b,d,s) {b==0, d==0, s==0}

   b = s = d = 0;

//  T(t) {b==0, d==0, s==0, t==1}

   t = 1;

//  T(b,s) {b==s, b#init==0, d==0, s#init==0, t==1, 0<=b}


   while (1) {

//  T() {b==s, t==1, d<=8}

      if (!(t==1&&b==s&&d<9))

//  T() {0==-1}

         assert_error();

//  T(b,s) {b==s, b#init==s#init, t==1, b#init+1<=b, d<=8}


      if (flip())

//  T(b,s) {b==s, b#init==s#init, t==1, b#init+1<=b, d<=8}

         if (rand()%2) {

//  T(b) {b==b#init+1, b==s+1, t==1, d<=8}

            if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, b==s+1, t==1, d<=8}

               b++;

//  T() {b==s+1, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T(b) {b==b#init+1, b==s+2, t==1, d<=8}

            if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, b==s+2, t==1, d<=8}

               b++;

//  T() {b==s+2, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T(b,s) {t==1, b#init<=b, s<=b+10, b<=s+19, d<=8, s#init<=s}

            while (rand()%2)

//  T(b,s) {t==1, b<=b#init+1, 12b<=11b#init+s+19,
//    b#init+s#init+1<=b+s, 13s<=b+12s#init+10, d<=8, s<=s#init+1}

               if (rand()%2) {

//  T(s) {b==s+1, b==s#init+2, t==1, d<=8}

                  if (t==1&&b>s-9&&b==s+2) {

//  T(s) {b==s+1, b==s#init+2, t==1, d<=8}

                     s++;

//  T() {b==s+1, t==1, d<=8}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {t==1, d<=8}

                     while (1)

//  T() {t==1, d<=8}

                        ;

//  T(b) {b==b#init+1, b==s+2, t==1, d<=8}

                  if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, b==s+2, t==1, d<=8}

                     b++;

//  T() {b==s+2, t==1, d<=8}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {0==-1}

                     while (1)

//  T() {0==-1}

                        ;
               }
               else if (rand()%2) {

//  T(b) {b==b#init+1, b==s+9, t==1, d<=8}

                  if (t==1&&b<s+9&&b==s+8) {

//  T(b) {b==b#init+1, b==s+9, t==1, d<=8}

                     b++;

//  T() {b==s+9, t==1, d<=8}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {t==1, d<=8}

                     while (1)

//  T() {t==1, d<=8}

                        ;

//  T(s) {b==s+8, b==s#init+9, t==1, d<=8}

                  if (t==1&&b>s-9) {

//  T(s) {b==s+8, b==s#init+9, t==1, d<=8}

                     s++;

//  T() {b==s+8, t==1, d<=8}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {0==-1}

                     while (1)

//  T() {0==-1}

                        ;
               }
               else if (rand()%2)

//  T(s) {s==s#init+1, t==1, s+2<=b, b<=s+19, d<=8}

                  if (t==1&&b>s-9&&b>s+2) {

//  T(s) {s==s#init+1, t==1, s+2<=b, d<=8}

                     s++;

//  T() {t==1, s+2<=b, b<=s+19, d<=8}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {t==1, b<=s+2, d<=8}

                     while (1)

//  T() {t==1, b<=s+2, d<=8}

                        ;
               else if (t==1&&b<s+9&&b<s+8) {

//  T(b) {b==b#init+1, t==1, b<=s+8, d<=8}

                  b++;

//  T() {t==1, s<=b+10, b<=s+8, d<=8}

                  if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {t==1, s+8<=b, d<=8}

                  while (1)

//  T() {t==1, s+8<=b, d<=8}

                     ;

//  T(s) {b==s+1, b==s#init+2, t==1, d<=8}

            if (t==1&&b>s-9&&b==s+2) {

//  T(s) {b==s+1, b==s#init+2, t==1, d<=8}

               s++;

//  T() {b==s+1, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {t==1, d<=8}

               while (1)

//  T() {t==1, d<=8}

                  ;

//  T(s) {b==s, b==s#init+1, t==1, d<=8}

            if (t==1&&b>s-9) {

//  T(s) {b==s, b==s#init+1, t==1, d<=8}

               s++;

//  T() {b==s, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;
         }
         else if (rand()%2) {

//  T(b) {b==b#init+1, b==s+1, t==1, d<=8}

            if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, b==s+1, t==1, d<=8}

               b++;

//  T() {b==s+1, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T(s) {b==s, b==s#init+1, t==1, d<=8}

            if (t==1&&b>s-9) {

//  T(s) {b==s, b==s#init+1, t==1, d<=8}

               s++;

//  T() {b==s, t==1, d<=8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;
         }
