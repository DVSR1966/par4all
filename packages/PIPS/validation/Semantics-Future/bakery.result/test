            if (rand()%2) {

//  P(a,b,s1,s2) {a==b+1, s1==2, s2==1}

               if (s2==1) {

//  P(a,b,s1,s2) {a==b+1, s1==2, s2==1}

                  b = a+1;

//  P(a,b,s1,s2) {a==b-1, s1==2, s2==1}

                  s2 = 2;

