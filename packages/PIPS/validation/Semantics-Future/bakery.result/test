
TRANSFORMERS


//  T() {0==-1}

void run(void)
{


//  T(a,b,s1,s2) {}

   int s1, s2, a, b;

//  T(s1,s2) {s1==1, s2==1}

   s1 = s2 = 1;

//  T(a,b) {a==0, b==0, s1==1, s2==1}

   a = b = 0;

//  T(a,b,s1,s2) {a==0, a#init==0, b#init==0, s1==1, s1#init==1,
//    s2==1, s2#init==1}


   while (1)

//  T(a,b,s1,s2) {a==0, s1==s1#init, s2==s2#init}


      if (rand()%2) {

//  T(a,s1) {a==b+1, s1==2, s1#init==1}

         if (s1==1) {

//  T(a) {a==b+1, s1==1}

            a = b+1;

//  T(s1) {a==b+1, s1==2, s1#init==1}

            s1 = 2;

//  T() {a==b+1, s1==2}

            if (s1==3&&s2==3)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;

//  T(s1) {a==1, b==0, s1==3, s1#init==2}

         if (s1==2&&(a<b||b==0)) {

//  T(s1) {a==1, b==0, s1==3, s1#init==2}

            s1 = 3;

//  T() {a==1, b==0, s1==3}

            if (s1==3&&s2==3)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {a==b+1, s1==2}

            while (1)

//  T() {a==b+1, s1==2}

               ;

//  T(a,s1) {a==0, a#init==1, b==0, s1==1, s1#init==3}

         if (s1==3) {

//  T(a) {a==0, a#init==1, b==0, s1==3}

            a = 0;

//  T(s1) {a==0, b==0, s1==1, s1#init==3}

            s1 = 1;

//  T() {a==0, b==0, s1==1}

            if (s1==3&&s2==3)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;
      }
      else {

//  T(a,b,s1,s2) {s1==s1#init, s2==2, s2#init==1}

         if (rand()%2)

//  T(b,s2) {a==b-1, s2==2, s2#init==1}

            if (s2==1) {

//  T(b) {a==b-1, s2==1}

               b = a+1;

//  T(s2) {a==b-1, s2==2, s2#init==1}

               s2 = 2;

//  T() {a==b-1, s2==2}

               if (s1==3&&s2==3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;
         else {

//  T(a,s1) {a==b+1, s1==2, s1#init==1}

            if (s1==1) {

//  T(a) {a==b+1, s1==1}

               a = b+1;

//  T(s1) {a==b+1, s1==2, s1#init==1}

               s1 = 2;

//  T() {a==b+1, s1==2}

               if (s1==3&&s2==3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;

//  T(a,b,s1,s2) {a==0, a#init==b-1, a#init==b#init+1, s1==1,
//    s1#init==2, s2==2, s2#init==1}

            if (rand()%2) {

//  T(b,s2) {a==b-1, a==b#init+1, s1==2, s2==2, s2#init==1}

               if (s2==1) {

//  T(b) {a==b-1, a==b#init+1, s1==2, s2==1}

                  b = a+1;

//  T(s2) {a==b-1, s1==2, s2==2, s2#init==1}

                  s2 = 2;

//  T() {a==b-1, s1==2, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {a==b+1, s1==2}

                  while (1)

//  T() {a==b+1, s1==2}

                     ;

//  T(s1) {a==b-1, s1==3, s1#init==2, s2==2}

               if (s1==2&&(a<b||b==0)) {

//  T(s1) {a==b-1, s1==3, s1#init==2, s2==2}

                  s1 = 3;

//  T() {a==b-1, s1==3, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T(a,s1) {a==0, a#init==b-1, s1==1, s1#init==3, s2==2}

               if (s1==3) {

//  T(a) {a==0, a#init==b-1, s1==3, s2==2}

                  a = 0;

//  T(s1) {a==0, s1==1, s1#init==3, s2==2}

                  s1 = 1;

//  T() {a==0, s1==1, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;
            }
            else {

//  T(s1) {a==1, b==0, s1==3, s1#init==2}

               if (s1==2&&(a<b||b==0)) {

//  T(s1) {a==1, b==0, s1==3, s1#init==2}

                  s1 = 3;

//  T() {a==1, b==0, s1==3}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {a==b+1, s1==2}

                  while (1)

//  T() {a==b+1, s1==2}

                     ;

//  T(b,s2) {a==1, b==2, b#init==0, s1==3, s2==2, s2#init==1}

               if (s2==1) {

//  T(b) {a==1, b==2, b#init==0, s1==3, s2==1}

                  b = a+1;

//  T(s2) {a==1, b==2, s1==3, s2==2, s2#init==1}

                  s2 = 2;

//  T() {a==1, b==2, s1==3, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {a==1, b==0, s1==3}

                  while (1)

//  T() {a==1, b==0, s1==3}

                     ;

//  T(a,s1) {a==0, a#init==1, b==2, s1==1, s1#init==3, s2==2}

               if (s1==3) {

//  T(a) {a==0, a#init==1, b==2, s1==3, s2==2}

                  a = 0;

//  T(s1) {a==0, b==2, s1==1, s1#init==3, s2==2}

                  s1 = 1;

//  T() {a==0, b==2, s1==1, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;
            }
         }

//  T(a,b,s1,s2) {s1==s1#init, s2==2, s2#init==2}

         while (rand()%2) {

//  T(a,s1,s2) {a==b+1, s1==2, s1#init==1, s2==3, s2#init==2}

            if (rand()%2) {

//  T(a,s1) {a==b+1, s1==2, s1#init==1}

               if (s1==1) {

//  T(a) {a==b+1, s1==1}

                  a = b+1;

//  T(s1) {a==b+1, s1==2, s1#init==1}

                  s1 = 2;

//  T() {a==b+1, s1==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {}

                  while (1)

//  T() {}

                     ;

//  T(s2) {a==b+1, s1==2, s2==3, s2#init==2}

               if (s2==2&&(b<a||a==0)) {

//  T(s2) {a==b+1, s1==2, s2==3, s2#init==2}

                  s2 = 3;

//  T() {a==b+1, s1==2, s2==3}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {a==b+1, s1==2}

                  while (1)

//  T() {a==b+1, s1==2}

                     ;
            }
            else {

//  T(s2) {s2==3, s2#init==2}

               if (s2==2&&(b<a||a==0)) {

//  T(s2) {s2==3, s2#init==2}

                  s2 = 3;

//  T() {s2==3}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {}

                  while (1)

//  T() {}

                     ;

//  T(a,s1) {a==b+1, s1==2, s1#init==1, s2==3}

               if (s1==1) {

//  T(a) {a==b+1, s1==1, s2==3}

                  a = b+1;

//  T(s1) {a==b+1, s1==2, s1#init==1, s2==3}

                  s1 = 2;

//  T() {a==b+1, s1==2, s2==3}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {s2==3}

                  while (1)

//  T() {s2==3}

                     ;
            }

//  T(a,s2) {a==0, a#init==b+1, s1==2, s2==1, s2#init==3}

            if (s2==3) {

//  T(a) {a==0, a#init==b+1, s1==2, s2==3}

               a = 0;

//  T(s2) {a==0, s1==2, s2==1, s2#init==3}

               s2 = 1;

//  T() {a==0, s1==2, s2==1}

               if (s1==3&&s2==3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T(a,b,s1,s2) {a==0, a#init==0, b==1, s1==1, s1#init==2, s2==2,
//    s2#init==1}

            if (rand()%2) {

//  T(b,s2) {a==0, b==1, s1==2, s2==2, s2#init==1}

               if (s2==1) {

//  T(b) {a==0, b==1, s1==2, s2==1}

                  b = a+1;

//  T(s2) {a==0, b==1, s1==2, s2==2, s2#init==1}

                  s2 = 2;

//  T() {a==0, b==1, s1==2, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T(s1) {a==0, b==1, s1==3, s1#init==2, s2==2}

               if (s1==2&&(a<b||b==0)) {

//  T(s1) {a==0, b==1, s1==3, s1#init==2, s2==2}

                  s1 = 3;

//  T() {a==0, b==1, s1==3, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T(a,s1) {a==0, a#init==0, b==1, s1==1, s1#init==3, s2==2}

               if (s1==3) {

//  T(a) {a==0, a#init==0, b==1, s1==3, s2==2}

                  a = 0;

//  T(s1) {a==0, b==1, s1==1, s1#init==3, s2==2}

                  s1 = 1;

//  T() {a==0, b==1, s1==1, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;
            }
            else {

//  T(s1) {a==0, s1==3, s1#init==2, s2==1, 0<=b}

               if (s1==2&&(a<b||b==0)) {

//  T(s1) {a==0, s1==3, s1#init==2, s2==1, 0<=b}

                  s1 = 3;

//  T() {a==0, s1==3, s2==1, 0<=b}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {a==0, s1==2, s2==1, b+1<=0}

                  while (1)

//  T() {a==0, s1==2, s2==1, b+1<=0}

                     ;

//  T(b,s2) {a==0, b==1, s1==3, s2==2, s2#init==1, 0<=b#init}

               if (s2==1) {

//  T(b) {a==0, b==1, s1==3, s2==1, 0<=b#init}

                  b = a+1;

//  T(s2) {a==0, b==1, s1==3, s2==2, s2#init==1}

                  s2 = 2;

//  T() {a==0, b==1, s1==3, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T(a,s1) {a==0, a#init==0, b==1, s1==1, s1#init==3, s2==2}

               if (s1==3) {

//  T(a) {a==0, a#init==0, b==1, s1==3, s2==2}

                  a = 0;

//  T(s1) {a==0, b==1, s1==1, s1#init==3, s2==2}

                  s1 = 1;

//  T() {a==0, b==1, s1==1, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;
            }
         }

//  T(a,s1,s2) {a==0, s1==s1#init, s2==1, s2#init==2}

         if (rand()%2) {

//  T(a,s1,s2) {a==b+1, s1==2, s1#init==1, s2==3, s2#init==2}

            if (rand()%2) {

//  T(a,s1) {a==b+1, s1==2, s1#init==1, s2==2}

               if (s1==1) {

//  T(a) {a==b+1, s1==1, s2==2}

                  a = b+1;

//  T(s1) {a==b+1, s1==2, s1#init==1, s2==2}

                  s1 = 2;

//  T() {a==b+1, s1==2, s2==2}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {s2==2}

                  while (1)

//  T() {s2==2}

                     ;

//  T(s2) {a==b+1, s1==2, s2==3, s2#init==2}

               if (s2==2&&(b<a||a==0)) {

//  T(s2) {a==b+1, s1==2, s2==3, s2#init==2}

                  s2 = 3;

//  T() {a==b+1, s1==2, s2==3}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;
            }
            else {

//  T(s2) {s2==3, s2#init==2}

               if (s2==2&&(b<a||a==0)) {

//  T(s2) {s2==3, s2#init==2}

                  s2 = 3;

//  T() {s2==3}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {s2==2, a<=b}

                  while (1)

//  T() {s2==2, a<=b}

                     ;

//  T(a,s1) {a==b+1, s1==2, s1#init==1, s2==3}

               if (s1==1) {

//  T(a) {a==b+1, s1==1, s2==3}

                  a = b+1;

//  T(s1) {a==b+1, s1==2, s1#init==1, s2==3}

                  s1 = 2;

//  T() {a==b+1, s1==2, s2==3}

                  if (s1==3&&s2==3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {s2==3}

                  while (1)

//  T() {s2==3}

                     ;
            }

//  T(a,s2) {a==0, a#init==b+1, s1==2, s2==1, s2#init==3}

            if (s2==3) {

//  T(a) {a==0, a#init==b+1, s1==2, s2==3}

               a = 0;

//  T(s2) {a==0, s1==2, s2==1, s2#init==3}

               s2 = 1;

//  T() {a==0, s1==2, s2==1}

               if (s1==3&&s2==3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T(s1) {a==0, s1==3, s1#init==2, s2==1, 0<=b}

            if (s1==2&&(a<b||b==0)) {

//  T(s1) {a==0, s1==3, s1#init==2, s2==1, 0<=b}

               s1 = 3;

//  T() {a==0, s1==3, s2==1, 0<=b}

               if (s1==3&&s2==3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {a==0, s1==2, s2==1, b+1<=0}

               while (1)

//  T() {a==0, s1==2, s2==1, b+1<=0}

                  ;

//  T(a,s1) {a==0, a#init==0, s1==1, s1#init==3, s2==1, 0<=b}

            if (s1==3) {

//  T(a) {a==0, a#init==0, s1==3, s2==1, 0<=b}

               a = 0;

//  T(s1) {a==0, s1==1, s1#init==3, s2==1, 0<=b}

               s1 = 1;

//  T() {a==0, s1==1, s2==1, 0<=b}

               if (s1==3&&s2==3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;
         }
         else {

//  T(s2) {s2==3, s2#init==2}

            if (s2==2&&(b<a||a==0)) {

//  T(s2) {s2==3, s2#init==2}

               s2 = 3;

//  T() {s2==3}

               if (s1==3&&s2==3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {s2==2, a<=b}

               while (1)

//  T() {s2==2, a<=b}

                  ;

//  T(a,s2) {a==0, s2==1, s2#init==3}

            if (s2==3) {

//  T(a) {a==0, s2==3}

               a = 0;

//  T(s2) {a==0, s2==1, s2#init==3}

               s2 = 1;

//  T() {a==0, s2==1}

               if (s1==3&&s2==3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;
         }
      }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int s1, s2, a, b;

//  P(a,b,s1,s2) {}

   s1 = s2 = 1;

//  P(a,b,s1,s2) {s1==1, s2==1}

   a = b = 0;

//  P(a,b,s1,s2) {a==0, b==0, s1==1, s2==1}


   while (1)

//  P(a,b,s1,s2) {a==0, s1==1, s2==1}


      if (rand()%2) {

//  P(a,b,s1,s2) {a==0, s1==1, s2==1}

         if (s1==1) {

//  P(a,b,s1,s2) {a==0, s1==1, s2==1}

            a = b+1;

//  P(a,b,s1,s2) {a==b+1, s1==1, s2==1}

            s1 = 2;

//  P(a,b,s1,s2) {a==b+1, s1==2, s2==1}

            if (s1==3&&s2==3)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P(a,b,s1,s2) {a==b+1, s1==2, s2==1}

         if (s1==2&&(a<b||b==0)) {

//  P(a,b,s1,s2) {a==1, b==0, s1==2, s2==1}

            s1 = 3;

//  P(a,b,s1,s2) {a==1, b==0, s1==3, s2==1}

            if (s1==3&&s2==3)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(a,b,s1,s2) {a==b+1, s1==2, s2==1}

            while (1)

//  P(a,b,s1,s2) {a==b+1, s1==2, s2==1}

               ;

//  P(a,b,s1,s2) {a==1, b==0, s1==3, s2==1}

         if (s1==3) {

//  P(a,b,s1,s2) {a==1, b==0, s1==3, s2==1}

            a = 0;

//  P(a,b,s1,s2) {a==0, b==0, s1==3, s2==1}

            s1 = 1;

//  P(a,b,s1,s2) {a==0, b==0, s1==1, s2==1}

            if (s1==3&&s2==3)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
      }
      else {

//  P(a,b,s1,s2) {a==0, s1==1, s2==1}

         if (rand()%2)

//  P(a,b,s1,s2) {a==0, s1==1, s2==1}

            if (s2==1) {

//  P(a,b,s1,s2) {a==0, s1==1, s2==1}

               b = a+1;

//  P(a,b,s1,s2) {a==0, b==1, s1==1, s2==1}

               s2 = 2;

//  P(a,b,s1,s2) {a==0, b==1, s1==1, s2==2}

               if (s1==3&&s2==3)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;
         else {

//  P(a,b,s1,s2) {a==0, s1==1, s2==1}

            if (s1==1) {

//  P(a,b,s1,s2) {a==0, s1==1, s2==1}

               a = b+1;

//  P(a,b,s1,s2) {a==b+1, s1==1, s2==1}

