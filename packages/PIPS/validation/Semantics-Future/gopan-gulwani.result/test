
TRANSFORMERS


//  T() {}

void run(void)
{

//  T(s,x,y) {}

   int s, x, y;

//  T(s) {s==1}

   s = 1;

//  T(x,y) {s==1, x==0, y==0}

   x = y = 0;

//  T(s,x,y) {s==1, s#init==1, x==y, x#init==y#init, 0<=x, x<=50,
//    x#init<=x}

   while (flip()) {

//  T(s,y) {s==2, s#init==1, y==y#init+1, x<=50}

      if (s==1&&x<=50) {

//  T(s) {s==2, s#init==1, x<=50}

         s = 2;

//  T(y) {s==2, y==y#init+1, x<=50}

         y++;

//  T() {s==2, x<=50}

         if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {}

         while (1)

//  T() {}

            ;

//  T(s,x) {s==1, s#init==2, x==x#init+1, x<=50, y<=x, x+y<=102, 0<=y}

      if (s==2&&y>=0&&x<=49) {

//  T(s) {s==1, s#init==2, x<=49, 0<=y}

         s = 1;

//  T(x) {s==1, x==x#init+1, x<=50, 0<=y}

         x++;

//  T() {s==1, x<=50, y<=x, x+y<=102, 0<=y}

         if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {s==2, x<=50}

         while (1)

//  T() {s==2, x<=50}

            ;
   }

//  T(s,y) {s==2, s#init==1, x==y-1, x==y#init, 0<=x, x<=50}

   if (s==1&&x<=50) {

//  T(s) {s==2, s#init==1, x==y, 0<=x, x<=50}

      s = 2;

//  T(y) {s==2, x==y-1, x==y#init, 0<=x, x<=50}

      y++;

//  T() {s==2, x==y-1, 0<=x, x<=50}

      if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

         checking_error();
   }
   else

//  T() {s==1, x==y, 51<=x}

      while (1)

//  T() {s==1, x==y, 51<=x}

         ;

//  T(s,x) {s==1, s#init==2, x==51, x#init==50, y==51}

   if (s==2&&y>=0&&x==50) {

//  T(s) {s==1, s#init==2, x==50, y==51}

      s = 1;

//  T(x) {s==1, x==51, x#init==50, y==51}

      x++;

//  T() {s==1, x==51, y==51}

      if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

         checking_error();
   }
   else

//  T() {s==2, x==y-1, 0<=x, x<=49}

      while (1)

//  T() {s==2, x==y-1, 0<=x, x<=49}

         ;

//  T(s,x,y) {s==1, s#init==1, x+y==102, x#init+y#init==102, 51<=x,
//    x<=102, x#init<=x}

   while (flip()) {

//  T(s,y) {s==2, s#init==1, y==y#init-1, 51<=x, 0<=y}

      if (s==1&&x>50&&y>=1) {

//  T(s) {s==2, s#init==1, 51<=x, 1<=y}

         s = 2;

//  T(y) {s==2, y==y#init-1, 51<=x, 0<=y}

         y--;

//  T() {s==2, 51<=x, 0<=y}

         if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {}

         while (1)

//  T() {}

            ;

//  T(s,x) {s==1, s#init==2, x==x#init+1, 52<=x, y<=x, x+y<=102, 0<=y}

      if (s==2&&y>=0) {

//  T(s) {s==1, s#init==2, 51<=x, 0<=y}

         s = 1;

//  T(x) {s==1, x==x#init+1, 52<=x, 0<=y}

         x++;

//  T() {s==1, 52<=x, y<=x, x+y<=102, 0<=y}

         if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0==-1}

         while (1)

//  T() {0==-1}

            ;
   }

//  T(s,y) {s==2, s#init==1, x==102, y==-1, y#init==0}

   if (s==1&&x>50&&y==0) {

//  T(s) {s==2, s#init==1, x==102, y==0}

      s = 2;

//  T(y) {s==2, x==102, y==-1, y#init==0}

      y--;

//  T() {s==2, x==102, y==-1}

      if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

         checking_error();
   }
   else

//  T() {s==1, x+y==102, 51<=x}

      while (1)

//  T() {s==1, x+y==102, 51<=x}

         ;
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int s, x, y;

//  P(s,x,y) {}

   s = 1;

//  P(s,x,y) {s==1}

   x = y = 0;

//  P(s,x,y) {s==1, x==0, y==0}

   while (flip()) {

//  P(s,x,y) {s==1, x==y, 0<=x, x<=50}

      if (s==1&&x<=50) {

//  P(s,x,y) {s==1, x==y, 0<=x, x<=50}

         s = 2;

//  P(s,x,y) {s==2, x==y, 0<=x, x<=50}

         y++;

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=50}

         if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=50}

      if (s==2&&y>=0&&x<=49) {

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=49}

         s = 1;

//  P(s,x,y) {s==1, x==y-1, 0<=x, x<=49}

         x++;

//  P(s,x,y) {s==1, x==y, 1<=x, x<=50}

         if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

            checking_error();
      }
      else

//  P(s,x,y) {s==2, x==50, y==51}

         while (1)

//  P(s,x,y) {s==2, x==50, y==51}

            ;
   }

//  P(s,x,y) {s==1, x==y, 0<=x, x<=50}

   if (s==1&&x<=50) {

//  P(s,x,y) {s==1, x==y, 0<=x, x<=50}

      s = 2;

//  P(s,x,y) {s==2, x==y, 0<=x, x<=50}

      y++;

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=50}

      if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

         checking_error();
   }
   else

//  P() {0==-1}

      while (1)

//  P() {0==-1}

         ;

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=50}

   if (s==2&&y>=0&&x==50) {

//  P(s,x,y) {s==2, x==50, y==51}

      s = 1;

//  P(s,x,y) {s==1, x==50, y==51}

      x++;

//  P(s,x,y) {s==1, x==51, y==51}

      if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

         checking_error();
   }
   else

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=49}

      while (1)

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=49}

         ;

//  P(s,x,y) {s==1, x==51, y==51}

   while (flip()) {

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=102}

      if (s==1&&x>50&&y>=1) {

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=101}

         s = 2;

//  P(s,x,y) {s==2, x+y==102, 51<=x, x<=101}

         y--;

//  P(s,x,y) {s==2, x+y==101, 51<=x, x<=101}

         if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

            checking_error();
      }
      else

//  P(s,x,y) {s==1, x==102, y==0}

         while (1)

//  P(s,x,y) {s==1, x==102, y==0}

            ;

//  P(s,x,y) {s==2, x+y==101, 51<=x, x<=101}

      if (s==2&&y>=0) {

//  P(s,x,y) {s==2, x+y==101, 51<=x, x<=101}

         s = 1;

//  P(s,x,y) {s==1, x+y==101, 51<=x, x<=101}

         x++;

//  P(s,x,y) {s==1, x+y==102, 52<=x, x<=102}

         if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;
   }

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=102}

   if (s==1&&x>50&&y==0) {

//  P(s,x,y) {s==1, x==102, y==0}

      s = 2;

//  P(s,x,y) {s==2, x==102, y==0}

      y--;

//  P(s,x,y) {s==2, x==102, y==-1}

      if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

         checking_error();
   }
   else

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=101}

      while (1)

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=101}

         ;
}
