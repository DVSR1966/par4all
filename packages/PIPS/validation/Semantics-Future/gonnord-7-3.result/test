
TRANSFORMERS


//  T() {0==-1}

void run(void)
{

//  T(x,z) {}

   int x, z;

//  T(x,z) {x==0, z==0}

   x = z = 0;

//  T(x,z) {x#init==0, z#init==0, 0<=9x+z, z<=x}

   while (1) {

//  T(x,z) {x+z#init==x#init+z, z#init<=z}

      while (flip())

//  T(x,z) {x==x#init+1, z==z#init+1, 0<=x, z<=x, 0<=z, z<=9}

         if (z<=8) {

//  T(x) {x==x#init+1, z<=8}

            x++;

//  T(z) {z==z#init+1, z<=9}

            z++;

//  T() {0<=x, z<=x, 0<=z, z<=9}

            if (!(x>=0&&z>=0&&z<=10&&z<=x))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {9<=z}

            while (1)

//  T() {9<=z}

               ;

//  T(x,z) {9x+z==9x#init+z#init, x#init<=x}

      while (flip()) {

//  T(x,z) {x==x#init+1, z==10, z#init==9, 10<=x}

         if (z==9) {

//  T(x) {x==x#init+1, z==9}

            x++;

//  T(z) {z==10, z#init==9}

            z++;

//  T() {z==10, 10<=x}

            if (!(x>=0&&z>=0&&z<=10&&z<=x))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;

//  T(z) {z==0, z#init==10, 10<=x}

         if (z==10) {

//  T(z) {z==0, z#init==10, 10<=x}

            z = 0;

//  T() {z==0, 10<=x}

            if (!(x>=0&&z>=0&&z<=10&&z<=x))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;
      }
   }
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int x, z;

//  P(x,z) {}

   x = z = 0;

//  P(x,z) {x==0, z==0}

   while (1) {

//  P(x,z) {z<=x, 0<=z, z<=9}

      while (flip())

//  P(x,z) {z<=x, 0<=z, z<=9}

         if (z<=8) {

//  P(x,z) {z<=x, 0<=z, z<=8}

            x++;

//  P(x,z) {z+1<=x, 0<=z, z<=8}

            z++;

//  P(x,z) {z<=x, 1<=z, z<=9}

            if (!(x>=0&&z>=0&&z<=10&&z<=x))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(x,z) {z==9, 9<=x}

            while (1)

//  P(x,z) {z==9, 9<=x}

               ;

//  P(x,z) {z<=x, 0<=z, z<=9}

      while (flip()) {

//  P(x,z) {z<=x, 0<=z, z<=9}

         if (z==9) {

//  P(x,z) {z==9, 9<=x}

            x++;

//  P(x,z) {z==9, 10<=x}

            z++;

//  P(x,z) {z==10, 10<=x}

            if (!(x>=0&&z>=0&&z<=10&&z<=x))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(x,z) {z<=x, 0<=z, z<=8}

            while (1)

//  P(x,z) {z<=x, 0<=z, z<=8}

               ;

//  P(x,z) {z==10, 10<=x}

         if (z==10) {

//  P(x,z) {z==10, 10<=x}

            z = 0;

//  P(x,z) {z==0, 10<=x}

            if (!(x>=0&&z>=0&&z<=10&&z<=x))

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
      }
   }
}
