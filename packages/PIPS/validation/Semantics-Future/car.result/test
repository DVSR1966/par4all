//  T(v) {s==1, t==3, v==2, v#init==1, 1<=d, d<=7}

            v++;

//  T(d) {d==d#init+1, s==1, t==3, v==2, 2<=d, d<=8}

            d++;

//  T() {s==1, t==3, v==2, 2<=d, d<=8}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

            while (1)

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

               ;

//  T() {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(s) {s==2, s#init==1, t==3, v==2, 2<=d, d<=8}

