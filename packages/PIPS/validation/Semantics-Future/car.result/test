//  T() {s==1, 0<=d, 0<=t}

            while (1)

//  T() {s==1, 0<=d, 0<=t}

               ;

//  T() {d==9, s==1, v==2, 0<=t}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==1, v==2, 0<=t}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(t,v) {d==9, s==1, t==3, t#init==2, v==0, v#init==2}

         if (s==1&&t==2) {

//  T(v) {d==9, s==1, t==2, v==0, v#init==2}

            v = 0;

//  T(t) {d==9, s==1, t==3, t#init==2, v==0}

            t++;

//  T() {d==9, s==1, t==3, v==0}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {d==9, s==1, v==2, 0<=t}

            while (1)

//  T() {d==9, s==1, v==2, 0<=t}

               ;

//  T() {d==9, s==1, t==3, v==0}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==1, t==3, v==0}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }

//  T(s) {d==9, s==2, s#init==1, t==3, v==0}

      if (s==1&&t>=3) {

//  T(s) {d==9, s==2, s#init==1, t==3, v==0}

         s = 2;

//  T() {d==9, s==2, t==3, v==0}

         if (s==3||s==4)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0==-1}

         while (1)

//  T() {0==-1}

            ;

//  T() {d==9, s==2, t==3, v==0}

      if (s==1&&d>=10)

//  T() {0==-1}

         checking_error();

//  T() {d==9, s==2, t==3, v==0}

      if (s==1&&v>=3)

//  T() {0==-1}

         checking_error();
   }
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int s, d, v, t;

//  P(d,s,t,v) {}

   s = 1;

//  P(d,s,t,v) {s==1}

   d = v = t = 0;

//  P(d,s,t,v) {d==0, s==1, t==0, v==0}

   if (s==1&&d>=10)

//  P() {0==-1}

      checking_error();

//  P(d,s,t,v) {d==0, s==1, t==0, v==0}

   if (s==1&&v>=3)

//  P() {0==-1}

      checking_error();

//  P(d,s,t,v) {d==0, s==1, t==0, v==0}

   while (flip())

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    0<=t, v<=1}

      if (flip()) {

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    0<=t, v<=1}

         if (s==1&&t<=1) {

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    0<=t, t<=1, v<=1}

            v = 0;

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=9, d<=t+8, 3d<=t+25, 0<=t, t<=1}

            t++;

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=9, d<=t+7, 3d<=t+24, 1<=t, t<=2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    2<=t, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    2<=t, v<=1}

               ;

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=9, d<=t+7, 3d<=t+24, 1<=t, t<=2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=9, d<=t+7, 3d<=t+24, 1<=t, t<=2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    0<=t, v<=1}

         if (s==1&&v<=0&&d<=7) {

//  P(d,s,t,v) {s==1, 0<=d, d<=7, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    0<=t, v<=0}

            v++;

//  P(d,s,t,v) {s==1, 0<=d, d<=7, d<=t+8, 3d<=t+25, v<=d+1, d+v<=10,
//    0<=t, v<=1}

            d++;

//  P(d,s,t,v) {s==1, 1<=d, d<=8, d<=t+9, 3d<=t+28, v<=d, d+v<=11,
//    0<=t, v<=1}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, 1<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    0<=t, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, 1<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    0<=t, v<=1}

               ;

//  P(d,s,t,v) {s==1, 1<=d, d<=8, d<=t+9, 3d<=t+28, v<=d, d+v<=11,
//    0<=t, v<=1}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, 1<=d, d<=8, d<=t+9, 3d<=t+28, v<=d, d+v<=11,
//    0<=t, v<=1}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, d+v<=9,
//    0<=t, v<=1}

         if (s==1&&v==1&&d<=7) {

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=7, d<=t+8, 3d<=t+25, 0<=t}

            v++;

//  P(d,s,t,v) {s==1, v==2, 1<=d, d<=7, d<=t+8, 3d<=t+25, 0<=t}

            d++;

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=t+9, 3d<=t+28, 0<=t}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, 8v<=d,
//    d+v<=9, 0<=t, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, 0<=d, d<=9, d<=t+8, 3d<=t+25, v<=d, 8v<=d,
//    d+v<=9, 0<=t, v<=1}

               ;

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=t+9, 3d<=t+28, 0<=t}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=t+9, 3d<=t+28, 0<=t}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=t+9, 3d<=t+28, 0<=t}

         if (s==1&&t<=1) {

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=t+9, 3d<=t+28, 0<=t, t<=1}

            v = 0;

//  P(d,s,t,v) {s==1, v==0, 2<=d, d<=8, d<=t+9, 3d<=t+28, 0<=t, t<=1}

            t++;

//  P(d,s,t,v) {s==1, v==0, 2<=d, d<=8, d<=t+8, 3d<=t+27, 1<=t, t<=2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=t+9, 3d<=t+28, 2<=t}

            while (1)

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=t+9, 3d<=t+28, 2<=t}

               ;

//  P(d,s,t,v) {s==1, v==0, 2<=d, d<=8, d<=t+8, 3d<=t+27, 1<=t, t<=2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==0, 2<=d, d<=8, d<=t+8, 3d<=t+27, 1<=t, t<=2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

   if (flip()) {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

      if (flip()) {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

         if (s==1&&v==1&&d<=7) {

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=7, 0<=t}

            v++;

//  P(d,s,t,v) {s==1, v==2, 1<=d, d<=7, 0<=t}

            d++;

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, 0<=t}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 8v<=d, 0<=t, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 8v<=d, 0<=t, v<=1}

               ;

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, 0<=t}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, 0<=t}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, 0<=t}

         if (s==1&&t==2) {

//  P(d,s,t,v) {s==1, t==2, v==2, 2<=d, d<=8}

            v = 0;

//  P(d,s,t,v) {s==1, t==2, v==0, 2<=d, d<=8}

            t++;

//  P(d,s,t,v) {s==1, t==3, v==0, 2<=d, d<=8}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, 0<=t}

            while (1)

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, 0<=t}

               ;

//  P(d,s,t,v) {s==1, t==3, v==0, 2<=d, d<=8}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==0, 2<=d, d<=8}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

         if (s==1&&t==2) {

//  P(d,s,t,v) {s==1, t==2, 0<=d, d<=8, v<=d, v<=1}

            v = 0;

//  P(d,s,t,v) {s==1, t==2, v==0, 0<=d, d<=8}

            t++;

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=8}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

               ;

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=8}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=8}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=8}

      while (flip()) {

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, 0<=v, v<=1}

         if (s==1&&v<=0&&d<=7) {

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=7}

            v++;

//  P(d,s,t,v) {s==1, t==3, v==1, 0<=d, d<=7}

            d++;

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=8}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, t==3, d<=8, 8<=d+7v, v<=d, 0<=v, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, t==3, d<=8, 8<=d+7v, v<=d, 0<=v, v<=1}

               ;

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=8}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=8}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, 0<=v, v<=1}

      if (flip()) {

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, 0<=v, v<=1}

         if (s==1&&t>=3) {

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, 0<=v, v<=1}

            s = 2;

//  P(d,s,t,v) {s==2, t==3, d<=8, v<=d, d<=v+9, 0<=v, v<=1}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P(d,s,t,v) {s==2, t==3, d<=8, v<=d, d<=v+9, 0<=v, v<=1}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==2, t==3, d<=8, v<=d, d<=v+9, 0<=v, v<=1}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, 0<=v, v<=1}

         if (s==1&&v==1&&d<=7) {

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=7}

            v++;

//  P(d,s,t,v) {s==1, t==3, v==2, 1<=d, d<=7}

            d++;

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, 8v<=d, 0<=v, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, t==3, d<=8, 8v<=d, 0<=v}

               ;

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&t>=3) {

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

            s = 2;

//  P(d,s,t,v) {s==2, t==3, v==2, 2<=d, d<=8}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P(d,s,t,v) {s==2, t==3, v==2, 2<=d, d<=8}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==2, t==3, v==2, 2<=d, d<=8}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, 0<=v, v<=1}

         if (s==1&&v==1&&d==8) {

//  P(d,s,t,v) {d==8, s==1, t==3, v==1}

            v++;

//  P(d,s,t,v) {d==8, s==1, t==3, v==2}

            d++;

//  P(d,s,t,v) {d==9, s==1, t==3, v==2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, d+v<=8, 0<=v, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, t==3, v<=d, d+v<=8, 0<=v, v<=1}

               ;

//  P(d,s,t,v) {d==9, s==1, t==3, v==2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==1, t==3, v==2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==1, t==3, v==2}

         if (s==1&&t>=3) {

//  P(d,s,t,v) {d==9, s==1, t==3, v==2}

            s = 2;

//  P(d,s,t,v) {d==9, s==2, t==3, v==2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P(d,s,t,v) {d==9, s==2, t==3, v==2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==2, t==3, v==2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, 0<=v, v<=1}

         if (s==1&&v<=0&&d==8) {

//  P(d,s,t,v) {d==8, s==1, t==3, v==0}

            v++;

//  P(d,s,t,v) {d==8, s==1, t==3, v==1}

            d++;

//  P(d,s,t,v) {d==9, s==1, t==3, v==1}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, d<=v+7, 0<=v, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, t==3, d<=8, v<=d, d<=v+7, 0<=v, v<=1}

               ;

//  P(d,s,t,v) {d==9, s==1, t==3, v==1}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==1, t==3, v==1}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==1, t==3, v==1}

         if (s==1&&t>=3) {

//  P(d,s,t,v) {d==9, s==1, t==3, v==1}

            s = 2;

//  P(d,s,t,v) {d==9, s==2, t==3, v==1}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P(d,s,t,v) {d==9, s==2, t==3, v==1}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==2, t==3, v==1}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
   }
   else {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

      if (flip()) {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

         if (flip()) {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

            if (s==1&&v<=0&&d==8) {

//  P(d,s,t,v) {d==8, s==1, 0<=t, v<=0}

               v++;

//  P(d,s,t,v) {d==8, s==1, 0<=t, v<=1}

               d++;

//  P(d,s,t,v) {d==9, s==1, 0<=t, v<=1}

               if (s==3||s==4)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

               while (1)

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

                  ;

//  P(d,s,t,v) {d==9, s==1, 0<=t, v<=1}

            if (s==1&&d>=10)

//  P() {0==-1}

               checking_error();

//  P(d,s,t,v) {d==9, s==1, 0<=t, v<=1}

            if (s==1&&v>=3)

//  P() {0==-1}

               checking_error();
         }
         else {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

            if (s==1&&v==1&&d==8) {

//  P(d,s,t,v) {d==8, s==1, v==1, 0<=t}

               v++;

//  P(d,s,t,v) {d==8, s==1, v==2, 0<=t}

               d++;

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

               if (s==3||s==4)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, d+v<=8, 0<=t, v<=1}

               while (1)

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, d+v<=8, 0<=t, v<=1}

                  ;

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

            if (s==1&&d>=10)

//  P() {0==-1}

               checking_error();

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

            if (s==1&&v>=3)

//  P() {0==-1}

               checking_error();

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

            if (s==1&&t<=1) {

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t, t<=1}

               v = 0;

//  P(d,s,t,v) {d==9, s==1, v==0, 0<=t, t<=1}

               t++;

//  P(d,s,t,v) {d==9, s==1, v==0, 1<=t, t<=2}

               if (s==3||s==4)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(d,s,t,v) {d==9, s==1, v==2, 2<=t}

               while (1)

//  P(d,s,t,v) {d==9, s==1, v==2, 2<=t}

                  ;

//  P(d,s,t,v) {d==9, s==1, v==0, 1<=t, t<=2}

            if (s==1&&d>=10)

//  P() {0==-1}

               checking_error();

//  P(d,s,t,v) {d==9, s==1, v==0, 1<=t, t<=2}

            if (s==1&&v>=3)

//  P() {0==-1}

               checking_error();
         }

//  P(d,s,t,v) {d==9, s==1, 0<=t, v<=1}

         while (flip()) {

//  P(d,s,t,v) {d==9, s==1, 0<=t, v<=1}

            if (s==1&&t<=1) {

//  P(d,s,t,v) {d==9, s==1, 0<=t, t<=1, v<=1}

               v = 0;

//  P(d,s,t,v) {d==9, s==1, v==0, 0<=t, t<=1}

               t++;

//  P(d,s,t,v) {d==9, s==1, v==0, 1<=t, t<=2}

               if (s==3||s==4)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(d,s,t,v) {d==9, s==1, 2<=t, v<=1}

               while (1)

//  P(d,s,t,v) {d==9, s==1, 2<=t, v<=1}

                  ;

//  P(d,s,t,v) {d==9, s==1, v==0, 1<=t, t<=2}

            if (s==1&&d>=10)

//  P() {0==-1}

               checking_error();

//  P(d,s,t,v) {d==9, s==1, v==0, 1<=t, t<=2}

            if (s==1&&v>=3)

//  P() {0==-1}

               checking_error();
         }

//  P(d,s,t,v) {d==9, s==1, 0<=t, v<=1}

         if (s==1&&t==2) {

//  P(d,s,t,v) {d==9, s==1, t==2, v<=1}

            v = 0;

//  P(d,s,t,v) {d==9, s==1, t==2, v==0}

            t++;

//  P(d,s,t,v) {d==9, s==1, t==3, v==0}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {d==9, s==1, 0<=t, v<=1}

            while (1)

//  P(d,s,t,v) {d==9, s==1, 0<=t, v<=1}

               ;

//  P(d,s,t,v) {d==9, s==1, t==3, v==0}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==1, t==3, v==0}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, 0<=t, v<=1}

         if (s==1&&v==1&&d==8) {

//  P(d,s,t,v) {d==8, s==1, v==1, 0<=t}

            v++;

//  P(d,s,t,v) {d==8, s==1, v==2, 0<=t}

            d++;

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, d+v<=8, 0<=t, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, 0<=d, d<=8, v<=d, d+v<=8, 0<=t, v<=1}

               ;

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

         if (s==1&&t==2) {

//  P(d,s,t,v) {d==9, s==1, t==2, v==2}

            v = 0;

//  P(d,s,t,v) {d==9, s==1, t==2, v==0}

            t++;

//  P(d,s,t,v) {d==9, s==1, t==3, v==0}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

            while (1)

//  P(d,s,t,v) {d==9, s==1, v==2, 0<=t}

               ;

//  P(d,s,t,v) {d==9, s==1, t==3, v==0}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {d==9, s==1, t==3, v==0}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }

//  P(d,s,t,v) {d==9, s==1, t==3, v==0}

      if (s==1&&t>=3) {

//  P(d,s,t,v) {d==9, s==1, t==3, v==0}

         s = 2;

//  P(d,s,t,v) {d==9, s==2, t==3, v==0}

         if (s==3||s==4)

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;

//  P(d,s,t,v) {d==9, s==2, t==3, v==0}

      if (s==1&&d>=10)

//  P() {0==-1}

         checking_error();

//  P(d,s,t,v) {d==9, s==2, t==3, v==0}

      if (s==1&&v>=3)

//  P() {0==-1}

         checking_error();
   }
}

WITH TRANSFORMER LISTS
======================


TRANSFORMERS


//  T() {}

void run(void)
{

//  T(d,s,t,v) {}

   int s, d, v, t;

//  T(s) {s==1}

   s = 1;

//  T(d,t,v) {d==0, s==1, t==0, v==0}

   d = v = t = 0;

//  T() {d==0, s==1, t==0, v==0}

   if (s==1&&d>=10)

//  T() {0==-1}

      checking_error();

//  T() {d==0, s==1, t==0, v==0}

   if (s==1&&v>=3)

//  T() {0==-1}

      checking_error();

//  T(d,t,v) {s==1, 0<=d, d<=9, d#init<=d, d<=t+8, v<=d, d+v<=9, 0<=t,
//    t#init<=t, v<=1}

   while (flip())

//  T(d,t,v) {s==1, d<=d#init+1, 2d<=d#init+9, d#init+t#init+1<=d+t,
//    t<=t#init+1, t+v<=t#init+1}

      if (flip()) {

//  T(t,v) {s==1, t==t#init+1, v==0, t<=2}

         if (s==1&&t<=1) {

//  T(v) {s==1, v==0, t<=1}

            v = 0;

//  T(t) {s==1, t==t#init+1, v==0, t<=2}

            t++;

//  T() {s==1, v==0, t<=2}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 2<=t}

            while (1)

//  T() {s==1, 2<=t}

               ;

//  T() {s==1, v==0, d<=9, t<=2}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, v==0, d<=9, t<=2}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  T(d,v) {d==d#init+1, s==1, v==v#init+1, d<=8, v<=1}

         if (s==1&&v<=0&&d<=7) {

//  T(v) {s==1, v==v#init+1, d<=7, v<=1}

            v++;

//  T(d) {d==d#init+1, s==1, d<=8, v<=1}

            d++;

//  T() {s==1, d<=8, v<=1}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1}

            while (1)

//  T() {s==1}

               ;

//  T() {s==1, d<=8, v<=1}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, d<=8, v<=1}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else {

//  T(d,v) {d==d#init+1, s==1, v==2, v#init==1, d<=8}

         if (s==1&&v==1&&d<=7) {

//  T(v) {s==1, v==2, v#init==1, d<=7}

            v++;

//  T(d) {d==d#init+1, s==1, v==2, d<=8}

            d++;

//  T() {s==1, v==2, d<=8}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1}

            while (1)

//  T() {s==1}

               ;

//  T() {s==1, v==2, d<=8}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, v==2, d<=8}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(t,v) {s==1, t==t#init+1, v==0, v#init==2, d<=8, t<=2}

         if (s==1&&t<=1) {

//  T(v) {s==1, v==0, v#init==2, d<=8, t<=1}

            v = 0;

//  T(t) {s==1, t==t#init+1, v==0, d<=8, t<=2}

            t++;

//  T() {s==1, v==0, d<=8, t<=2}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, v==2, d<=8, 2<=t}

            while (1)

//  T() {s==1, v==2, d<=8, 2<=t}

               ;

//  T() {s==1, v==0, d<=8, t<=2}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, v==0, d<=8, t<=2}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }

//  T(d,s,t,v) {s#init==1, t==3, 2d<=d#init+s+2v+8,
//    2d#init+2v+2<=2d+t#init, d<=d#init+v+1, d<=v+9,
//    8<=d#init+4t#init, s<=2, 4<=2s+t#init, t#init<=2, 0<=v}

   if (flip()) {

//  T(d,t,v) {s==1, t==3, t#init==2, v==0, d#init<=d, d<=d#init+1,
//    2d<=d#init+9, 0<=d#init}

      if (flip()) {

//  T(d,v) {d==d#init+1, s==1, v==2, v#init==1, 1<=d, d<=8, 0<=t}

         if (s==1&&v==1&&d<=7) {

//  T(v) {s==1, v==2, v#init==1, 0<=d, d<=7, 0<=t}

            v++;

//  T(d) {d==d#init+1, s==1, v==2, 1<=d, d<=8, 0<=t}

            d++;

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 0<=d, 0<=t}

            while (1)

//  T() {s==1, 0<=d, 0<=t}

               ;

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(t,v) {s==1, t==3, t#init==2, v==0, v#init==2, 1<=d, d<=8}

         if (s==1&&t==2) {

//  T(v) {s==1, t==2, v==0, v#init==2, 1<=d, d<=8}

            v = 0;

//  T(t) {s==1, t==3, t#init==2, v==0, 1<=d, d<=8}

            t++;

//  T() {s==1, t==3, v==0, 1<=d, d<=8}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

            while (1)

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

               ;

//  T() {s==1, t==3, v==0, 1<=d, d<=8}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, t==3, v==0, 1<=d, d<=8}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else {

//  T(t,v) {s==1, t==3, t#init==2, v==0, 0<=d}

         if (s==1&&t==2) {

//  T(v) {s==1, t==2, v==0, 0<=d}

            v = 0;

//  T(t) {s==1, t==3, t#init==2, v==0, 0<=d}

            t++;

//  T() {s==1, t==3, v==0, 0<=d}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 0<=d, 0<=t}

            while (1)

//  T() {s==1, 0<=d, 0<=t}

               ;

//  T() {s==1, t==3, v==0, 0<=d, d<=9}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, t==3, v==0, 0<=d, d<=9}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }

//  T(d,v) {d+v#init==d#init+v, s==1, t==3, v<=d, d+v<=9, 0<=v, v<=1,
//    v#init<=v}

      while (flip()) {

//  T(d,v) {d==d#init+1, s==1, t==3, v==v#init+1, d<=8, v<=1}

         if (s==1&&v<=0&&d<=7) {

//  T(v) {s==1, t==3, v==v#init+1, d<=7, v<=1}

            v++;

//  T(d) {d==d#init+1, s==1, t==3, d<=8, v<=1}

            d++;

//  T() {s==1, t==3, d<=8, v<=1}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, t==3}

            while (1)

//  T() {s==1, t==3}

               ;

//  T() {s==1, t==3, d<=8, v<=1}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, t==3, d<=8, v<=1}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }

//  T(d,s,v) {d+v#init==d#init+v, s#init==1, t==3, v<=d,
//    9v<=d+16v#init, d<=v#init+9, s<=2, v+1<=s+v#init, v#init<=v,
//    0<=v#init}

      if (flip()) {

//  T(s) {s==2, s#init==1, t==3, v<=d, d<=v+9, 0<=v}

         if (s==1&&t>=3) {

//  T(s) {s==2, s#init==1, t==3, v<=d, d<=v+9, 0<=v}

            s = 2;

//  T() {s==2, t==3, v<=d, d<=v+9, 0<=v}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T() {s==2, t==3, v<=d, d<=v+9, 0<=v}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==2, t==3, v<=d, d<=v+9, 0<=v}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  T(d,v) {d==d#init+1, s==1, t==3, v==2, v#init==1, 2<=d, d<=8}

         if (s==1&&v==1&&d<=7) {

//  T(v) {s==1, t==3, v==2, v#init==1, 1<=d, d<=7}

            v++;

//  T(d) {d==d#init+1, s==1, t==3, v==2, 2<=d, d<=8}

            d++;

//  T() {s==1, t==3, v==2, 2<=d, d<=8}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

            while (1)

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

               ;

//  T() {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(s) {s==2, s#init==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&t>=3) {

//  T(s) {s==2, s#init==1, t==3, v==2, 2<=d, d<=8}

            s = 2;

//  T() {s==2, t==3, v==2, 2<=d, d<=8}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T() {s==2, t==3, v==2, 2<=d, d<=8}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==2, t==3, v==2, 2<=d, d<=8}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  T(d,v) {d==9, d#init==8, s==1, t==3, v==2, v#init==1}

         if (s==1&&v==1&&d==8) {

//  T(v) {d==8, s==1, t==3, v==2, v#init==1}

            v++;

//  T(d) {d==9, d#init==8, s==1, t==3, v==2}

            d++;

//  T() {d==9, s==1, t==3, v==2}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

            while (1)

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

               ;

//  T() {d==9, s==1, t==3, v==2}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==1, t==3, v==2}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(s) {d==9, s==2, s#init==1, t==3, v==2}

         if (s==1&&t>=3) {

//  T(s) {d==9, s==2, s#init==1, t==3, v==2}

            s = 2;

//  T() {d==9, s==2, t==3, v==2}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T() {d==9, s==2, t==3, v==2}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==2, t==3, v==2}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  T(d,v) {d==9, d#init==8, s==1, t==3, v==1, v#init==0}

         if (s==1&&v<=0&&d==8) {

//  T(v) {d==8, s==1, t==3, v==1, v#init==0}

            v++;

//  T(d) {d==9, d#init==8, s==1, t==3, v==1}

            d++;

//  T() {d==9, s==1, t==3, v==1}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

            while (1)

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

               ;

//  T() {d==9, s==1, t==3, v==1}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==1, t==3, v==1}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(s) {d==9, s==2, s#init==1, t==3, v==1}

         if (s==1&&t>=3) {

//  T(s) {d==9, s==2, s#init==1, t==3, v==1}

            s = 2;

//  T() {d==9, s==2, t==3, v==1}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T() {d==9, s==2, t==3, v==1}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==2, t==3, v==1}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
   }
   else {

//  T(d,t,v) {d==9, d#init==8, s==1, t==3, v==0, 0<=t#init, t#init<=2,
//    v#init<=1}

      if (flip()) {

//  T(d,t,v) {d==9, d#init==8, s==1, 2t+v==2t#init+v#init+1,
//    v#init+1<=2t+v, v#init<=v+1, v<=v#init+1, v+v#init<=1}

         if (flip()) {

//  T(d,v) {d==9, d#init==8, s==1, v==v#init+1, 0<=t, v<=1}

            if (s==1&&v<=0&&d==8) {

//  T(v) {d==8, s==1, v==v#init+1, 0<=t, v<=1}

               v++;

//  T(d) {d==9, d#init==8, s==1, 0<=t, v<=1}

               d++;

//  T() {d==9, s==1, 0<=t, v<=1}

               if (s==3||s==4)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {s==1, 0<=d, 0<=t}

               while (1)

//  T() {s==1, 0<=d, 0<=t}

                  ;

//  T() {d==9, s==1, 0<=t, v<=1}

            if (s==1&&d>=10)

//  T() {0==-1}

               checking_error();

//  T() {d==9, s==1, 0<=t, v<=1}

            if (s==1&&v>=3)

//  T() {0==-1}

               checking_error();
         }
         else {

//  T(d,v) {d==9, d#init==8, s==1, v==2, v#init==1, 0<=t}

            if (s==1&&v==1&&d==8) {

//  T(v) {d==8, s==1, v==2, v#init==1, 0<=t}

               v++;

//  T(d) {d==9, d#init==8, s==1, v==2, 0<=t}

               d++;

//  T() {d==9, s==1, v==2, 0<=t}

               if (s==3||s==4)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {s==1, 0<=d, 0<=t}

               while (1)

//  T() {s==1, 0<=d, 0<=t}

                  ;

//  T() {d==9, s==1, v==2, 0<=t}

            if (s==1&&d>=10)

//  T() {0==-1}

               checking_error();

//  T() {d==9, s==1, v==2, 0<=t}

            if (s==1&&v>=3)

//  T() {0==-1}

               checking_error();

//  T(t,v) {d==9, s==1, t==t#init+1, v==0, v#init==2, 1<=t, t<=2}

            if (s==1&&t<=1) {

//  T(v) {d==9, s==1, v==0, v#init==2, 0<=t, t<=1}

               v = 0;

//  T(t) {d==9, s==1, t==t#init+1, v==0, 1<=t, t<=2}

               t++;

//  T() {d==9, s==1, v==0, 1<=t, t<=2}

               if (s==3||s==4)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {d==9, s==1, v==2, 2<=t}

               while (1)

//  T() {d==9, s==1, v==2, 2<=t}

                  ;

//  T() {d==9, s==1, v==0, 1<=t, t<=2}

            if (s==1&&d>=10)

//  T() {0==-1}

               checking_error();

//  T() {d==9, s==1, v==0, 1<=t, t<=2}

            if (s==1&&v>=3)

//  T() {0==-1}

               checking_error();
         }

//  T(t,v) {d==9, s==1, 0<=t, t#init<=t, v<=1}

         while (flip()) {

//  T(t,v) {d==9, s==1, t==t#init+1, v==0, t<=2}

            if (s==1&&t<=1) {

//  T(v) {d==9, s==1, v==0, t<=1}

               v = 0;

//  T(t) {d==9, s==1, t==t#init+1, v==0, t<=2}

               t++;

//  T() {d==9, s==1, v==0, t<=2}

               if (s==3||s==4)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {d==9, s==1, 2<=t}

               while (1)

//  T() {d==9, s==1, 2<=t}

                  ;

//  T() {d==9, s==1, v==0, t<=2}

            if (s==1&&d>=10)

//  T() {0==-1}

               checking_error();

//  T() {d==9, s==1, v==0, t<=2}

            if (s==1&&v>=3)

//  T() {0==-1}

               checking_error();
         }

//  T(t,v) {d==9, s==1, t==3, t#init==2, v==0}

         if (s==1&&t==2) {

//  T(v) {d==9, s==1, t==2, v==0}

            v = 0;

//  T(t) {d==9, s==1, t==3, t#init==2, v==0}

            t++;

//  T() {d==9, s==1, t==3, v==0}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {d==9, s==1, 0<=t}

            while (1)

//  T() {d==9, s==1, 0<=t}

               ;

//  T() {d==9, s==1, t==3, v==0}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==1, t==3, v==0}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else {

//  T(d,v) {d==9, d#init==8, s==1, v==2, v#init==1, 0<=t}

         if (s==1&&v==1&&d==8) {

//  T(v) {d==8, s==1, v==2, v#init==1, 0<=t}

            v++;

//  T(d) {d==9, d#init==8, s==1, v==2, 0<=t}

            d++;

//  T() {d==9, s==1, v==2, 0<=t}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 0<=d, 0<=t}

            while (1)

//  T() {s==1, 0<=d, 0<=t}

               ;

//  T() {d==9, s==1, v==2, 0<=t}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==1, v==2, 0<=t}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(t,v) {d==9, s==1, t==3, t#init==2, v==0, v#init==2}

         if (s==1&&t==2) {

//  T(v) {d==9, s==1, t==2, v==0, v#init==2}

            v = 0;

//  T(t) {d==9, s==1, t==3, t#init==2, v==0}

            t++;

//  T() {d==9, s==1, t==3, v==0}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {d==9, s==1, v==2, 0<=t}

            while (1)

//  T() {d==9, s==1, v==2, 0<=t}

               ;

//  T() {d==9, s==1, t==3, v==0}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==1, t==3, v==0}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }

//  T(s) {d==9, s==2, s#init==1, t==3, v==0}

      if (s==1&&t>=3) {

//  T(s) {d==9, s==2, s#init==1, t==3, v==0}

         s = 2;

//  T() {d==9, s==2, t==3, v==0}

         if (s==3||s==4)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0==-1}

         while (1)

//  T() {0==-1}

            ;

//  T() {d==9, s==2, t==3, v==0}

      if (s==1&&d>=10)

//  T() {0==-1}

         checking_error();

//  T() {d==9, s==2, t==3, v==0}

      if (s==1&&v>=3)

//  T() {0==-1}

         checking_error();
   }
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int s, d, v, t;

//  P(d,s,t,v) {}

   s = 1;

//  P(d,s,t,v) {s==1}

   d = v = t = 0;

//  P(d,s,t,v) {d==0, s==1, t==0, v==0}

   if (s==1&&d>=10)

//  P() {0==-1}

      checking_error();

//  P(d,s,t,v) {d==0, s==1, t==0, v==0}

   if (s==1&&v>=3)

//  P() {0==-1}

      checking_error();

//  P(d,s,t,v) {d==0, s==1, t==0, v==0}

   while (flip())

//  P(d,s,t,v) {s==1, 0<=d, d<=8, d<=2t+v, v<=d, 0<=t, t<=2, 0<=v,
//    v<=1}

      if (flip()) {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, d<=2t+v, v<=d, 0<=t, t<=2, 0<=v,
//    v<=1}

         if (s==1&&t<=1) {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, d<=2t+v, v<=d, 0<=t, t<=1, 0<=v,
//    v<=1}

            v = 0;

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=8, d<=2t+1, 0<=t, t<=1}

            t++;

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=8, d+1<=2t, 1<=t, t<=2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, t==2, 0<=d, d<=8, v<=d, d<=v+4, 0<=v, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, t==2, 0<=d, d<=8, v<=d, d<=v+4, 0<=v, v<=1}

               ;

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=8, d+1<=2t, 1<=t, t<=2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=8, d+1<=2t, 1<=t, t<=2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, d<=2t+v, v<=d, 0<=t, t<=2, 0<=v,
//    v<=1}

         if (s==1&&v<=0&&d<=7) {

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=7, d<=2t, 0<=t, t<=2}

            v++;

//  P(d,s,t,v) {s==1, v==1, 0<=d, d<=7, d<=2t, 0<=t, t<=2}

            d++;

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=8, d<=2t+1, 0<=t, t<=2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=8, d<=2t+1, 0<=t, t<=2}

            while (1)

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=8, d<=2t+1, 0<=t, t<=2}

               ;

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=8, d<=2t+1, 0<=t, t<=2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=8, d<=2t+1, 0<=t, t<=2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(d,s,t,v) {s==1, 0<=d, d<=8, d<=2t+v, v<=d, 0<=t, t<=2, 0<=v,
//    v<=1}

         if (s==1&&v==1&&d<=7) {

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=7, d<=2t+1, 0<=t, t<=2}

            v++;

//  P(d,s,t,v) {s==1, v==2, 1<=d, d<=7, d<=2t+1, 0<=t, t<=2}

            d++;

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=8, d<=2t, 0<=t, t<=2}

            while (1)

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=8, d<=2t, 0<=t, t<=2}

               ;

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=2}

         if (s==1&&t<=1) {

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=1}

            v = 0;

//  P(d,s,t,v) {s==1, v==0, 2<=d, d<=8, d<=2t+2, 0<=t, t<=1}

            t++;

//  P(d,s,t,v) {s==1, v==0, 2<=d, d<=8, d<=2t, 1<=t, t<=2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, t==2, v==2, 2<=d, d<=6}

            while (1)

//  P(d,s,t,v) {s==1, t==2, v==2, 2<=d, d<=6}

               ;

//  P(d,s,t,v) {s==1, v==0, 2<=d, d<=8, d<=2t, 1<=t, t<=2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==0, 2<=d, d<=8, d<=2t, 1<=t, t<=2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

   if (flip()) {

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

      if (flip()) {

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

         if (s==1&&v==1&&d<=7) {

//  P(d,s,t,v) {s==1, v==1, 1<=d, d<=7, d<=2t+1, t<=2}

            v++;

//  P(d,s,t,v) {s==1, v==2, 1<=d, d<=7, d<=2t+1, 0<=t, t<=2}

            d++;

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=2}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=2t, t<=2}

            while (1)

//  P(d,s,t,v) {s==1, v==0, 0<=d, d<=2t, t<=2}

               ;

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=2}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=2}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=2}

         if (s==1&&t==2) {

//  P(d,s,t,v) {s==1, t==2, v==2, 2<=d, d<=6}

            v = 0;

//  P(d,s,t,v) {s==1, t==2, v==0, 2<=d, d<=6}

            t++;

//  P(d,s,t,v) {s==1, t==3, v==0, 2<=d, d<=6}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=1}

            while (1)

//  P(d,s,t,v) {s==1, v==2, 2<=d, d<=8, d<=2t+2, 0<=t, t<=1}

               ;

//  P(d,s,t,v) {s==1, t==3, v==0, 2<=d, d<=6}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==0, 2<=d, d<=6}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

         if (s==1&&t==2) {

//  P(d,s,t,v) {s==1, t==2, v<=d, d<=v+4, 0<=v, v<=1}

            v = 0;

//  P(d,s,t,v) {s==1, t==2, v==0, 0<=d, d<=5}

            t++;

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=5}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=1, 0<=v, v<=1}

            while (1)

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=1, 0<=v, v<=1}

               ;

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=5}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=5}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=6}

      while (flip()) {

//  P(d,s,t,v) {s==1, t==3, v<=d, d<=v+6, 0<=v, v<=1}

         if (s==1&&v<=0&&d<=7) {

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=6}

            v++;

//  P(d,s,t,v) {s==1, t==3, v==1, 0<=d, d<=6}

            d++;

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=7}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=7}

            while (1)

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=7}

               ;

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=7}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=7}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }

//  P(d,s,t,v) {s==1, t==3, v<=d, d<=v+6, 0<=v, v<=1}

      if (flip()) {

//  P(d,s,t,v) {s==1, t==3, v<=d, d<=v+6, 0<=v, v<=1}

         if (s==1&&t>=3) {

//  P(d,s,t,v) {s==1, t==3, v<=d, d<=v+6, 0<=v, v<=1}

            s = 2;

//  P(d,s,t,v) {s==2, t==3, v<=d, d<=v+6, 0<=v, v<=1}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P(d,s,t,v) {s==2, t==3, v<=d, d<=v+6, 0<=v, v<=1}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==2, t==3, v<=d, d<=v+6, 0<=v, v<=1}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  P(d,s,t,v) {s==1, t==3, v<=d, d<=v+6, 0<=v, v<=1}

         if (s==1&&v==1&&d<=7) {

//  P(d,s,t,v) {s==1, t==3, v==1, 1<=d, d<=7}

            v++;

//  P(d,s,t,v) {s==1, t==3, v==2, 1<=d, d<=7}

            d++;

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

            if (s==3||s==4)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=6}

            while (1)

//  P(d,s,t,v) {s==1, t==3, v==0, 0<=d, d<=6}

               ;

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

         if (s==1&&t>=3) {

//  P(d,s,t,v) {s==1, t==3, v==2, 2<=d, d<=8}

            s = 2;

//  P(d,s,t,v) {s==2, t==3, v==2, 2<=d, d<=8}

