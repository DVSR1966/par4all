               ;

//  P() {0==-1}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P() {0==-1}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();

//  P() {0==-1}

         if (s==1&&t>=3) {

//  P() {0==-1}

            s = 2;

//  P() {0==-1}

            if (s==3||s==4)

