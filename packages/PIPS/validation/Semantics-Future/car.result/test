//  T(s) {s==2, s#init==1, t==3, v==2, 2<=d, d<=8}

            s = 2;

//  T() {s==2, t==3, v==2, 2<=d, d<=8}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T() {s==2, t==3, v==2, 2<=d, d<=8}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==2, t==3, v==2, 2<=d, d<=8}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  T(d,v) {d==9, d#init==8, s==1, t==3, v==2, v#init==1}

         if (s==1&&v==1&&d==8) {

//  T(v) {d==8, s==1, t==3, v==2, v#init==1}

            v++;

//  T(d) {d==9, d#init==8, s==1, t==3, v==2}

            d++;

//  T() {d==9, s==1, t==3, v==2}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

            while (1)

//  T() {s==1, t==3, v<=d, d<=v+9, 0<=v}

               ;

//  T() {d==9, s==1, t==3, v==2}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {d==9, s==1, t==3, v==2}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(s) {d==9, s==2, s#init==1, t==3, v==2}

         if (s==1&&t>=3) {

//  T(s) {d==9, s==2, s#init==1, t==3, v==2}

            s = 2;

//  T() {d==9, s==2, t==3, v==2}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T() {d==9, s==2, t==3, v==2}

         if (s==1&&d>=10)

//  T() {0==-1}

