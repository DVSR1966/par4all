//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

            while (1)

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

               ;

//  T() {s==1, t==3, v==0, 1<=d, d<=8}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, t==3, v==0, 1<=d, d<=8}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else {

//  T(t,v) {s==1, t==3, t#init==2, v==0, 0<=d}

         if (s==1&&t==2) {

//  T(v) {s==1, t==2, v==0, 0<=d}

            v = 0;

//  T(t) {s==1, t==3, t#init==2, v==0, 0<=d}

