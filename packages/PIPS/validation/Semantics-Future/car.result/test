
WITHOUT TRANSFORMER LISTS
=========================


TRANSFORMERS


//  T() {}

void run(void)
{

//  T(d,s,t,v) {}

   int s, d, v, t;

//  T(s) {s==1}

   s = 1;

//  T(d,t,v) {d==0, s==1, t==0, v==0}

   d = v = t = 0;

//  T() {d==0, s==1, t==0, v==0}

   if (s==1&&d>=10)

//  T() {0==-1}

      checking_error();

//  T() {d==0, s==1, t==0, v==0}

   if (s==1&&v>=3)

//  T() {0==-1}

      checking_error();

//  T(d,t,v) {s==1, 0<=d, d<=9, d#init<=d, d<=t+8, v<=d, d+v<=9, 0<=t,
//    t#init<=t, v<=1}

   while (flip())

//  T(d,t,v) {s==1, d<=d#init+1, 2d<=d#init+9, d#init+t#init+1<=d+t,
//    t<=t#init+1, t+v<=t#init+1}

      if (flip()) {

//  T(t,v) {s==1, t==t#init+1, v==0, t<=2}

         if (s==1&&t<=1) {

//  T(v) {s==1, v==0, t<=1}

            v = 0;

//  T(t) {s==1, t==t#init+1, v==0, t<=2}

            t++;

//  T() {s==1, v==0, t<=2}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 2<=t}

            while (1)

//  T() {s==1, 2<=t}

               ;

//  T() {s==1, v==0, d<=9, t<=2}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, v==0, d<=9, t<=2}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  T(d,v) {d==d#init+1, s==1, v==v#init+1, d<=8, v<=1}

         if (s==1&&v<=0&&d<=7) {

//  T(v) {s==1, v==v#init+1, d<=7, v<=1}

            v++;

//  T(d) {d==d#init+1, s==1, d<=8, v<=1}

            d++;

//  T() {s==1, d<=8, v<=1}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1}

            while (1)

//  T() {s==1}

               ;

//  T() {s==1, d<=8, v<=1}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, d<=8, v<=1}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
      else {

//  T(d,v) {d==d#init+1, s==1, v==2, v#init==1, d<=8}

         if (s==1&&v==1&&d<=7) {

//  T(v) {s==1, v==2, v#init==1, d<=7}

            v++;

//  T(d) {d==d#init+1, s==1, v==2, d<=8}

            d++;

//  T() {s==1, v==2, d<=8}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1}

            while (1)

//  T() {s==1}

               ;

//  T() {s==1, v==2, d<=8}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, v==2, d<=8}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(t,v) {s==1, t==t#init+1, v==0, v#init==2, d<=8, t<=2}

         if (s==1&&t<=1) {

//  T(v) {s==1, v==0, v#init==2, d<=8, t<=1}

            v = 0;

//  T(t) {s==1, t==t#init+1, v==0, d<=8, t<=2}

            t++;

//  T() {s==1, v==0, d<=8, t<=2}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, v==2, d<=8, 2<=t}

            while (1)

//  T() {s==1, v==2, d<=8, 2<=t}

               ;

//  T() {s==1, v==0, d<=8, t<=2}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, v==0, d<=8, t<=2}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }

//  T(d,s,t,v) {s#init==1, t==3, 2d<=d#init+s+2v+8,
//    2d#init+2v+2<=2d+t#init, d<=d#init+v+1, d<=v+9,
//    8<=d#init+4t#init, s<=2, 4<=2s+t#init, t#init<=2, 0<=v}

   if (flip()) {

//  T(d,t,v) {s==1, t==3, t#init==2, v==0, d#init<=d, d<=d#init+1,
//    2d<=d#init+9, 0<=d#init}

      if (flip()) {

//  T(d,v) {d==d#init+1, s==1, v==2, v#init==1, 1<=d, d<=8, 0<=t}

         if (s==1&&v==1&&d<=7) {

//  T(v) {s==1, v==2, v#init==1, 0<=d, d<=7, 0<=t}

            v++;

//  T(d) {d==d#init+1, s==1, v==2, 1<=d, d<=8, 0<=t}

            d++;

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 0<=d, 0<=t}

            while (1)

//  T() {s==1, 0<=d, 0<=t}

               ;

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, v==2, 1<=d, d<=8, 0<=t}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();

//  T(t,v) {s==1, t==3, t#init==2, v==0, v#init==2, 1<=d, d<=8}

         if (s==1&&t==2) {

//  T(v) {s==1, t==2, v==0, v#init==2, 1<=d, d<=8}

            v = 0;

//  T(t) {s==1, t==3, t#init==2, v==0, 1<=d, d<=8}

            t++;

//  T() {s==1, t==3, v==0, 1<=d, d<=8}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
