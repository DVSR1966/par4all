               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;

//  P() {0==-1}

         if (s==1&&d>=10)

//  P() {0==-1}

            checking_error();

//  P() {0==-1}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
      else if (flip()) {

//  P(d,s,t,v) {s==1, t==3, v<=d, d<=v+6, 0<=v, v<=1}

         if (s==1&&v<=0&&d==8) {

//  P() {0==-1}

            v++;

//  P() {0==-1}

            d++;

//  P() {0==-1}

            if (s==3||s==4)

