//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 0<=d, 0<=t}

            while (1)

//  T() {s==1, 0<=d, 0<=t}

               ;

//  T() {s==1, t==3, v==0, 0<=d, d<=9}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, t==3, v==0, 0<=d, d<=9}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }

//  T(d,v) {d+v#init==d#init+v, s==1, t==3, v<=d, d+v<=9, 0<=v, v<=1,
//    v#init<=v}

      while (flip()) {

//  T(d,v) {d==d#init+1, s==1, t==3, v==v#init+1, d<=8, v<=1}

         if (s==1&&v<=0&&d<=7) {

//  T(v) {s==1, t==3, v==v#init+1, d<=7, v<=1}

            v++;

//  T(d) {d==d#init+1, s==1, t==3, d<=8, v<=1}

            d++;

//  T() {s==1, t==3, d<=8, v<=1}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, t==3}

            while (1)

//  T() {s==1, t==3}

               ;

//  T() {s==1, t==3, d<=8, v<=1}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==1, t==3, d<=8, v<=1}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }

//  T(d,s,v) {d+v#init==d#init+v, s#init==1, t==3, v<=d,
//    9v<=d+16v#init, d<=v#init+9, s<=2, v+1<=s+v#init, v#init<=v,
//    0<=v#init}

      if (flip()) {

//  T(s) {s==2, s#init==1, t==3, v<=d, d<=v+9, 0<=v}

         if (s==1&&t>=3) {

//  T(s) {s==2, s#init==1, t==3, v<=d, d<=v+9, 0<=v}

            s = 2;

//  T() {s==2, t==3, v<=d, d<=v+9, 0<=v}

            if (s==3||s==4)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;

//  T() {s==2, t==3, v<=d, d<=v+9, 0<=v}

         if (s==1&&d>=10)

//  T() {0==-1}

            checking_error();

//  T() {s==2, t==3, v<=d, d<=v+9, 0<=v}

         if (s==1&&v>=3)

//  T() {0==-1}

            checking_error();
      }
