            checking_error();

//  P() {0==-1}

         if (s==1&&v>=3)

//  P() {0==-1}

            checking_error();
      }
   }
   else {

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

      if (flip()) {

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

         if (flip()) {

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

            if (s==1&&v<=0&&d==8) {

//  P() {0==-1}

               v++;

//  P() {0==-1}

               d++;

//  P() {0==-1}

               if (s==3||s==4)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

               while (1)

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

                  ;

//  P() {0==-1}

            if (s==1&&d>=10)

//  P() {0==-1}

               checking_error();

//  P() {0==-1}

            if (s==1&&v>=3)

//  P() {0==-1}

               checking_error();
         }
         else {

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

            if (s==1&&v==1&&d==8) {

//  P() {0==-1}

               v++;

//  P() {0==-1}

               d++;

//  P() {0==-1}

               if (s==3||s==4)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

               while (1)

//  P(d,s,t,v) {s==1, d<=2t+v, v<=d, t<=2, 0<=v, v<=1}

                  ;

//  P() {0==-1}

            if (s==1&&d>=10)

//  P() {0==-1}

               checking_error();

//  P() {0==-1}

            if (s==1&&v>=3)

//  P() {0==-1}

               checking_error();

//  P() {0==-1}

            if (s==1&&t<=1) {

//  P() {0==-1}

               v = 0;

//  P() {0==-1}

               t++;

//  P() {0==-1}

               if (s==3||s==4)

