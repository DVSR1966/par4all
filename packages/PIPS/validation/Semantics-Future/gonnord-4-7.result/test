         while (1)

//  P(i,j,k) {2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

            ;

//  P() {0==-1}

      if (i<=100&&j==9&&i>98) {

//  P() {0==-1}

         i += 2;

//  P() {0==-1}

         k += 2;

//  P() {0==-1}

         j = 0;

//  P() {0==-1}

         if (20*k>11*i)

