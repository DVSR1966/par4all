
TRANSFORMERS


//  T() {}

void run(void)
{

//  T(i,j,k) {}

   int i, j, k;

//  T(i,j,k) {i==0, j==0, k==0}

   i = j = k = 0;

//  T(i,j,k) {i<=102, 2i#init+j#init<=2i+j, i#init+2j<=i+2j#init,
//    11i#init+2j+20k<=11i+2j#init+20k#init,
//    i+2j#init+10k#init<=i#init+2j+10k, 0<=2i+j, 2j<=i, 4i+j<=408,
//    2j+20k<=11i, 2j+20k<=i+1000, i<=2j+10k, 20k<=11i, j<=8}

   while (flip())

//  T(i,j,k) {9i+2j==9i#init+2j#init+20, i#init+2<=i, i<=i#init+4,
//    4i+j<=4i#init+16, i#init+k+1<=i+k#init,
//    2i#init+2k<=i+2k#init+98, i+k#init<=i#init+k+2, 20k<=11i,
//    i#init<=98}

      if (flip())

//  T(i,j,k) {i==i#init+2, j==j#init+1, k==k#init+1, i<=100, 20k<=11i,
//    j<=8}

         if (i<=98&&j<8) {

//  T(i) {i==i#init+2, i<=100, j<=7}

            i += 2;

//  T(k) {k==k#init+1, i<=100, j<=7}

            k++;

//  T(j) {j==j#init+1, i<=100, j<=8}

            j++;

//  T() {i<=100, 20k<=11i, j<=8}

            if (20*k>11*i)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;
      else if (flip())

//  T(i,j) {i==i#init+2, j==j#init+1, i<=100, 20k<=11i, j<=8}

         if (i<=98&&j<8) {

//  T(i) {i==i#init+2, i<=100, j<=7}

            i += 2;

//  T(j) {j==j#init+1, i<=100, j<=8}

            j++;

//  T() {i<=100, 20k<=11i, j<=8}

            if (20*k>11*i)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;
      else if (flip()) {

//  T(i,j,k) {i==i#init+2, j==9, j#init==8, k==k#init+1, i<=100,
//    20k<=11i}

         if (i<=98&&j==8) {

//  T(i) {i==i#init+2, j==8, i<=100}

            i += 2;

//  T(k) {j==8, k==k#init+1, i<=100}

            k++;

//  T(j) {j==9, j#init==8, i<=100}

            j++;

//  T() {j==9, i<=100, 20k<=11i}

            if (20*k>11*i)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;

//  T(i,j,k) {i==i#init+2, j==0, j#init==9, k==k#init+2, i<=100,
//    20k<=11i}

         if (i<=98&&j==9) {

//  T(i) {i==i#init+2, j==9, i<=100, 20k+22<=11i}

            i += 2;

//  T(k) {j==9, k==k#init+2, i<=100, 20k<=11i+18}

            k += 2;

//  T(j) {j==0, j#init==9, i<=100, 20k<=11i+18}

            j = 0;

//  T() {j==0, i<=100, 20k<=11i}

            if (20*k>11*i)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {j==9, 99<=i, i<=100, 20k<=11i}

            while (1)

//  T() {j==9, 99<=i, i<=100, 20k<=11i}

               ;
      }
      else {

//  T(i,j) {i==i#init+2, j==9, j#init==8, i<=100, 20k<=11i}

         if (i<=98&&j==8) {

//  T(i) {i==i#init+2, j==8, i<=100}

            i += 2;

//  T(j) {j==9, j#init==8, i<=100}

            j++;

//  T() {j==9, i<=100, 20k<=11i}

            if (20*k>11*i)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;

//  T(i,j,k) {i==i#init+2, j==0, j#init==9, k==k#init+2, i<=102,
//    20k<=11i}

         if (i<=100&&j==9) {

//  T(i) {i==i#init+2, j==9, i<=102, 20k+22<=11i}

            i += 2;

//  T(k) {j==9, k==k#init+2, i<=102, 20k<=11i+18}

            k += 2;

//  T(j) {j==0, j#init==9, i<=102, 20k<=11i+18}

            j = 0;

//  T() {j==0, i<=102, 20k<=11i}

            if (20*k>11*i)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;
      }

//  T(i,j,k) {9i+2j==9i#init+2j#init+20, 101<=i, i<=102, i#init+2<=i,
//    9i+2j<=9i#init+36, 193i<=197i#init+2j+384,
//    199i<=199i#init+2j+796, 10i#init+20<=9i+2j+10k#init,
//    9i+2j+20k#init<=20i#init+20, i#init+k+1<=i+k#init,
//    i+k#init<=i#init+k+2, 19i+20k#init<=30i#init+60, 20k<=11i}

   if (flip())

//  T(i,j,k) {i==i#init+2, j==j#init+1, k==k#init+1, 101<=i, i<=102,
//    5<=2i+j, 2j<=i, 2j+20k<=11i, i+10<=2j+10k, 20k<=11i, j<=9}

      if (i<=100&&j<9&&i>98) {

//  T(i) {i==i#init+2, 101<=i, i<=102, 4<=2i+j, 2j+2<=i,
//    2j+20k+22<=11i, i<=2j+10k+2, j<=8}

         i += 2;

//  T(k) {k==k#init+1, 101<=i, i<=102, 4<=2i+j, 2j+2<=i,
//    2j+20k+2<=11i, i+8<=2j+10k, j<=8}

         k++;

//  T(j) {j==j#init+1, 101<=i, i<=102, 5<=2i+j, 2j<=i, 2j+20k<=11i,
//    i+10<=2j+10k, j<=9}

         j++;

//  T() {101<=i, i<=102, 5<=2i+j, 2j<=i, 2j+20k<=11i, i+10<=2j+10k,
//    20k<=11i, j<=9}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k}

         while (1)

//  T() {0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k}

            ;
   else if (flip())

//  T(i,j) {i==i#init+2, j==j#init+1, 101<=i, i<=102, 5<=2i+j, 2j<=i,
//    2j+20k+20<=11i, i<=2j+10k, 20k<=11i, j<=9}

      if (i<=100&&j<9&&i>98) {

//  T(i) {i==i#init+2, 101<=i, i<=102, 4<=2i+j, 2j+2<=i,
//    2j+20k+22<=11i, i<=2j+10k+2, j<=8}

         i += 2;

//  T(j) {j==j#init+1, 101<=i, i<=102, 5<=2i+j, 2j<=i, 2j+20k+20<=11i,
//    i<=2j+10k, j<=9}

         j++;

//  T() {101<=i, i<=102, 5<=2i+j, 2j<=i, 2j+20k+20<=11i, i<=2j+10k,
//    20k<=11i, j<=9}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k}

         while (1)

//  T() {0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k}

            ;
   else if (flip()) {

//  T(i,j,k) {i==i#init+2, j==9, j#init==8, k==k#init+1, 18<=i,
//    i<=100, 20k+18<=11i, i<=10k+8}

      if (i<=98&&j==8) {

//  T(i) {i==i#init+2, j==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

         i += 2;

//  T(k) {j==8, k==k#init+1, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

         k++;

//  T(j) {j==9, j#init==8, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

         j++;

//  T() {j==9, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k}

         while (1)

//  T() {0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k}

            ;

//  T(i,j,k) {i==i#init+2, j==0, j#init==9, k==k#init+2, 101<=i,
//    i<=102, 20k<=11i, i+10<=10k}

      if (i<=100&&j==9&&i>98) {

//  T(i) {i==i#init+2, j==9, 101<=i, i<=102, 20k+40<=11i, i<=10k+10}

         i += 2;

//  T(k) {j==9, k==k#init+2, 101<=i, i<=102, 20k<=11i, i+10<=10k}

         k += 2;

//  T(j) {j==0, j#init==9, 101<=i, i<=102, 20k<=11i, i+10<=10k}

         j = 0;

//  T() {j==0, 101<=i, i<=102, 20k<=11i, i+10<=10k}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {j==9, 18<=i, i<=98, 20k+18<=11i, i<=10k+8}

         while (1)

//  T() {j==9, 18<=i, i<=98, 20k+18<=11i, i<=10k+8}

            ;
   }
   else {

//  T(i,j) {i==i#init+2, j==9, j#init==8, 18<=i, i<=100, 20k+38<=11i,
//    i<=10k+18}

      if (i<=98&&j==8) {

//  T(i) {i==i#init+2, j==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

         i += 2;

//  T(j) {j==9, j#init==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

         j++;

//  T() {j==9, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k}

         while (1)

//  T() {0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k}

            ;

//  T(i,j,k) {i==i#init+2, j==0, j#init==9, k==k#init+2, 101<=i,
//    i<=102, 20k+20<=11i, i<=10k}

      if (i<=100&&j==9&&i>98) {

//  T(i) {i==i#init+2, j==9, 101<=i, i<=102, 20k+60<=11i, i<=10k+20}

         i += 2;

//  T(k) {j==9, k==k#init+2, 101<=i, i<=102, 20k+20<=11i, i<=10k}

         k += 2;

//  T(j) {j==0, j#init==9, 101<=i, i<=102, 20k+20<=11i, i<=10k}

         j = 0;

//  T() {j==0, 101<=i, i<=102, 20k+20<=11i, i<=10k}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {j==9, 18<=i, i<=98, 20k+38<=11i, i<=10k+18}

         while (1)

//  T() {j==9, 18<=i, i<=98, 20k+38<=11i, i<=10k+18}

            ;
   }
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int i, j, k;

//  P(i,j,k) {}

   i = j = k = 0;

//  P(i,j,k) {i==0, j==0, k==0}

   while (flip())

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

      if (flip())

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

         if (i<=98&&j<8) {

//  P(i,j,k) {j==0, i<=98, 1020k<=551i, i<=10k}

            i += 2;

//  P(i,j,k) {j==0, i<=100, 1020k+1102<=551i, i<=10k+2}

            k++;

//  P(i,j,k) {j==0, i<=100, 1020k+82<=551i, i+8<=10k}

            j++;

//  P(i,j,k) {j==1, i<=100, 1020k+82<=551i, i+8<=10k}

            if (20*k>11*i)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,j,k) {j==0, 99<=i, i<=102, 1020k<=551i, i<=10k}

            while (1)

//  P(i,j,k) {j==0, 99<=i, i<=102, 1020k<=551i, i<=10k}

               ;
      else if (flip())

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

         if (i<=98&&j<8) {

//  P(i,j,k) {j==0, i<=98, 1020k<=551i, i<=10k}

            i += 2;

//  P(i,j,k) {j==0, i<=100, 1020k+1102<=551i, i<=10k+2}

            j++;

//  P(i,j,k) {j==1, i<=100, 1020k+1102<=551i, i<=10k+2}

            if (20*k>11*i)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,j,k) {j==0, 99<=i, i<=102, 1020k<=551i, i<=10k}

            while (1)

//  P(i,j,k) {j==0, 99<=i, i<=102, 1020k<=551i, i<=10k}

               ;
      else if (flip()) {

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

         if (i<=98&&j==8) {

//  P() {0==-1}

            i += 2;

//  P() {0==-1}

            k++;

//  P() {0==-1}

            j++;

//  P() {0==-1}

            if (20*k>11*i)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

            while (1)

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

               ;

//  P() {0==-1}

         if (i<=98&&j==9) {

//  P() {0==-1}

            i += 2;

//  P() {0==-1}

            k += 2;

//  P() {0==-1}

            j = 0;

//  P() {0==-1}

            if (20*k>11*i)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
      }
      else {

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

         if (i<=98&&j==8) {

//  P() {0==-1}

            i += 2;

//  P() {0==-1}

            j++;

//  P() {0==-1}

            if (20*k>11*i)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

            while (1)

//  P(i,j,k) {j==0, i<=102, 1020k<=551i, i<=10k}

               ;

//  P() {0==-1}

         if (i<=100&&j==9) {

//  P() {0==-1}

            i += 2;

//  P() {0==-1}

            k += 2;

//  P() {0==-1}

            j = 0;

//  P() {0==-1}

            if (20*k>11*i)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
      }

//  P(i,j,k) {2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

   if (flip())

//  P(i,j,k) {2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

      if (i<=100&&j<9&&i>98) {

//  P(i,j,k) {99<=i, i<=100, 2j<=i, i<=100j, 82j+1020k<=551i,
//    i<=2j+10k, j<=1}

         i += 2;

//  P(i,j,k) {101<=i, i<=102, 4<=2i+j, 2j+2<=i, i<=100j+2,
//    82j+1020k+1102<=551i, 2j+20k+22<=11i, i<=2j+10k+2, j<=1}

         k++;

//  P(i,j,k) {101<=i, i<=102, 4<=2i+j, 2j+2<=i, i<=100j+2,
//    82j+1020k+82<=551i, 2j+20k+2<=11i, i+8<=2j+10k, j<=1}

         j++;

//  P(i,j,k) {101<=i, i<=102, 5<=2i+j, 2j<=i, i+98<=100j,
//    82j+1020k<=551i, 2j+20k<=11i, i+10<=2j+10k, j<=2}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
      else

//  P(i,j,k) {i<=98, 2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

         while (1)

//  P(i,j,k) {i<=98, 2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

            ;
   else if (flip())

//  P(i,j,k) {2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

      if (i<=100&&j<9&&i>98) {

//  P(i,j,k) {99<=i, i<=100, 2j<=i, i<=100j, 82j+1020k<=551i,
//    i<=2j+10k, j<=1}

         i += 2;

//  P(i,j,k) {101<=i, i<=102, 4<=2i+j, 2j+2<=i, i<=100j+2,
//    82j+1020k+1102<=551i, 2j+20k+22<=11i, i<=2j+10k+2, j<=1}

         j++;

//  P(i,j,k) {101<=i, i<=102, 5<=2i+j, 2j<=i, i+98<=100j,
//    82j+1020k+1020<=551i, 2j+20k+20<=11i, i<=2j+10k, j<=2}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
      else

//  P(i,j,k) {i<=98, 2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

         while (1)

//  P(i,j,k) {i<=98, 2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

