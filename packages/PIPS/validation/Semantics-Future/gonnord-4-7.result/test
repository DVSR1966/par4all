            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;
   }
   else {

//  P(i,j,k) {2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

      if (i<=98&&j==8) {

//  P() {0==-1}

         i += 2;

//  P() {0==-1}

         j++;

//  P() {0==-1}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
      else

//  P(i,j,k) {2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

         while (1)

//  P(i,j,k) {2j<=i, i<=100j, 82j+1020k<=551i, i<=2j+10k, j<=1}

            ;

