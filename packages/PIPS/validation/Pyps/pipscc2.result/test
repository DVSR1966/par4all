int foo(int a)
{
   return a;
}
void bar(int *c)
{
   *c = foo(2);
}
void megablast(int n)
{
   int i, j;
   for(i = 0; i <= n-1; i += 1)
      j = foo(i);
}
int foo(int a);

void bar(int *c);
void malabar(int *c);

void megablast(int n);
int main();
int main()
{
   return 1;
}
