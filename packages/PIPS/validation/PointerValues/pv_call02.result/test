void foo(int a)
{

// Pointer values: none

   printf("%d\n", a);
}
int main()
{

// Pointer values: none

   int a = 1, *p;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   p = &a;

// Pointer values:
// p == &a (exact);

   foo(*p);

// Pointer values:
// p == &a (exact);

   foo(a);

// Pointer values:
// p == &a (exact);

   return 0;
}
