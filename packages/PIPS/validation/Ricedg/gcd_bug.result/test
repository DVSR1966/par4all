// OMP style

int main(void)
{
   int i, j;                                                            /*0010*/

   Matrix = (int **) malloc(N*sizeof(int *));                           /*0012*/
   for(i = 0; i <= N-1; i += 1) {
      Matrix[i] = (int *) malloc(Z*sizeof(int));                        /*0015*/
#pragma omp parallel for 
      for(j = 0; j <= Z-1; j += 1)
         Matrix[i][j] = i*j;                                            /*0018*/
   }
   return 0;                                                            /*0021*/
}

