
Points-to


Transformers with point-to


//  T() {}

void memcopy05a(int size, void *src, void *dst)
{

//  T() {}

   if (src!=(void *) 0&&dst!=(void *) 0) {

//  T(s) {}

      char *s = (char *) src;

//  T(d) {}

      char *d = (char *) dst;

//  T(ls) {}

      char *ls = s+size;

//  T(d,s) {s+1<=ls, s#init+1<=ls}

      while (s<ls)

//  T(d,s) {s#init+1<=ls}

         *d++ = *s++;
   }
}

Preconditions with point-to


//  P() {}

void memcopy05a(int size, void *src, void *dst)
{

//  P() {}

   if (src!=(void *) 0&&dst!=(void *) 0) {

//  P() {}

      char *s = (char *) src;

//  P(s) {}

      char *d = (char *) dst;

//  P(d,s) {}

      char *ls = s+size;

//  P(d,ls,s) {}

      while (s<ls)

//  P(d,ls,s) {s+1<=ls}

         *d++ = *s++;
   }
}

