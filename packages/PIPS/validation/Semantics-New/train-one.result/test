//  T() {t==1, s<=b+10, b+9<=s}

               while (1)

//  T() {t==1, s<=b+10, b+9<=s}

                  ;
         }
         else if (rand()%2) {

//  T(s) {s==s#init+1, t==1, s<=b+9, b<=s+19}

            if (t==1&&b>s-9) {

//  T(s) {s==s#init+1, t==1, s<=b+9}

               s++;

//  T() {t==1, s<=b+9, b<=s+19}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;

//  T(b) {b==b#init+1, t==1, s<=b+8, b<=s+9}

            if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, t==1, s<=b+8, b<=s+9}

               b++;

//  T() {t==1, s<=b+8, b<=s+9}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {t==1, s+9<=b, b<=s+19}

               while (1)

//  T() {t==1, s+9<=b, b<=s+19}

                  ;
         }
         else {

//  T(s) {s==s#init+1, t==1, s<=b+9, b<=s+19}

            if (t==1&&b>s-9) {

//  T(s) {s==s#init+1, t==1, s<=b+9}

               s++;

//  T() {t==1, s<=b+9, b<=s+19}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;

//  T(s) {s==s#init+1, t==1, s<=b+9, b<=s+18}

            if (t==1&&b>s-9) {

//  T(s) {s==s#init+1, t==1, s<=b+9, b<=s+18}

               s++;

//  T() {t==1, s<=b+9, b<=s+18}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {b==s-9, t==1}

               while (1)

//  T() {b==s-9, t==1}

                  ;

//  T(b,s) {t==1, b#init<=b, 8b#init+27s#init<=8b+27s, s<=b+10,
//    b<=s+19}

            while (rand()%2)

//  T(b,s) {t==1, b<=b#init+1, 22b<=21b#init+s+19,
//    b#init+s#init+1<=b+s, 3s<=b+2s#init+10, s<=s#init+1}

               if (rand()%2)

//  T(b,s) {t==1, b<=b#init+1, b#init+s#init+1<=b+s,
//    28b+5s<=27b#init+6s#init+25, 3s<=b+2s#init+10, s<=s#init+1}

                  if (rand()%2)

//  T(b,s) {b+s==b#init+s#init+1, t==1, b#init<=b, b<=b#init+1,
//    2b#init+s<=3b+8, 22b<=21b#init+s+19}

                     if (rand()%2)

//  T(b) {b==b#init+1, t==1, s<=b+10, b+2<=s}

                        if (t==1&&b<s+9&&b<s-2) {

//  T(b) {b==b#init+1, t==1, b+2<=s}

                           b++;

//  T() {t==1, s<=b+10, b+2<=s}

                           if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                              checking_error();
                        }
                        else

//  T() {t==1, s<=b+2}

                           while (1)

//  T() {t==1, s<=b+2}

                              ;
                     else if (t==1&&b>s-9&&b>s-8) {

//  T(s) {s==s#init+1, t==1, s<=b+8}

                        s++;

//  T() {t==1, s<=b+8, b<=s+19}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {t==1, b+8<=s}

                        while (1)

//  T() {t==1, b+8<=s}

                           ;
                  else {

//  T(s) {b==s-9, b==s#init-8, t==1}

                     if (t==1&&b>s-9&&b==s-8) {

//  T(s) {b==s-9, b==s#init-8, t==1}

                        s++;

//  T() {b==s-9, t==1}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {t==1}

                        while (1)

//  T() {t==1}

                           ;

//  T(b) {b==b#init+1, b==s-8, t==1}

                     if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, b==s-8, t==1}

                        b++;

//  T() {b==s-8, t==1}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {0==-1}

                        while (1)

//  T() {0==-1}

                           ;
                  }
               else {

//  T(b) {b==b#init+1, b==s-1, t==1}

                  if (t==1&&b<s+9&&b==s-2) {

//  T(b) {b==b#init+1, b==s-1, t==1}

                     b++;

//  T() {b==s-1, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {t==1}

                     while (1)

//  T() {t==1}

                        ;

//  T(s) {b==s-2, b==s#init-1, t==1}

                  if (t==1&&b>s-9) {

//  T(s) {b==s-2, b==s#init-1, t==1}

                     s++;

//  T() {b==s-2, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
