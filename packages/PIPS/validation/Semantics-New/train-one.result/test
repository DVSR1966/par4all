            if (t==1&&b<s+9&&b==s-2) {

//  T(b) {b==b#init+1, b==s-1, t==1}

               b++;

//  T() {b==s-1, t==1}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
