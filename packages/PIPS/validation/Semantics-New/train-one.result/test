            s++;

//  T() {0==-1}

            d = 0;

//  T() {0==-1}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;
      }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int b, s, d, t;

//  P(b,d,s,t) {}

   b = s = d = 0;

//  P(b,d,s,t) {b==0, d==0, s==0}

   t = 1;

//  P(b,d,s,t) {b==0, d==0, s==0, t==1}


   while (1)

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}

      // can run without transformer lists, with the assertion:
      
      if (flip())

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}

         if (rand()%2) {

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}

            if (t==1&&b<s+9) {

//  P(b,d,s,t) {b==s, d==0, t==1, 0<=b}

               b++;

//  P(b,d,s,t) {b==s+1, d==0, t==1, 1<=b}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;

//  P(b,d,s,t) {b==s+1, d==0, t==1, 1<=b}

            if (t==1&&b<s+9) {

//  P(b,d,s,t) {b==s+1, d==0, t==1, 1<=b}

               b++;

//  P(b,d,s,t) {b==s+2, d==0, t==1, 2<=b}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;

//  P(b,d,s,t) {b==s+2, d==0, t==1, 2<=b}

            while (rand()%2)

//  P(b,d,s,t) {d==0, t==1, 2<=b, s+2<=b, b<=s+8, 0<=s}

               if (rand()%2) {

//  P(b,d,s,t) {d==0, t==1, 2<=b, s+2<=b, b<=s+8, 0<=s}

                  if (t==1&&b>s-9&&b==s+2) {

//  P(b,d,s,t) {b==s+2, d==0, t==1, 2<=b}

