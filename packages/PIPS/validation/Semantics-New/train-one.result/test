//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;
         }
      else if (flip()) {

//  T(s) {s==s#init+1, t==1, s<=b+9, b<=s+19}

         if (t==1&&b>s-9) {

//  T(s) {s==s#init+1, t==1, s<=b+9}

