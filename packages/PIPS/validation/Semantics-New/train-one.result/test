
TRANSFORMERS


//  T() {0==-1}

void run(void)
{


//  T(b,d,s,t) {}

   int b, s, d, t;

//  T(b,d,s) {b==0, d==0, s==0}

   b = s = d = 0;

//  T(t) {b==0, d==0, s==0, t==1}

   t = 1;

//  T(b,s) {b#init==0, d==0, s#init==0, t==1, s<=21b, s<=11b, 2b<=37s}


   while (1)

//  T(b,s) {t==1, 9b#init+29s<=29b+9s#init+180,
//    21b#init+s+20<=21b+s#init, 3b#init+32s#init+35<=3b+32s,
//    2s+9s#init<=2b+9b#init+99, b+63s#init<=18b#init+46s+108,
//    11b+9b#init<=11s+9s#init+180, 20b+9s#init<=9b#init+20s+99,
//    35b+63s#init<=18b#init+80s+108}

      // can run without transformer lists, with the assertion:
      
      if (flip())

//  T(b,s) {t==1, 9b#init+29s<=29b+9s#init+180,
//    21b#init+s+20<=21b+s#init, 3b#init+32s#init+35<=3b+32s,
//    2s+9s#init<=2b+9b#init+99, b+63s#init<=18b#init+46s+108,
//    11b+9b#init<=11s+9s#init+180, 20b+9s#init<=9b#init+20s+99,
//    35b+63s#init<=18b#init+80s+108}

         if (rand()%2) {

//  T(b) {b==b#init+1, t==1, s<=b+10, b<=s+9}

            if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, t==1, b<=s+9}

               b++;

//  T() {t==1, s<=b+10, b<=s+9}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;

//  T(b) {b==b#init+1, t==1, s<=b+9, b<=s+9}

            if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, t==1, s<=b+9, b<=s+9}

               b++;

//  T() {t==1, s<=b+9, b<=s+9}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {b==s+9, t==1}

               while (1)

//  T() {b==s+9, t==1}

                  ;

//  T(b,s) {t==1, b#init<=b, s<=b+10, b<=s+19, s#init<=s}

            while (rand()%2)

//  T(b,s) {t==1, b<=b#init+1, 12b<=11b#init+s+19,
//    b#init+s#init+1<=b+s, 13s<=b+12s#init+10, s<=s#init+1}

               if (rand()%2) {

//  T(s) {b==s+1, b==s#init+2, t==1}

                  if (t==1&&b>s-9&&b==s+2) {

//  T(s) {b==s+1, b==s#init+2, t==1}

                     s++;

//  T() {b==s+1, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {t==1}

                     while (1)

//  T() {t==1}

                        ;

//  T(b) {b==b#init+1, b==s+2, t==1}

                  if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, b==s+2, t==1}

                     b++;

//  T() {b==s+2, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {0==-1}

                     while (1)

//  T() {0==-1}

                        ;
               }
               else if (rand()%2) {

//  T(b) {b==b#init+1, b==s+9, t==1}

                  if (t==1&&b<s+9&&b==s+8) {

//  T(b) {b==b#init+1, b==s+9, t==1}

                     b++;

//  T() {b==s+9, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {t==1}

                     while (1)

//  T() {t==1}

                        ;

//  T(s) {b==s+8, b==s#init+9, t==1}

                  if (t==1&&b>s-9) {

//  T(s) {b==s+8, b==s#init+9, t==1}

                     s++;

//  T() {b==s+8, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {0==-1}

                     while (1)

//  T() {0==-1}

                        ;
               }
               else if (rand()%2)

//  T(s) {s==s#init+1, t==1, s+2<=b, b<=s+19}

                  if (t==1&&b>s-9&&b>s+2) {

//  T(s) {s==s#init+1, t==1, s+2<=b}

                     s++;

//  T() {t==1, s+2<=b, b<=s+19}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {t==1, b<=s+2}

                     while (1)

//  T() {t==1, b<=s+2}

                        ;
               else if (t==1&&b<s+9&&b<s+8) {

//  T(b) {b==b#init+1, t==1, b<=s+8}

                  b++;

//  T() {t==1, s<=b+10, b<=s+8}

                  if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {t==1, s+8<=b}

                  while (1)

//  T() {t==1, s+8<=b}

                     ;

//  T(s) {b==s+1, b==s#init+2, t==1}

            if (t==1&&b>s-9&&b==s+2) {

//  T(s) {b==s+1, b==s#init+2, t==1}

               s++;

//  T() {b==s+1, t==1}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {t==1}

               while (1)

//  T() {t==1}

                  ;

//  T(s) {b==s, b==s#init+1, t==1}

            if (t==1&&b>s-9) {

//  T(s) {b==s, b==s#init+1, t==1}

               s++;

//  T() {b==s, t==1}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;
         }
