//  T(b) {b==b#init+1, t==1, s<=b+10, b<=s+9}

            if (t==1&&b<s+9) {

//  T(b) {b==b#init+1, t==1, b<=s+9}

               b++;

//  T() {t==1, s<=b+10, b<=s+9}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;

