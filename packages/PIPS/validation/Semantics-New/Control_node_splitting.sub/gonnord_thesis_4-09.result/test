
TRANSFORMERS


//  T() {}

void ts_singlestate(void)
{

//  T(i,j,s) {}

   int s, i, j;

//  T(s) {s==1}

   s = 1;

//  T(i) {i==0, s==1}

   i = 0;

//  T(j) {i==0, j==-100, s==1}

   j = -100;

//  T(i,j,s) {0<=i, i#init<=i, i<=j+201, 101s<=i+202, 0<=j+100,
//    j#init<=j, 1<=s, s<=3}

   while (rand_b())

//  T(i,j,s) {i#init<=i, 102i+j#init+101s+202s#init<=101i#init+2j+706,
//    101i#init+j+202<=102i+j#init+101s#init,
//    101i#init+405<=101i+j#init+101s+202s#init,
//    i#init+5<=i+s+2s#init, i+s+s#init<=i#init+4, i+s#init<=i#init+2,
//    i<=j+201, 0<=j+100, j#init<=j}

      if (rand_b()) {

//  T() {s==1, i<=100}

         if (!(s==1&&i<=100))

//  T() {0==-1}

            deadlock();

//  T(s) {s==2, s#init==1, i<=100}

         s = 2;

//  T(i) {i==i#init+1, s==2, i<=101}

         i++;

//  T() {s==2, 0<=i, i<=101, i<=j+201, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {s==2, j<=19}

         if (!(s==2&&j<=19))

//  T() {0==-1}

            deadlock();

//  T(j) {i+j#init==j, s==2, j#init<=19}

         j += i;

//  T() {s==2, 0<=i, j<=i+19, i<=j+201, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {s==2}

         if (!(s==2))

//  T() {0==-1}

            deadlock();

//  T(s) {s==1, s#init==2}

         s = 1;

//  T() {s==1, 0<=i, i<=j+201, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {s==1, 101<=i}

         if (!(s==1&&i>100))

//  T() {0==-1}

            deadlock();

//  T(s) {s==3, s#init==1, 101<=i}

         s = 3;

//  T() {s==3, 101<=i, i<=j+201, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
}

//  T() {}

void ts_restructured(void)
{

//  T(i,j,s) {}

   int s, i, j;

//  T(s) {s==1}

   s = 1;

//  T(i) {i==0, s==1}

   i = 0;

//  T(j) {i==0, j==-100, s==1}

   j = -100;

//  T(i,j,s) {i==101, i#init==0, j#init==-100, s==3, s#init==1,
//    0<=j+100}

   if (rand_b()) {

//  T(i,j,s) {s==1, s#init==1, 0<=i, i<=99, i#init<=i, 0<=j+100,
//    j#init<=j}

      while (rand_b()) {

//  T() {s==1, i<=98}

         if (!(s==1&&i<99))

//  T() {0==-1}

            deadlock();

//  T(s) {s==2, s#init==1, i<=98}

         s = 2;

//  T(i) {i==i#init+1, s==2, i<=99}

         i++;

//  T() {s==2, 0<=i, i<=99, i<=j+201, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();

//  T(j) {s==2, 0<=i, i<=99, 0<=j+100, j#init<=j}

         while (rand_b()) {

//  T() {s==2, 0<=i, i<=99, i+j<=19, j<=19}

            if (!(s==2&&j<=19&&j+i<=19))

//  T() {0==-1}

               deadlock();

//  T(j) {i+j#init==j, s==2, 0<=i, i<=99, i+j#init<=19, j#init<=19}

            j += i;

//  T() {s==2, 0<=i, i<=99, j<=i+19, i<=j+201, 0<=j+100, j<=19}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }

//  T() {s==2, 0<=i, i<=99, i<=j+201, 0<=j+100}

         if (!(s==2))

//  T() {0==-1}

            deadlock();

//  T(s) {s==1, s#init==2, 0<=i, i<=99, i<=j+201, 0<=j+100}

         s = 1;

//  T() {s==1, 0<=i, i<=99, i<=j+201, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }

//  T() {i==100, s==1, 0<=j+100}

      if (!(s==1&&i==100))

//  T() {0==-1}

         deadlock();

//  T(s) {i==100, s==2, s#init==1, 0<=j+100}

      s = 2;

//  T(i) {i==101, i#init==100, s==2, 0<=j+100}

      i++;

//  T() {i==101, s==2, 0<=j+100}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

         checking_error();

//  T(j) {i==101, s==2, 0<=j+100, j#init<=j}

      while (rand_b()) {

//  T() {i==101, s==2, j+82<=0}

         if (!(s==2&&j<=19&&j+i<=19))

//  T() {0==-1}

            deadlock();

//  T(j) {i==101, j==j#init+101, s==2, j#init+82<=0}

         j += i;

//  T() {i==101, s==2, 0<=j+100, j<=19}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }

//  T() {i==101, s==2, 0<=j+100}

      if (!(s==2))

//  T() {0==-1}

         deadlock();

//  T(s) {i==101, s==1, s#init==2, 0<=j+100}

      s = 1;

//  T() {i==101, s==1, 0<=j+100}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

         checking_error();

//  T() {i==101, s==1, 0<=j+100}

      if (!(s==1&&i>100))

//  T() {0==-1}

         deadlock();

//  T(s) {i==101, s==3, s#init==1, 0<=j+100}

      s = 3;

//  T() {i==101, s==3, 0<=j+100}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

         checking_error();
   }
   else {

//  T(i,j,s) {i==101, i#init==0, j#init==-100, s==2, s#init==1, 20<=j,
//    j<=119}

      if (rand_b()) {

//  T() {i==0, j==-100, s==1}

         if (!(s==1&&i<99))

//  T() {0==-1}

            deadlock();

//  T(s) {i==0, j==-100, s==2, s#init==1}

         s = 2;

//  T(i) {i==1, i#init==0, j==-100, s==2}

         i++;

//  T() {i==1, j==-100, s==2}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();

//  T(i,j,s) {s==2, s#init==2, 1<=i, i<=220, i#init<=i, 20i<=11j+3080,
//    0<=j+100, j#init<=j}

         while (rand_b())

//  T(i,j,s) {s==2, s#init==2, 99i#init+j#init<=98i+j,
//    220i+j<=220i#init+j#init+220, 101i<=101i#init+j#init+201,
//    j<=i#init+j#init, 0<=j+100, j#init<=j}

            if (rand_b()) {

//  T() {s==2, i+j<=19, j<=19}

               if (!(s==2&&j<=19&&j+i<=19))

//  T() {0==-1}

                  deadlock();

//  T(j) {i+j#init==j, s==2, i+j#init<=19, j#init<=19}

               j += i;

//  T() {s==2, 0<=i, j<=i+19, i<=j+201, 0<=j+100, j<=19}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                  checking_error();
            }
            else {

//  T() {s==2}

               if (!(s==2))

//  T() {0==-1}

                  deadlock();

//  T(s) {s==1, s#init==2}

               s = 1;

//  T() {s==1, 0<=i, i<=j+201, 0<=j+100}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                  checking_error();

//  T() {s==1, 0<=i, i<=98, i<=j+201, 0<=j+100}

               if (!(s==1&&i<99))

//  T() {0==-1}

                  deadlock();

//  T(s) {s==2, s#init==1, 0<=i, i<=98, i<=j+201, 0<=j+100}

               s = 2;

//  T(i) {i==i#init+1, s==2, 1<=i, i<=99, i<=j+202, 0<=j+100}

               i++;

//  T() {s==2, 1<=i, i<=99, i<=j+201, 0<=j+100}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

                  checking_error();
            }

//  T() {s==2, 1<=i, 20<=i+j, 0<=j+100, j<=19}

         if (!(s==2&&j<=19&&j+i>19))

//  T() {0==-1}

            deadlock();

//  T(j) {i+j#init==j, s==2, 1<=i, 20<=i+j#init, 0<=j#init+100,
//    j#init<=19}

         j += i;

//  T() {s==2, 1<=i, j<=i+19, i<=j+100, 20<=j}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();

//  T(i,s) {s==2, s#init==2, i#init<=i, j<=i+19, i<=j+100, 20<=j}

         while (rand_b()) {

//  T() {s==2, 20<=j}

            if (!(s==2))

//  T() {0==-1}

               deadlock();

//  T(s) {s==1, s#init==2, 20<=j}

            s = 1;

//  T() {s==1, 0<=i, i<=j+201, 20<=j}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();

//  T() {s==1, 0<=i, i<=98, i<=j+201, 20<=j}

            if (!(s==1&&i<99))

//  T() {0==-1}

               deadlock();

//  T(s) {s==2, s#init==1, 0<=i, i<=98, i<=j+201, 20<=j}

            s = 2;

//  T(i) {i==i#init+1, s==2, 1<=i, i<=99, i<=j+202, 20<=j}

            i++;

//  T() {s==2, 1<=i, i<=99, i<=j+201, 20<=j}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }

//  T() {s==2, 1<=i, j<=i+19, 20<=j}

         if (!(s==2))

//  T() {0==-1}

            deadlock();

//  T(s) {s==1, s#init==2, 1<=i, j<=i+19, 20<=j}

         s = 1;

//  T() {s==1, 1<=i, j<=i+19, i<=j+201, 20<=j}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();

//  T() {i==100, s==1, 20<=j, j<=119}

         if (!(s==1&&i==100))

//  T() {0==-1}

            deadlock();

//  T(s) {i==100, s==2, s#init==1, 20<=j, j<=119}

         s = 2;

//  T(i) {i==101, i#init==100, s==2, 20<=j, j<=119}

         i++;

//  T() {i==101, s==2, 20<=j, j<=119}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {0==-1}

         if (!(s==1&&i==100))

//  T() {0==-1}

            deadlock();

//  T() {0==-1}

         s = 2;

//  T() {0==-1}

         i++;

//  T() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();

//  T() {0==-1}

         while (rand_b()) {

//  T() {0==-1}

            if (!(s==2&&j<=19&&j+i<=19))

//  T() {0==-1}

               deadlock();

//  T() {0==-1}

            j += i;

//  T() {0==-1}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

               checking_error();
         }

//  T() {0==-1}

         if (!(s==2&&j<=19&&j+i>19))

//  T() {0==-1}

            deadlock();

//  T() {0==-1}

         j += i;

//  T() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

            checking_error();
      }

//  T() {i==101, s==2, 20<=j, j<=119}

      if (!(s==2))

//  T() {0==-1}

         deadlock();

//  T(s) {i==101, s==1, s#init==2, 20<=j, j<=119}

      s = 1;

//  T() {i==101, s==1, 20<=j, j<=119}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

         checking_error();

//  T() {i==101, s==1, 20<=j, j<=119}

      if (!(s==1&&i>100))

//  T() {0==-1}

         deadlock();

//  T(s) {i==101, s==3, s#init==1, 20<=j, j<=119}

      s = 3;

//  T() {i==101, s==3, 20<=j, j<=119}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  T() {0==-1}

         checking_error();
   }
}

PRECONDITIONS


//  P() {}

void ts_singlestate(void)
{

//  P() {}

   int s, i, j;

//  P(i,j,s) {}

   s = 1;

//  P(i,j,s) {s==1}

   i = 0;

//  P(i,j,s) {i==0, s==1}

   j = -100;

//  P(i,j,s) {i==0, j==-100, s==1}

   while (rand_b())

//  P(i,j,s) {}

      if (rand_b()) {

//  P(i,j,s) {}

         if (!(s==1&&i<=100))

//  P(i,j,s) {}

            deadlock();

//  P(i,j,s) {s==1, i<=100}

         s = 2;

//  P(i,j,s) {s==2, i<=100}

         i++;

//  P(i,j,s) {s==2, i<=101}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P(i,j,s) {s==2, i<=101}

            checking_error();
      }
      else if (rand_b()) {

//  P(i,j,s) {}

         if (!(s==2&&j<=19))

//  P(i,j,s) {}

            deadlock();

//  P(i,j,s) {s==2, j<=19}

         j += i;

//  P(i,j,s) {s==2, j<=i+19}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P(i,j,s) {s==2, j<=i+19}

            checking_error();
      }
      else if (rand_b()) {

//  P(i,j,s) {}

         if (!(s==2))

//  P(i,j,s) {}

            deadlock();

//  P(i,j,s) {s==2}

         s = 1;

//  P(i,j,s) {s==1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P(i,j,s) {s==1}

            checking_error();
      }
      else {

//  P(i,j,s) {}

         if (!(s==1&&i>100))

//  P(i,j,s) {}

            deadlock();

//  P(i,j,s) {s==1, 101<=i}

         s = 3;

//  P(i,j,s) {s==3, 101<=i}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P(i,j,s) {s==3, 101<=i, j+202<=i}

            checking_error();
      }
}

//  P() {}

void ts_restructured(void)
{

//  P() {}

   int s, i, j;

//  P(i,j,s) {}

   s = 1;

//  P(i,j,s) {s==1}

   i = 0;

//  P(i,j,s) {i==0, s==1}

   j = -100;

//  P(i,j,s) {i==0, j==-100, s==1}

   if (rand_b()) {

//  P(i,j,s) {i==0, j==-100, s==1}

      while (rand_b()) {

//  P(i,j,s) {s==1, 0<=i, i<=99, 0<=j+100}

         if (!(s==1&&i<99))

//  P(i,j,s) {i==99, s==1, 0<=j+100}

            deadlock();

//  P(i,j,s) {s==1, 0<=i, i<=98, 0<=j+100}

         s = 2;

//  P(i,j,s) {s==2, 0<=i, i<=98, 0<=j+100}

         i++;

//  P(i,j,s) {s==2, 1<=i, i<=99, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();

//  P(i,j,s) {s==2, 1<=i, i<=99, i<=j+201, 0<=j+100}

         while (rand_b()) {

//  P(i,j,s) {s==2, 1<=i, i<=99, 0<=j+100}

            if (!(s==2&&j<=19&&j+i<=19))

//  P(i,j,s) {s==2, 1<=i, i<=99, 20<=i+j, 0<=j+100}

               deadlock();

//  P(i,j,s) {s==2, 1<=i, i<=99, i+j<=19, 0<=j+100, j<=19}

            j += i;

//  P(i,j,s) {s==2, 1<=i, i<=99, j<=i+19, i<=j+100, j<=19}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }

//  P(i,j,s) {s==2, 1<=i, i<=99, 0<=j+100}

         if (!(s==2))

//  P() {0==-1}

            deadlock();

//  P(i,j,s) {s==2, 1<=i, i<=99, 0<=j+100}

         s = 1;

//  P(i,j,s) {s==1, 1<=i, i<=99, i<=j+201, 0<=j+100}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }

//  P(i,j,s) {s==1, 0<=i, i<=99, 0<=j+100}

      if (!(s==1&&i==100))

//  P(i,j,s) {s==1, 0<=i, i<=99, 0<=j+100}

         deadlock();

//  P() {0==-1}

      s = 2;

//  P() {0==-1}

      i++;

//  P() {0==-1}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

         checking_error();

//  P() {0==-1}

      while (rand_b()) {

//  P() {0==-1}

         if (!(s==2&&j<=19&&j+i<=19))

//  P() {0==-1}

            deadlock();

//  P() {0==-1}

         j += i;

//  P() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }

//  P() {0==-1}

      if (!(s==2))

//  P() {0==-1}

         deadlock();

//  P() {0==-1}

      s = 1;

//  P() {0==-1}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

         checking_error();

//  P() {0==-1}

      if (!(s==1&&i>100))

//  P() {0==-1}

         deadlock();

//  P() {0==-1}

      s = 3;

//  P() {0==-1}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

         checking_error();
   }
   else {

//  P(i,j,s) {i==0, j==-100, s==1}

      if (rand_b()) {

//  P(i,j,s) {i==0, j==-100, s==1}

         if (!(s==1&&i<99))

//  P() {0==-1}

            deadlock();

//  P(i,j,s) {i==0, j==-100, s==1}

         s = 2;

//  P(i,j,s) {i==0, j==-100, s==2}

         i++;

//  P(i,j,s) {i==1, j==-100, s==2}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();

//  P(i,j,s) {i==1, j==-100, s==2}

         while (rand_b())

//  P(i,j,s) {s==2, 1<=i, i<=220, 20i<=11j+3080, 4860i<=1331j+616660,
//    0<=j+100}

            if (rand_b()) {

//  P(i,j,s) {s==2, 1<=i, i<=220, 20i<=11j+3080, 4860i<=1331j+616660,
//    0<=j+100}

               if (!(s==2&&j<=19&&j+i<=19))

//  P(i,j,s) {s==2, 1<=i, i<=220, 20<=i+j, 20i<=11j+3080,
//    4860i<=1331j+616660, 0<=j+100}

                  deadlock();

//  P(i,j,s) {s==2, 1<=i, i<=220, i+j<=19, 20i<=11j+3080,
//    4860i<=1331j+616660, 0<=j+100, j<=19}

               j += i;

//  P(i,j,s) {s==2, 1<=i, i<=220, j<=i+19, i<=j+100, 31i<=11j+3080,
//    6191i<=1331j+616660, j<=19}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                  checking_error();
            }
            else {

//  P(i,j,s) {s==2, 1<=i, i<=220, 20i<=11j+3080, 4860i<=1331j+616660,
//    0<=j+100}

               if (!(s==2))

//  P() {0==-1}

                  deadlock();

//  P(i,j,s) {s==2, 1<=i, i<=220, 20i<=11j+3080, 4860i<=1331j+616660,
//    0<=j+100}

               s = 1;

//  P(i,j,s) {s==1, 1<=i, i<=220, 20i<=11j+3080, 4860i<=1331j+616660,
//    0<=j+100}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                  checking_error();

//  P(i,j,s) {s==1, 1<=i, i<=220, i<=j+201, 20i<=11j+3080,
//    4860i<=1331j+616660, 0<=j+100}

               if (!(s==1&&i<99))

//  P(i,j,s) {s==1, 99<=i, i<=220, i<=j+201, 20i<=11j+3080,
//    4860i<=1331j+616660, 0<=j+100}

                  deadlock();

//  P(i,j,s) {s==1, 1<=i, i<=98, i<=j+201, 20i<=11j+3080,
//    4860i<=1331j+616660, 0<=j+100}

               s = 2;

//  P(i,j,s) {s==2, 1<=i, i<=98, i<=j+201, 20i<=11j+3080,
//    4860i<=1331j+616660, 0<=j+100}

               i++;

//  P(i,j,s) {s==2, 2<=i, i<=99, i<=j+202, 20i<=11j+3100,
//    4860i<=1331j+621520, 0<=j+100}

               if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

                  checking_error();
            }

//  P(i,j,s) {s==2, 1<=i, i<=103, 736729i<=29040j+75840171, 0<=j+100}

         if (!(s==2&&j<=19&&j+i>19))

//  P(i,j,s) {s==2, 1<=i, i<=103, 736729i<=29040j+75840171,
//    19289425i<=620569j+1974399395, 0<=j+100}

            deadlock();

//  P(i,j,s) {s==2, 1<=i, i<=103, 20<=i+j, 736729i<=29040j+75840171,
//    0<=j+100, j<=19}

         j += i;

//  P(i,j,s) {s==2, 1<=i, i<=103, j<=i+19, i<=j+100,
//    765769i<=29040j+75840171, 20<=j}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();

//  P(i,j,s) {s==2, 1<=i, i<=103, j<=i+19, i<=j+100,
//    765769i<=29040j+75840171, 20<=j}

         while (rand_b()) {

//  P(i,j,s) {s==2, i<=103, j<=i+19, 765769i<=29040j+75840171, 20<=j}

            if (!(s==2))

//  P() {0==-1}

               deadlock();

//  P(i,j,s) {s==2, i<=103, j<=i+19, 765769i<=29040j+75840171, 20<=j}

            s = 1;

//  P(i,j,s) {s==1, i<=103, j<=i+19, 765769i<=29040j+75840171, 20<=j}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();

//  P(i,j,s) {s==1, 0<=i, i<=103, j<=i+19, i<=j+201,
//    765769i<=29040j+75840171, 20<=j}

            if (!(s==1&&i<99))

//  P(i,j,s) {s==1, 99<=i, i<=103, j<=i+19, i<=j+201,
//    765769i<=29040j+75840171, 20<=j}

               deadlock();

//  P(i,j,s) {s==1, 0<=i, i<=98, j<=i+19, i<=j+201,
//    765769i<=29040j+75840171, 20<=j}

            s = 2;

//  P(i,j,s) {s==2, 0<=i, i<=98, j<=i+19, i<=j+201,
//    765769i<=29040j+75840171, 20<=j}

            i++;

//  P(i,j,s) {s==2, 1<=i, i<=99, j<=i+18, i<=j+202,
//    765769i<=29040j+76605940, 20<=j}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }

//  P(i,j,s) {s==2, i<=103, j<=i+19, 765769i<=29040j+75840171, 20<=j}

         if (!(s==2))

//  P() {0==-1}

            deadlock();

//  P(i,j,s) {s==2, i<=103, j<=i+19, 765769i<=29040j+75840171, 20<=j}

         s = 1;

//  P(i,j,s) {s==1, 1<=i, i<=103, j<=i+19, 765769i<=29040j+75840171,
//    20<=j}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();

//  P(i,j,s) {s==1, 1<=i, i<=103, j<=i+19, i<=j+201,
//    765769i<=29040j+75840171, 20<=j}

         if (!(s==1&&i==100))

//  P(i,j,s) {s==1, 1<=i, i<=103, j<=i+19, i<=j+201,
//    460849i<=29040j+45043251, 765769i<=29040j+75840171, 20<=j}

            deadlock();

//  P(i,j,s) {i==100, s==1, 26<=j, j<=119}

         s = 2;

//  P(i,j,s) {i==100, s==2, 26<=j, j<=119}

         i++;

//  P(i,j,s) {i==101, s==2, 26<=j, j<=119}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(i,j,s) {i==0, j==-100, s==1}

         if (!(s==1&&i==100))

//  P(i,j,s) {i==0, j==-100, s==1}

            deadlock();

//  P() {0==-1}

         s = 2;

//  P() {0==-1}

         i++;

//  P() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();

//  P() {0==-1}

         while (rand_b()) {

//  P() {0==-1}

            if (!(s==2&&j<=19&&j+i<=19))

//  P() {0==-1}

               deadlock();

//  P() {0==-1}

            j += i;

//  P() {0==-1}

            if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

               checking_error();
         }

//  P() {0==-1}

         if (!(s==2&&j<=19&&j+i>19))

//  P() {0==-1}

            deadlock();

//  P() {0==-1}

         j += i;

//  P() {0==-1}

         if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

            checking_error();
      }

//  P(i,j,s) {i==101, s==2, 26<=j, j<=119}

      if (!(s==2))

//  P() {0==-1}

         deadlock();

//  P(i,j,s) {i==101, s==2, 26<=j, j<=119}

      s = 1;

//  P(i,j,s) {i==101, s==1, 26<=j, j<=119}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

         checking_error();

//  P(i,j,s) {i==101, s==1, 26<=j, j<=119}

      if (!(s==1&&i>100))

//  P() {0==-1}

         deadlock();

//  P(i,j,s) {i==101, s==1, 26<=j, j<=119}

      s = 3;

//  P(i,j,s) {i==101, s==3, 26<=j, j<=119}

      if (!(0<=i&&0<=j+100&&i<=j+201))

//  P() {0==-1}

         checking_error();
   }
}
