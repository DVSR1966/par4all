
TRANSFORMERS


//  T() {}

void run()
{

//  T(a,b) {a==0, b==0}

   int a = 0, b = 0;

//  T() {a==0, b==0}

   float z;

//  T(a) {a==1, a#init==0, b==0}


TT:   a = b+1;

//  T() {a==1, b==0}


WT:   if (alea()) goto CT;

//  T(b) {a==1, b==2, b#init==0}

   b = a+1;

//  T() {a==1, b==2}

   z = 0.;

//  T() {a==1, b==2}


WW1:   z = 0.;

//  T() {a==1, b==2}


CW:   z = 0.;

//  T() {a==1, b==2}

end:   ;
   return;

//  T() {a==1, b==0}


CT:   if (alea()) goto l99999;

//  T(b) {a==1, b==2, b#init==0}

   b = a+1;
   goto CW;
l99999:   ;

//  T() {a==1, b==0}

   //goto end;
   ;

//  T() {a==1, 0<=b, b<=2}

   return;
}

PRECONDITIONS


//  P() {}

void run()
{

//  P() {}

   int a = 0, b = 0;

//  P(a,b) {a==0, b==0}

   float z;

//  P(a,b) {a==0, b==0}


TT:   a = b+1;

//  P(a,b) {a==1, b==0}


WT:   if (alea()) goto CT;

//  P(a,b) {a==1, b==0}

   b = a+1;

//  P(a,b) {a==1, b==2}

   z = 0.;

//  P(a,b) {a==1, b==2}


WW1:   z = 0.;

//  P(a,b) {a==1, b==2}


CW:   z = 0.;

//  P(a,b) {a==1, b==2}

end:   ;
   return;

//  P(a,b) {a==1, b==0}


CT:   if (alea()) goto l99999;

//  P(a,b) {a==1, b==0}

   b = a+1;
   goto CW;
l99999:   ;

//  P(a,b) {a==1, b==0}

   //goto end;
   ;

//  P(a,b) {a==1, 0<=b, b<=2}

   return;
}
