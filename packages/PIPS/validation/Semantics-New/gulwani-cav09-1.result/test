
TRANSFORMERS


//  T() {}

void run(void)
{


//  T(i,m,x,y) {}

   int x, y, m, i;

//  T(i,x,y) {i==0, x==0, y==0}

   x = y = i = 0;

//  T(m) {i==0, x==0, y==0, 0<=m}

   m = rand();

//  T(i,x,y) {i==m+100, i==y+100, i#init==0, x==100, x#init==0,
//    y#init==0, 100<=i}


   if (m>=0) {

//  T(i,y) {i==m, i==y, i#init==0, x==0, y#init==0, 0<=i}

      if (y<m) {

//  T(i,y) {i==y, i#init==y#init, x==0, 0<=i, i#init<=i, i+1<=m}

         while (flip())

//  T(i,y) {i==i#init+1, x==0, y==y#init+1, i<=m+100, 1<=m, y+1<=m}

            if (x<100&&y<m-1) {

//  T(y) {x==0, y==y#init+1, 1<=m, y+1<=m}

               y++;

//  T(i) {i==i#init+1, x==0, 1<=m, y+1<=m}

               i++;

//  T() {x==0, i<=m+100, 1<=m, y+1<=m}

               if (!(i<=100+m))

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {x==0, 1<=m, m<=y+1}

               while (1)

//  T() {x==0, 1<=m, m<=y+1}

                  ;

//  T(i,y) {i==i#init+1, i==m, i==y, i==y#init+1, x==0, 1<=i}

         if (x<100&&y==m-1) {

//  T(y) {i==m-1, i==y-1, i==y#init, x==0, 0<=i}

            y++;

//  T(i) {i==i#init+1, i==m, i==y, x==0, 1<=i}

            i++;

//  T() {i==m, i==y, x==0, 1<=i}

            if (!(i<=100+m))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {i==y, x==0, 0<=i, 2<=i+m, 1<=m}

            while (1)

//  T() {i==y, x==0, 0<=i, 2<=i+m, 1<=m}

               ;
      }

//  T(i,x) {i+x#init==i#init+x, m+x==i, m==y, i#init<=i, m<=i,
//    i<=m+99, 0<=m}

      while (flip())

//  T(i,x) {i==i#init+1, m==y, x==x#init+1, i<=m+100, 0<=m, x<=99}

         if (x<99&&y>=m) {

//  T(x) {m==y, x==x#init+1, 0<=m, x<=99}

            x++;

//  T(i) {i==i#init+1, m==y, 0<=m, x<=99}

            i++;

//  T() {m==y, i<=m+100, 0<=m, x<=99}

            if (!(i<=100+m))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {m==y, 0<=m, 99<=x}

            while (1)

//  T() {m==y, 0<=m, 99<=x}

               ;

//  T(i,x) {i==i#init+1, i==m+100, i==y+100, x==100, x#init==99,
//    100<=i}

      if (x==99&&y>=m) {

//  T(x) {i==m+99, i==y+99, x==100, x#init==99, 0<=m}

         x++;

//  T(i) {i==i#init+1, i==m+100, i==y+100, x==100, 100<=i}

         i++;

//  T() {i==m+100, i==y+100, x==100, 100<=i}

         if (!(i<=100+m))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {m+x==i, x+y==i, m<=i, 0<=m}

         while (1)

//  T() {m+x==i, x+y==i, 0<=m, 0<=x}

            ;
   }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int x, y, m, i;

//  P(i,m,x,y) {}

   x = y = i = 0;

//  P(i,m,x,y) {i==0, x==0, y==0}

   m = rand();

//  P(i,m,x,y) {i==0, x==0, y==0, 0<=m}


   if (m>=0) {

//  P(i,m,x,y) {i==0, x==0, y==0, 0<=m}

      if (y<m) {

//  P(i,m,x,y) {i==0, x==0, y==0, 1<=m}

         while (flip())

//  P(i,m,x,y) {i==y, x==0, 0<=i, i+1<=m}

            if (x<100&&y<m-1) {

//  P(i,m,x,y) {i==y, x==0, 0<=i, i+2<=m}

               y++;

//  P(i,m,x,y) {i==y-1, x==0, 0<=i, i+2<=m, 1<=m}

               i++;

//  P(i,m,x,y) {i==y, x==0, 1<=i, i+1<=m, 1<=m}

               if (!(i<=100+m))

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(i,m,x,y) {i==m-1, i==y, x==0, 0<=i}

               while (1)

//  P(i,m,x,y) {i==m-1, i==y, x==0, 0<=i}

                  ;

//  P(i,m,x,y) {i==y, x==0, 0<=i, i+1<=m}

         if (x<100&&y==m-1) {

//  P(i,m,x,y) {i==m-1, i==y, x==0, 0<=i}

            y++;

//  P(i,m,x,y) {i==m-1, i==y-1, x==0, 0<=i}

            i++;

//  P(i,m,x,y) {i==m, i==y, x==0, 1<=i}

            if (!(i<=100+m))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,m,x,y) {i==y, x==0, 0<=i, i+2<=m}

            while (1)

//  P(i,m,x,y) {i==y, x==0, 0<=i, i+2<=m}

               ;
      }

//  P(i,m,x,y) {i==m, i==y, x==0, 0<=i}

      while (flip())

//  P(i,m,x,y) {m+x==i, m==y, x<=i, 0<=x, x<=99}

         if (x<99&&y>=m) {

//  P(i,m,x,y) {m+x==i, m==y, x<=i, 0<=x, x<=98}

            x++;

//  P(i,m,x,y) {m+x==i+1, m==y, x<=i+1, 1<=x, x<=99}

            i++;

//  P(i,m,x,y) {m+x==i, m==y, x<=i, 1<=x, x<=99}

            if (!(i<=100+m))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(i,m,x,y) {i==m+99, m==y, x==99, 99<=i}

            while (1)

//  P(i,m,x,y) {i==m+99, i==y+99, x==99, 99<=i}

               ;

//  P(i,m,x,y) {m+x==i, m==y, x<=i, 0<=x, x<=99}

      if (x==99&&y>=m) {

//  P(i,m,x,y) {i==m+99, i==y+99, x==99, 99<=i}

         x++;

//  P(i,m,x,y) {i==m+99, i==y+99, x==100, 99<=i}

         i++;

//  P(i,m,x,y) {i==m+100, i==y+100, x==100, 100<=i}

         if (!(i<=100+m))

//  P() {0==-1}

            checking_error();
      }
      else

//  P(i,m,x,y) {m+x==i, m==y, x<=i, 0<=x, x<=98}

         while (1)

//  P(i,m,x,y) {m+x==i, m==y, m<=i, i<=m+98, 0<=m}

            ;
   }
}
