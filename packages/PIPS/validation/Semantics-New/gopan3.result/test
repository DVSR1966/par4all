
TRANSFORMERS


//  T(main) {103<=main}

int main()
{


//  T(k,x,y,z) {}

   int x, y, z, k;

//  T(x) {x==0}


   x = 0;

//  T(y) {x==0, y==0}

   y = 0;

//  T(z) {x==0, y==0, z==0}

   z = 0;

//  T() {x==0, y==0, z==0}


   while (y>=0) {

//  T(x,y) {x+y#init==x#init+y, x<=50, x#init<=x, x#init<=50, 0<=y,
//    0<=y#init}

      while (y>=0&&x<=50) {

//  T(x) {x==x#init+1, x<=51, 0<=y}

         x++;

//  T(y) {y==y#init+1, x<=51, 1<=y}

         y++;
      }

//  T(x,y) {x+y==x#init+y#init, 51<=x, 51<=x#init, 0<=y, y<=y#init,
//    0<=y#init}

      while (y>=0&&x>50) {

//  T(y) {y==y#init-1, 51<=x, 0<=y+1}

         y--;

//  T(x) {x==x#init+1, 52<=x, 0<=y+1}

         x++;
      }

//  T(z) {x==z, y==-1, 52<=x}

      z = x;
   }

//  T(main) {main==x, main==z, y==-1, 103<=main}

   /*
     if(x==103)
       printf("property  verified\n");
     else
       printf("property not found\n");
     */
   return z;
}

PRECONDITIONS


//  P() {}

int main()
{


//  P() {}

   int x, y, z, k;

//  P(k,x,y,z) {}


   x = 0;

//  P(k,x,y,z) {x==0}

   y = 0;

//  P(k,x,y,z) {x==0, y==0}

   z = 0;

//  P(k,x,y,z) {x==0, y==0, z==0}


   while (y>=0) {

//  P(k,x,y,z) {x==0, y==0, z==0}

      while (y>=0&&x<=50) {

//  P(k,x,y,z) {x==y, z==0, 0<=x, x<=50}

         x++;

//  P(k,x,y,z) {x==y+1, z==0, 1<=x, x<=51}

         y++;
      }

//  P(k,x,y,z) {x==51, y==51, z==0}

      while (y>=0&&x>50) {

//  P(k,x,y,z) {x+y==102, z==0, 51<=x, x<=102}

         y--;

//  P(k,x,y,z) {x+y==101, z==0, 51<=x, x<=102}

         x++;
      }

//  P(k,x,y,z) {x==103, y==-1, z==0}

      z = x;
   }

//  P(k,x,y,z) {x==103, y==-1, z==103}

   /*
     if(x==103)
       printf("property  verified\n");
     else
       printf("property not found\n");
     */
   return z;
}
