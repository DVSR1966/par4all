void P4A_scmp_dealloc(void *dest)
{
}
void * P4A_scmp_flow(void **flow)
{
}
void P4A_scmp_kernel_0(int *a, int *b, int c[10])
{
   //PIPS generated variable
   int i;

   for(i = 0; i <= 9; i += 1) {
      (*a)++;
      (*b)++;
      c[i]++;
   }
}
void P4A_scmp_kernel_1(int *a, int *b)
{
   //PIPS generated variable
   int i;

   for(i = 0; i <= 4; i += 1) {
      (*a)--;
      (*b)--;
   }
}
void P4A_scmp_malloc(void **dest, size_t n)
{

   if (*dest==(void *) 0) 
      *dest = malloc(n);
}
void P4A_scmp_read(const int *in_address, int *out_address, size_t n)
{
   memcpy(in_address, out_address, n);
}
void P4A_scmp_write(void *out_address, const void *buffer_address, size_t n)
{
   memcpy(out_address, buffer_address, n);
}
int main()
{
   int i;
   int b;
   int c[10] = {0};
   int a = 2;
   //PIPS generated variable
   int (*c0)[10] = (int (*)[10]) 0, *b0 = (int *) 0, *a0 = (int *) 0, *b1 = (int *) 0, *a1 = (int *) 0;

   a = 0;
   b = 0;
   P4A_scmp_malloc(&a0, sizeof(int)*1);
   P4A_scmp_malloc(&b0, sizeof(int)*1);
   P4A_scmp_malloc(&c0, sizeof(int)*10);
   P4A_scmp_read(&a, a0, sizeof(int)*1);
   P4A_scmp_read(&b, b0, sizeof(int)*1);
   P4A_scmp_read(&c[0], *c0, sizeof(int)*10);

   P4A_scmp_kernel_0(&(*a0), &(*b0), (*c0));
   P4A_scmp_write(&a, a0, sizeof(int)*1);
   P4A_scmp_write(&b, b0, sizeof(int)*1);
   P4A_scmp_write(&c[0], *c0, sizeof(int)*10);
   P4A_scmp_malloc(&a1, sizeof(int)*1);
   P4A_scmp_malloc(&b1, sizeof(int)*1);
   P4A_scmp_read(&a, a1, sizeof(int)*1);
   P4A_scmp_read(&b, b1, sizeof(int)*1);

   P4A_scmp_kernel_1(&(*a1), &(*b1));
   P4A_scmp_write(&a, a1, sizeof(int)*1);
   P4A_scmp_write(&b, b1, sizeof(int)*1);

   return 0;
}
