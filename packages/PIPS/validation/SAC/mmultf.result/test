      SUBROUTINE MMUL(A,B,C)
      INTEGER*4 SIZE
      PARAMETER (SIZE=4)


      INTEGER*4 A(SIZE,SIZE), B(SIZE,SIZE), C(SIZE,SIZE)
      INTEGER I, J,K

      DO 10 I = 1, SIZE                                                 0009
         DO 20 J = 1, SIZE                                              0010
            C(I,J) = 0                                                  0011
            DO 30 K = 1, SIZE                                           0012
               C(I,J) = C(I,J)+A(I,K)*B(K,J)                            0013
30             CONTINUE                                                 0014
20          CONTINUE                                                    0015
10       CONTINUE                                                       0016
      END

