
Initial code

      SUBROUTINE MMUL(A,B,C)
      INTEGER*4 SIZE
      PARAMETER (SIZE=4)


      INTEGER*4 A(SIZE,SIZE), B(SIZE,SIZE), C(SIZE,SIZE)
      INTEGER I, J,K

      DO 10 I = 1, SIZE                                                 0009
         DO 20 J = 1, SIZE                                              0010
            C(I,J) = 0                                                  0011
            DO 30 K = 1, SIZE                                           0012
               C(I,J) = C(I,J)+A(I,K)*B(K,J)                            0013
30             CONTINUE                                                 0014
20          CONTINUE                                                    0015
10       CONTINUE                                                       0016
      END

simdized code

      SUBROUTINE MMUL(A,B,C)
      INTEGER*4 SIZE
      PARAMETER (SIZE=4)


      INTEGER*4 A(SIZE,SIZE), B(SIZE,SIZE), C(SIZE,SIZE)
      INTEGER I, J,K

99995 CONTINUE
      DO 99994 LU_IND2 = 0, 3, 2
         LU_IND1 = 0
         DO LU_IND1 = 0, 3, 2
            C(LU_IND2+1,LU_IND1+1) = 0                                  0011
            LU_IND0 = 0
            DO LU_IND0 = 0, 3, 2
               CALL SIMD_LOAD_V2SI(v2si_vec2, B(LU_IND0+1,LU_IND1+1))
               CALL SIMD_LOAD_GENERIC_V2SI(v2si_vec1, A(LU_IND2+1,
     &         LU_IND0+1), A(LU_IND2+1,LU_IND0+2))
               CALL SIMD_MULD(v2si_vec0, v2si_vec1, v2si_vec2)
               CALL SIMD_SAVE_GENERIC_V2SI(v2si_vec0, I_00, I_10)
               C(LU_IND2+1,LU_IND1+1) = C(LU_IND2+1,LU_IND1+1)+I_00     0013
               C(LU_IND2+1,LU_IND1+1) = C(LU_IND2+1,LU_IND1+1)+I_10     0013
            ENDDO
            C(LU_IND2+1,LU_IND1+2) = 0                                  0011
            LU_IND0 = 0
            DO LU_IND0 = 0, 3, 2
               CALL SIMD_LOAD_V2SI(v2si_vec5, B(LU_IND0+1,LU_IND1+2))
               CALL SIMD_LOAD_GENERIC_V2SI(v2si_vec4, A(LU_IND2+1,
     &         LU_IND0+1), A(LU_IND2+1,LU_IND0+2))
               CALL SIMD_MULD(v2si_vec3, v2si_vec4, v2si_vec5)
               CALL SIMD_SAVE_GENERIC_V2SI(v2si_vec3, I_20, I_30)
               C(LU_IND2+1,LU_IND1+2) = C(LU_IND2+1,LU_IND1+2)+I_20     0013
               C(LU_IND2+1,LU_IND1+2) = C(LU_IND2+1,LU_IND1+2)+I_30     0013
            ENDDO
         ENDDO
         LU_IND1 = 0
         DO LU_IND1 = 0, 3, 2
            C(LU_IND2+2,LU_IND1+1) = 0                                  0011
            LU_IND0 = 0
            DO LU_IND0 = 0, 3, 2
               CALL SIMD_LOAD_V2SI(v2si_vec8, B(LU_IND0+1,LU_IND1+1))
               CALL SIMD_LOAD_GENERIC_V2SI(v2si_vec7, A(LU_IND2+2,
     &         LU_IND0+1), A(LU_IND2+2,LU_IND0+2))
               CALL SIMD_MULD(v2si_vec6, v2si_vec7, v2si_vec8)
               CALL SIMD_SAVE_GENERIC_V2SI(v2si_vec6, I_40, I_50)
               C(LU_IND2+2,LU_IND1+1) = C(LU_IND2+2,LU_IND1+1)+I_40     0013
               C(LU_IND2+2,LU_IND1+1) = C(LU_IND2+2,LU_IND1+1)+I_50     0013
            ENDDO
            C(LU_IND2+2,LU_IND1+2) = 0                                  0011
            LU_IND0 = 0
            DO LU_IND0 = 0, 3, 2
               CALL SIMD_LOAD_V2SI(v2si_vec11, B(LU_IND0+1,LU_IND1+2)
     &         )
               CALL SIMD_LOAD_GENERIC_V2SI(v2si_vec10, A(LU_IND2+2,
     &         LU_IND0+1), A(LU_IND2+2,LU_IND0+2))
               CALL SIMD_MULD(v2si_vec9, v2si_vec10, v2si_vec11)
               CALL SIMD_SAVE_GENERIC_V2SI(v2si_vec9, I_60, I_70)
               C(LU_IND2+2,LU_IND1+2) = C(LU_IND2+2,LU_IND1+2)+I_60     0013
               C(LU_IND2+2,LU_IND1+2) = C(LU_IND2+2,LU_IND1+2)+I_70     0013
            ENDDO
         ENDDO
99994    CONTINUE
      END
