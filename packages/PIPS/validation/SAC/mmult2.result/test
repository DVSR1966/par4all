               c[i+4*i_t-5][j+4*j_t-5] = 0;
               LU_NUB0 = 16;
               LU_IB0 = 0;
               LU_IND0 = 0;
               for(LU_IND0 = 0; LU_IND0 <= 15; LU_IND0 += 4) {
                  //PIPS:SAC generated float vector(s)
                  float v4sf_vec0[4], v4sf_vec1[4], v4sf_vec2[4];
                  SIMD_LOAD_GENERIC_V4SF(v4sf_vec2, b[LU_IND0][j+4*j_t-5], b[1+LU_IND0][j+4*j_t-5], b[2+LU_IND0][j+4*j_t-5], b[3+LU_IND0][j+4*j_t-5]);
                  SIMD_LOAD_V4SF(v4sf_vec1, &a[i+4*i_t-5][LU_IND0]);
                  SIMD_MULPS(v4sf_vec0, v4sf_vec1, v4sf_vec2);
                  SIMD_STORE_V4SF(v4sf_vec0, &aligned[0]);
                  //c SIMD_COMMENT_0
                  c[i+4*i_t-5][j+4*j_t-5] = c[i+4*i_t-5][j+4*j_t-5]+aligned[0];
