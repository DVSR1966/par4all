      for(j_t = 0; j_t <= 3; j_t += 1)
         for(i = 4*i_t; i <= 4*i_t+3; i += 1)
            for(j = 4*j_t; j <= 4*j_t+3; j += 1) {
               c[i][j] = 0;
loop2:
               for(k = 0; k <= 15; k += 1)
                  c[i][j] = c[i][j]+a[i][k]*b[k][j];
            }
}
