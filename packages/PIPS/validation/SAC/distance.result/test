double distance(double X[2], double Y[2])
{
// (0,2)
   double tmp;
   //PIPS generated variable
// (0,3)
   double F_0, F_1, F_2, F_3, F_4, F_5, F_6, F_7, F_8;
// (0,5)
   F_0 = cos(X[0]);
// (0,6)
   F_1 = cos(Y[0]);
// (0,7)
   F_2 = F_0*F_1;
// (0,8)
   F_3 = X[1]-Y[0];
// (0,9)
   F_4 = cos(F_3);
// (0,10)
   F_5 = F_2*F_4;
// (0,11)
   F_6 = sin(X[0]);
// (0,12)
   F_7 = sin(Y[0]);
// (0,13)
   F_8 = F_6*F_7;
// (0,14)
   tmp = F_5+F_8;
// (0,15)
   return 6368.*acos(tmp);
}
