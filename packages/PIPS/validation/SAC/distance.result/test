simdized code

double distance(double X[2], double Y[2])
{
   //PIPS:SAC generated double vector(s)
// (0,2)
   double v2df_vec0[2], v2df_vec1[2], v2df_vec2[2], v2df_vec3[2], v2df_vec4[2], v2df_vec5[2], v2df_vec6[2];
   //PIPS:SAC generated variable
// (0,3)
   double aligned[2] = {0, 0}, aligned0[2] = {0, 0}, aligned1[2] = {0, 0};
   {
// (0,5)
      double tmp;
      //PIPS generated variable
// (0,6)
      double F_50, F_40, F_30;
// (0,7)
      SIMD_LOAD_GENERIC_V2DF(v2df_vec1, X[0], Y[0]);
// (0,8)
      SIMD_COSPD(v2df_vec0, v2df_vec1);
// (0,9)
      SIMD_SAVE_V2DF(v2df_vec0, &aligned[0]);
// (0,10)
      SIMD_LOAD_GENERIC_V2DF(v2df_vec4, aligned[1], aligned1[1]);
// (0,11)
      SIMD_LOAD_GENERIC_V2DF(v2df_vec3, aligned[0], aligned1[0]);
// (0,12)
      SIMD_MULPD(v2df_vec2, v2df_vec3, v2df_vec4);
// (0,13)
      SIMD_SAVE_V2DF(v2df_vec2, &aligned0[0]);
// (0,14)
      F_30 = X[1]-Y[0];
// (0,15)
      F_40 = cos(F_30);
// (0,16)
      F_50 = aligned0[0]*F_40;
// (0,17)
      SIMD_LOAD_GENERIC_V2DF(v2df_vec6, X[0], Y[0]);
// (0,18)
      SIMD_SINPD(v2df_vec5, v2df_vec6);
// (0,19)
      SIMD_SAVE_V2DF(v2df_vec5, &aligned1[0]);
