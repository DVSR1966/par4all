void foo_l(int a[4], int b[40])
{
   int i;
   for(i = 0; i <= 9; i += 1) {
      a[0] = a[0]+b[i*4];
      a[1] = a[1]+b[1+i*4];
      a[2] = a[2]+b[2+i*4];
      a[3] = a[3]+b[3+i*4];
   }
}
void foo_l(int a[4], int b[40])
{
   int i;
   for(i = 0; i <= 9; i += 1) {
      //PIPS:SAC generated v4sf vector(s)
      v4sf vec00, vec10;
      SIMD_LOAD_V4SI_TO_V4SF(vec10, &b[4*i]);
      SIMD_LOAD_V4SI_TO_V4SF(vec00, &a[0]);
      SIMD_ADDPS(vec00, vec00, vec10);
      SIMD_STORE_V4SF_TO_V4SI(vec00, &a[0]);
   }
}
void foo_l(int a[4], int b[40])
{
   int i;
   //PIPS generated variable
   v4sf vec00_0, vec10_0;
   SIMD_LOAD_V4SI_TO_V4SF(vec00_0, &a[0]);
   for(i = 0; i <= 9; i += 1) {
      //PIPS:SAC generated v4sf vector(s)
      ;
      SIMD_LOAD_V4SI_TO_V4SF(vec10_0, &b[4*i]);
      SIMD_ADDPS(vec00_0, vec00_0, vec10_0);
   }
   SIMD_STORE_V4SF_TO_V4SI(vec00_0, &a[0]);
}

