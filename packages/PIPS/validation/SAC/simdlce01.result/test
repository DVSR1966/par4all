void foo_l(int a[4], int b[4], int c[4])
{
   int i;
   for(i = 0; i <= 9; i += 1) {
      a[0] = b[0]+c[0];
      a[1] = b[1]+c[1];
      a[2] = b[2]+c[2];
      a[3] = b[3]+c[3];
   }
}
void foo_l(int a[4], int b[4], int c[4])
{
   int i;
   for(i = 0; i <= 9; i += 1) {
      //PIPS:SAC generated int vector(s)
      int v4si_vec0[4], v4si_vec1[4], v4si_vec2[4];
      SIMD_LOAD_V4SI(v4si_vec2, &c[0]);
      SIMD_LOAD_V4SI(v4si_vec1, &b[0]);
      SIMD_ADDD(v4si_vec0, v4si_vec1, v4si_vec2);
      SIMD_STORE_V4SI(v4si_vec0, &a[0]);
   }
}
void foo_l(int a[4], int b[4], int c[4])
{
   int i;
   {
      //PIPS:SAC generated int vector(s)
      int v4si_vec0[4], v4si_vec1[4], v4si_vec2[4];
      SIMD_LOAD_V4SI(v4si_vec2, &c[0]);
      SIMD_LOAD_V4SI(v4si_vec1, &b[0]);
      for(i = 0; i <= 9; i += 1)
         SIMD_ADDD(v4si_vec0, v4si_vec1, v4si_vec2);
      SIMD_STORE_V4SI(v4si_vec0, &a[0]);
   }
}

