void foo(int i)
{
   float a[4];
   for(i = 2; i <= 5; i += 1)
      a[i-2] = 1;
   for(i = 1; i <= 3; i += 1)
      a[i-1] = 0;
}
void foo(int i)
{
   float a[4];
   a[2-2] = 1;
   a[3-2] = 1;
   a[4-2] = 1;
   a[5-2] = 1;
   i = 6;
   a[1-1] = 0;
   a[2-1] = 0;
   a[3-1] = 0;
   i = 4;
}
void foo(int i)
{
   float a[4];
   a[0] = 1;
   a[1] = 1;
   a[2] = 1;
   a[3] = 1;
   i = 6;
   a[0] = 0;
   a[1] = 0;
   a[2] = 0;
   i = 4;
}
