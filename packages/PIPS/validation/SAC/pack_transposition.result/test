      LU_IND0 = 0;
      //c SIMD_COMMENT_4
      a[LU_IND1][1] = a[LU_IND1][0]+a[LU_IND1][2];
      //c SIMD_COMMENT_5
      a[LU_IND1][2] = a[LU_IND1][1]+a[LU_IND1][3];
      //c SIMD_COMMENT_6
      j = 2;
      //c SIMD_COMMENT_0
      LU_NUB0 = 2;
      //c SIMD_COMMENT_1
      LU_IB0 = 0;
      //c SIMD_COMMENT_2
      LU_IND0 = 0;
      //c SIMD_COMMENT_3
      LU_IND0 = 0;
      //c SIMD_COMMENT_4
