void pack_transposition(double a[5][5])
{
   int i, j;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0, LU_NUB1, LU_IB1, LU_IND1;
   LU_NUB1 = 4;
   LU_IB1 = 0;
   LU_IND1 = 0;
   for(LU_IND1 = 0; LU_IND1 <= 3; LU_IND1 += 2) {
      //PIPS:SAC generated v2df vector(s)
      v2df vec00, vec10, vec20, vec30, vec40, vec50;
      //c SIMD_COMMENT_0
      LU_NUB0 = 2;
      //c SIMD_COMMENT_1
      LU_IB0 = 0;
      //c SIMD_COMMENT_2
      LU_IND0 = 0;
      SIMD_LOAD_GENERIC_V2DF(vec20, a[LU_IND1][2], a[1+LU_IND1][2]);
      SIMD_LOAD_GENERIC_V2DF(vec10, a[LU_IND1][0], a[1+LU_IND1][0]);
      SIMD_ADDPD(vec00, vec10, vec20);
      SIMD_STORE_GENERIC_V2DF(vec00, &a[LU_IND1][1], &a[1+LU_IND1][1]);
      //c SIMD_COMMENT_6
      j = 2;
      //c SIMD_COMMENT_3
      LU_IND0 = 0;
      SIMD_LOAD_GENERIC_V2DF(vec50, a[LU_IND1][3], a[1+LU_IND1][3]);
      SIMD_ADDPD(vec30, vec00, vec50);
      SIMD_STORE_GENERIC_V2DF(vec30, &a[LU_IND1][2], &a[1+LU_IND1][2]);
      //c SIMD_COMMENT_0
      LU_NUB0 = 2;
      //c SIMD_COMMENT_1
      LU_IB0 = 0;
      //c SIMD_COMMENT_6
      j = 2;
      //c SIMD_COMMENT_2
      LU_IND0 = 0;
      //c SIMD_COMMENT_3
      LU_IND0 = 0;
   }
   i = 4;
   ;
}
