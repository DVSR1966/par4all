{
   //PIPS generated variable
   int LU_IND1;
   4;
   0;
   LU_IND1 = 0;
   for(LU_IND1 = 0; LU_IND1 <= 3; LU_IND1 += 2) {
      //PIPS:SAC generated v2df vector(s)
      v2df vec00, vec10, vec20, vec30, vec40, vec50;
      //c SIMD_COMMENT_0
      2;
      //c SIMD_COMMENT_1
      0;
      //c SIMD_COMMENT_2
      0;
      SIMD_LOAD_GENERIC_V2DF(vec20, a[LU_IND1][2], a[1+LU_IND1][2]);
      SIMD_LOAD_GENERIC_V2DF(vec10, a[LU_IND1][0], a[1+LU_IND1][0]);
      SIMD_ADDPD(vec00, vec10, vec20);
