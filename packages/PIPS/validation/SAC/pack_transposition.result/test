void pack_transposition(double a[5][5])
{
   int i, j;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0;
   for(i = 0; i <= 3; i += 1) {
      LU_NUB0 = 2;
      LU_IB0 = 0;
      LU_IND0 = 0;
      LU_IND0 = 0;
      //c SIMD_COMMENT_0
      a[i][LU_IND0+1] = a[i][LU_IND0]+a[i][LU_IND0+2];
      //c SIMD_COMMENT_0
      a[i][LU_IND0+2] = a[i][LU_IND0+1]+a[i][LU_IND0+3];
      j = 2;
   }
   ;
}
void pack_transposition(double a[5][5])
{
   int i, j;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0, LU_NUB1, LU_IB1, LU_IND1;
   LU_NUB1 = 4;
   LU_IB1 = 0;
   LU_IND1 = 0;
   for(LU_IND1 = 0; LU_IND1 <= 3; LU_IND1 += 2) {
