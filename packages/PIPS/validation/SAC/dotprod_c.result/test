   a1 = 0;
   LU_IND0 = 0;
   RED0[0] = 0.000000;
   RED0[1] = 0.000000;
   RED0[2] = 0.000000;
   RED0[3] = 0.000000;
   {
      //PIPS:SAC generated v4sf vector(s)
      v4sf vec00, vec30;
      SIMD_LOAD_V4SF(vec30, &RED0[0]);
