float dotprod(float b[200], float c[200])
{
   //PIPS generated variable
   int LU_IND0;
   //PIPS generated variable
   float RED0[4], a0, a1;
   a1 = 0;
   RED0[0] = 0.000000;
   RED0[1] = 0.000000;
   RED0[2] = 0.000000;
   RED0[3] = 0.000000;
   {
      //PIPS:SAC generated v4sf vector(s)
      v4sf vec00, vec30;
      SIMD_LOAD_V4SF(vec30, &RED0[0]);
      for(LU_IND0 = 0; LU_IND0 <= 199; LU_IND0 += 4) {
         //PIPS:SAC generated v4sf vector(s)
         v4sf vec10, vec20, vec40;
         SIMD_LOAD_V4SF(vec20, &c[LU_IND0]);
         SIMD_LOAD_V4SF(vec10, &b[LU_IND0]);
         SIMD_MULPS(vec00, vec10, vec20);
         SIMD_ADDPS(vec30, vec30, vec00);
      }
      SIMD_STORE_V4SF(vec30, &RED0[0]);
   }
   a0 = RED0[3]+RED0[2]+RED0[1]+RED0[0]+a1;
   return a0;
}
