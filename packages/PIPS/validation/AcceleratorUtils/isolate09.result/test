void alphablending(short src0[4][4], short src1[4][4], short result[4][4])
{
   unsigned int i, j;
   //PIPS generated variable
   unsigned int i_t, j_t;

//  <result[PHI1][PHI2]-W-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3}>
//  <src0[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3}>
//  <src1[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3}>

l99998:
   for(i_t = 0; i_t <= 0; i_t += 1)

//  <result[PHI1][PHI2]-W-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3,
//    i_t==0}>
//  <src0[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3,
//    i_t==0}>
//  <src1[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3,
//    i_t==0}>

l99995:
      for(j_t = 0; j_t <= 0; j_t += 1)

//  <result[PHI1][PHI2]-W-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3,
//    i_t==0, j_t==0}>
//  <src0[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3,
//    i_t==0, j_t==0}>
//  <src1[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=3, 0<=PHI2, PHI2<=3,
//    i_t==0, j_t==0}>

l99996:
         for(i = 10*i_t; i <= 3; i += 1)

//  <result[PHI1][PHI2]-W-MAY-{PHI1==i, 0<=PHI2, PHI2<=3, i_t==0,
//    j_t==0, 0<=i, i<=3}>
//  <src0[PHI1][PHI2]-R-MAY-{PHI1==i, 0<=PHI2, PHI2<=3, i_t==0,
//    j_t==0, 0<=i, i<=3}>
//  <src1[PHI1][PHI2]-R-MAY-{PHI1==i, 0<=PHI2, PHI2<=3, i_t==0,
