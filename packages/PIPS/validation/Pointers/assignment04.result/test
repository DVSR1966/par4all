
PROPER  EFFECTS

void assignment04()
{
   int i;
   int *p;
   int *q;
//             <    is written>: p

   p = &i;
//             <    is read   >: p
//             <    is written>: q
   q = p;

   return;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void assignment04()
{

// Points To: none

   int i;

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = &i;

// Points To:
// p -> i , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = p;

// Points To:
// p -> i , EXACT
// q -> i , EXACT


   return;
}
