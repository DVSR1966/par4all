//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b2,
//   -Exact-)}
   int i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b2,
//   -Exact-)}

   r = &i;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(r,i,-Exact-)}
   //q = &r;
