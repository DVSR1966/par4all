
PROPER  EFFECTS

int main()
{
//             <    is written>: j
   int j = 0, k, tab[5];
//             <    is written>: p
   int *p = &j;
//             <    is read   >: tab[1]
//             <    is written>: q
   int *q = tab[1];
//             <    is written>: k

   k = 1;
//             <    is read   >: k
//             <    is written>: j
   j = k;
//             <    is written>: p
   p = &k;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int j = 0, k, tab[5];

// Points To: none

   int *p = &j;

// Points To:
// p -> j , EXACT

   int *q = tab[1];

// Points To:
// p -> j , EXACT
// q -> tab[1] , EXACT


   k = 1;

// Points To:
// p -> j , EXACT
// q -> tab[1] , EXACT

   j = k;

// Points To:
// p -> j , EXACT
// q -> tab[1] , EXACT

   p = &k;

// Points To:
// p -> k , EXACT
// q -> tab[1] , EXACT


   return 0;
}
