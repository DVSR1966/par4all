int struct04()
{
   // BEGIN BLOCK
   struct one {
      int first;
      int second;
   } x;
   int y[10];
//               <must be read   >: x[.second]
//               <must be written>: x[.first]
   
   //  x.first = 1;
   //x.second = 2;
   
   //y[0] = 1;
   
   x.first = x.second+1;
   // END BLOCK
}
POINTS TO
//  {}
int struct04()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   struct one {
      int first;
      int second;
   } x;
//  points to = {}
   int y[10];
//  points to = {}
   
   //  x.first = 1;
   //x.second = 2;
   
   //y[0] = 1;
   
   x.first = x.second+1;
   // END BLOCK
}
