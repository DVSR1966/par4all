
PROPER  EFFECTS

void update01()
{
   int i;
   int *p;
//             <    is written>: p

   p = &i;
//             <    is read   >: p
//             <    is written>: p
   p++;
//             <    is written>: i
   i = 2;
}

POINTS-TO


// Points To: none

void update01()
{

// Points To: none

   int i;

// Points To: none

   int *p;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = &i;

// Points To:
// p -> &i , EXACT

   p++;

// Points To:
// p -> &*ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   i = 2;
}
