
PROPER EFFECTS

void call16(int _i, int *_a, int *_b, int *_c)
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   _a[_i] = _b[_i]+_c[_i];
}
void dotprod(int *a, int *b, int *c)
{
   int i;
//             <    is written>: i
   for(i = 0; i <= 999999; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      call16(i, a, b, c);
   
   
   return;
}

PROPER POINTER EFFECTS

void dotprod(int *a, int *b, int *c)
{
   int i;
//             <    is written>: i
   for(i = 0; i <= 999999; i += 1)
//             <may be read   >: b[*] c[*]
//             <may be written>: a[*]
//             <    is read   >: a b c i
      call16(i, a, b, c);
   
   
   return;
}

POINTS TO


// Points To IN: none


// Points To OUT: none

void call16(int _i, int *_a, int *_b, int *_c)
{

// Points To: none

   _a[_i] = _b[_i]+_c[_i];
}

// Points To IN: none


// Points To OUT: none

void dotprod(int *a, int *b, int *c)
{

// Points To: none

   int i;

// Points To: none

   for(i = 0; i <= 999999; i += 1)

// Points To: none

      call16(i, a, b, c);

// Points To: none

   
   
   return;
}
