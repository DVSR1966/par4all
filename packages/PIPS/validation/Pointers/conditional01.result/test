
PROPER  EFFECTS

char * conditional01(int i)
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   char *p[3] = {"a", "b", "c"};
//             <may be read   >: p[0] p[*]
//             <    is read   >: i
   return i<0||i>2?p[0]:p[i];
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

char * conditional01(int i)
{

// Points To: none

   char *p[3] = {"a", "b", "c"};

// Points To: none

   return i<0||i>2?p[0]:p[i];
}
