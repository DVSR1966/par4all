
POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a[10];

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b = 0, c = 1;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (b==c)

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &a[0];
   else

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &a[1];

// Points To:
// p -> a[0] , MAY
// p -> a[1] , MAY

   p++;

// Points To:
// p -> a[1] , MAY
// p -> a[2] , MAY

   ++p;

// Points To:
// p -> a[2] , MAY
// p -> a[3] , MAY

   p--;

// Points To:
// p -> a[1] , MAY
// p -> a[2] , MAY

   --p;

// Points To:
// p -> a[0] , MAY
// p -> a[1] , MAY

   return 0;
}
