// Points To OUT:
// _p_1[*] -> *NULL_POINTER* , MAY
// _p_1[0] -> *NULL_POINTER* , MAY
// _p_1[*] -> _q_2_2[0][0] , MAY
// _p_1[0] -> _q_2_2[0][0] , MAY
// _q_2[*] -> *NULL_POINTER* , MAY
// _q_2[0] -> *NULL_POINTER* , MAY
// _q_2[*] -> _p_1_1[0][0] , MAY
// _q_2[0] -> _p_1_1[0][0] , MAY

void exchange(int **p, int **q)
{

// Points To: none

   int *r;

// Points To:
// _p_1[0] -> *NULL_POINTER* , MAY
// _p_1[0] -> _p_1_1[0][0] , MAY
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   r = *p;

// Points To:
// _p_1[0] -> *NULL_POINTER* , MAY
// _p_1[0] -> _p_1_1[0][0] , MAY
// _q_2[0] -> *NULL_POINTER* , MAY
// _q_2[0] -> _q_2_2[0][0] , MAY
// p -> _p_1[0] , EXACT
// q -> *NULL_POINTER* , MAY
// q -> _q_2[0] , MAY
// r -> *NULL_POINTER* , MAY
// r -> _p_1_1[0][0] , MAY

   *p = *q;

// Points To:
// _p_1[*] -> *NULL_POINTER* , MAY
// _p_1[0] -> *NULL_POINTER* , MAY
// _p_1[*] -> _q_2_2[0][0] , MAY
// _p_1[0] -> _q_2_2[0][0] , MAY
// _q_2[0] -> *NULL_POINTER* , MAY
// _q_2[0] -> _q_2_2[0][0] , MAY
// p -> _p_1[0] , EXACT
// q -> _q_2[0] , EXACT
// r -> *NULL_POINTER* , MAY
// r -> _p_1_1[0][0] , MAY

   *q = r;
}

