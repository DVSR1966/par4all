
PROPER EFFECTS

void assign05()
{
   int *r;
   int i;
//             <    is read   >: i
//             <    is written>: r

   r = bar(i);
//             <    is written>: i
   i = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: r
   *r = 0;
}
int * bar(int j)
{
   return &j;
}
void foo()
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   assign05();
}

PROPER POINTER EFFECTS

void foo()
{
   assign05();
}

POINTS TO


// Points To IN: none


// Points To OUT: none

void assign05()
{

// Points To: none

   int *r;

// Points To:
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   r = bar(i);

// Points To:
// r -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

   i = 1;

// Points To:
// r -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

   *r = 0;
}

// Points To IN: none


// Points To OUT:
// bar -> j , EXACT

int * bar(int j)
{

// Points To: none

   return &j;
}

// Points To IN: none


// Points To OUT: none

void foo()
{

// Points To: none

   assign05();
}
