void global01()
{
   // BEGIN BLOCK
//               <must be written>: i
   i = &k;
//               <must be read   >: i
//               <must be written>: j
   j = i;
//               <must be read   >: i __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("i = %p", i);
   // END BLOCK
}
POINTS TO
//  {}
void global01()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   i = &k;
//  {(i,k,-Exact-)}
   j = i;
//  {(i,k,-Exact-);(j,k,-Exact-)}
   printf("i = %p", i);
   // END BLOCK
}
