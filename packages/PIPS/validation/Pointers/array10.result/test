
POINTS-TO


// Points To IN: none


// Points To OUT:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

int bar(int *p)
{

// Points To: none

   int b[100];

// Points To: none

   p = &b[0];

// Points To:
// p -> b[0] , EXACT

   a[*p++] = 2.;

// Points To:
// p -> b[1] , EXACT

   return 0;
}

// Points To IN: none


// Points To OUT:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

int foo(int *p)
{

// Points To: none

   int b[100];

// Points To: none

   p = &b[0];

// Points To:
// p -> b[0] , EXACT

   a[*p+1] = 2.;

// Points To:
// p -> b[0] , EXACT

   return 0;
}

// Points To IN: none


// Points To OUT:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

int toto(int *p)
{

// Points To: none

   int *q;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b[100];

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = &b[0];

// Points To:
// p -> b[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   a[*(q = p++)] = 2.;

// Points To:
// p -> b[1] , EXACT
// q -> b[0] , EXACT

   return 0;
}
