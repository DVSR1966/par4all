int * call22(int k)
{

// Points To: none

   int *p = k?&i:&j;

// Points To:
// p -> i , MAY
// p -> j , MAY

   return p;
}

// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *q;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   q = call22(1);

// Points To:
