int main()
{
   // BEGIN BLOCK
   struct one {
      int *first;
   } x;
   int y[10];
//               <must be written>: x[.first]

   x.first = &y[5];
   
   
   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   struct one {
      int *first;
   } x;
//  points to = {}
   int y[10];
//  points to = {}

   x.first = &y[5];
//  {(x[first],y[5],-Exact-)}
   
   
   return 0;
   // END BLOCK
}
