int main()
{
   // BEGIN BLOCK
   struct one {
      int *first;
   } x;
   int y[10];
//             <    is written>: x.first

   x.first = &y[5];
   
   
   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   struct one {
      int *first;
   } x;
//  points to = {}
   int y[10];
//  points to = {}

   x.first = &y[5];
//  {(*ANY_MODULE*:*NOWHERE*_b0,y[5],-Exact-)}
   
   
   return 0;
   // END BLOCK
}
