
PROPER  EFFECTS

int main()
{
   int *x, x1, **y, *y1, y2;
//             <    is written>: x1

   x1 = 0;
//             <    is written>: y2
   y2 = 1;
//             <    is written>: x
   x = &x1;
//             <    is written>: y1
   y1 = &y2;
//             <    is written>: y
   y = &y1;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: x
   x = *y;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x y
   *y = x;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *x, x1, **y, *y1, y2;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// y1 -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   x1 = 0;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// y1 -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   y2 = 1;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// y1 -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   x = &x1;

// Points To:
// x -> x1 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// y1 -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   y1 = &y2;

// Points To:
// x -> x1 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// y1 -> y2 , EXACT

   y = &y1;

// Points To:
// x -> x1 , EXACT
// y -> y1 , EXACT
// y1 -> y2 , EXACT

   x = *y;

// Points To:
// x -> y2 , EXACT
// y -> y1 , EXACT
// y1 -> y2 , EXACT

   *y = x;

// Points To:
// x -> y2 , EXACT
// y -> y1 , EXACT
// y1 -> y2 , EXACT

   return 0;
}
