
PROPER  EFFECTS

int main(int argc, char *argv[])
{
   int i, j;
   int *a[64];
//             <    is written>: i

   for(i = 0; i <= 63; i += 1) {
//             <    is read   >: i _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a[i] _MALLOC_EFFECTS:_MALLOC_
      a[i] = (float) malloc(64*sizeof(float));
//             <    is written>: j
      for(j = 0; j <= 63; j += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: a[i] i j
         a[i][j] = 0.;
   }

   return 0;
}

POINTS-TO

//  {(argv[*],_argv_1[0],-Exact-)}
int main(int argc, char *argv[])
{
//  {(argv[*],_argv_1[0],-Exact-)}
   int i, j;
//  {(argv[*],_argv_1[0],-Exact-)}
   int *a[64];
//  {(a[*],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(argv[*],_argv_1[0],
//   -Exact-)}

   for(i = 0; i <= 63; i += 1) {
//  {(a[*],*HEAP*_l_13[0],-MAY-);(a[*],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(argv[*],_argv_1[0],-Exact-)}
      a[i] = (float) malloc(64*sizeof(float));
//  {(a[*],*HEAP*_l_13[0],-MAY-);(a[*],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(argv[*],_argv_1[0],-Exact-)}
      for(j = 0; j <= 63; j += 1)
//  {(a[*],*HEAP*_l_13[0],-Exact-);(a[*],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(argv[*],_argv_1[0],-Exact-)}
         a[i][j] = 0.;
   }
//  {(a[*],*HEAP*_l_13[0],-MAY-);(a[*],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(argv[*],_argv_1[0],-Exact-)}

   return 0;
}
