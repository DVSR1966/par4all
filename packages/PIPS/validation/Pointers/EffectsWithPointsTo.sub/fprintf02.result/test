
POINTS-TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY


// Points To OUT: none

void fprintf02(int *p)
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY

   printf("%d", *p);

// Points To:
// p -> _p_1[0] , EXACT

   return;
}

PROPER EFFECTS WITH POINTS-TO

void fprintf02(int *p)
{
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: _p_1[0] p stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d", *p);
   return;
}
