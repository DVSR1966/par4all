
Pointer proper effects

int main()
{
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
   my_str *m, *n;
   int j;
//             <    is written>: j
   j = 2;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: m _MALLOC_EFFECTS:_MALLOC_
   m = (my_str *) malloc(sizeof(my_str));
//             <    is read   >: m
//             <    is written>: m[0].i
   m->i = 1;
//             <    is read   >: m
//             <    is written>: m[0].p
   m->p = &j;
//             <    is read   >: m
//             <    is written>: n
   n = m;

   return 0;
}

Points-to

//  {}
int main()
{
//  points to = {}
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
//  {(m,*ANY_MODULE*:*NOWHERE*,-Exact-);(n,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   my_str *m, *n;
//  {(m,*ANY_MODULE*:*NOWHERE*,-Exact-);(n,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int j;
//  {(m,*ANY_MODULE*:*NOWHERE*,-Exact-);(n,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   j = 2;
//  {(m,*ANY_MODULE*:*NOWHERE*,-Exact-);(n,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   m = (my_str *) malloc(sizeof(my_str));
//  {(m,*HEAP*_l_10,-Exact-);(n,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   m->i = 1;
//  {(m,*HEAP*_l_10,-Exact-);(n,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   m->p = &j;
//  {(*HEAP*_l_10[p],j,-Exact-);(m,*HEAP*_l_10,-Exact-);(n,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}
   n = m;
//  {(*HEAP*_l_10[p],j,-Exact-);(m,*HEAP*_l_10,-Exact-);(n,
//   *HEAP*_l_10,-Exact-)}

   return 0;
}
