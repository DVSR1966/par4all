
Pointer proper effects

int main()
{
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
   my_str *m, *n;
   int j;
//               <must be written>: j
   j = 2;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ m
   m = (my_str *) malloc(sizeof(my_str));
//               <must be read   >: m
//               <must be written>: m[0][.i]
   m->i = 1;
//               <must be read   >: m
//               <must be written>: m[0][.p]
   m->p = &j;
//               <must be read   >: m
//               <must be written>: n
   n = m;

   return 0;
}

Points-to

//  {}
int main()
{
//  points to = {}
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
//  points to = {}
   my_str *m, *n;
//  points to = {}
   int j;
//  points to = {}
   j = 2;
//  points to = {}
   m = (my_str *) malloc(sizeof(my_str));
//  {(m,*ANY_MODULE*:*HEAP**ANYWHERE*,-MAY-)}
   m->i = 1;
//  {(m,*ANY_MODULE*:*HEAP**ANYWHERE*,-MAY-)}
   m->p = &j;
//  {(m,*ANY_MODULE*:*HEAP**ANYWHERE*,-MAY-);(m[0][p],j,-Exact-)}
   n = m;
//  {(m,*ANY_MODULE*:*HEAP**ANYWHERE*,-MAY-);(m[0][p],j,-Exact-);(n,
//   *ANY_MODULE*:*HEAP**ANYWHERE*,-MAY-)}

   return 0;
}
