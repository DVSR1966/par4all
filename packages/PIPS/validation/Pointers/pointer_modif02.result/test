
PROPER  EFFECTS

int main()
{
   int *p, *q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   foo(&q, p);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = 2;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: unreachable

int main()
{

// Points To: none

   int *p, *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   *p = 1;

// Points To: unreachable


   foo(&q, p);

// Points To: unreachable

   *q = 2;

// Points To: unreachable

   return 0;
}
