
Proper effects for "main"

int main()
{
   int t[100][10][3], i;                                                /*0006*/

   int (*p)[3];                                                         /*0008*/
//             <    is written>: i
   for(i = 0; i <= 2; i += 1)
//             <    is read   >: i
//             <    is written>: p
      p = t[i];                                                         /*0010*/

   return 0;                                                            /*0012*/
}

POINTS TO for "main"

//  {}
int main()
{
//  points to = {}
   int t[100][10][3], i;                                                /*0006*/

//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int (*p)[3];                                                         /*0008*/
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   for(i = 0; i <= 2; i += 1)
//  {(p,*ANY_MODULE*:*NOWHERE*,-MAY-);(p,t[*][0],-MAY-)}
      p = t[i];                                                         /*0010*/
//  {(p,*ANY_MODULE*:*NOWHERE*,-MAY-);(p,t[*][0],-MAY-)}

   return 0;                                                            /*0012*/
}
