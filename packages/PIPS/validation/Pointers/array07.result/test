
PROPER  EFFECTS

int main()
{
   int t[100][10][3], i;

   int (*p)[3];
//             <    is written>: i
   for(i = 0; i <= 2; i += 1)
//             <    is read   >: i
//             <    is written>: p
      p = t[i];

   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int t[100][10][3], i;

//  {}
   int (*p)[3];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   for(i = 0; i <= 2; i += 1)
//  {(p,t[*][0],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
      p = t[i];
//  {(p,t[*][0],-MAY-);(p,*ANY_MODULE*:*NOWHERE*_b0,-MAY-)}

   return 0;
}
