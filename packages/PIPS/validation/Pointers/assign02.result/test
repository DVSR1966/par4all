
POINTS TO for "make_s"

C  {}
s_t make_s(int i, float x)
{
//  points to = {}
   s_t *p;
//  points to = {}
   char *malloc(int);
//  points to = {}

   p = (s_t *) malloc(sizeof(s_t));
//  {(p,*ANYWHERE*,-MAY-)}
   p->entier = i;
//  {(p,*ANYWHERE*,-MAY-)}
   p->reel = x;
//  {(p,*ANYWHERE*,-MAY-)}
   return *p;
}

