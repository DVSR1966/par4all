//  {}
void assign02()
{
//  points to = {}
   s_t s;
//  points to = {}
   int j;
//  points to = {}

   s = make_s(1, 2.);
//  points to = {}
   s.entier = 4;
//  points to = {}
   
   /* invalid lvalue in assignment */
   /* make_s(1, 2.).entier = 4; */
   
   /* But this is OK in rhs */
   j = make_s(1, 2.).entier;
}
