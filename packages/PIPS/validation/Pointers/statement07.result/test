int main()
{
   // BEGIN BLOCK
   int *x, *a, i;
//               <must be written>: i
   i = 1;
//               <must be written>: a
   a = &i;
//               <must be read   >: a
//               <must be written>: x
   x = a;

   return 0;
   // END BLOCK
}
POINTS TO
int main()
{
   // BEGIN BLOCK
   int *x, *a, i;
   i = 1;
   a = &i;
//  points to = {(a,i,-Exact-)}
   x = a;
//  points to = {(x,i,-Exact-);(a,i,-Exact-)}

   return 0;
   // END BLOCK
}
