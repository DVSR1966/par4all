
PROPER  EFFECTS

int main()
{
   int *x, *a, i;
//             <    is written>: i
   i = 1;
//             <    is written>: a
   a = &i;
//             <    is read   >: a
//             <    is written>: x
   x = a;

   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int *x, *a, i;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   i = 1;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   a = &i;
//  {(a,i,-Exact-);(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   x = a;
//  {(a,i,-Exact-);(x,i,-Exact-)}

   return 0;
}
