
PROPER POINTER EFFECTS

int main()
{
   FLGR_Data2D d2d;
   FLGR_Data2D d1;
   FLGR_Data2D d2;
//             <may be read   >: d1.row[0] d1.row[0][0].array 
//                               d1.row[0][0].array[*] d2.row[0] 
//                               d2.row[0][0].array 
//                               d2.row[0][0].array[*] d2d.row[0] 
//                               d2d.row[0][0].array 
//                               d2d.row[0][0].length 
//                               d2d.row[0][0].spp
//             <may be written>: d2d.row[0][0].array[*]
//             <    is read   >: d1.row d1.size_y d2.row d2d.row
   flgr2d_arith_add_fgINT32(&d2d, &d1, &d2);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: d2d.row d2d.row[0] 
//                               d2d.row[0][0].array 
//                               d2d.row[0][0].array[0] d2d.size 
//                               stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d%d\n", d2d.size, ((d2d.row)[0]->array)[0]);
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   FLGR_Data2D d2d;
//  {(d2d[row],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   FLGR_Data2D d1;
//  {(d1[row],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(d2d[row],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   FLGR_Data2D d2;
//  {(d1[row],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(d2[row],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(d2d[row],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   flgr2d_arith_add_fgINT32(&d2d, &d1, &d2);
//  {(d1[row],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(d2[row],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(d2d[row],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%d%d\n", d2d.size, ((d2d.row)[0]->array)[0]);
}
