
PROPER  EFFECTS

int main()
{
   FLGR_Data2D d2d;
   FLGR_Data2D d1;
   FLGR_Data2D d2;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   flgr2d_arith_add_fgINT32(&d2d, &d1, &d2);
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d%d\n", d2d.size, ((d2d.row)[0]->array)[0]);
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   FLGR_Data2D d2d;

// Points To:
// d2d.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   FLGR_Data2D d1;

// Points To:
// d1.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// d2d.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   FLGR_Data2D d2;

// Points To:
// d1.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// d2.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// d2d.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   flgr2d_arith_add_fgINT32(&d2d, &d1, &d2);

// Points To:
// d1.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// d2.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// d2d.row -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   printf("%d%d\n", d2d.size, ((d2d.row)[0]->array)[0]);
}
