
PROPER POINTER EFFECTS

int main()
{
//             <    is written>: s
   char *s = "sortie de puts";
//             <may be read   >: s[*] stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: s stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   puts(s);
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   char *s = "sortie de puts";
//  {(s,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   puts(s);
}
