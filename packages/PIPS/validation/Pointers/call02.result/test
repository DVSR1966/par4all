
POINTS TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_5 , MAY
// q[*] -> _q_4[*] , MAY


// Points To OUT: none

void call02(int i, int j, int y[10], int *q[10], tf_t *p)
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_5 , MAY
// q[*] -> _q_4[*] , MAY

   /* i can be modified locally, but it won't show in the summary
        effects... which creates a problem for transformer and
        precondition computation. */
   i = j+1;

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_5 , MAY
// q[*] -> _q_4[*] , EXACT

   y[i] = 0;

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_5 , MAY
// q[*] -> _q_4[*] , EXACT

   p->one = 1;

// Points To:
// p -> _p_5 , EXACT
// q[*] -> _q_4[*] , EXACT

   (p->two)[j] = 2.;

// Points To:
// p -> _p_5 , EXACT
// q[*] -> _q_4[*] , EXACT

   *q[i] = 3;
}
