int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
   fp = fopen("file.txt", "rb");
   n = fread(&buf[0], sizeof(double), 50, fp);
   if (n==-1) 
      printf("fread failed");
   fclose(fp);
}
//               <may be read    >: stdout stdout[0] 
//                                  __IO_EFFECTS:LUNS[1] 
//                                  __IO_EFFECTS:LUNS[*]
//               <may be written >: stdout[0] __IO_EFFECTS:LUNS[1] 
//                                  __IO_EFFECTS:LUNS[*]
int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
//               <must be written>: fp
   fp = fopen("file.txt", "rb");
//               <may be read    >: __IO_EFFECTS:LUNS[*] fp[0]
//               <may be written >: __IO_EFFECTS:LUNS[*] buf[*] fp[0]
//               <must be read   >: fp
//               <must be written>: n
   n = fread(&buf[0], sizeof(double), 50, fp);
//               <may be read    >: stdout stdout[0] 
//                                  __IO_EFFECTS:LUNS[1]
//               <may be written >: stdout[0] __IO_EFFECTS:LUNS[1]
//               <must be read   >: n
   if (n==-1) 
//               <may be read    >: stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: stdout __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
      printf("fread failed");
//               <may be read    >: __IO_EFFECTS:LUNS[*] fp[0]
//               <may be written >: __IO_EFFECTS:LUNS[*] fp[0]
//               <must be read   >: fp
   fclose(fp);
}

POINTS TO

//  {}
int main()
{
//  points to = {}
   FILE *fp;
//  points to = {}
   size_t n;
//  points to = {}
   char buf[200];
//  points to = {}
   fp = fopen("file.txt", "rb");
//  points to = {}
   n = fread(&buf[0], sizeof(double), 50, fp);
//  points to = {}
   if (n==-1) 
//  points to = {}
      printf("fread failed");
//  points to = {}
   fclose(fp);
}
