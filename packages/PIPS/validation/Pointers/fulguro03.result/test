
PROPER POINTER EFFECTS

void flgr2d_set_data_vector_fgUINT16(FLGR_Data2D *dat, int row, int col, FLGR_Vector *vct)
{
//             <    is read   >: vct vct[0].array
//             <    is written>: array_s
   fgUINT16 *array_s = (fgUINT16 *) vct->array;
//             <    is read   >: dat dat[0].array dat[0].array[row] 
//                               row
//             <    is written>: array_d
   fgUINT16 *array_d = (fgUINT16 *) (dat->array)[row];
//             <    is read   >: vct vct[0].spp
//             <    is written>: I_0
   int I_0 = vct->spp;
   {
      register fgUINT16 val;
      register int i;
      register int k;
      unsigned short _return0;
//             <    is read   >: I_0 col
//             <    is written>: i
      i = col*I_0;
//             <    is read   >: I_0
//             <    is written>: k
      for(k = 0; k <= I_0-1; k += 1) {
//             <    is read   >: I_0 array_s array_s[k] k
//             <    is written>: _return0
         _return0 = array_s[k];
//             <    is read   >: I_0 _return0
//             <    is written>: val
         val = _return0;
//             <    is read   >: I_0 array_d i val
//             <    is written>: array_d[i]
         array_d[i] = val;
//             <    is read   >: I_0 i
//             <    is written>: i
         i++;
      }
   }
}

POINTS-TO

//  {(dat,_dat_1,-Exact-);(vct,_vct_2,-Exact-)}
void flgr2d_set_data_vector_fgUINT16(FLGR_Data2D *dat, int row, int col, FLGR_Vector *vct)
{
//  {(dat,_dat_1,-Exact-);(vct,_vct_2,-Exact-)}
   fgUINT16 *array_s = (fgUINT16 *) vct->array;
//  {(array_s,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(dat,_dat_1,
//   -Exact-);(vct,_vct_2,-Exact-)}
   fgUINT16 *array_d = (fgUINT16 *) (dat->array)[row];
//  {(array_d,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(array_s,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(dat,_dat_1,-Exact-);(vct,
//   _vct_2,-Exact-)}
   int I_0 = vct->spp;
   {
//  {(array_d,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(array_s,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(dat,_dat_1,-Exact-);(vct,
//   _vct_2,-Exact-)}
      register fgUINT16 val;
//  {(array_d,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(array_s,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(dat,_dat_1,-Exact-);(vct,
//   _vct_2,-Exact-)}
      register int i;
//  {(array_d,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(array_s,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(dat,_dat_1,-Exact-);(vct,
//   _vct_2,-Exact-)}
      register int k;
//  {(array_d,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(array_s,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(dat,_dat_1,-Exact-);(vct,
//   _vct_2,-Exact-)}
      unsigned short _return0;
//  {(array_d,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(array_s,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(dat,_dat_1,-Exact-);(vct,
//   _vct_2,-Exact-)}
      i = col*I_0;
//  {(array_d,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(array_s,
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(dat,_dat_1,-Exact-);(vct,
//   _vct_2,-Exact-)}
      for(k = 0; k <= I_0-1; k += 1) {
//  points to = {}
         _return0 = array_s[k];
//  points to = {}
         val = _return0;
//  points to = {}
         array_d[i] = val;
//  points to = {}
         i++;
      }
   }
}
