
PROPER  EFFECTS

int foo(int *p, int *q)
{
   int b[*(q = p)];
   return 0;
}

POINTS-TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_1 , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_2 , MAY


// Points To OUT: none

int foo(int *p, int *q)
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1 , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_2 , MAY

   int b[*(q = p)];

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1 , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_2 , MAY

   return 0;
}
