
PROPER  EFFECTS

int main()
{
   int *a, *b, c, d;
//             <    is written>: c
   c = 0;
//             <    is written>: d
   d = 1;
//             <    is written>: b
   b = &c;
//             <    is read   >: c

   while (c>0) {
//             <    is read   >: b
//             <    is written>: a
      a = b;
//             <    is written>: b
      b = &d;
   }
//             <    is written>: b

   b = &c;

   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int *a, *b, c, d;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(b,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   c = 0;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(b,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   d = 1;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(b,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   b = &c;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(b,c,-Exact-)}

   while (c>0) {
//  {(a,d,-MAY-);(a,*ANY_MODULE*:*NOWHERE*_b0,-MAY-);(a,c,-MAY-);(b,d,
//   -MAY-);(b,c,-MAY-)}
      a = b;
//  {(a,d,-MAY-);(a,*ANY_MODULE*:*NOWHERE*_b0,-MAY-);(a,c,-MAY-);(b,d,
//   -MAY-);(b,c,-MAY-)}
      b = &d;
   }
//  {(a,d,-MAY-);(a,*ANY_MODULE*:*NOWHERE*_b0,-MAY-);(a,c,-MAY-);(b,d,
//   -MAY-);(b,c,-MAY-)}

   b = &c;
//  {(a,d,-MAY-);(a,*ANY_MODULE*:*NOWHERE*_b0,-MAY-);(a,c,-MAY-);(b,c,
//   -Exact-)}

   return 0;
}
