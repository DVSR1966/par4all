
POINTS TO for "main"


// Points To IN: none


// Points To OUT: unreachable

int main()
{

// Points To: none

   int *p = (void *) 0, i;

// Points To:
// p -> *NULL_POINTER* , EXACT


   if (p)

// Points To: unreachable

      p = &i;
   else

// Points To:
// p -> *NULL_POINTER* , EXACT

      *p = 1;

// Points To: unreachable


