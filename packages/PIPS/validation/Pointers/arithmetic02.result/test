
PROPER POINTER EFFECTS

int main()
{
   int *pdeb, *pfin, *p;
//             <    is written>: pdeb

   pdeb = &t[0];
//             <    is written>: pfin
   /*   repère le premier élément de t   */
   pfin = &t[10-1];
//             <    is read   >: pdeb
//             <    is written>: p
   /*   repère le dernier élément de t   */
   
   p = pdeb;
//             <    is read   >: p pfin
   while (p<=pfin) {
//             <    is read   >: p p[0]
      if (*p==0)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: p pdeb stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
         printf("%d ", p-pdeb);
//             <    is read   >: p
//             <    is written>: p
      p++;
   }
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("\n");
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *pdeb, *pfin, *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(pdeb,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(pfin,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   pdeb = &t[0];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(pdeb,t[0],-Exact-);(pfin,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   /*   repère le premier élément de t   */
   pfin = &t[10-1];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(pdeb,t[0],-Exact-);(pfin,
//   t[10-1],-Exact-)}
   /*   repère le dernier élément de t   */
   
   p = pdeb;
//  {(p,t[0],-Exact-);(pdeb,t[0],-Exact-);(pfin,t[10-1],-Exact-)}
   while (p<=pfin) {
//  points to = {}
      if (*p==0)
//  {(p,t[0],-MAY-);(p,*ANY_MODULE*:*ANYWHERE*_b0,-MAY-);(pdeb,t[0],
//   -Exact-);(pfin,t[10-1],-Exact-)}
         printf("%d ", p-pdeb);
//  points to = {}
      p++;
   }
//  {(p,t[0],-MAY-);(p,*ANY_MODULE*:*ANYWHERE*_b0,-MAY-);(pdeb,t[0],
//   -Exact-);(pfin,t[10-1],-Exact-)}
   printf("\n");
//  {(p,t[0],-MAY-);(p,*ANY_MODULE*:*ANYWHERE*_b0,-MAY-);(pdeb,t[0],
//   -Exact-);(pfin,t[10-1],-Exact-)}
   return 0;
}
