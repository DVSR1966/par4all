
PROPER  EFFECTS

int use_def_elim01()
{
   int i, *x, *y;
//             <    is written>: i
   i = 2;
//             <    is written>: x
   x = &i;
//             <    is read   >: x
//             <    is written>: y
   y = x;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: y
   *y = 1;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
   return *y;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int use_def_elim01()
{

// Points To: none

   int i, *x, *y;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   i = 2;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   x = &i;

// Points To:
// x -> i , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   y = x;

// Points To:
// x -> i , EXACT
// y -> i , EXACT

   *y = 1;

// Points To:
// x -> i , EXACT
// y -> i , EXACT

   return *y;
}
