
PROPER  EFFECTS

void pointer01(int n, float *p, float *q)
{
   int i;
//             <    is read   >: n
//             <    is written>: i

   for(i = 0; i <= n-1; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      p[i] = q[i];
}

POINTS-TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_2[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_3[0] , MAY


// Points To OUT: none

void pointer01(int n, float *p, float *q)
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_2[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_3[0] , MAY

   int i;

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_2[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_3[0] , MAY


   for(i = 0; i <= n-1; i += 1)

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_2[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_3[0] , MAY

      p[i] = q[i];
}
