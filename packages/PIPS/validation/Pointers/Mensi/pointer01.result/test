
PROPER  EFFECTS

void pointer01(int n, float *p, float *q)
{
   int i;
//             <    is read   >: p q
   p!=0&&q!=0?(void) 0:__assert_fail("p!=0&&q!=0", "./pointer01.c", 9, (const char *) 0);
//             <    is read   >: n
//             <    is written>: i

   for(i = 0; i <= n-1; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      p[i] = q[i];

   return;
}

POINTS-TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_2[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_3[0] , MAY


// Points To OUT: none

void pointer01(int n, float *p, float *q)
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_2[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_3[0] , MAY

   int i;

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_2[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_3[0] , MAY

   p!=0&&q!=0?(void) 0:__assert_fail("p!=0&&q!=0", "./pointer01.c", 9, (const char *) 0);

// Points To:
// p -> _p_2[0] , EXACT
// q -> _q_3[0] , EXACT


   for(i = 0; i <= n-1; i += 1)

// Points To:
// p -> _p_2[0] , EXACT
// q -> _q_3[0] , EXACT

      p[i] = q[i];

// Points To:
// p -> _p_2[0] , EXACT
// q -> _q_3[0] , EXACT


   return;
}
