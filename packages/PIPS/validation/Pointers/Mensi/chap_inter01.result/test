
PROPER  EFFECTS

int main()
{
   struct {
      int *q;
   } s1, s2, *p, *r;
   int i, i1, i2;
//             <    is written>: s1.q

   s1.q = &i1;
//             <    is written>: s2.q
   s2.q = &i2;
//             <    is read   >: i

   if (i>0)
//             <    is written>: p r
      p = &s1, r = &s2;
   else
//             <    is written>: p r
      p = &s2, r = &s1;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   
   
   foo(p->q, r->q);

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   struct {
      int *q;
   } s1, s2, *p, *r;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s1.q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2.q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i, i1, i2;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s1.q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s2.q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   s1.q = &i1;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s1.q -> i1 , EXACT
// s2.q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   s2.q = &i2;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s1.q -> i1 , EXACT
// s2.q -> i2 , EXACT


   if (i>0)

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s1.q -> i1 , EXACT
// s2.q -> i2 , EXACT

      p = &s1, r = &s2;
   else

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// s1.q -> i1 , EXACT
// s2.q -> i2 , EXACT

      p = &s2, r = &s1;

// Points To:
// p -> s1 , MAY
// p -> s2 , MAY
// r -> s1 , MAY
// r -> s2 , MAY
// s1.q -> i1 , EXACT
// s2.q -> i2 , EXACT

   
   
   foo(p->q, r->q);

// Points To:
// p -> s1 , MAY
// p -> s2 , MAY
// r -> s1 , MAY
// r -> s2 , MAY
// s1.q -> i1 , EXACT
// s2.q -> i2 , EXACT


   return 0;
}
