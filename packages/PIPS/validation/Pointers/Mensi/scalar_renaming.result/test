
PROPER  EFFECTS

int scalar_renaming()
{
   int *p, i, T, A[100], B[100], C[100], D[100];
//             <    is written>: p
   p = &T;
//             <    is written>: i
   for(i = 0; i <= 99; i += 1) {
//             <    is read   >: A[i] B[i] i
//             <    is written>: T
      T = A[i]+B[i];
//             <    is read   >: T i
//             <    is written>: C[i]
      /* S1 */
      C[i] = T+T;
//             <    is read   >: B[i] D[i] i
//             <    is written>: T
      /* S2 */
      T = D[i]-B[i];
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
      /* S3 */
      *p = 1;
//             <    is read   >: T i
//             <    is written>: A[i]
      /* S4 */
      A[i] = T*T;
   }
//             <    is read   >: T

   return T;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int scalar_renaming()
{

// Points To: none

   int *p, i, T, A[100], B[100], C[100], D[100];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = &T;

// Points To:
// p -> T , EXACT

   for(i = 0; i <= 99; i += 1) {

// Points To:
// p -> T , EXACT

      T = A[i]+B[i];

// Points To:
// p -> T , EXACT

      /* S1 */
      C[i] = T+T;

// Points To:
// p -> T , EXACT

      /* S2 */
      T = D[i]-B[i];

// Points To:
// p -> T , EXACT

      /* S3 */
      *p = 1;

// Points To:
// p -> T , EXACT

      /* S4 */
      A[i] = T*T;
   }

// Points To:
// p -> T , EXACT


   return T;
}
