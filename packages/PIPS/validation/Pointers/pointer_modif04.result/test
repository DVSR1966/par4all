void init_s(s_t s, int max)
{
   // BEGIN BLOCK
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_ max
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ s[.tab]

   s.tab = (float *) malloc(max*sizeof(float));
//               <must be read   >: max
//               <must be written>: s[.max]
   s.max = max;
   // END BLOCK
}

