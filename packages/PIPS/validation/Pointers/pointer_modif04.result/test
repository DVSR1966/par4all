
PROPER POINTER EFFECTS

int main()
{
   s_t s;
   int j;
//             <    is read   >: s.max s.tab _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   init_s(s, 10);
//             <may be written>: s.tab[*]
//             <    is read   >: s.max s.tab

   compute_s(s, 10);
   
   
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   s_t s;
//  {(s[tab],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int j;
//  {(s[tab],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   init_s(s, 10);
//  {(s[tab],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   compute_s(s, 10);
//  {(s[tab],*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   
   
   return 0;
}
