
Proper effects for "count"

int count(list *p)
{
//             <    is read   >: p
//             <    is written>: q
   list *q = p;
//             <    is written>: i
   int i = 0;
//             <    is read   >: q
   while (q!=(void *) 0) {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: q
      q = q->next;
//             <    is read   >: i
//             <    is written>: i
      // Separate statement to observe the points-to information
      i++;
   }
//             <    is read   >: i
   return i;
}

