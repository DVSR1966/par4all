
POINTS TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   struct p_s {
      struct q_s *q;
   };

// Points To: none

   struct q_s {
      int r;
   };

// Points To: none

   struct p_s *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT

   struct q_s *x;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// x -> *ANY_MODULE*:*NOWHERE* , EXACT


   p = (struct p_s *) malloc(sizeof(struct p_s ));

// Points To:
// p -> *ANY_MODULE*:*HEAP**ANYWHERE* , MAY
// x -> *ANY_MODULE*:*NOWHERE* , EXACT

   p->q = x;

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> *ANY_MODULE*:*NOWHERE* , MAY
// p -> *ANY_MODULE*:*HEAP**ANYWHERE* , MAY
// x -> *ANY_MODULE*:*NOWHERE* , EXACT


