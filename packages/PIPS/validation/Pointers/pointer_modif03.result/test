
PROPER POINTER EFFECTS

int main()
{
   s_t s;
   int j;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: s.entier s.reel 
//                               _MALLOC_EFFECTS:_MALLOC_

   s = make_s(1, 2.);
//             <    is written>: s.entier
   s.entier = 4;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: j _MALLOC_EFFECTS:_MALLOC_

   j = make_s(1, 2.).entier;
//             <    is read   >: j

   return j;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   s_t s;
//  points to = {}
   int j;
//  points to = {}

   s = make_s(1, 2.);
//  points to = {}
   s.entier = 4;
//  points to = {}

   j = make_s(1, 2.).entier;
//  points to = {}

   return j;
}
