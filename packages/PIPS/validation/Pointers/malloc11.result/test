
PROPER  EFFECTS

int main()
{
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: i p _MALLOC_EFFECTS:_MALLOC_
   int i = 1, *p = foo();
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   free(p);

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 1, *p = foo();

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY


   free(p);

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   return 0;
}
