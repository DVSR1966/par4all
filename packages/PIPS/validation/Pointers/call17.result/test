
PROPER  EFFECTS

void call17(int *x)
{
   /* check aliases */
//             <    is read   >: x
//             <    is written>: y
   int *y = x;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: y
   y[0] = 1;
}

POINTS-TO


// Points To IN:
// x -> *NULL_POINTER* , MAY
// x -> _x_1 , MAY


// Points To OUT: none

void call17(int *x)
{
   /* check aliases */

// Points To:
// x -> *NULL_POINTER* , MAY
// x -> _x_1 , MAY

   int *y = x;

// Points To:
// x -> *NULL_POINTER* , MAY
// x -> _x_1 , MAY
// y -> *NULL_POINTER* , MAY
// y -> _x_1 , MAY

   y[0] = 1;
}
