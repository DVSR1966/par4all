
Pointer proper effects

int main()
{
   // BEGIN BLOCK
   int i;
//             <    is written>: p
   int *p = 0;
//             <    is written>: q
   int **q = 0;
//             <    is read   >: p
//             <    is written>: p[0]

   *p = 1;
//             <    is read   >: q
//             <    is written>: q[0]
   *q = &i;

   return 0;
   // END BLOCK
}

Points-to

//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int i;
//  points to = {}
   int *p = 0;
//  {(p,*NULL_POINTER*,-Exact-)}
   int **q = 0;
//  {(p,*NULL_POINTER*,-Exact-);(q,*NULL_POINTER*,-Exact-)}

   *p = 1;
//  {(p,*NULL_POINTER*,-Exact-);(q,*NULL_POINTER*,-Exact-)}
   *q = &i;
//  {(*NULL_POINTER*,i,-Exact-);(p,*NULL_POINTER*,-Exact-);(q,
//   *NULL_POINTER*,-Exact-)}

   return 0;
   // END BLOCK
}
