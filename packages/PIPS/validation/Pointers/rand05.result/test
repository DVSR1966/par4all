
PROPER  EFFECTS

int main(void)
{
   int i1;
//             <    is written>: pi1
   int *pi1 = &i1;
   int **pp;
//             <    is read   >: __TIME_EFFECTS:KALI
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED 
//                               __TIME_EFFECTS:KALI
   srand(time((void *) 0));
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout _RANDOM_EFFECTS:GEN_SEED 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED 
//                               __IO_EFFECTS:LUNS[1]
   if (alea())
//             <    is written>: pp
      pp = &pi1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pp
   *pp = (void *) 0;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: pi1 pp stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%p %p \n", pp, pi1);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(void)
{

// Points To: none

   int i1;

// Points To: none

   int *pi1 = &i1;

// Points To:
// pi1 -> i1 , EXACT

   int **pp;

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   srand(time((void *) 0));

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (alea())

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      pp = &pi1;

// Points To:
// pi1 -> i1 , EXACT
// pp -> pi1 , MAY
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY

   *pp = (void *) 0;

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> *NULL_POINTER* , MAY
// pi1 -> *NULL_POINTER* , MAY
// pi1 -> i1 , MAY
// pp -> pi1 , MAY
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY

   printf("%p %p \n", pp, pi1);

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> *NULL_POINTER* , MAY
// pi1 -> *NULL_POINTER* , MAY
// pi1 -> i1 , MAY
// pp -> pi1 , MAY
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY

   return 0;
}
