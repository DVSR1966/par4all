
POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(void)
{

// Points To: none

   int i1;

// Points To: none

   int *pi1 = &i1;

// Points To:
// pi1 -> i1 , EXACT

   int **pp;

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   srand(time((void *) 0));

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (alea())

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      pp = &pi1;

// Points To:
// pi1 -> i1 , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// pp -> pi1 , MAY

   *pp = (void *) 0;

// Points To:
// pi1 -> *NULL_POINTER* , EXACT
// pp -> pi1 , EXACT

   printf("%p %p \n", pp, pi1);

// Points To:
// pi1 -> *NULL_POINTER* , EXACT
// pp -> pi1 , EXACT

   return 0;
}
