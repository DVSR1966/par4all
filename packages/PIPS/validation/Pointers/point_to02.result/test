
PROPER POINTER EFFECTS

void point_to02()
{
   struct s {
      int a;
      int b[10];
   } c, *p;
//             <    is read   >: p
//             <    is written>: p[0].a

   p->a = 1;
//             <    is read   >: p
//             <    is written>: p[0].b[2]
   (p->b)[2] = 3;
}

POINTS-TO

//  {}
void point_to02()
{
//  points to = {}
   struct s {
      int a;
      int b[10];
   } c, *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p->a = 1;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   (p->b)[2] = 3;
}
