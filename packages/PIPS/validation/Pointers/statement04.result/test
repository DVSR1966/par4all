
POINTS-TO


// Points To IN: none


// Points To OUT: unreachable

int main()
{

// Points To: none

   int **x, *y, k, h;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   k = 1;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   h = 2;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   y = &h;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> h , EXACT

   *x = y;

// Points To: unreachable

   printf("x = %p \n", x);

// Points To: unreachable

   return 0;
}
