int main()
{
   // BEGIN BLOCK
   int **x, *y, k, h;
//             <    is written>: k
   k = 1;
//             <    is written>: h
   h = 2;
//             <    is written>: y
   y = &h;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x y
   *x = y;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("x = %p \n", x);
   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int **x, *y, k, h;
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   k = 1;
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   h = 2;
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   y = &h;
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,h,-Exact-)}
   *x = y;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,*ANY_MODULE*:*ANYWHERE*_b0,
//   -Exact-);(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,h,-Exact-)}
   printf("x = %p \n", x);
//  {(*ANY_MODULE*:*ANYWHERE*_b0,*ANY_MODULE*:*ANYWHERE*_b0,
//   -Exact-);(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,h,-Exact-)}
   return 0;
   // END BLOCK
}
