
PROPER  EFFECTS

int struct05()
{
   struct two {
      int first;
      int second;
   } x[10];
//             <    is written>: i
   int i = 0;
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
      if (1)
//             <    is read   >: i x[i].second
//             <    is written>: x[i].first
         x[i].first = x[i].second;
      else
//             <    is read   >: i x[i].first
//             <    is written>: x[i].second
         x[i].second = x[i].first;
}

POINTS-TO

//  {}
int struct05()
{
//  {}
   struct two {
      int first;
      int second;
   } x[10];
//  {}
   int i = 0;
//  {}

   for(i = 0; i <= 9; i += 1)
//  {}
      if (1)
//  {}
         x[i].first = x[i].second;
      else
//  {}
         x[i].second = x[i].first;
}
