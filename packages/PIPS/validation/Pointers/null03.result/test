
PROPER  EFFECTS

int main()
{
   int *p;
   int i;
//             <    is written>: p
   p = &i;
//             <    is written>: p
   p = (void *) 0;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = &i;

// Points To:
// p -> i , EXACT

   p = (void *) 0;

// Points To:
// p -> *NULL_POINTER* , EXACT


   return 0;
}
