
PROPER EFFECTS

int main()
{
   int i;
//             <    is written>: i
   i = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p = &i;

   return 0;
}

PROPER POINTER EFFECTS

int main()
{
   int i;
//             <    is written>: i
   i = 1;
//             <    is read   >: p
//             <    is written>: p[0]
   *p = &i;

   return 0;
}

POINTS TO


// Points To IN: none


// Points To OUT:
// p -> p , EXACT

int main()
{

// Points To: none

   int i;

// Points To: none

   i = 1;

// Points To: none

   *p = &i;

// Points To:
// p -> i , EXACT
// p -> p , EXACT


   return 0;
}
