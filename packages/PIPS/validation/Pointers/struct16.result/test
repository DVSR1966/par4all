
PROPER  EFFECTS

int main()
{
   int y[10];
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x.first
   *x.first = &y[5];
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x.second
   *x.second = &y[6];
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x.third
   *x.third = &y[7];

   return 0;
}

POINTS-TO


// Points To IN:
// x.first -> *NULL_POINTER* , MAY
// x.second -> *NULL_POINTER* , MAY
// x.third -> *NULL_POINTER* , MAY
// x.first -> _x_1__0[0] , MAY
// x.second -> _x_2__0[0] , MAY
// x.third -> _x_3__0[0] , MAY


// Points To OUT: none

int main()
{

// Points To: none

   int y[10];

// Points To:
// x.first -> *NULL_POINTER* , MAY
// x.first -> _x_1__0[0] , MAY

   *x.first = &y[5];

// Points To:
// _x_1__0[*] -> y[5] , MAY
// _x_1__0[0] -> y[5] , EXACT
// x.second -> *NULL_POINTER* , MAY
// x.first -> _x_1__0[0] , EXACT
// x.second -> _x_2__0[0] , MAY

   *x.second = &y[6];

// Points To:
// _x_1__0[*] -> y[5] , MAY
// _x_1__0[0] -> y[5] , EXACT
// _x_2__0[*] -> y[6] , MAY
// _x_2__0[0] -> y[6] , EXACT
// x.third -> *NULL_POINTER* , MAY
// x.first -> _x_1__0[0] , EXACT
// x.second -> _x_2__0[0] , EXACT
// x.third -> _x_3__0[0] , MAY

   *x.third = &y[7];

// Points To:
// _x_1__0[*] -> y[5] , MAY
// _x_1__0[0] -> y[5] , EXACT
// _x_2__0[*] -> y[6] , MAY
// _x_2__0[0] -> y[6] , EXACT
// _x_3__0[*] -> y[7] , MAY
// _x_3__0[0] -> y[7] , EXACT
// x.first -> _x_1__0[0] , EXACT
// x.second -> _x_2__0[0] , EXACT
// x.third -> _x_3__0[0] , EXACT


   return 0;
}
