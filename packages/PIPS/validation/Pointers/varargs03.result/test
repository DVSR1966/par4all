
PROPER POINTER EFFECTS

int main()
{
   FILE *s;
   char *fmt;
   char *ch;
   int tab[5];
   int r;
//             <may be read   >: ch[*] fmt[*] s[0] tab[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: ch[*] s[0] tab[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <    is read   >: ch fmt s
//             <    is written>: r
   r = varargs03(s, fmt, ch, tab);
//             <    is read   >: r
   return r;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   FILE *s;
//  {(s,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   char *fmt;
//  {(fmt,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(s,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   char *ch;
//  {(ch,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(fmt,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(s,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   int tab[5];
//  {(ch,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(fmt,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(s,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   int r;
//  {(ch,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(fmt,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(s,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   r = varargs03(s, fmt, ch, tab);
//  {(ch,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(fmt,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-);(s,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   return r;
}
