
POINTS-TO


// Points To IN: none


// Points To OUT: none

void sort01()
{

// Points To: none

   int a[10];

// Points To: none

   int *p = foo(), *q = foo();

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

   if (p==q)

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

      p = &a[4];
   else

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

      p = &a[2];

// Points To:
// p -> a[2] , MAY
// p -> a[4] , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

   return;
}
