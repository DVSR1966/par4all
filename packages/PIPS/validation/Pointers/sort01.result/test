
PROPER  EFFECTS

void sort01()
{
   int a[10];
   int *p, *q;
//             <    is read   >: p q
   if (p==q)
//             <    is written>: p
      p = &a[4];
   else
//             <    is written>: p
      p = &a[2];
   return;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void sort01()
{

// Points To: none

   int a[10];

// Points To: none

   int *p, *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (p==q)

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &a[4];
   else

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &a[2];

// Points To:
// p -> a[2] , MAY
// p -> a[4] , MAY
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return;
}
