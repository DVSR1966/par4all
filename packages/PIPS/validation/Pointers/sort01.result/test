
Points-to for "foo"


// Points To IN: none


// Points To OUT:
// foo -> *NULL_POINTER* , EXACT

int * foo()
{

// Points To: none

   return (void *) 0;
}

Points-to for "sort01"


// Points To IN: none


// Points To OUT: none

void sort01()
{

// Points To: none

   int a[10];

// Points To:
// p -> *NULL_POINTER* , EXACT

   int *p = foo(), *q = foo();

// Points To:
// p -> *NULL_POINTER* , EXACT
// q -> *NULL_POINTER* , EXACT


   a[0] = 1;

// Points To:
// p -> *NULL_POINTER* , EXACT
// q -> *NULL_POINTER* , EXACT


   if (p==q)

// Points To:
// p -> *NULL_POINTER* , EXACT
// q -> *NULL_POINTER* , EXACT

      p = &a[4];
   else

// Points To: unreachable

      p = &a[2];

// Points To:
// p -> a[4] , EXACT
// q -> *NULL_POINTER* , EXACT


   return;
}
