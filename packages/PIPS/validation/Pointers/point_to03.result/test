
PROPER POINTER EFFECTS

void point_to03()
{
   typedef double a_t[10][20];
   a_t a;
   a_t *p;
//             <    is written>: p

   p = &a;
//             <    is read   >: p
//             <    is written>: p[0][2][3]
   ((*p)[2])[3] = 1.5;
}

POINTS-TO

//  {}
void point_to03()
{
//  points to = {}
   typedef double a_t[10][20];
//  points to = {}
   a_t a;
//  points to = {}
   a_t *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   p = &a;
//  {(p,a,-Exact-)}
   ((*p)[2])[3] = 1.5;
}
