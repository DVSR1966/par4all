Proper effects using points_to

int main()
{
   int ***a1;
   int ***a2;

   int **b3;
   int *c4;
   int **l;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ a1

   a1 = (int ***) malloc(sizeof(int * *));
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_ a1
//               <must be written>: *ANY_MODULE*:*HEAP**ANYWHERE* 
//                                  _MALLOC_EFFECTS:_MALLOC_
   *a1 = (int **) malloc(sizeof(int *));
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: *ANY_MODULE*:*HEAP**ANYWHERE* 
//                                  _MALLOC_EFFECTS:_MALLOC_ a1
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_
   **a1 = (int *) malloc(sizeof(int));
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: *ANY_MODULE*:*HEAP**ANYWHERE* a1

   ***a1 = 1;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ a2

   a2 = (int ***) malloc(sizeof(int * *));
//               <must be read   >: *ANY_MODULE*:*HEAP**ANYWHERE* a1 
//                                  a2
//               <must be written>: *ANY_MODULE*:*HEAP**ANYWHERE*
   *a2 = *a1;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ b3

   b3 = (int **) malloc(sizeof(int *));
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_ b3
//               <must be written>: *ANY_MODULE*:*HEAP**ANYWHERE* 
//                                  _MALLOC_EFFECTS:_MALLOC_
   *b3 = (int *) malloc(sizeof(int));
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: *ANY_MODULE*:*HEAP**ANYWHERE* b3
   **b3 = 3;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ c4

   c4 = (int *) malloc(sizeof(int));
//               <must be read   >: c4
//               <must be written>: *ANY_MODULE*:*HEAP**ANYWHERE*
   *c4 = 4;
//               <must be read   >: *ANY_MODULE*:*HEAP**ANYWHERE* a2
//               <must be written>: l

   l = *a2;
//               <must be read   >: a1 b3
//               <must be written>: *ANY_MODULE*:*HEAP**ANYWHERE*
   *a1 = b3;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: c4 l
   *l = c4;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE* 
//                                  *ANY_MODULE*:*ANYWHERE* 
//                                  *ANY_MODULE*:*HEAP**ANYWHERE* 
//                                  *ANY_MODULE*:*HEAP**ANYWHERE* 
//                                  stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: *ANY_MODULE*:*HEAP**ANYWHERE* 
//                                  stdout __IO_EFFECTS:LUNS[1] a1
//               <must be written>: __IO_EFFECTS:LUNS[1]

   printf("%d\n", ***a1);
//               <may be read    >: stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: *ANY_MODULE*:*HEAP**ANYWHERE* 
//                                  *ANY_MODULE*:*HEAP**ANYWHERE*[0] 
//                                  *ANY_MODULE*:*HEAP**ANYWHERE*[0][0
//                                  ] stdout __IO_EFFECTS:LUNS[1] a2
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", ***a2);

   return 0;
}

