
Proper effects for "main"

int main()
{
   int ***a1;
   int ***a2;

   int **b3;
   int *c4;
   int **l;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ a1

   a1 = (int ***) malloc(sizeof(int * *));
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_ a1
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_
   *a1 = (int **) malloc(sizeof(int *));
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_
   **a1 = (int *) malloc(sizeof(int));
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <may be written >: *ANY_MODULE*:*ANYWHERE*

   ***a1 = 1;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ a2

   a2 = (int ***) malloc(sizeof(int * *));
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
   *a2 = *a1;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ b3

   b3 = (int **) malloc(sizeof(int *));
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_ b3
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_
   *b3 = (int *) malloc(sizeof(int));
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
   **b3 = 3;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ c4

   c4 = (int *) malloc(sizeof(int));
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: c4
   *c4 = 4;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <must be written>: l

   l = *a2;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: a1 b3
   *a1 = b3;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: c4 l
   *l = c4;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE* stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: stdout __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]

   printf("%d\n", ***a1);
//               <may be read    >: *ANY_MODULE*:*ANYWHERE* stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: stdout __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", ***a2);

   return 0;
}
POINTS TO for "main"

//  {}
int main()
{
//  points to = {}
   int ***a1;
//  points to = {}
   int ***a2;

//  points to = {}
   int **b3;
//  points to = {}
   int *c4;
//  points to = {}
   int **l;
//  points to = {}

   a1 = (int ***) malloc(sizeof(int * *));
//  {(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-)}
   *a1 = (int **) malloc(sizeof(int *));
//  {(a1[0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,
//   *ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-)}
   **a1 = (int *) malloc(sizeof(int));
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-)}

   ***a1 = 1;
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-)}

   a2 = (int ***) malloc(sizeof(int * *));
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a2,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-)}
   *a2 = *a1;
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a2,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-)}

   b3 = (int **) malloc(sizeof(int *));
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a2,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-);(b3,
//   *ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-)}
   *b3 = (int *) malloc(sizeof(int));
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a2,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-);(b3[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(b3,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-)}
   **b3 = 3;
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a2,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-);(b3[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(b3,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-)}

   c4 = (int *) malloc(sizeof(int));
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a2,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-);(b3[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(b3,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(c4,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-)}
   *c4 = 4;
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a2,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-);(b3[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(b3,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(c4,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-)}

   l = *a2;
//  {(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(a1,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a2,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-);(b3[0],
//   *ANY_MODULE*:*ANYWHERE*,-MAY-);(b3,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(c4,*ANY_MODULE*:*HEAP**ANYWHERE*,-Exact-)}
   *a1 = b3;
//  {(*ANY_MODULE*:*HEAP**ANYWHERE*,*ANY_MODULE*:*HEAP**ANYWHERE*,
//   -Exact-);(a1[0][0],*ANY_MODULE*:*ANYWHERE*,-MAY-);(a1[0],
