{
   int nbout;
//             <    is written>: col
   int col = 0;
//             <    is written>: indent
   int indent = 0;
   int ui32;
   int i32;
   static char buffer[2000];
//             <    is read   >: my_fputc

   if (!my_fputc)
      return 0;
//             <    is written>: nbout

   nbout = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: col
   *va_arg(pa, int *) = col;
}

