
POINTS-TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_1 , MAY


// Points To OUT:
// _p_1[3] -> a , EXACT

int ptr_to_array01(int *(*p)[10])
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1 , MAY

   int a;

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1 , MAY

   (*p)[3] = &a;

// Points To:
// _p_1[3] -> a , EXACT
// p -> _p_1 , EXACT


   return 0;
}
