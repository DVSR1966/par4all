
Proper effects for "main"

int main()
{
   int t[100][10][3];                                                   /*0005*/

   int (*p)[3];                                                         /*0007*/
   int *pu;                                                             /*0008*/

   int (*q)[10][3];                                                     /*0010*/
   int (*qu)[3];                                                        /*0011*/

   int (*r)[100][10][3];                                                /*0013*/
   int (*ru)[10][3];                                                    /*0014*/
//             <    is written>: p
   
   
   p = &t[1][2];                                                        /*0017*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   (*p)[1] = 0;                                                         /*0018*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   p[1][1] = 0;                                                         /*0019*/
//             <    is written>: pu

   pu = &t[1][2][0];                                                    /*0021*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pu
   *pu = 0;                                                             /*0022*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pu
   pu[2] = 0;                                                           /*0023*/
//             <    is written>: q

   q = &t[1];                                                           /*0025*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   ((*q)[2])[3] = 0;                                                    /*0026*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   q[1][2][3] = 0;                                                      /*0027*/
//             <    is written>: qu

   qu = &t[1][0];                                                       /*0029*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: qu
   (*qu)[1] = 0;                                                        /*0030*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: qu
   qu[1][1] = 0;                                                        /*0031*/
//             <    is written>: r

   r = &t;                                                              /*0033*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: r
   (((*r)[0])[0])[0] = 0;                                               /*0034*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: r
   (((*r)[1])[2])[3] = 0;                                               /*0035*/
//             <    is written>: ru

   ru = &t[0];                                                          /*0037*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: ru
   ((*ru)[0])[0] = 0;                                                   /*0038*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: ru
   ru[0][0][0] = 0;                                                     /*0039*/
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: ru
   ru[1][2][1] = 0;                                                     /*0040*/

   return 0;                                                            /*0042*/
}

