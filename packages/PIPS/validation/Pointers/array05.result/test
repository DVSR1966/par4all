int main()
{
//  points to = {}
   int t[100][10][3];                                                   /*0005*/

//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int (*p)[3];                                                         /*0007*/
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int *pu;                                                             /*0008*/

//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int (*q)[10][3];                                                     /*0010*/
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-);(qu,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}
   int (*qu)[3];                                                        /*0011*/

//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-);(qu,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(r,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int (*r)[100][10][3];                                                /*0013*/
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-);(qu,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(r,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(ru,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int (*ru)[10][3];                                                    /*0014*/
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-);(qu,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(r,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(ru,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   
   
   p = &t[1][2];                                                        /*0017*/
//  {(p,t[1][2],-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(qu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(r,*ANY_MODULE*:*NOWHERE*,-Exact-);(ru,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}
   (*p)[1] = 0;                                                         /*0018*/
//  {(p,t[1][2],-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(qu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(r,*ANY_MODULE*:*NOWHERE*,-Exact-);(ru,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}
   p[1][1] = 0;                                                         /*0019*/
//  {(p,t[1][2],-Exact-);(pu,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(qu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(r,*ANY_MODULE*:*NOWHERE*,-Exact-);(ru,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}

   pu = &t[1][2][0];                                                    /*0021*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(qu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(r,*ANY_MODULE*:*NOWHERE*,-Exact-);(ru,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}
   *pu = 0;                                                             /*0022*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(qu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(r,*ANY_MODULE*:*NOWHERE*,-Exact-);(ru,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}
   pu[2] = 0;                                                           /*0023*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(qu,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(r,*ANY_MODULE*:*NOWHERE*,-Exact-);(ru,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}

   q = &t[1];                                                           /*0025*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   *ANY_MODULE*:*NOWHERE*,-Exact-);(r,*ANY_MODULE*:*NOWHERE*,
