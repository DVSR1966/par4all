
POINTS TO

//  {}
int main()
{
//  points to = {}
   double x = 1.;
//  {(p,x,-Exact-)}
   double *p = &x;
//  {(p,x,-Exact-);(q,*NULL_POINTER*,-Exact-)}
   double *q = 0;
//  {(p,x,-Exact-);(q,*NULL_POINTER*,-Exact-)}
   printf("q = %p", q);
//  {(p,x,-Exact-);(q,*NULL_POINTER*,-Exact-)}
   q = p;
//  {(p,x,-Exact-);(q,x,-Exact-)}
   *p = 2.;
//  {(p,x,-Exact-);(q,x,-Exact-)}
   return 0;
}
