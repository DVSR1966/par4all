
PROPER POINTER EFFECTS

int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <    is read   >: s.one sp sp[0].one

   call09(s.one, sp->one);
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   tf_t s;
//  points to = {}
   tf_t *sp = &s;
//  {(sp,s,-Exact-)}

   call09(s.one, sp->one);
}
