
PROPER  EFFECTS

int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*

   call09(s.one, sp->one);
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   tf_t s;

// Points To: none

   tf_t *sp = &s;

// Points To:
// sp -> &s , EXACT


   call09(s.one, sp->one);
}
