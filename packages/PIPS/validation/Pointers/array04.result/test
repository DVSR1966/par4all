//  {}
int main()
{
//  points to = {}
   int t[100][10][3];                                                   /*0007*/

//  points to = {}
   int (*p)[3];                                                         /*0009*/
//  points to = {}
   int *pu;                                                             /*0010*/

//  points to = {}
   int (*q)[10][3];                                                     /*0012*/
//  points to = {}
   int (*qu)[3];                                                        /*0013*/

//  points to = {}
   int (*r)[100][10][3];                                                /*0015*/
//  points to = {}
   int (*ru)[10][3];                                                    /*0016*/
//  points to = {}

   printf("sizeof(t) = %zd\n", sizeof(t));                              /*0018*/
//  points to = {}
   printf("sizeof(t[1]) = %zd\n", sizeof(t[1]));                        /*0019*/
//  points to = {}
   printf("sizeof(t[1][2]) = %zd\n", sizeof(t[1][2]));                  /*0020*/
//  points to = {}
   printf("sizeof(t[1][2][3]) = %zd\n", sizeof(t[1][2][3]));            /*0021*/
//  points to = {}

   p = &t[1][2];                                                        /*0023*/
//  {(p,t[1][2],-Exact-)}
   pu = t[1][2];                                                        /*0024*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-)}
   pu = &t[1][2][0];                                                    /*0025*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-)}

   q = &t[1];                                                           /*0027*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-)}
   qu = t[1];                                                           /*0028*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-)}
   qu = &t[1][0];                                                       /*0029*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-)}

   r = &t;                                                              /*0031*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-)}
   ru = t;                                                              /*0032*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-);(ru,t[0],-Exact-)}
   ru = &t[0];                                                          /*0033*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-);(ru,t[0],-Exact-)}
   return 0;                                                            /*0034*/
}
