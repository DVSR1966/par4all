int main()
{
   // BEGIN BLOCK
   struct one {
      int *first;
   } *x, x1;
   int y[10], z[5];
//               <must be written>: x1[1]
   x1.first = &z[1];
//               <must be read   >: x x1[1]
//               <must be written>: x[0][1]
   (*x).first = x1.first;
//               <must be read   >: x
//               <must be written>: x[0][1]
   (*x).first = &y[5];
   
   
   return 0;
   // END BLOCK
}
POINTS TO
int main()
{
   // BEGIN BLOCK
   struct one {
      int *first;
   } *x, x1;
   int y[10], z[5];
   x1.first = &z[1];
//  points to = {(x1[1],z[1],-Exact-)}
   (*x).first = x1.first;
//  points to = {(x[0][1],z[1],-Exact-);(x1[1],z[1],-Exact-)}
   (*x).first = &y[5];
//  points to = {(x[0][1],y[5],-Exact-);(x1[1],z[1],-Exact-)}
   
   
   return 0;
   // END BLOCK
}
