
PROPER  EFFECTS

int main()
{
   int a, aa, *b, *bb, **c;
//             <    is written>: a

   a = 1;
//             <    is written>: b
   b = &a;
//             <    is written>: c
   c = &b;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: bb

   bb = *c;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: aa
   aa = **c;
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int a, aa, *b, *bb, **c;

// Points To:
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// bb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   a = 1;

// Points To:
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// bb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   b = &a;

// Points To:
// b -> a , EXACT
// bb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   c = &b;

// Points To:
// b -> a , EXACT
// bb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> b , EXACT


   bb = *c;

// Points To:
// b -> a , EXACT
// bb -> a , EXACT
// c -> b , EXACT

   aa = **c;

// Points To:
// b -> a , EXACT
// bb -> a , EXACT
// c -> b , EXACT

   return 0;
}
