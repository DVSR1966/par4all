
PROPER  EFFECTS

int main()
{
   int *a[2];
//             <    is written>: b
   int b = 0;
//             <    is written>: c
   int c = 1;
//             <    is written>: a[0]
   a[0] = &b;
//             <    is written>: a[1]
   a[1] = &b;
//             <    is written>: a[1]
   a[1] = &c;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a[2];

// Points To: none

   int b = 0;

// Points To: none

   int c = 1;

// Points To: none

   a[0] = &b;

// Points To:
// a[0] -> b , EXACT

   a[1] = &b;

// Points To:
// a[1] -> b , EXACT
// a[0] -> b , EXACT

   a[1] = &c;

// Points To:
// a[0] -> b , EXACT
// a[1] -> c , EXACT

   return 0;
}
