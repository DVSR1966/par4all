
PROPER POINTER EFFECTS

int main(int argc, char *argv[])
{
   int t, i;
//             <    is written>: i

   for(i = 1; i <= 62; i += 1)
//             <    is read   >: i tab3 _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: tab3[i] _MALLOC_EFFECTS:_MALLOC_
      tab3[i] = malloc(64*sizeof(float));
//             <    is written>: t
   for(t = 0; t <= 63; t += 1)
//             <may be read   >: tab3
//             <may be written>: tab1[*][*] tab2[*][*] tab3[*][*]
      compute();
//             <    is written>: i

   for(i = 1; i <= 62; i += 1)
//             <    is read   >: i tab3 tab3[i] 
//                               _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
      free(tab3[i]);

   return 0;
}

POINTS-TO

//  {}
int main(int argc, char *argv[])
{
//  points to = {}
   int t, i;
//  points to = {}

   for(i = 1; i <= 62; i += 1)
//  points to = {}
      tab3[i] = malloc(64*sizeof(float));
//  points to = {}
   for(t = 0; t <= 63; t += 1)
//  points to = {}
      compute();
//  points to = {}

   for(i = 1; i <= 62; i += 1)
//  points to = {}
      free(tab3[i]);
//  points to = {}

   return 0;
}
