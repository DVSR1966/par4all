
PROPER POINTER EFFECTS

int main()
{
   int tab[10];
   int *tab2[10];
   int tab3[10][10];
   int **tab4;
   int tab5[15];

   int i;
//             <may be written>: tab[*]

   foo(tab);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%s\n", "tab");
//             <may be read   >: stdout stdout[0] tab[*] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
   myprint(tab);
//             <may be written>: tab2[4][*]
//             <    is read   >: tab2[4]
   
   
   foo(tab2[4]);
//             <may be written>: tab3[5][*]
   foo(tab3[5]);
//             <may be written>: tab4[6][*]
//             <    is read   >: tab4 tab4[6]
   foo(tab4[6]);
//             <may be written>: tab[*]
   foo(&tab[0]);
//             <may be written>: tab5[*]
   foo(&tab5[3]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("%s:\n", "tab2[4]");
//             <may be read   >: stdout stdout[0] tab2[4][*] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
//             <    is read   >: tab2[4]
   myprint(tab2[4]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%s\n", "tab3[5]");
//             <may be read   >: stdout stdout[0] tab3[5][*] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
   myprint(tab3[5]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%s\n", "tab4[6]");
//             <may be read   >: stdout stdout[0] tab4[6][*] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
//             <    is read   >: tab4 tab4[6]
   myprint(tab4[6]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%s\n", "tab");
//             <may be read   >: stdout stdout[0] tab[*] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
   myprint(&tab[0]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%s\n", "tab5");
//             <may be read   >: stdout stdout[0] tab5[*] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
   myprint(&tab5[3]);
   return 1;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int tab[10];
//  points to = {}
   int *tab2[10];
//  points to = {}
   int tab3[10][10];
//  points to = {}
   int **tab4;
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int tab5[15];

//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i;
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   foo(tab);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%s\n", "tab");
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   myprint(tab);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   
   
   foo(tab2[4]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(tab3[5]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(tab4[6]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(&tab[0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(&tab5[3]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   printf("%s:\n", "tab2[4]");
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   myprint(tab2[4]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%s\n", "tab3[5]");
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   myprint(tab3[5]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%s\n", "tab4[6]");
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   myprint(tab4[6]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%s\n", "tab");
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   myprint(&tab[0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%s\n", "tab5");
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   myprint(&tab5[3]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 1;
}
