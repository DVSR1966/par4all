
Points-to for "foo"


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY


// Points To OUT: none

void foo(int *p)
{

// Points To: none

   int i;

// Points To: none


   for(i = 0; i <= 9; i += 1)

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY

      p[i] = i;

// Points To:
// p -> _p_1[0] , EXACT

   return;
}

Points-to for "myprint"


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY


// Points To OUT: none

void myprint(int *p)
{

// Points To: none

   int i;

// Points To: none


   for(i = 0; i <= 9; i += 1)

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY

      printf("\t%d\n", p[i]);

// Points To:
// p -> _p_1[0] , EXACT

   return;
}

Points-to for "main"


// Points To IN: none


// Points To OUT: unreachable

int main(int argc, char **argv)
{

// Points To: none

   int tab[10];

// Points To: none

   int *tab2[10];

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int tab3[10][10];

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int **tab4;

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int tab5[15];

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   
   
   foo(tab);

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   printf("%s\n", "tab");

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   myprint(tab);

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   
   
   foo(tab2[4]);

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   foo(tab3[5]);

// Points To:
// tab2[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   foo(tab4[6]);

// Points To: unreachable

   foo(&tab[0]);

// Points To: unreachable

   foo(&tab5[3]);

// Points To: unreachable


   printf("%s:\n", "tab2[4]");

// Points To: unreachable

   myprint(tab2[4]);

// Points To: unreachable

   printf("%s\n", "tab3[5]");

// Points To: unreachable

   myprint(tab3[5]);

// Points To: unreachable

   printf("%s\n", "tab4[6]");

// Points To: unreachable

   myprint(tab4[6]);

// Points To: unreachable

   printf("%s\n", "tab");

// Points To: unreachable

   myprint(&tab[0]);

// Points To: unreachable

   printf("%s\n", "tab5");

// Points To: unreachable

   myprint(&tab5[3]);

// Points To: unreachable

   return 1;
}
