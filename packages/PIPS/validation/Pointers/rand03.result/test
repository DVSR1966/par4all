   for(iy = 0; iy <= ny-1; iy += 1)
//  points to = {}
      for(ix = 0; ix <= nx-1; ix += 1) {
//  points to = {}

         while (u==0||u>1) 
//  points to = {}
            u = rand()/32768.;
//  points to = {}
         A = sqrt(-(2.*log(u)));
//  points to = {}
         v = rand()/32768.;
//  points to = {}
         B = 2.*3.141592653589793238*v;
//  points to = {}
         A = 1.0;
//  points to = {}
         v = 1.0;
//  points to = {}
         B = 1.0;
//  points to = {}

