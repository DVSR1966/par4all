
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 5;
//             <    is written>: j
   int j = 6;
   float x;
   double y[10];
//             <    is written>: fmt
   char *fmt = "i=%d, %d, %f\n";
//             <    is written>: y[0]

   y[0] = 0.;
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf(fmt, i, i+j, x);
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int i = 5;

// Points To: none

   int j = 6;

// Points To: none

   float x;

// Points To: none

   double y[10];

// Points To: none

   char *fmt = "i=%d, %d, %f\n";

// Points To:
// fmt -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT


   y[0] = 0.;

// Points To:
// fmt -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   printf(fmt, i, i+j, x);
}
