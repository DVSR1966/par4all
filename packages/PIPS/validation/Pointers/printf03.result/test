int main()
{
   int i = 5;
   int j = 6;
   float x;
   double y[10];
   char *fmt = "i=%d, %d, %f\n";

   y[0] = 0.;
   printf(fmt, i, i+j, x);
}
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <may be written >: stdout[0]
//               <must be read   >: __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
int main()
{
//               <must be written>: i
   int i = 5;
//               <must be written>: j
   int j = 6;
   float x;
   double y[10];
//               <must be written>: fmt
   char *fmt = "i=%d, %d, %f\n";
//               <must be written>: y[0]

   y[0] = 0.;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE* stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: stdout __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf(fmt, i, i+j, x);
}

POINTS TO

//  {}
int main()
{
//  points to = {}
   int i = 5;
//  points to = {}
   int j = 6;
//  points to = {}
   float x;
//  points to = {}
   double y[10];
//  points to = {}
   char *fmt = "i=%d, %d, %f\n";
//  points to = {}

   y[0] = 0.;
//  points to = {}
   printf(fmt, i, i+j, x);
}
