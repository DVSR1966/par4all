int conditional_01()
{
   // BEGIN BLOCK
   int *q;
   int *p;
//             <    is written>: i j
   int i = 0, j = 1;
//             <    is written>: q
   q = &i;
//             <may be read   >: q
//             <    is read   >: i
//             <    is written>: p
   p = i>0?&j:q;
   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int conditional_01()
{
//  points to = {}
   // BEGIN BLOCK
//  {(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int *q;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int i = 0, j = 1;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   q = &i;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,i,-Exact-)}
   p = i>0?&j:q;
//  {(q,i,-Exact-)}
   return 0;
   // END BLOCK
}
