
PROPER  EFFECTS

void bar(int **pp, int **qq)
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   *pp = *qq;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("pointers exchanged");
   return;
}
void foo()
{
//             <    is written>: i j p pp q qq
   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   bar(pp, qq);
   return;
}

POINTS-TO


// Points To IN:
// _qq_2[0] -> *NULL_POINTER* , MAY
// _qq_2[0] -> _qq_2_2[0][0] , MAY
// pp -> *NULL_POINTER* , MAY
// pp -> _pp_1[0] , MAY
// qq -> *NULL_POINTER* , MAY
// qq -> _qq_2[0] , MAY


// Points To OUT:
// _pp_1[0] -> *NULL_POINTER* , MAY
// _pp_1[0] -> _qq_2_2[0][0] , MAY
// _qq_2[0] -> *NULL_POINTER* , MAY
// _qq_2[0] -> _qq_2_2[0][0] , MAY

void bar(int **pp, int **qq)
{

// Points To:
// _qq_2[0] -> *NULL_POINTER* , MAY
// _qq_2[0] -> _qq_2_2[0][0] , MAY
// pp -> *NULL_POINTER* , MAY
// pp -> _pp_1[0] , MAY
// qq -> *NULL_POINTER* , MAY
// qq -> _qq_2[0] , MAY

   *pp = *qq;

// Points To:
// _pp_1[0] -> *NULL_POINTER* , MAY
// _pp_1[0] -> _qq_2_2[0][0] , MAY
// _qq_2[0] -> *NULL_POINTER* , MAY
// _qq_2[0] -> _qq_2_2[0][0] , MAY
// pp -> _pp_1[0] , EXACT
// qq -> _qq_2[0] , EXACT

   printf("pointers exchanged");

// Points To:
// _pp_1[0] -> *NULL_POINTER* , MAY
// _pp_1[0] -> _qq_2_2[0][0] , MAY
// _qq_2[0] -> *NULL_POINTER* , MAY
// _qq_2[0] -> _qq_2_2[0][0] , MAY
// pp -> _pp_1[0] , EXACT
// qq -> _qq_2[0] , EXACT

   return;
}

// Points To IN: none


// Points To OUT: none

void foo()
{

// Points To: none

   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;

// Points To:
// p -> i , EXACT
// pp -> p , EXACT
// q -> j , EXACT
// qq -> q , EXACT


   bar(pp, qq);

// Points To:
// *ANY_MODULE*:*ANYWHERE* -> *ANY_MODULE*:*ANYWHERE* , EXACT

   return;
}
