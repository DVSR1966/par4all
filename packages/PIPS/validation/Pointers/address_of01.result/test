
PROPER  EFFECTS

int foo()
{
//             <    is written>: i
   int *p, i = 1;
//             <    is written>: p
   p = 1+&i;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int foo()
{

// Points To: none

   int *p, i = 1;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = 1+&i;

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   return 0;
}
