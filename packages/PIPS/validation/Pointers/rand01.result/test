
PROPER  EFFECTS

int main()
{
   int entier;
   int graine;
   double virgule;
//             <    is written>: graine
   graine = 50;
//             <    is read   >: graine
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   srandom(graine);
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: virgule _RANDOM_EFFECTS:GEN_SEED
   /* generator initialization */
   virgule = (double) rand()/(2147483647+1);
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: entier _RANDOM_EFFECTS:GEN_SEED
   entier = 1+rand();
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int entier;

// Points To: none

   int graine;

// Points To: none

   double virgule;

// Points To: none

   graine = 50;

// Points To: none

   srandom(graine);

// Points To: none

   /* generator initialization */
   virgule = (double) rand()/(2147483647+1);

// Points To: none

   entier = 1+rand();
}
