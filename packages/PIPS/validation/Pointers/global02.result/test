
POINTS TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_0[0] , MAY


// Points To OUT: none

int main()
{

// Points To: none

   int **q, *r, i;

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_0[0] , MAY
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   q = p;

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_0[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _p_0[0] , MAY
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   *q = &i;

// Points To:
// _p_0[*] -> i , MAY
// _p_0[0] -> i , EXACT
// p -> *NULL_POINTER* , MAY
// p -> _p_0[0] , MAY
// q -> _p_0[0] , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   r = &**q;

// Points To:
// _p_0[*] -> i , MAY
// _p_0[0] -> i , EXACT
// p -> *NULL_POINTER* , MAY
// p -> _p_0[0] , MAY
// q -> _p_0[0] , EXACT
