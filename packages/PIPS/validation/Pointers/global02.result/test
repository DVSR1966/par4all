
PROPER EFFECTS

int main()
{
   int **q, *r, i;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: r
   r = &**q;

   return 0;
}

PROPER POINTER EFFECTS

int main()
{
   int **q, *r, i;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <    is read   >: q
//             <    is written>: q[0]
   *q = &i;
//             <    is read   >: q q[0]
//             <    is written>: r
   r = &**q;

   return 0;
}

POINTS TO


// Points To: none

int main()
{

// Points To: none

   int **q, *r, i;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE* , EXACT
// r -> *ANY_MODULE*:*NOWHERE* , EXACT


   q = p;

// Points To:
// _p_1 -> __p_1_2 , EXACT
// p -> _p_1 , EXACT
// q -> _p_1 , EXACT
// r -> *ANY_MODULE*:*NOWHERE* , EXACT

   *q = &i;

// Points To:
// _p_1 -> i , EXACT
// p -> _p_1 , EXACT
// q -> _p_1 , EXACT
// r -> *ANY_MODULE*:*NOWHERE* , EXACT

   r = &**q;

// Points To:
// _p_1 -> i , EXACT
// p -> _p_1 , EXACT
// q -> _p_1 , EXACT
// r -> i , EXACT


   return 0;
}
