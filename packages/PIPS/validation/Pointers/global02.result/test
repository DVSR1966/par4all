
PROPER EFFECTS

int main()
{
   int **q, *r, i;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: r
   r = &**q;

   return 0;
}

PROPER POINTER EFFECTS

int main()
{
   int **q, *r, i;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <    is read   >: q
//             <    is written>: q[0]
   *q = &i;
//             <    is read   >: q q[0]
//             <    is written>: r
   r = &**q;

   return 0;
}

POINTS TO


// Points To IN:
// _p_0[*] -> *NULL_POINTER* , MAY
// _p_0[*] -> _p_0_0[*] , MAY
// p -> *NULL_POINTER* , MAY
// p -> _p_0 , MAY


// Points To OUT:
// _p_0[*] -> *NULL_POINTER* , MAY
// _p_0[*] -> _p_0_0[*] , MAY
// _p_0[0] -> i , EXACT
// p -> *NULL_POINTER* , MAY
// p -> _p_0 , MAY

int main()
{

// Points To: none

   int **q, *r, i;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   q = p;

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_0 , MAY
// q -> *NULL_POINTER* , MAY
// q -> _p_0 , MAY
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   *q = &i;

// Points To:
// _p_0[0] -> i , EXACT
// p -> *NULL_POINTER* , MAY
// p -> _p_0 , MAY
// q -> _p_0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   r = &**q;

// Points To:
// _p_0[*] -> *NULL_POINTER* , MAY
// _p_0[*] -> _p_0_0[*] , MAY
// _p_0[0] -> i , EXACT
// p -> *NULL_POINTER* , MAY
// p -> _p_0 , MAY
// q -> _p_0 , EXACT
// r -> *NULL_POINTER* , MAY
// r -> _p_0_0[*] , MAY


   return 0;
}
