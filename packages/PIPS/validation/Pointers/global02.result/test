
PROPER EFFECTS

int main()
{
   int **q, *r, i;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: r
   r = &**q;

   return 0;
}

PROPER POINTER EFFECTS

int main()
{
   int **q, *r, i;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <    is read   >: q
//             <    is written>: q[0]
   *q = &i;
//             <    is read   >: q q[0]
//             <    is written>: r
   r = &**q;

   return 0;
}

POINTS TO


// Points To: none

int main()
{

// Points To: none

   int **q, *r, i;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   q = p;

// Points To:
// _p_23987312[0] -> __p_23987312_23987312[0] , EXACT
// p -> _p_23987312[0] , EXACT
// q -> _p_23987312[0] , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   *q = &i;

// Points To:
// _p_23987312[0] -> i , EXACT
// p -> _p_23987312[0] , EXACT
// q -> _p_23987312[0] , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   r = &**q;

// Points To:
// _p_23987312[0] -> i , EXACT
// p -> _p_23987312[0] , EXACT
// q -> _p_23987312[0] , EXACT
// r -> i , EXACT


   return 0;
}
