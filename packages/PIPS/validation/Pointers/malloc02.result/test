
PROPER  EFFECTS

int main(int argc, char *argv[])
{
   int *pi;
   float *px;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pi _MALLOC_EFFECTS:_MALLOC_

   pi = (int *) malloc(sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: px _MALLOC_EFFECTS:_MALLOC_
   px = (float *) malloc(sizeof(int));

   return 0;
}

POINTS-TO


// Points To:
// argv[*] -> _argv_2[0] , EXACT

int main(int argc, char *argv[])
{

// Points To:
// argv[*] -> _argv_2[0] , EXACT

   int *pi;

// Points To:
// argv[*] -> _argv_2[0] , EXACT
// pi -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   float *px;

// Points To:
// argv[*] -> _argv_2[0] , EXACT
// pi -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// px -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   pi = (int *) malloc(sizeof(int));

// Points To:
// argv[*] -> _argv_2[0] , EXACT
// pi -> *HEAP*_l_10 , EXACT
// px -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   px = (float *) malloc(sizeof(int));

// Points To:
// argv[*] -> _argv_2[0] , EXACT
// pi -> *HEAP*_l_10 , EXACT
// px -> *HEAP*_l_11 , EXACT


   return 0;
}
