
PROPER EFFECTS

int formal_parameter01(int **pi)
{
   int **q;
   int *i;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is written>: pi
   pi = &i;
   return 0;
}

PROPER POINTER EFFECTS

int formal_parameter01(int **pi)
{
   int **q;
   int *i;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is written>: pi
   pi = &i;
   return 0;
}

POINTS TO


// Points To:
// _pi_1 -> __pi_1_2 , EXACT
// pi -> _pi_1 , EXACT

int formal_parameter01(int **pi)
{

// Points To:
// _pi_1 -> __pi_1_2 , EXACT
// pi -> _pi_1 , EXACT

   int **q;

// Points To:
// _pi_1 -> __pi_1_2 , EXACT
// pi -> _pi_1 , EXACT
// q -> *ANY_MODULE*:*NOWHERE* , EXACT

   int *i;

// Points To:
// _pi_1 -> __pi_1_2 , EXACT
// i -> *ANY_MODULE*:*NOWHERE* , EXACT
// pi -> _pi_1 , EXACT
// q -> *ANY_MODULE*:*NOWHERE* , EXACT


   i = 0;

// Points To:
// _pi_1 -> __pi_1_2 , EXACT
// i -> *NULL_POINTER* , EXACT
// pi -> _pi_1 , EXACT
// q -> *ANY_MODULE*:*NOWHERE* , EXACT

   q = pi;

// Points To:
// _pi_1 -> __pi_1_2 , EXACT
// i -> *NULL_POINTER* , EXACT
// pi -> _pi_1 , EXACT
// q -> _pi_1 , EXACT

   pi = &i;

// Points To:
// _pi_1 -> __pi_1_2 , EXACT
// i -> *NULL_POINTER* , EXACT
// pi -> i , EXACT
// q -> _pi_1 , EXACT

   return 0;
}
