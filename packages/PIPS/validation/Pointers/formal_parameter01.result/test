
PROPER EFFECTS

int formal_parameter01(int **pi)
{
   int **q;
   int *i;
   int j;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is read   >: q
//             <    is written>: q
   q++;
//             <    is written>: pi
   pi = &i;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pi
   *pi = &j;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &j;
   return 0;
}

PROPER POINTER EFFECTS

int formal_parameter01(int **pi)
{
   int **q;
   int *i;
   int j;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is read   >: q
//             <    is written>: q
   q++;
//             <    is written>: pi
   pi = &i;
//             <    is read   >: pi
//             <    is written>: pi[0]
   *pi = &j;
//             <    is read   >: q
//             <    is written>: q[0]
   *q = &j;
   return 0;
}

POINTS TO


// Points To IN:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY


// Points To OUT:
// _pi_1[1] -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

int formal_parameter01(int **pi)
{

// Points To: none

   int **q;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int *i;

// Points To:
// i -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int j;

// Points To:
// i -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   i = 0;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = pi;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _pi_1[0] , MAY

   q++;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> _pi_1[1] , EXACT

   pi = &i;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> i , EXACT
// q -> _pi_1[1] , EXACT

   *pi = &j;

// Points To:
// i -> j , EXACT
// pi -> i , EXACT
// q -> _pi_1[1] , EXACT

   *q = &j;

// Points To:
// _pi_1[1] -> j , EXACT
// i -> j , EXACT
// pi -> i , EXACT
// q -> _pi_1[1] , EXACT

   return 0;
}
