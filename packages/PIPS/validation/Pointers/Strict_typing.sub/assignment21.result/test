
POINTS TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a[2], i, j;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   a[0] = &i;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a[*] -> i , MAY
// a[0] -> i , EXACT

   a[j] = &j;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a[*] -> i , MAY
// a[0] -> i , MAY
// a[*] -> j , MAY

   return 0;
}
