
PROPER  EFFECTS

void dereferencing04()
{
   double *p;
   double *q;
   double *r;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p q r
   *(p+q-r) = 0.;
}

POINTS-TO

//  {}
void dereferencing04()
{
//  points to = {}
   double *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   double *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   double *r;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-)}
   *(p+q-r) = 0.;
}
