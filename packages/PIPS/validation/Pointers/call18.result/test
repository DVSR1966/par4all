void call18(int i, ...)
{
//             <    is read   >: i
//             <    is written>: i
   i++;
//             <    is read   >: i
   if (i) {
      va_list pa;
      int r;
//             <    is read   >: pa
//             <    is written>: pa

      va_start(pa, fmt);
//             <may be written>: pa
//             <    is read   >: pa

      va_end(pa);
   }
}
int main()
{
//             <    is written>: i
   int i = 1;
//             <    is written>: j
   int j = 2;
//             <    is written>: k
   int k = 3;
//             <    is read   >: i j k

   call18(i, i, j, k, 0);
}

POINTS TO

//  {}
void call18(int i, ...)
{
//  points to = {}
   i++;
//  points to = {}
   if (i) {
//  points to = {}
      va_list pa;
//  points to = {}
      int r;
//  points to = {}

      va_start(pa, fmt);
//  points to = {}

      va_end(pa);
   }
}
//  {}
int main()
{
//  points to = {}
   int i = 1;
//  points to = {}
   int j = 2;
//  points to = {}
   int k = 3;
//  points to = {}

   call18(i, i, j, k, 0);
}
