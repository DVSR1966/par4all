int main()
{
   int i = 5;
   printf("i=%d", i);
}
//               <may be read    >: stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: stdout __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
int main()
{
//               <must be written>: i
   int i = 5;
//               <may be read    >: stdout[0]
//               <may be written >: stdout[0]
//               <must be read   >: stdout __IO_EFFECTS:LUNS[1] i
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("i=%d", i);
}

POINTS TO

//  {}
int main()
{
//  points to = {}
   int i = 5;
//  points to = {}
   printf("i=%d", i);
}
