
PROPER  EFFECTS

void uninitialized01()
{
   int **pp;
   int i;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pp
   *pp = &i;
   return;
}

POINTS-TO


// Points To IN: none


// Points To OUT: unreachable

void uninitialized01()
{

// Points To: none

   int **pp;

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   *pp = &i;

// Points To: unreachable

   return;
}
