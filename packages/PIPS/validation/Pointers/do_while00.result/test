int main()
{
   // BEGIN BLOCK
   int *a, *b, c, d, i;
//             <    is written>: c
   c = 0;
//             <    is written>: d
   d = 1;
//             <    is written>: i
   i = 2;
//             <    is written>: b
   b = &c;
//             <    is read   >: i
   do {
      // BEGIN BLOCK
//             <    is read   >: i
//             <    is written>: i
      i = i+1;
//             <    is read   >: b
//             <    is written>: a
      a = b;
//             <    is written>: b
      b = &d;
      // END BLOCK
   }
   while (i<5);

   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int *a, *b, c, d, i;
//  {(a,*ANY_MODULE*:*NOWHERE*,-Exact-);(b,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   c = 0;
//  {(a,*ANY_MODULE*:*NOWHERE*,-Exact-);(b,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   d = 1;
//  {(a,*ANY_MODULE*:*NOWHERE*,-Exact-);(b,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   i = 2;
//  {(a,*ANY_MODULE*:*NOWHERE*,-Exact-);(b,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   b = &c;
//  {(a,*ANY_MODULE*:*NOWHERE*,-Exact-);(b,c,-Exact-)}
   do {
//  {(a,d,-MAY-);(a,c,-MAY-);(b,d,-Exact-)}
      // BEGIN BLOCK
//  points to = {}
      i = i+1;
//  points to = {}
      a = b;
//  points to = {}
      b = &d;
      // END BLOCK
   }
   while (i<5);
//  {(a,d,-MAY-);(a,c,-MAY-);(b,d,-Exact-)}

   return 0;
   // END BLOCK
}
