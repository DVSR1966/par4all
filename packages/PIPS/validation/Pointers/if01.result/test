
PROPER  EFFECTS

int main()
{
   int *x, x1, x2, y;
//             <    is written>: x1
   x1 = 1;
//             <    is written>: x2
   x2 = 2;

   if (0==0)
//             <    is written>: x
      x = &x1;
   else
//             <    is written>: x
      x = &x2;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: y

   y = *x;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *x, x1, x2, y;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   x1 = 1;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   x2 = 2;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   if (0==0)

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      x = &x1;
   else

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      x = &x2;

// Points To:
// x -> x2 , MAY
// x -> x1 , MAY


   y = *x;

// Points To:
// x -> x2 , MAY
// x -> x1 , MAY

   return 0;
}
