
PROPER POINTER EFFECTS

int main(int argc, char *argv[])
{
//             <    is read   >: argc
   if (argc!=8)
      return 1;
   {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
//             <    is read   >: argv[1] _MALLOC_EFFECTS:_MALLOC_ 
//                               __IO_EFFECTS:LUNS[2]
//             <    is written>: t.data t.nb _MALLOC_EFFECTS:_MALLOC_ 
//                               __IO_EFFECTS:LUNS[2]
      towns t = read_towns(argv[1]);
      
      
      
      
      
//             <    is read   >: argv[2] argv[3] argv[4] argv[5] 
//                               argv[6] argv[7]
//             <    is written>: range step xmax xmin ymax ymin
      data_t xmin = atof(argv[2])*3.14159265358979323846/180., ymin = atof(argv[3])*3.14159265358979323846/180., xmax = atof(argv[4])*3.14159265358979323846/180., ymax = atof(argv[5])*3.14159265358979323846/180., step = atof(argv[6])*3.14159265358979323846/180., range = atof(argv[7]);
//             <may be read   >: stderr[0] t.data[*][0] t.data[*][0] 
//                               t.data[*][1] t.data[*][2]
//             <may be written>: stderr[0]
//             <    is read   >: range stderr step t.data t.nb xmax 
//                               xmin ymax ymin 
//                               _MALLOC_EFFECTS:_MALLOC_ 
//                               __IO_EFFECTS:LUNS[2]
//             <    is written>: out.data out.nb 
//                               _MALLOC_EFFECTS:_MALLOC_ 
//                               __IO_EFFECTS:LUNS[2]
      towns out = run(t, xmin, ymin, xmax, ymax, step, range);
//             <may be read   >: out.data[*][0] out.data[*][1] 
//                               out.data[*][2] stdout stdout[0] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
//             <    is read   >: out.data out.nb step xmax xmin ymax 
//                               ymin
      display(out, xmin, ymin, xmax, ymax, step);
   }
   return 0;
}

POINTS-TO

//  {}
int main(int argc, char *argv[])
{
//  points to = {}
   if (argc!=8)
//  points to = {}
      return 1;
   {
//  points to = {}
      towns t = read_towns(argv[1]);
      
      
      
      
      
//  points to = {}
      data_t xmin = atof(argv[2])*3.14159265358979323846/180., ymin = atof(argv[3])*3.14159265358979323846/180., xmax = atof(argv[4])*3.14159265358979323846/180., ymax = atof(argv[5])*3.14159265358979323846/180., step = atof(argv[6])*3.14159265358979323846/180., range = atof(argv[7]);
//  points to = {}
      towns out = run(t, xmin, ymin, xmax, ymax, step, range);
//  points to = {}
      display(out, xmin, ymin, xmax, ymax, step);
   }
//  points to = {}
   return 0;
}
