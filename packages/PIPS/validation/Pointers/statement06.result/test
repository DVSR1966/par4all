
PROPER  EFFECTS

int main()
{
   int a, b, c, d;
   int *pa, *pb, *pc, *pd;
   int **ppa, **ppb, **ppc, **ppd;
//             <    is written>: a
   a = 1;
//             <    is written>: b
   b = 2;
//             <    is written>: c
   c = 3;
//             <    is written>: d
   d = 4;
//             <    is written>: pa

   pa = &a;
//             <    is written>: ppa
   ppa = &pa;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: ppa
   *ppa = &b;
//             <    is written>: pb

   pb = &b;
//             <    is written>: ppb
   ppb = &pb;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: ppb
   *ppb = &a;
//             <    is written>: pc

   pc = &c;
//             <    is written>: ppc
   ppc = &pc;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: ppc
   *ppc = &a;
//             <    is written>: pd

   pd = &d;
//             <    is written>: ppd
   ppd = &pd;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: ppd
   *ppd = &c;
//             <    is read   >: pd
//             <    is written>: pc

   pc = pd;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: ppc

   *ppc = &b;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pc ppa

   *ppa = pc;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: pa

   pa = *ppd;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

   *ppa = *ppd;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a, b, c, d;

// Points To: none

   int *pa, *pb, *pc, *pd;

// Points To:
// pa -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int **ppa, **ppb, **ppc, **ppd;

// Points To:
// pa -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   a = 1;

// Points To:
// pa -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   b = 2;

// Points To:
// pa -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   c = 3;

// Points To:
// pa -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   d = 4;

// Points To:
// pa -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   pa = &a;

// Points To:
// pa -> a , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   ppa = &pa;

// Points To:
// pa -> a , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> pa , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   *ppa = &b;

// Points To:
// pa -> b , EXACT
// pb -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> pa , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   pb = &b;

// Points To:
// pa -> b , EXACT
// pb -> b , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> pa , EXACT
// ppb -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   ppb = &pb;

// Points To:
// pa -> b , EXACT
// pb -> b , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   *ppb = &a;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   pc = &c;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> c , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   ppc = &pc;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> c , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   *ppc = &a;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> a , EXACT
// pd -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   pd = &d;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> a , EXACT
// pd -> d , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   ppd = &pd;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> a , EXACT
// pd -> d , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> pd , EXACT

   *ppd = &c;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> a , EXACT
// pd -> c , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> pd , EXACT


   pc = pd;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> c , EXACT
// pd -> c , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> pd , EXACT


   *ppc = &b;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> b , EXACT
// pd -> c , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> pd , EXACT


   *ppa = pc;

// Points To:
// pa -> b , EXACT
// pb -> a , EXACT
// pc -> b , EXACT
// pd -> c , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> pd , EXACT


   pa = *ppd;

// Points To:
// pa -> c , EXACT
// pb -> a , EXACT
// pc -> b , EXACT
// pd -> c , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> pd , EXACT


   *ppa = *ppd;

// Points To:
// pa -> c , EXACT
// pb -> a , EXACT
// pc -> b , EXACT
// pd -> c , EXACT
// ppa -> pa , EXACT
// ppb -> pb , EXACT
// ppc -> pc , EXACT
// ppd -> pd , EXACT


   return 0;
}
