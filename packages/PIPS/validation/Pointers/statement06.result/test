int main()
{
   // BEGIN BLOCK
   int a, b, c, d;
   int *pa, *pb, *pc, *pd;
   int **ppa, **ppb, **ppc, **ppd;
//               <must be written>: a
   a = 1;
//               <must be written>: b
   b = 2;
//               <must be written>: c
   c = 3;
//               <must be written>: d
   d = 4;
//               <must be written>: pa

   pa = &a;
//               <must be written>: ppa
   ppa = &b;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppa
   *ppa = &pa;
//               <must be written>: pb

   pb = &b;
//               <must be written>: ppb
   ppb = &a;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppb
   *ppb = &pb;
//               <must be written>: pc

   pc = &c;
//               <must be written>: ppc
   ppc = &d;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppc
   *ppc = &pc;
//               <must be written>: pd

   pd = &d;
//               <must be written>: ppd
   ppd = &c;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppd
   *ppd = &pd;
//               <must be read   >: pd
//               <must be written>: pc

   pc = pd;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppc

   *ppc = &a;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: pc ppa

   *ppa = pc;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <must be written>: pa

   pa = *ppd;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <may be written >: *ANY_MODULE*:*ANYWHERE*

   *ppa = *ppd;

   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int a, b, c, d;
//  points to = {}
   int *pa, *pb, *pc, *pd;
//  points to = {}
   int **ppa, **ppb, **ppc, **ppd;
//  points to = {}
   a = 1;
//  points to = {}
   b = 2;
//  points to = {}
   c = 3;
//  points to = {}
   d = 4;
//  points to = {}

   pa = &a;
//  {(pa,a,-Exact-)}
   ppa = &b;
//  {(pa,a,-Exact-);(ppa,b,-Exact-)}
   *ppa = &pa;
//  {(pa,a,-Exact-);(ppa,b,-Exact-)}

   pb = &b;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(ppa,b,-Exact-)}
   ppb = &a;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(ppa,b,-Exact-);(ppb,a,-Exact-)}
   *ppb = &pb;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(ppa,b,-Exact-);(ppb,a,-Exact-)}

   pc = &c;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(pc,c,-Exact-);(ppa,b,
//   -Exact-);(ppb,a,-Exact-)}
   ppc = &d;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(pc,c,-Exact-);(ppa,b,
//   -Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-)}
   *ppc = &pc;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(pc,c,-Exact-);(ppa,b,
//   -Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-)}

   pd = &d;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(pc,c,-Exact-);(pd,d,-Exact-);(ppa,
//   b,-Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-)}
   ppd = &c;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(pc,c,-Exact-);(pd,d,-Exact-);(ppa,
//   b,-Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-);(ppd,c,-Exact-)}
   *ppd = &pd;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(pc,c,-Exact-);(pd,d,-Exact-);(ppa,
//   b,-Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-);(ppd,c,-Exact-)}

   pc = pd;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(pc,d,-Exact-);(pd,d,-Exact-);(ppa,
//   b,-Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-);(ppd,c,-Exact-)}

   *ppc = &a;
//  {(pa,a,-Exact-);(pb,b,-Exact-);(pc,d,-Exact-);(pd,d,-Exact-);(ppa,
//   b,-Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-);(ppd,c,-Exact-)}

   *ppa = pc;
//  {(b,d,-Exact-);(pa,a,-Exact-);(pb,b,-Exact-);(pc,d,-Exact-);(pd,d,
//   -Exact-);(ppa,b,-Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-);(ppd,c,
//   -Exact-)}

   pa = *ppd;
//  {(b,d,-Exact-);(pb,b,-Exact-);(pc,d,-Exact-);(pd,d,-Exact-);(ppa,
//   b,-Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-);(ppd,c,-Exact-)}

   *ppa = *ppd;
//  {(b,d,-Exact-);(pb,b,-Exact-);(pc,d,-Exact-);(pd,d,-Exact-);(ppa,
//   b,-Exact-);(ppb,a,-Exact-);(ppc,d,-Exact-);(ppd,c,-Exact-)}

   return 0;
   // END BLOCK
}
