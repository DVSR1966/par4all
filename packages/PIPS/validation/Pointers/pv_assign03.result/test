
PROPER  EFFECTS

int main()
{
   mystruct s1;
   mystruct s2;
//             <may be read   >: s1.b[*]
//             <may be written>: s2.b[*]
//             <    is read   >: s1.a s1.c
//             <    is written>: s2.a s2.c
   s2 = s1;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   mystruct s1;

// Points To: none

   mystruct s2;

// Points To: none

   s2 = s1;

// Points To:
// s2.a -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT
// s2.c -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   return 0;
}
