
PROPER  EFFECTS

int main(int argc, char *argv[])
{
   int *p, i, j;
//             <    is written>: i
   i = 0;
//             <    is written>: j
   j = 1;
//             <    is written>: i

   for(i = 1; i <= 4; i += 1)
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

      p = (int *) malloc(sizeof(int));
   
   
   return 0;
}

POINTS-TO


// Points To IN:
// argv[*] -> _argv_2[*] , MAY


// Points To OUT: none

int main(int argc, char *argv[])
{

// Points To:
// argv[*] -> _argv_2[*] , MAY

   int *p, i, j;

// Points To:
// argv[*] -> _argv_2[*] , EXACT
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   i = 0;

// Points To:
// argv[*] -> _argv_2[*] , EXACT
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   j = 1;

// Points To:
// argv[*] -> _argv_2[*] , EXACT
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   for(i = 1; i <= 4; i += 1)

// Points To:
// argv[*] -> _argv_2[*] , EXACT
// p -> *HEAP*_l_13 , MAY
// p -> *ANY_MODULE*:*NOWHERE*_b0 , MAY


      p = (int *) malloc(sizeof(int));

// Points To:
// argv[*] -> _argv_2[*] , EXACT
// p -> *HEAP*_l_13 , MAY
// p -> *ANY_MODULE*:*NOWHERE*_b0 , MAY

   
   
   return 0;
}
