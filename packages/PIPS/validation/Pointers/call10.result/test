
PROPER POINTER EFFECTS

int main()
{
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <may be written>: s.two[*] sp[0].two[*]
//             <    is read   >: s.one sp sp[0].one

   call10(s.one, s.two, sp->one, sp->two);
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   tf_t s;
//  points to = {}
   tf_t *sp = &s;
//  {(sp,s,-Exact-)}

   call10(s.one, s.two, sp->one, sp->two);
}
