
Pointer proper effects

int main()
{
   // BEGIN BLOCK
   int i;
   int *p;
   int **q;
//               <must be written>: p

   p = 0;
//               <must be read   >: q q[0]
//               <must be written>: q[0][0]
   **q = 0;
//               <must be read   >: p
//               <must be written>: p[0]

   *p = 1;
//               <must be read   >: q
//               <must be written>: q[0]
   *q = &i;

   return 0;
   // END BLOCK
}

Points-to

//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int i;
//  points to = {}
   int *p;
//  points to = {}
   int **q;
//  points to = {}

   p = 0;
//  points to = {}
   **q = 0;
//  points to = {}

   *p = 1;
//  points to = {}
   *q = &i;
//  points to = {}

   return 0;
   // END BLOCK
}
