
PROPER  EFFECTS

int main()
{
   int i;
   int *p;
   int **q;
//             <    is written>: p

   p = 0;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   **q = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;

   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int **q;

// Points To:
// p -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b1 , EXACT


   p = 0;

// Points To:
// p -> &*NULL_POINTER* , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b1 , EXACT

   **q = 0;

// Points To:
// p -> &*NULL_POINTER* , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b1 , EXACT


   *p = 1;

// Points To:
// p -> &*NULL_POINTER* , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b1 , EXACT

   *q = &i;

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> &i , EXACT
// p -> &*NULL_POINTER* , EXACT
// q -> &*ANY_MODULE*:*NOWHERE*_b1 , EXACT


   return 0;
}
