
PROPER  EFFECTS

int main(int argc, char *argv[])
{
   int *pi;
   float *px;
   float *pz;
   int (*pt)[30];
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pi _MALLOC_EFFECTS:_MALLOC_

   pi = (int *) malloc(sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: px _MALLOC_EFFECTS:_MALLOC_
   px = (float *) malloc(sizeof(float));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pz _MALLOC_EFFECTS:_MALLOC_
   pz = (float *) malloc(sizeof(float));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pt _MALLOC_EFFECTS:_MALLOC_
   pt = (int (*)[30]) malloc(sizeof(*pt));

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(int argc, char *argv[])
{

// Points To: none

   int *pi;

// Points To:
// pi -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   float *px;

// Points To:
// pi -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// px -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   float *pz;

// Points To:
// pi -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// px -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// pz -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int (*pt)[30];

// Points To:
// pi -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// pt -> *ANY_MODULE*:*NOWHERE*_b2 , EXACT
// px -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// pz -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   pi = (int *) malloc(sizeof(int));

// Points To:
// pi -> *HEAP*_l_12 , MAY
// pt -> *ANY_MODULE*:*NOWHERE*_b2 , EXACT
// px -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// pz -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   px = (float *) malloc(sizeof(float));

// Points To:
// pi -> *HEAP*_l_12 , MAY
// pt -> *ANY_MODULE*:*NOWHERE*_b2 , EXACT
// px -> *HEAP*_l_13 , MAY
// pz -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   pz = (float *) malloc(sizeof(float));

// Points To:
// pi -> *HEAP*_l_12 , MAY
// pt -> *ANY_MODULE*:*NOWHERE*_b2 , EXACT
// px -> *HEAP*_l_13 , MAY
// pz -> *HEAP*_l_14 , MAY

   pt = (int (*)[30]) malloc(sizeof(*pt));

// Points To:
// pi -> *HEAP*_l_12 , MAY
// pt -> *HEAP*_l_15[0] , MAY
// px -> *HEAP*_l_13 , MAY
// pz -> *HEAP*_l_14 , MAY


   return 0;
}
