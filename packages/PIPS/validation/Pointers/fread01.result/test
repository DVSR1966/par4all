int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
   fp = fopen("file.txt", "rb");
   n = fread(buf, 8, 50, fp);
   if (n==-1)
      printf("fread failed");
   fclose(fp);
}
int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
   fp = fopen("file.txt", "rb");
   n = fread(buf, 8, 50, fp);
   if (n==-1)
      printf("fread failed");
   fclose(fp);
}

POINTS TO

//  {}
int main()
{
//  points to = {}
   FILE *fp;
//  {(fp,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   size_t n;
//  {(fp,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   char buf[200];
//  {(fp,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   fp = fopen("file.txt", "rb");
//  {(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   n = fread(buf, 8, 50, fp);
//  {(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   if (n==-1)
//  {(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
      printf("fread failed");
//  {(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   fclose(fp);
}
