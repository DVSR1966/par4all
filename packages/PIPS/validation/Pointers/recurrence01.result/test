
Proper effects for "recurrence01"

void recurrence01(list l)
{
//             <    is written>: i
   int i = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: l
   l->content = &i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   l->next->next->next->content = &i;
   return;
}

