int foo(float (*b)[3])
{
//  {(b,_b_1,-Exact-)}
   float c;                                                             /*0011*/
//  {(b,_b_1,-Exact-)}
   (*b)[3] = 2.0;                                                       /*0012*/
//  {(b,_b_1,-Exact-)}
   c = (*b)[3];                                                         /*0013*/
//  {(b,_b_1,-Exact-)}
   b[1][3] = 2.0;                                                       /*0014*/
//  {(b,_b_1,-Exact-)}
   c = b[1][3];                                                         /*0015*/
//  {(b,_b_1,-Exact-)}
   (*b)[3]++;                                                           /*0017*/
//  {(b,_b_1,-Exact-)}
   (*b)[3] += 5.0;                                                      /*0018*/
//  {(b,_b_1,-Exact-)}
   b[1][3]++;                                                           /*0019*/
//  {(b,_b_1,-Exact-)}
   b[1][3] += 5.0;                                                      /*0020*/
//  {(b,_b_1,-Exact-)}
   return 1;                                                            /*0022*/
}

