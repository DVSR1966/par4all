
PROPER  EFFECTS

int main()
{
   float a[5][3], ret;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

   ret = foo(a);
//             <may be read   >: a[*][3]
//             <may be written>: a[*][3]
//             <    is written>: ret
   ret = foo2(a);
//             <may be read   >: d[*][3]
//             <may be written>: d[*][3]
//             <    is written>: ret
   ret = foo3();

   return 1;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   float a[5][3], ret;

// Points To: none


   ret = foo(a);

// Points To: none

   ret = foo2(a);

// Points To: none

   ret = foo3();

// Points To: none


   return 1;
}
