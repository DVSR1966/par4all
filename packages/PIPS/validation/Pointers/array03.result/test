
PROPER POINTER EFFECTS

int main()
{
   float a[5][3], ret;
//             <    is read   >: a[0][3] a[1][3]
//             <    is written>: a[0][3] a[1][3] ret

   ret = foo(a);
//             <    is read   >: a[0][3] a[1][3]
//             <    is written>: a[0][3] a[1][3] ret
   ret = foo2(a);
//             <    is read   >: d[0][3] d[1][3]
//             <    is written>: d[0][3] d[1][3] ret
   ret = foo3();

   return 1;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   float a[5][3], ret;
//  points to = {}

   ret = foo(a);
//  points to = {}
   ret = foo2(a);
//  points to = {}
   ret = foo3();
//  points to = {}

   return 1;
}
