
Proper effects

int main()
{
//             <may be written>: tab[*]
   int tab[3] = {0, 1, 2};                                              /*0006*/
//             <    is read   >: tab[0]
//             <    is written>: tab[0]

   tab[0] = tab[0]+2;                                                   /*0008*/
   return 0;                                                            /*0009*/
}

POINTS TO

//  {}
int main()
{
//  points to = {}
   int tab[3] = {0, 1, 2};                                              /*0006*/
//  points to = {}

   tab[0] = tab[0]+2;                                                   /*0008*/
//  points to = {}
   return 0;                                                            /*0009*/
}
