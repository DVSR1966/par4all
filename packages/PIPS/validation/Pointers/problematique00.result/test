int main()
{
   int *a, b, c, d;
//             <    is written>: c
   c = 3;
//             <    is written>: a
   /* S1 */
   a = &c;
//             <    is read   >: d __IO_EFFECTS:LUNS[d]
//             <    is written>: __IO_EFFECTS:LUNS[d]
   /* S2 */
   read(d);
//             <    is read   >: a a[0]
//             <    is written>: b
   /* S3 */
   b = *a;
   /* S4 */
   
   
   return 0;
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   int *a, b, c, d;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   c = 3;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   /* S1 */
   a = &c;
//  {(a,c,-Exact-)}
   /* S2 */
   read(d);
//  {(a,c,-Exact-)}
   /* S3 */
   b = *a;
//  {(a,c,-Exact-)}
   /* S4 */
   
   
   return 0;
}
