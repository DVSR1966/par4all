// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int ****p, ***p_1, **p_2, *p_3;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_3 -> *ANY_MODULE*:*NOWHERE* , EXACT

   int i, j;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_3 -> *ANY_MODULE*:*NOWHERE* , EXACT

   int ***q_1, **q_2, *q_3;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_3 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_3 -> *ANY_MODULE*:*NOWHERE* , EXACT

   i = 0;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_3 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_3 -> *ANY_MODULE*:*NOWHERE* , EXACT

   j = 1;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_3 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_3 -> *ANY_MODULE*:*NOWHERE* , EXACT

   p_3 = &i;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_3 -> i , EXACT
// q_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_3 -> *ANY_MODULE*:*NOWHERE* , EXACT

   q_3 = &j;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_3 -> i , EXACT
// q_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_3 -> j , EXACT

   p_2 = &p_3;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> p_3 , EXACT
// p_3 -> i , EXACT
// q_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_2 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_3 -> j , EXACT

   q_2 = &q_3;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_2 -> p_3 , EXACT
// p_3 -> i , EXACT
// q_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_2 -> q_3 , EXACT
// q_3 -> j , EXACT

   p_1 = &p_2;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> p_2 , EXACT
// p_2 -> p_3 , EXACT
// p_3 -> i , EXACT
// q_1 -> *ANY_MODULE*:*NOWHERE* , EXACT
// q_2 -> q_3 , EXACT
// q_3 -> j , EXACT

   q_1 = &q_2;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE* , EXACT
// p_1 -> p_2 , EXACT
// p_2 -> p_3 , EXACT
// p_3 -> i , EXACT
// q_1 -> q_2 , EXACT
// q_2 -> q_3 , EXACT
// q_3 -> j , EXACT

   p = &p_1;

// Points To:
// p -> p_1 , EXACT
// p_1 -> p_2 , EXACT
// p_2 -> p_3 , EXACT
// p_3 -> i , EXACT
// q_1 -> q_2 , EXACT
// q_2 -> q_3 , EXACT
// q_3 -> j , EXACT

   ***p = q_3;

// Points To:
// p -> p_1 , EXACT
// p_1 -> p_2 , EXACT
// p_2 -> p_3 , EXACT
// p_3 -> j , EXACT
// q_1 -> q_2 , EXACT
// q_2 -> q_3 , EXACT
// q_3 -> j , EXACT

   
   
   
   
   return 0;
}
