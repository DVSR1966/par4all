
PROPER POINTER EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int i = 0;
//  points to = {}
   int *p[10];
//  points to = {}
   int a[10];
//  points to = {}

   while (i<10) {
//  points to = {}
      p[i] = &a[i];
//  points to = {}
      i++;
   }
//  {(p[i],a[i],-MAY-)}
   return 0;
}
