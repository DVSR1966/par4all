
PROPER  EFFECTS

int main()
{
   int ***p;
   int **q;
//             <    is written>: a
   int a = 0;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: q _MALLOC_EFFECTS:_MALLOC_
   q = (int **) malloc(sizeof(int *));
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   q[0] = &a;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int ***) malloc(sizeof(int * *));
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p q
   p[0] = q;
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   free(p);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int ***p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int **q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   int a = 0;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   q = (int **) malloc(sizeof(int *));
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,*HEAP*_l_9,-Exact-)}
   q[0] = &a;
//  {(*HEAP*_l_9,a,-Exact-);(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *HEAP*_l_9,-Exact-)}
   p = (int ***) malloc(sizeof(int * *));
//  {(*HEAP*_l_9,a,-Exact-);(p,*HEAP*_l_11,-Exact-);(q,*HEAP*_l_9,
//   -Exact-)}
   p[0] = q;
//  {(*HEAP*_l_11,*HEAP*_l_9,-Exact-);(*HEAP*_l_9,a,-Exact-);(p,
//   *HEAP*_l_11,-Exact-);(q,*HEAP*_l_9,-Exact-)}
   free(p);
//  {(*HEAP*_l_11,*HEAP*_l_9,-Exact-);(*HEAP*_l_9,a,-Exact-);(p,
//   *HEAP*_l_11,-Exact-);(q,*HEAP*_l_9,-Exact-)}
   return 0;
}
