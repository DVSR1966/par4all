
PROPER POINTER EFFECTS

int main()
{
   int a[10];
   int *p;
   int i, j;
//             <    is written>: i
   i = 3;
//             <    is written>: j
   j = 2;
//             <    is written>: p
   p = &a[0];
//             <    is read   >: i p
//             <    is written>: p
   p = p+i;
//             <    is read   >: j p
//             <    is written>: p
   p = p+j;
//             <    is read   >: i p
//             <    is written>: p
   p = p-i;
//             <    is read   >: j p
//             <    is written>: p
   p = p-j;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int a[10];
//  points to = {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i, j;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   i = 3;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   j = 2;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p = &a[0];
//  {(p,a[0],-Exact-)}
   p = p+i;
//  {(p,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   p = p+j;
//  {(p,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   p = p-i;
//  {(p,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   p = p-j;
//  {(p,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   return 0;
}
