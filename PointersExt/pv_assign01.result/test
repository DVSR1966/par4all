
PROPER POINTER EFFECTS

int main()
{
   int *a;
//             <    is written>: b
   int b = 0;
//             <    is written>: a
   a = &b;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *a;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b = 0;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   a = &b;
//  {(a,b,-Exact-)}
   return 0;
}
