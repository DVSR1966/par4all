//   *ANY_MODULE*:*NOWHERE*_b4,-Exact-);(i_name,
//   *ANY_MODULE*:*NOWHERE*_b3,-Exact-)}

   c = _IO_getc(fp);
//  {(buf2,*HEAP*_l_10,-Exact-);(fmt,*ANY_MODULE*:*ANYWHERE*,
//   -Exact-);(fp[0][__pad1],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][__pad2],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][__pad3],*ANY_MODULE*:*NOWHERE*_b0,
