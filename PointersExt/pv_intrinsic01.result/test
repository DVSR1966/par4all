   (void) fgets(buf1, max, fp);
//  {(buf2,*HEAP*_l_10,-Exact-);(fmt,*ANY_MODULE*:*ANYWHERE*,
//   -Exact-);(fp[0][__pad1],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][__pad2],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][__pad3],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][__pad4],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][_lock],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][_markers],*ANY_MODULE*:*NOWHERE*_b2,
//   -Exact-);(fp[0][_IO_backup_base],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_buf_base],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_buf_end],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_read_base],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_read_end],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_read_ptr],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_save_base],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_save_end],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_write_base],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_write_end],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_write_ptr],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-);(fp_pos,
//   *ANY_MODULE*:*NOWHERE*_b4,-Exact-);(i_name,
//   *ANY_MODULE*:*NOWHERE*_b3,-Exact-)}
   (void) fputc(c, fp);
//  {(buf2,*HEAP*_l_10,-Exact-);(fmt,*ANY_MODULE*:*ANYWHERE*,
//   -Exact-);(fp[0][__pad1],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][__pad2],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][__pad3],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][__pad4],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][_lock],*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(fp[0][_markers],*ANY_MODULE*:*NOWHERE*_b2,
//   -Exact-);(fp[0][_IO_backup_base],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_buf_base],*ANY_MODULE*:*NOWHERE*_b3,
