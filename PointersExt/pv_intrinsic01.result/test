//   -Exact-);(fp[0][_IO_read_base],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_read_end],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_read_ptr],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_save_base],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_save_end],*ANY_MODULE*:*NOWHERE*_b3,
//   -Exact-);(fp[0][_IO_write_base],*ANY_MODULE*:*NOWHERE*_b3,
