
PROPER POINTER EFFECTS

int main()
{
   FILE *fp;
   size_t n, nr;
   char buf1[200];
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: buf2 _MALLOC_EFFECTS:_MALLOC_
   char *buf2 = malloc(200*sizeof(char));
   char *fmt;
   char *i_name;
   int i, r, c, max;
   fpos_t *fp_pos, pos;
   long int fp_pos_indic;
   va_list vl;
//             <    is written>: fmt
   
   // formatted IO functions
   fmt = "%s%d";
//             <    is written>: max
   max = 100;
//             <    is written>: n
   n = 200;
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: __IO_EFFECTS:LUNS[*]
//             <    is written>: fp
   fp = fopen("file.txt", "rb");
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: buf1[*] fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
   
   
   (void) fread(buf1, 8, 50, fp);
//             <may be read   >: fmt[*] fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] i_name[*] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fmt fp i_name
//             <    is written>: i
   (void) fscanf(fp, fmt, i_name, &i);
//             <may be read   >: fp[0] i_name[*] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp i i_name
   (void) fprintf(fp, "%s%d", i_name, i);
//             <may be read   >: stdin[0]
//             <may be written>: i_name[*] stdin[0]
//             <    is read   >: i_name stdin __IO_EFFECTS:LUNS[0]
//             <    is written>: i __IO_EFFECTS:LUNS[0]
   
   
   (void) scanf("%s%d", i_name, &i);
//             <may be read   >: fmt[*] i_name[*] stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: fmt i i_name stdout 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   (void) printf(fmt, i_name, i);
//             <may be read   >: buf1[*] fmt[*]
//             <may be written>: i_name[*]
//             <    is read   >: fmt i_name
//             <    is written>: i
   
   
   (void) sscanf(buf1, fmt, i_name, &i);
//             <may be read   >: fmt[*] i_name[*]
//             <may be written>: buf2[*]
//             <    is read   >: buf2 fmt i i_name
   (void) sprintf(buf2, fmt, i_name, i);
//             <may be read   >: fmt[*] i_name[*]
//             <may be written>: buf2[*]
//             <    is read   >: buf2 fmt i i_name
   (void) snprintf(buf2, 100, fmt, i_name, i);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
//             <    is written>: c
   
   
   // character IO functions
   
   c = fgetc(fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: buf1[*] fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp max
   (void) fgets(buf1, max, fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: c fp
   (void) fputc(c, fp);
//             <may be read   >: buf1[*] fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
   (void) fputs(buf1, fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
//             <    is written>: c

   c = _IO_getc(fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: c fp
   (void) _IO_putc(c, fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: c fp
   (void) ungetc(c, fp);
//             <may be read   >: stdin[0]
//             <may be written>: stdin[0]
//             <    is read   >: stdin __IO_EFFECTS:LUNS[0]
//             <    is written>: c __IO_EFFECTS:LUNS[0]

   c = getchar();
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: c stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   (void) putchar(c);
//             <may be read   >: stdin[0]
//             <may be written>: buf1[*] stdin[0]
//             <    is read   >: stdin __IO_EFFECTS:LUNS[0]
//             <    is written>: __IO_EFFECTS:LUNS[0]

   (void) gets(buf1);
//             <may be read   >: buf1[*] stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   (void) puts(buf1);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: buf2[*] fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: buf2 fp n
//             <    is written>: nr
   
   // direct IO functions
   
   nr = fread(buf2, sizeof(char), n, fp);
//             <may be read   >: buf2[*] fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: buf2 fp n
//             <    is written>: nr
   nr = fwrite(buf2, sizeof(char), n, fp);
//             <    is written>: fp_pos
   
   // file positionning functions
   fp_pos = &pos;
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] fp_pos[*] fp_pos[*].__pos 
//                               fp_pos[*].__state.__count 
//                               fp_pos[*].__state.__value.__wch 
//                               fp_pos[*].__state.__value.__wchb[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp fp_pos
   (void) fgetpos(fp, fp_pos);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] pos.__state.__value.__wchb[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
//             <    is written>: pos pos.__pos pos.__state.__count 
//                               pos.__state.__value.__wch
   (void) fgetpos(fp, &pos);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp

   (void) fseek(fp, 0L, 0);
//             <may be read   >: fp[0] fp_pos[*] fp_pos[*].__pos 
//                               fp_pos[*].__state.__count 
//                               fp_pos[*].__state.__value.__wch 
//                               fp_pos[*].__state.__value.__wchb[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp fp_pos

   (void) fsetpos(fp, fp_pos);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
//             <    is written>: fp_pos_indic

   fp_pos_indic = ftell(fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
   rewind(fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
   
   // error handling functions
   
   clearerr(fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
//             <    is written>: r
   r = feof(fp);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
//             <    is written>: r
   r = ferror(fp);
//             <may be read   >: buf1[*] stderr[0]
//             <may be written>: stderr[0]
//             <    is read   >: stderr __IO_EFFECTS:LUNS[2]
//             <    is written>: __IO_EFFECTS:LUNS[2]
   perror(buf1);
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp

   fclose(fp);
//             <may be read   >: stderr[0]
//             <may be written>: stderr[0]
//             <    is read   >: stderr __IO_EFFECTS:LUNS[2]
//             <    is written>: __IO_EFFECTS:LUNS[2]

   fprintf(stderr, "The END\n");
   return 0;
}

POINTS-TO

