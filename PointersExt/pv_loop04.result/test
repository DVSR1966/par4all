
PROPER POINTER EFFECTS

int main()
{
   int i;
   int *p[10];
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int i;
//  points to = {}
   int *p[10];
//  points to = {}
   int a[10];
//  points to = {}

   for(i = 0; i <= 9; i += 1)
//  {(p[i],a[i],-MAY-)}
      p[i] = &a[i];
//  {(p[i],a[i],-MAY-)}
   return 0;
}
