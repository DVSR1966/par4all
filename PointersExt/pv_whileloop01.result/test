
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: a[i]
      a[i] = i;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int i = 0;
//  points to = {}
   int a[10];
//  points to = {}

   while (i<10) {
//  points to = {}
      a[i] = i;
//  points to = {}
      i++;
   }
//  points to = {}
   return 0;
}
