
PROPER  EFFECTS

int main()
{
   int **a;
   int b;
//             <    is read   >: NULL
//             <    is written>: a
   a = (int **) NULL;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: a
   a[0] = &b;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  {}
   int **a;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   a = (int **) NULL;
//  {(a,*NULL_POINTER*,-Exact-)}
   a[0] = &b;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,b,-Exact-);(a,*NULL_POINTER*,
//   -Exact-)}
   return 0;
}
