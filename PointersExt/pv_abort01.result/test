
PROPER POINTER EFFECTS

int main()
{
   int **a;
   int b;
//             <    is read   >: a
//             <    is written>: a[0]
   a[0] = &b;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int **a;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int b;
//  {(a,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   a[0] = &b;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,b,-Exact-);(a,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 0;
}
