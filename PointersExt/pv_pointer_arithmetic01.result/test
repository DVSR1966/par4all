
PROPER  EFFECTS

int main()
{
   int a[10];
   int *p;
//             <    is written>: p
   p = &a[0];
//             <    is read   >: p
//             <    is written>: p
   p = p+1;
//             <    is read   >: p
//             <    is written>: p
   p = p+3;
//             <    is read   >: p
//             <    is written>: p
   p = p-1;
//             <    is read   >: p
//             <    is written>: p
   p = p-2;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int a[10];
//  points to = {}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   p = &a[0];
//  {(p,a[0],-Exact-)}
   p = p+1;
//  {(p,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   p = p+3;
//  {(p,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   p = p-1;
//  {(p,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   p = p-2;
//  {(p,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   return 0;
}
