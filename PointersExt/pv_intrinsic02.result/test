
PROPER POINTER EFFECTS

int main()
{
   char *end;
   long int li;
//             <may be read   >: end[*]
//             <may be written>: end[*]
//             <    is read   >: end
//             <    is written>: end li

   li = strtol("32445667 is a number", &end, 0);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   char *end;
//  {(end,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   long int li;
//  {(end,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   li = strtol("32445667 is a number", &end, 0);
//  {(end,*ANY_MODULE*:*ANYWHERE*_b0,-Exact-)}
   return 0;
}
