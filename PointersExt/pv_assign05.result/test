
PROPER POINTER EFFECTS

int main()
{
   mystruct s1, s2;
   mystruct tab_s[2];
//             <    is read   >: s1.a
//             <    is written>: s2.a
   s2.a = s1.a;
//             <    is read   >: s1.b[0]
//             <    is written>: s2.b[0]
   (s2.b)[0] = (s1.b)[0];
//             <    is read   >: s1.a
//             <    is written>: tab_s[0].a
   tab_s[0].a = s1.a;
//             <    is read   >: s1.b[0]
//             <    is written>: tab_s[0].b[0]
   (tab_s[0].b)[0] = (s1.b)[0];
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   mystruct s1, s2;
//  {(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   mystruct tab_s[2];
//  {(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   s2.a = s1.a;
//  {(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   (s2.b)[0] = (s1.b)[0];
//  {(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][0],
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   tab_s[0].a = s1.a;
//  {(*ANY_MODULE*:*NOWHERE*_b0,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][0],
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   (tab_s[0].b)[0] = (s1.b)[0];
//  {(*ANY_MODULE*:*NOWHERE*_b0,*ANY_MODULE*:*NOWHERE*_b0,
//   -Exact-);(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2[b][0],
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(s2[a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[0][b][0],
//   *ANY_MODULE*:*ANYWHERE*_b0,-Exact-);(tab_s[*][a],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(tab_s[*][c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 0;
}
