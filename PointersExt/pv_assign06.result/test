
PROPER POINTER EFFECTS

int main()
{
   mystruct s1, *s2;
//             <    is written>: i
   int i = 1;
//             <    is written>: j
   int j = 2;
//             <    is written>: s2
   s2 = &s1;
//             <    is read   >: s2
//             <    is written>: s2[0].a
   s2->a = &i;
//             <    is written>: s1.a
   s1.a = &j;
//             <    is read   >: s2
//             <    is written>: s2[0].b[0]
   (s2->b)[0] = &i;
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   mystruct s1, *s2;
//  {(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   int i = 1;
//  {(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   int j = 2;
//  {(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   s2 = &s1;
//  {(s1[a],*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s1[c],
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(s2,s1,-Exact-)}
   s2->a = &i;
//  {(s1[a],i,-Exact-);(s2,s1,-Exact-)}
   s1.a = &j;
//  {(s1[a],j,-Exact-);(s2,s1,-Exact-)}
   (s2->b)[0] = &i;
//  {(s1[a],j,-Exact-);(s2[0][b][0],i,-Exact-);(s2,s1,-Exact-)}
   return 0;
}
