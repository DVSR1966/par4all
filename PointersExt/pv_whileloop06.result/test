//  {}
int main()
{
//  points to = {}
   int i;
//  points to = {}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int a[10];
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int b[10];
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}

   p = &a[0];
//  {(p,a[0],-Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   q = &b[0];
//  {(p,a[0],-Exact-);(q,b[0],-Exact-)}
   i = 0;
//  {(p,a[0],-Exact-);(q,b[0],-Exact-)}
   while (i<10) {
//  points to = {}
      *p++ = 2**q++;
//  points to = {}
      i++;
   }
//  {(p,a[0],-MAY-);(p,*ANY_MODULE*:*ANYWHERE*_b0,-MAY-);(q,b[0],
//   -MAY-);(q,*ANY_MODULE*:*ANYWHERE*_b0,-MAY-)}
   return 0;
}
