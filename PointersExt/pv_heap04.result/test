
PROPER POINTER EFFECTS

int main()
{
   int **p;
   int *q;
//             <    is written>: a
   int a = 0;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: q _MALLOC_EFFECTS:_MALLOC_
   q = (int *) malloc(sizeof(int));
//             <    is read   >: a q
//             <    is written>: q[0]
   q[0] = a;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int **) malloc(sizeof(int *));
//             <    is read   >: p q
//             <    is written>: p[0]
   p[0] = q;
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   free(p);
   return 0;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int **p;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int *q;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   int a = 0;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b1,-Exact-)}
   q = (int *) malloc(sizeof(int));
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,*HEAP*_l_8,-Exact-)}
   q[0] = a;
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,*HEAP*_l_8,-Exact-)}
   p = (int **) malloc(sizeof(int *));
//  {(p,*HEAP*_l_10,-Exact-);(q,*HEAP*_l_8,-Exact-)}
   p[0] = q;
//  {(*HEAP*_l_10,*HEAP*_l_8,-Exact-);(p,*HEAP*_l_10,-Exact-);(q,
//   *HEAP*_l_8,-Exact-)}
   free(p);
//  {(*HEAP*_l_10,*HEAP*_l_8,-Exact-);(p,*HEAP*_l_10,-Exact-);(q,
//   *HEAP*_l_8,-Exact-)}
   return 0;
}
