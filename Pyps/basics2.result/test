void do_convol(int i, int j, int n, int a[n][n], int b[n][n], int kernel[3][3])
{
   int k, l;
   b[i][j] = 0;
   for(k = 0; k <= 2; k += 1)
      for(l = 0; l <= 2; l += 1)
         b[i][j] += a[i+k-1][j+l-1]*kernel[k][l];
}
