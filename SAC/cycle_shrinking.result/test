
Initial code

void shrink(float a[100+4], float b[100+4], float c[100+4])
{
   int i;
   // icc cannot vectorize this
   for(i = 0; i <= 99; i += 1) {
      a[i+4] = b[i];
      b[i+4] = a[i]+c[i];
   }
}
