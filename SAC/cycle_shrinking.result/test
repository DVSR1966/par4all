      a[LU_IND0+4] = b[LU_IND0];
      //c SIMD_COMMENT_1
      b[LU_IND0+4] = a[LU_IND0]+c[LU_IND0];
      //c SIMD_COMMENT_0
      a[LU_IND0+5] = b[LU_IND0+1];
      //c SIMD_COMMENT_1
      b[LU_IND0+5] = a[LU_IND0+1]+c[LU_IND0+1];
      //c SIMD_COMMENT_0
