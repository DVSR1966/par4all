void foo()
{
   int i, tab[4];
   if (i<0) 
      i = 0;
   for(i = 0; i <= 0; i += 1)
      if (tab[i]>i) 
         tab[i] = i;
}
void foo()
{
   int i, tab[4];
   //PIPS generated variable
   int I_0;
   //PIPS generated variable
   int I_1;
   I_0 = i<0;
#pragma PIPS IF_TO_CONVERT
   if (I_0) 
      i = 0;
   ;
   for(i = 0; i <= 0; i += 1) {
      I_1 = tab[i]>i;
#pragma PIPS IF_TO_CONVERT
      if (I_1) 
         tab[i] = i;
      ;
   }
}
void foo()
{
   int i, tab[4];
   //PIPS generated variable
   int I_0;
   //PIPS generated variable
   int I_1;
   I_0 = i<0;
   i = PHI(I_0, 0, i);
   ;
   for(i = 0; i <= 0; i += 1) {
      I_1 = tab[i]>i;
      tab[i] = PHI(I_1, i, tab[i]);
      ;
   }
