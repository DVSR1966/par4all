void foo_l(int a[4], int b[4], int c[4])
{
   int i, j;
   for(j = 0; j <= 9; j += 1)
      for(i = 0; i <= 9; i += 1) {
         a[0] = b[0]+c[0];
         a[1] = b[1]+c[1];
         a[2] = b[2]+c[2];
         a[3] = b[3]+c[3];
      }
}
void foo_l(int a[4], int b[4], int c[4])
{
   int i, j;
   for(j = 0; j <= 9; j += 1)
      for(i = 0; i <= 9; i += 1) {
         //PIPS:SAC generated v4sf vector(s)
         v4sf vec00, vec10, vec20;
         SIMD_LOAD_V4SI_TO_V4SF(vec20, &c[0]);
         SIMD_LOAD_V4SI_TO_V4SF(vec10, &b[0]);
         SIMD_ADDPS(vec00, vec10, vec20);
         SIMD_STORE_V4SF_TO_V4SI(vec00, &a[0]);
      }
}
void foo_l(int a[4], int b[4], int c[4])
{
   int i, j;
   //PIPS generated variable
   v4sf vec00_0, vec10_0, vec20_0, vec00_1, vec10_1, vec20_1;
   SIMD_LOAD_V4SI_TO_V4SF(vec10_0, &b[0]);
   SIMD_LOAD_V4SI_TO_V4SF(vec20_0, &c[0]);
   for(j = 0; j <= 9; j += 1)
      for(i = 0; i <= 9; i += 1) {
         //PIPS:SAC generated v4sf vector(s)
         SIMD_ADDPS(vec00_0, vec10_0, vec20_0);
      }
   SIMD_STORE_V4SF_TO_V4SI(vec00_0, &a[0]);
}
