      SUBROUTINE DOTPROD(A, B, C)

! include "DOTPROD_INC.f"
        INTEGER*2 SIZE
        PARAMETER (SIZE=10000)
! end include "DOTPROD_INC.f"
!       INCLUDE 'DOTPROD_INC.f'

      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
      DO 10 I = 1, SIZE                                                 0002
         A(1) = A(1)+B(I)*C(I)                                          0003
10       CONTINUE                                                       0004

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
      DO 10 I = 1, SIZE                                                 0002
         A(1) = A(1)+B(I)*C(I)                                          0003
10       CONTINUE                                                       0004

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
      DO 10 I = 1, SIZE                                                 0002
         A(1) = A(1)+B(I)*C(I)                                          0003
10       CONTINUE                                                       0004

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
      DO 10 I = 1, SIZE                                                 0002
         A(1) = A(1)+B(I)*C(I)                                          0003
10       CONTINUE                                                       0004

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_0
      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
      DO 10 I = 1, 10000                                                0002
         I_0 = B(I)*C(I)
         A(1) = A(1)+I_0                                                0003
10       CONTINUE                                                       0004

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
      LU_NUB0 = 10000
      LU_IB0 = 0
      LU_IND0 = 0
99999 CONTINUE
      DO 99998 LU_IND0 = 0, 9999, 4
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+1)*C(LU_IND0+1)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+2)*C(LU_IND0+2)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+3)*C(LU_IND0+3)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+4)*C(LU_IND0+4)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0003
c SIMD_COMMENT_2
99998    CONTINUE
      I = 10001

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 RED0(0:3), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
      LU_NUB0 = 10000
      LU_IB0 = 0
      LU_IND0 = 0
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      DO 99998 LU_IND0 = 0, 9999, 4
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+1)*C(LU_IND0+1)
c SIMD_COMMENT_1
         RED0(0) = RED0(0)+I_0                                          0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+2)*C(LU_IND0+2)
c SIMD_COMMENT_1
         RED0(1) = RED0(1)+I_0                                          0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+3)*C(LU_IND0+3)
c SIMD_COMMENT_1
         RED0(2) = RED0(2)+I_0                                          0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+4)*C(LU_IND0+4)
c SIMD_COMMENT_1
         RED0(3) = RED0(3)+I_0                                          0003
c SIMD_COMMENT_2
99998    CONTINUE
      A(1) = RED0(3)+RED0(2)+RED0(1)+RED0(0)+A(1)
      I = 10001

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 RED0(0:3), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      DO 99998 LU_IND0 = 0, 9999, 4
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+1)*C(LU_IND0+1)
c SIMD_COMMENT_1
         RED0(0) = RED0(0)+I_0                                          0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+2)*C(LU_IND0+2)
c SIMD_COMMENT_1
         RED0(1) = RED0(1)+I_0                                          0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+3)*C(LU_IND0+3)
c SIMD_COMMENT_1
         RED0(2) = RED0(2)+I_0                                          0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+4)*C(LU_IND0+4)
c SIMD_COMMENT_1
         RED0(3) = RED0(3)+I_0                                          0003
c SIMD_COMMENT_2
99998    CONTINUE
      A(1) = RED0(3)+RED0(2)+RED0(1)+RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_03, I_02, I_01, I_00, I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 RED0(0:3), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      DO 99998 LU_IND0 = 0, 9999, 4
c SIMD_COMMENT_0
         I_03 = B(LU_IND0+1)*C(LU_IND0+1)
c SIMD_COMMENT_1
         RED0(0) = RED0(0)+I_03                                         0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_02 = B(LU_IND0+2)*C(LU_IND0+2)
c SIMD_COMMENT_1
         RED0(1) = RED0(1)+I_02                                         0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_01 = B(LU_IND0+3)*C(LU_IND0+3)
c SIMD_COMMENT_1
         RED0(2) = RED0(2)+I_01                                         0003
c SIMD_COMMENT_2
c SIMD_COMMENT_0
         I_00 = B(LU_IND0+4)*C(LU_IND0+4)
c SIMD_COMMENT_1
         RED0(3) = RED0(3)+I_00                                         0003
c SIMD_COMMENT_2
99998    CONTINUE
      A(1) = RED0(3)+RED0(2)+RED0(1)+RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_03, I_02, I_01, I_00, I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 v4hi_vec11(0:3), v4hi_vec10(0:3), v4hi_vec9(0:3), 
     &v4hi_vec8(0:3), v4hi_vec7(0:3), v4hi_vec6(0:3), v4hi_vec5(0:3)
     &, v4hi_vec4(0:3), v4hi_vec3(0:3), v4hi_vec2(0:3), v4hi_vec1(0:3
     &), v4hi_vec0(0:3), RED0(0:3), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      DO 99998 LU_IND0 = 0, 9999, 4
         CALL SIMD_LOAD_V4HI(v4hi_vec0, C(LU_IND0+1))
         CALL SIMD_LOAD_V4HI(v4hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v4hi_vec2, v4hi_vec1, v4hi_vec0)
         CALL SIMD_SAVE_GENERIC_V4HI(v4hi_vec2, I_03, I_02, I_01, 
     &   I_00)
         CALL SIMD_LOAD_V4HI(v4hi_vec4, RED0(0))
         CALL SIMD_ADDW(v4hi_vec5, v4hi_vec4, v4hi_vec2)
         CALL SIMD_SAVE_V4HI(v4hi_vec5, RED0(0))
c SIMD_COMMENT_2
99998    CONTINUE
      A(1) = RED0(3)+RED0(2)+RED0(1)+RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_03, I_02, I_01, I_00, I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 v4hi_vec11(0:3), v4hi_vec10(0:3), v4hi_vec9(0:3), 
     &v4hi_vec8(0:3), v4hi_vec7(0:3), v4hi_vec6(0:3), v4hi_vec5(0:3)
     &, v4hi_vec4(0:3), v4hi_vec3(0:3), v4hi_vec2(0:3), v4hi_vec1(0:3
     &), v4hi_vec0(0:3), RED0(0:3), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      DO 99998 LU_IND0 = 0, 9999, 4
         CALL SIMD_LOAD_V4HI(v4hi_vec0, C(LU_IND0+1))
         CALL SIMD_LOAD_V4HI(v4hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v4hi_vec2, v4hi_vec1, v4hi_vec0)
         CALL SIMD_LOAD_V4HI(v4hi_vec4, RED0(0))
         CALL SIMD_ADDW(v4hi_vec5, v4hi_vec4, v4hi_vec2)
         CALL SIMD_SAVE_V4HI(v4hi_vec5, RED0(0))
c SIMD_COMMENT_2
99998    CONTINUE
      A(1) = RED0(3)+RED0(2)+RED0(1)+RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_03, I_02, I_01, I_00, I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 v4hi_vec11(0:3), v4hi_vec10(0:3), v4hi_vec9(0:3), 
     &v4hi_vec8(0:3), v4hi_vec7(0:3), v4hi_vec6(0:3), v4hi_vec5(0:3)
     &, v4hi_vec4(0:3), v4hi_vec3(0:3), v4hi_vec2(0:3), v4hi_vec1(0:3
     &), v4hi_vec0(0:3), RED0(0:3), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      CALL SIMD_LOAD_V4HI(v4hi_vec4, RED0(0))
      DO 99998 LU_IND0 = 0, 9999, 4
         CALL SIMD_LOAD_V4HI(v4hi_vec0, C(LU_IND0+1))
         CALL SIMD_LOAD_V4HI(v4hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v4hi_vec2, v4hi_vec1, v4hi_vec0)
         CALL SIMD_ADDW(v4hi_vec5, v4hi_vec4, v4hi_vec2)
c SIMD_COMMENT_2
99998    CONTINUE
      CALL SIMD_SAVE_V4HI(v4hi_vec5, RED0(0))
      A(1) = RED0(3)+RED0(2)+RED0(1)+RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER LU_IND0
      INTEGER*2 v4hi_vec5(0:3), v4hi_vec4(0:3), v4hi_vec2(0:3), 
     &v4hi_vec1(0:3), v4hi_vec0(0:3), RED0(0:3), A(1:1), B(1:SIZE), C
     &(1:SIZE)

      A(1) = 0                                                          0001
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      CALL SIMD_LOAD_V4HI(v4hi_vec4, RED0(0))
      DO 99998 LU_IND0 = 0, 9999, 4
         CALL SIMD_LOAD_V4HI(v4hi_vec0, C(LU_IND0+1))
         CALL SIMD_LOAD_V4HI(v4hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v4hi_vec2, v4hi_vec1, v4hi_vec0)
         CALL SIMD_ADDW(v4hi_vec5, v4hi_vec4, v4hi_vec2)
c SIMD_COMMENT_2
99998    CONTINUE
      CALL SIMD_SAVE_V4HI(v4hi_vec5, RED0(0))
      A(1) = RED0(3)+RED0(2)+RED0(1)+RED0(0)+A(1)

      END

