      SUBROUTINE DOTPROD(A, B, C)

! include "DOTPROD_INC.f"
      PARAMETER (SIZE=10000)
! end include "DOTPROD_INC.f"
!       INCLUDE 'DOTPROD_INC.f'

      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99998 CONTINUE
      A_RED0(0) = 0
      A_RED0(1) = 0
      A_RED0(2) = 0
      A_RED0(3) = 0
      DO 99997 LU_IND0 = 0, 9999, 4
         CALL SIMD_LOAD_V4HI(v4hi_vec0, C(LU_IND0+1))
         CALL SIMD_LOAD_V4HI(v4hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v4hi_vec2, v4hi_vec1, v4hi_vec0)
         CALL SIMD_SAVE_GENERIC_V4HI(v4hi_vec2, I_03, I_02, I_01, 
     &   I_00)
         CALL SIMD_LOAD_GENERIC_V4HI(v4hi_vec4, A_RED0(0), A_RED0(1)
     &   , A_RED0(2), A_RED0(3))
         CALL SIMD_ADDW(v4hi_vec5, v4hi_vec4, v4hi_vec2)
         CALL SIMD_SAVE_GENERIC_V4HI(v4hi_vec5, A_RED0(0), A_RED0(1)
     &   , A_RED0(2), A_RED0(3))
c SIMD_COMMENT_2
99997    CONTINUE
      A(1) = A_RED0(3)+A_RED0(2)+A_RED0(1)+A_RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)

! include "DOTPROD_INC.f"
      PARAMETER (SIZE=10000)
! end include "DOTPROD_INC.f"
!       INCLUDE 'DOTPROD_INC.f'

      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99998 CONTINUE
      A_RED0(0) = 0
      A_RED0(1) = 0
      A_RED0(2) = 0
      A_RED0(3) = 0
      DO 99997 LU_IND0 = 0, 9999, 4
         CALL SIMD_LOAD_V4HI(v4hi_vec0, C(LU_IND0+1))
         CALL SIMD_LOAD_V4HI(v4hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v4hi_vec2, v4hi_vec1, v4hi_vec0)
         CALL SIMD_LOAD_GENERIC_V4HI(v4hi_vec4, A_RED0(0), A_RED0(1)
     &   , A_RED0(2), A_RED0(3))
         CALL SIMD_ADDW(v4hi_vec5, v4hi_vec4, v4hi_vec2)
         CALL SIMD_SAVE_GENERIC_V4HI(v4hi_vec5, A_RED0(0), A_RED0(1)
     &   , A_RED0(2), A_RED0(3))
c SIMD_COMMENT_2
99997    CONTINUE
      A(1) = A_RED0(3)+A_RED0(2)+A_RED0(1)+A_RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)

! include "DOTPROD_INC.f"
      PARAMETER (SIZE=10000)
! end include "DOTPROD_INC.f"
!       INCLUDE 'DOTPROD_INC.f'

      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99998 CONTINUE
      A_RED0(0) = 0
      A_RED0(1) = 0
      A_RED0(2) = 0
      A_RED0(3) = 0
      CALL SIMD_LOAD_GENERIC_V4HI(v4hi_vec4, A_RED0(0), A_RED0(1), 
     &A_RED0(2), A_RED0(3))
      DO 99997 LU_IND0 = 0, 9999, 4
         CALL SIMD_LOAD_V4HI(v4hi_vec0, C(LU_IND0+1))
         CALL SIMD_LOAD_V4HI(v4hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v4hi_vec2, v4hi_vec1, v4hi_vec0)
         CALL SIMD_ADDW(v4hi_vec4, v4hi_vec4, v4hi_vec2)
c SIMD_COMMENT_2
99997    CONTINUE
      CALL SIMD_SAVE_GENERIC_V4HI(A_RED0(3), A_RED0(2), A_RED0(1), 
     &A_RED0(0), v4hi_vec4)
      A(1) = A_RED0(3)+A_RED0(2)+A_RED0(1)+A_RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)

! include "DOTPROD_INC.f"
      PARAMETER (SIZE=10000)
! end include "DOTPROD_INC.f"
!       INCLUDE 'DOTPROD_INC.f'

      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0001
99998 CONTINUE
      A_RED0(0) = 0
      A_RED0(1) = 0
      A_RED0(2) = 0
      A_RED0(3) = 0
      CALL SIMD_SAVE_GENERIC_V4HI(A_RED0(3), A_RED0(2), A_RED0(1), 
     &A_RED0(0), v4hi_vec4)
      A(1) = A_RED0(3)+A_RED0(2)+A_RED0(1)+A_RED0(0)+A(1)

      END
/* C pretty print for module DOTPROD. */
#define size 10000

void
dotprod (short a[1], short b[10000], short c[10000])
{
  short a_red0[4];
  v4hi v4hi_vec4;

  short i_03;
  short i_02;
  short i_01;
  short i_00;
  int lu_ind0;
  int lu_ib0;
  int lu_nub0;
  short i_0;
#define size 10000
  ;
  int i;
  short a_red0[4];
  v4hi v4hi_vec0;
  v4hi v4hi_vec1;
  v4hi v4hi_vec2;
  v4hi v4hi_vec3;
  v4hi v4hi_vec4;
  v4hi v4hi_vec5;
  v4hi v4hi_vec6;
  v4hi v4hi_vec7;
  v4hi v4hi_vec8;
  v4hi v4hi_vec9;
  v4hi v4hi_vec10;
  v4hi v4hi_vec11;
  a[1] = 0;
  ;
  a_red0[0] = 0;
  a_red0[1] = 0;
  a_red0[2] = 0;
  a_red0[3] = 0;
  simd_save_generic_v4hi (a_red0[3], a_red0[2], a_red0[1], a_red0[0],
			  v4hi_vec4);
  a[1] = (a_red0[3] + (a_red0[2] + (a_red0[1] + (a_red0[0] + a[1]))));
  ;
}

