
Initial code

void threshold(float data[128], float val)
{
   int i;
   for(i = 0; i <= 127; i += 1)
      if (data[i]>val) 
         data[i] = val;
}
void threshold(float data[128], float val)
{
   int i;
   //PIPS generated variable
   int I_0;
   for(i = 0; i <= 127; i += 1) {
      I_0 = data[i]>val;
      data[i] = I_0?val:data[i];
      ;
   }
}
