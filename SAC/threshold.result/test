         float aligned[4] = {val, val, val, val};
         //PIPS:SAC generated float vector(s)
         float v4sf_vec2[4];
         SIMD_LOAD_V4SF(v4sf_vec2, &aligned[0]);
         for(LU_IND0 = 0; LU_IND0 <= 127; LU_IND0 += 4) {
            //PIPS:SAC generated int vector(s)
            int v4si_vec0[4];
            //PIPS:SAC generated float vector(s)
            float v4sf_vec1[4];
            //PIPS:SAC generated float vector(s)
            float v4sf_vec3[4];
            //PIPS:SAC generated int vector(s)
            int v4si_vec4[4];
            //PIPS:SAC generated float vector(s)
            float v4sf_vec5[4];
