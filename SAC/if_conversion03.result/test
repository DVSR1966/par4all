void foo()
{
   int i, tab[4][4];
   for(i = 0; i <= 0; i += 1)
      if (tab[i][i]>i) {
         int j = i*2;
         tab[i] = i;
         tab[i] -= j;
      }
}
void foo()
{
   int i, tab[4][4];
   //PIPS generated variable
   int I_0;
   for(i = 0; i <= 0; i += 1) {
      I_0 = tab[i][i]>i;
#pragma PIPS IF_TO_CONVERT
      if (I_0) {
         int j = i*2;
         tab[i] = i;
         tab[i] -= j;
      }
      ;
   }
}
void foo()
{
   int i, tab[4][4];
   //PIPS generated variable
   int I_0;
   //PIPS generated variable
   int j0;
   //PIPS generated variable
   int *tab0;
   for(i = 0; i <= 0; i += 1) {
      I_0 = tab[i][i]>i;
      int j;
      j0 = i*2;
      j = PHI(I_0, j0, j);
      tab[i] = PHI(I_0, i, tab[i]);
      tab0 = tab[i]-j;
      tab[i] = PHI(I_0, tab0, tab[i]);
      ;
   }
}

