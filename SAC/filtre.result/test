void filtre(float x[100], float y[100], float h[4])
{
   int i, k;
   //PIPS generated variable
   float F_0;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0;
   //PIPS generated variable
   float RED0[1], RED1[4], F_00, F_01, F_02, F_03;
   //PIPS generated variable
   int LU_IB00, LU_NUB00;
   for(i = 3; i <= 99; i += 1) {
      LU_NUB00 = 4;
      LU_IB00 = 0;
      RED0[0] = 0.000000;
      LU_IND0 = 0;
      y[i] = RED0[0]+y[i];
      RED1[0] = 0.000000;
      RED1[1] = 0.000000;
      RED1[2] = 0.000000;
      RED1[3] = 0.000000;
      LU_IND0 = 0;
      //c SIMD_COMMENT_0
      F_03 = x[i-LU_IND0]*h[LU_IND0];
      //c SIMD_COMMENT_1
      RED1[0] = RED1[0]+F_03;
      //c SIMD_COMMENT_0
      F_02 = x[i-LU_IND0-1]*h[LU_IND0+1];
      //c SIMD_COMMENT_1
      RED1[1] = RED1[1]+F_02;
      //c SIMD_COMMENT_0
      F_01 = x[i-LU_IND0-2]*h[LU_IND0+2];
      //c SIMD_COMMENT_1
      RED1[2] = RED1[2]+F_01;
      //c SIMD_COMMENT_0
      F_00 = x[i-LU_IND0-3]*h[LU_IND0+3];
      //c SIMD_COMMENT_1
      RED1[3] = RED1[3]+F_00;
      y[i] = RED1[3]+RED1[2]+RED1[1]+RED1[0]+y[i];
      k = 4;
   }
   ;
}
