void filtre(float x[100], float y[100], float h[4])
{
   int i, k;
   for(i = 4-1; i <= 99; i += 1)
loop:
      for(k = 0; k <= 3; k += 1)
         y[i] = y[i]+x[i-k]*h[k];
}
void filtre(float x[100], float y[100], float h[4])
{
   int i, k;
   //PIPS generated variable
   float F_0;
   for(i = 4-1; i <= 99; i += 1)
loop:
      for(k = 0; k <= 3; k += 1) {
         F_0 = x[i-k]*h[k];
         y[i] = y[i]+F_0;
      }
}
