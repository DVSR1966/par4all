
Initial code

void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   for(i = 0; i <= n-1; i += 1)
      result[i] = alpha*src1[i]+(1-alpha)*src2[i];
}
void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   for(i = 0; i <= n-1; i += 1)
      result[i] = alpha*src1[i]+(1-alpha)*src2[i];
}
