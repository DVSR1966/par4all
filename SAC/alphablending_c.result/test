{
   size_t i;
   for(i = 0; i <= n-1; i += 1)
      result[i] = alpha*src1[i]+(1-alpha)*src2[i];
}
void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   for(i = 0; i <= n-1; i += 1)
      result[i] = alpha*src1[i]+(1-alpha)*src2[i];
}
void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   //PIPS generated variable
   float F_0;
   //PIPS generated variable
   float F_1;
   //PIPS generated variable
   float F_2;
   for(i = 0; i <= n-1; i += 1) {
      F_0 = alpha*src1[i];
      F_1 = 1-alpha;
      F_2 = F_1*src2[i];
      result[i] = F_0+F_2;
   }
}
void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   //PIPS generated variable
   float F_0;
   //PIPS generated variable
   float F_1;
   //PIPS generated variable
   float F_2;
   //PIPS generated variable
   size_t LU_NUB0;
   //PIPS generated variable
   size_t LU_IB0;
   //PIPS generated variable
   size_t LU_IND0;
   LU_NUB0 = n;
   LU_IB0 = MOD(LU_NUB0, 4);
   for(LU_IND0 = 0; LU_IND0 <= LU_IB0-1; LU_IND0 += 1) {
      F_0 = alpha*src1[LU_IND0];
      F_1 = 1-alpha;
      F_2 = F_1*src2[LU_IND0];
      result[LU_IND0] = F_0+F_2;
   }
   for(LU_IND0 = LU_IB0; LU_IND0 <= LU_NUB0-1; LU_IND0 += 4) {
      //c SIMD_COMMENT_0
      F_0 = alpha*src1[LU_IND0];
      //c SIMD_COMMENT_1
      F_1 = 1-alpha;
      //c SIMD_COMMENT_2
      F_2 = F_1*src2[LU_IND0];
      //c SIMD_COMMENT_3
      result[LU_IND0] = F_0+F_2;
      //c SIMD_COMMENT_0
      F_0 = alpha*src1[LU_IND0+1];
      //c SIMD_COMMENT_1
      F_1 = 1-alpha;
      //c SIMD_COMMENT_2
      F_2 = F_1*src2[LU_IND0+1];
      //c SIMD_COMMENT_3
      result[LU_IND0+1] = F_0+F_2;
      //c SIMD_COMMENT_0
      F_0 = alpha*src1[LU_IND0+2];
      //c SIMD_COMMENT_1
      F_1 = 1-alpha;
      //c SIMD_COMMENT_2
      F_2 = F_1*src2[LU_IND0+2];
      //c SIMD_COMMENT_3
      result[LU_IND0+2] = F_0+F_2;
      //c SIMD_COMMENT_0
      F_0 = alpha*src1[LU_IND0+3];
      //c SIMD_COMMENT_1
      F_1 = 1-alpha;
      //c SIMD_COMMENT_2
      F_2 = F_1*src2[LU_IND0+3];
      //c SIMD_COMMENT_3
      result[LU_IND0+3] = F_0+F_2;
   }
   i = MAX0(LU_NUB0, 0);
   ;
}
void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   //PIPS generated variable
   float F_0;
   //PIPS generated variable
   float F_1;
   //PIPS generated variable
   float F_2;
   //PIPS generated variable
   size_t LU_NUB0;
   //PIPS generated variable
   size_t LU_IB0;
   //PIPS generated variable
   size_t LU_IND0;
   LU_NUB0 = n;
   LU_IB0 = MOD(LU_NUB0, 4);
   for(LU_IND0 = 0; LU_IND0 <= LU_IB0-1; LU_IND0 += 1) {
      F_0 = alpha*src1[LU_IND0];
      F_1 = 1-alpha;
      F_2 = F_1*src2[LU_IND0];
      result[LU_IND0] = F_0+F_2;
   }
   for(LU_IND0 = LU_IB0; LU_IND0 <= LU_NUB0-1; LU_IND0 += 4) {
      //c SIMD_COMMENT_0
      F_0 = alpha*src1[LU_IND0];
      //c SIMD_COMMENT_1
      F_1 = 1-alpha;
      //c SIMD_COMMENT_2
      F_2 = F_1*src2[LU_IND0];
      //c SIMD_COMMENT_3
      result[LU_IND0] = F_0+F_2;
      //c SIMD_COMMENT_0
      F_0 = alpha*src1[LU_IND0+1];
      //c SIMD_COMMENT_1
      F_1 = 1-alpha;
      //c SIMD_COMMENT_2
      F_2 = F_1*src2[LU_IND0+1];
      //c SIMD_COMMENT_3
      result[LU_IND0+1] = F_0+F_2;
      //c SIMD_COMMENT_0
      F_0 = alpha*src1[LU_IND0+2];
      //c SIMD_COMMENT_1
      F_1 = 1-alpha;
      //c SIMD_COMMENT_2
      F_2 = F_1*src2[LU_IND0+2];
      //c SIMD_COMMENT_3
      result[LU_IND0+2] = F_0+F_2;
      //c SIMD_COMMENT_0
      F_0 = alpha*src1[LU_IND0+3];
      //c SIMD_COMMENT_1
      F_1 = 1-alpha;
      //c SIMD_COMMENT_2
      F_2 = F_1*src2[LU_IND0+3];
      //c SIMD_COMMENT_3
      result[LU_IND0+3] = F_0+F_2;
   }
   i = MAX0(LU_NUB0, 0);
   ;
}
void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   //PIPS generated variable
   float F_0;
   //PIPS generated variable
   float F_1;
   //PIPS generated variable
   float F_2;
   //PIPS generated variable
   size_t LU_NUB0;
   //PIPS generated variable
   size_t LU_IB0;
   //PIPS generated variable
   size_t LU_IND0;
   //PIPS generated variable
   float F_20;
   //PIPS generated variable
   float F_10;
   //PIPS generated variable
   float F_00;
   //PIPS generated variable
   float F_21;
   //PIPS generated variable
   float F_11;
   //PIPS generated variable
   float F_01;
   //PIPS generated variable
   float F_22;
   //PIPS generated variable
   float F_12;
   //PIPS generated variable
   float F_02;
   //PIPS generated variable
   float F_23;
   //PIPS generated variable
   float F_13;
   //PIPS generated variable
   float F_03;
   //PIPS generated variable
   float F_24;
   //PIPS generated variable
   float F_14;
   //PIPS generated variable
   float F_04;
   LU_NUB0 = n;
   LU_IB0 = MOD(LU_NUB0, 4);
   for(LU_IND0 = 0; LU_IND0 <= LU_IB0-1; LU_IND0 += 1) {
      F_04 = alpha*src1[LU_IND0];
      F_14 = 1-alpha;
      F_24 = F_14*src2[LU_IND0];
      result[LU_IND0] = F_04+F_24;
   }
   for(LU_IND0 = LU_IB0; LU_IND0 <= LU_NUB0-1; LU_IND0 += 4) {
      //c SIMD_COMMENT_0
      F_03 = alpha*src1[LU_IND0];
      //c SIMD_COMMENT_1
      F_13 = 1-alpha;
      //c SIMD_COMMENT_2
      F_23 = F_13*src2[LU_IND0];
      //c SIMD_COMMENT_3
      result[LU_IND0] = F_03+F_23;
      //c SIMD_COMMENT_0
      F_02 = alpha*src1[LU_IND0+1];
      //c SIMD_COMMENT_1
      F_12 = 1-alpha;
      //c SIMD_COMMENT_2
      F_22 = F_12*src2[LU_IND0+1];
      //c SIMD_COMMENT_3
      result[LU_IND0+1] = F_02+F_22;
      //c SIMD_COMMENT_0
      F_01 = alpha*src1[LU_IND0+2];
      //c SIMD_COMMENT_1
      F_11 = 1-alpha;
      //c SIMD_COMMENT_2
      F_21 = F_11*src2[LU_IND0+2];
      //c SIMD_COMMENT_3
      result[LU_IND0+2] = F_01+F_21;
      //c SIMD_COMMENT_0
      F_00 = alpha*src1[LU_IND0+3];
      //c SIMD_COMMENT_1
      F_10 = 1-alpha;
      //c SIMD_COMMENT_2
      F_20 = F_10*src2[LU_IND0+3];
      //c SIMD_COMMENT_3
      result[LU_IND0+3] = F_00+F_20;
   }
   i = MAX0(LU_NUB0, 0);
   ;
}
void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   //PIPS generated variable
   float F_0;
   //PIPS generated variable
   float F_1;
   //PIPS generated variable
   float F_2;
   //PIPS generated variable
   size_t LU_NUB0;
   //PIPS generated variable
   size_t LU_IB0;
   //PIPS generated variable
   size_t LU_IND0;
   //PIPS generated variable
   float F_20;
   //PIPS generated variable
   float F_10;
   //PIPS generated variable
   float F_00;
   //PIPS generated variable
   float F_21;
   //PIPS generated variable
   float F_11;
   //PIPS generated variable
   float F_01;
   //PIPS generated variable
   float F_22;
   //PIPS generated variable
   float F_12;
   //PIPS generated variable
   float F_02;
   //PIPS generated variable
   float F_23;
   //PIPS generated variable
   float F_13;
   //PIPS generated variable
   float F_03;
   //PIPS generated variable
   float F_24;
   //PIPS generated variable
   float F_14;
   //PIPS generated variable
   float F_04;
   //PIPS generated variable
   float v4sf_vec0[3+1];
   //PIPS generated variable
   float v4sf_vec1[3+1];
   //PIPS generated variable
   float v4sf_vec2[3+1];
   //PIPS generated variable
   float v4sf_vec3[3+1];
   //PIPS generated variable
   float v4sf_vec4[3+1];
   //PIPS generated variable
   float v4sf_vec5[3+1];
   //PIPS generated variable
   float v4sf_vec6[3+1];
   //PIPS generated variable
   float v4sf_vec7[3+1];
   //PIPS generated variable
   float v4sf_vec8[3+1];
   //PIPS generated variable
   float v4sf_vec9[3+1];
   //PIPS generated variable
   float v4sf_vec10[3+1];
   //PIPS generated variable
   float v4sf_vec11[3+1];
   //PIPS generated variable
   float v4sf_vec12[3+1];
   //PIPS generated variable
   float v4sf_vec13[3+1];
   //PIPS generated variable
   float v4sf_vec14[3+1];
   //PIPS generated variable
   float v4sf_vec15[3+1];
   //PIPS generated variable
   float v4sf_vec16[3+1];
   //PIPS generated variable
   float v4sf_vec17[3+1];
   //PIPS generated variable
   float v4sf_vec18[3+1];
   //PIPS generated variable
   float v4sf_vec19[3+1];
   //PIPS generated variable
   float v4sf_vec20[3+1];
   LU_NUB0 = n;
   LU_IB0 = MOD(LU_NUB0, 4);
   for(LU_IND0 = 0; LU_IND0 <= LU_IB0-1; LU_IND0 += 1) {
      F_04 = alpha*src1[LU_IND0];
      F_14 = 1-alpha;
      F_24 = F_14*src2[LU_IND0];
      result[LU_IND0] = F_04+F_24;
   }
   for(LU_IND0 = LU_IB0; LU_IND0 <= LU_NUB0-1; LU_IND0 += 4) {
      SIMD_LOAD_V4SF(v4sf_vec2, &src1[LU_IND0]);
      SIMD_LOAD_GENERIC_V4SF(v4sf_vec1, alpha, alpha, alpha, alpha);
      SIMD_MULPS(v4sf_vec0, v4sf_vec1, v4sf_vec2);
      SIMD_SAVE_GENERIC_V4SF(v4sf_vec0, &F_03, &F_02, &F_01, &F_00);
      SIMD_LOAD_CONSTANT_V4SF(v4sf_vec4, 1, 1, 1, 1);
      SIMD_SUBPS(v4sf_vec3, v4sf_vec4, v4sf_vec1);
      SIMD_SAVE_GENERIC_V4SF(v4sf_vec3, &F_13, &F_12, &F_11, &F_10);
      SIMD_LOAD_V4SF(v4sf_vec8, &src2[LU_IND0]);
      SIMD_MULPS(v4sf_vec6, v4sf_vec3, v4sf_vec8);
      SIMD_SAVE_GENERIC_V4SF(v4sf_vec6, &F_23, &F_22, &F_21, &F_20);
      SIMD_ADDPS(v4sf_vec9, v4sf_vec0, v4sf_vec6);
      SIMD_SAVE_V4SF(v4sf_vec9, &result[LU_IND0]);
   }
   i = MAX0(LU_NUB0, 0);
   ;
}
void alphablending(size_t n, float src1[n], float src2[n], float result[n], float alpha)
{
   size_t i;
   //PIPS generated variable
   float F_0;
   //PIPS generated variable
   float F_1;
   //PIPS generated variable
   float F_2;
   //PIPS generated variable
   size_t LU_NUB0;
   //PIPS generated variable
   size_t LU_IB0;
   //PIPS generated variable
   size_t LU_IND0;
   //PIPS generated variable
   float F_20;
   //PIPS generated variable
   float F_10;
   //PIPS generated variable
   float F_00;
   //PIPS generated variable
   float F_21;
   //PIPS generated variable
   float F_11;
   //PIPS generated variable
   float F_01;
   //PIPS generated variable
   float F_22;
   //PIPS generated variable
   float F_12;
   //PIPS generated variable
   float F_02;
   //PIPS generated variable
   float F_23;
   //PIPS generated variable
   float F_13;
   //PIPS generated variable
   float F_03;
   //PIPS generated variable
   float F_24;
   //PIPS generated variable
   float F_14;
   //PIPS generated variable
   float F_04;
   //PIPS generated variable
   float v4sf_vec0[3+1];
   //PIPS generated variable
   float v4sf_vec1[3+1];
   //PIPS generated variable
   float v4sf_vec2[3+1];
   //PIPS generated variable
   float v4sf_vec3[3+1];
   //PIPS generated variable
   float v4sf_vec4[3+1];
   //PIPS generated variable
   float v4sf_vec5[3+1];
   //PIPS generated variable
   float v4sf_vec6[3+1];
   //PIPS generated variable
   float v4sf_vec7[3+1];
   //PIPS generated variable
   float v4sf_vec8[3+1];
   //PIPS generated variable
   float v4sf_vec9[3+1];
   //PIPS generated variable
   float v4sf_vec10[3+1];
   //PIPS generated variable
   float v4sf_vec11[3+1];
   //PIPS generated variable
   float v4sf_vec12[3+1];
   //PIPS generated variable
   float v4sf_vec13[3+1];
   //PIPS generated variable
   float v4sf_vec14[3+1];
   //PIPS generated variable
   float v4sf_vec15[3+1];
   //PIPS generated variable
   float v4sf_vec16[3+1];
   //PIPS generated variable
   float v4sf_vec17[3+1];
   //PIPS generated variable
   float v4sf_vec18[3+1];
   //PIPS generated variable
   float v4sf_vec19[3+1];
   //PIPS generated variable
   float v4sf_vec20[3+1];
   //PIPS generated variable
   float F_3;
   LU_NUB0 = n;
   LU_IB0 = MOD(LU_NUB0, 4);
   F_3 = 1-alpha;
   LU_IND0 = MAX(INT(LU_IB0), 0);
   F_14 = F_3;
   for(LU_IND0 = 0; LU_IND0 <= LU_IB0-1; LU_IND0 += 1) {
      F_04 = alpha*src1[LU_IND0];
      F_24 = F_14*src2[LU_IND0];
      result[LU_IND0] = F_04+F_24;
   }
   SIMD_LOAD_GENERIC_V4SF(v4sf_vec1, alpha, alpha, alpha, alpha);
   SIMD_LOAD_CONSTANT_V4SF(v4sf_vec4, 1, 1, 1, 1);
   LU_IND0 = LU_IB0+MAX(INT((LU_NUB0-LU_IB0+3)/4), 0)*4;
   SIMD_SUBPS(v4sf_vec3, v4sf_vec4, v4sf_vec1);
   for(LU_IND0 = LU_IB0; LU_IND0 <= LU_NUB0-1; LU_IND0 += 4) {
      SIMD_LOAD_V4SF(v4sf_vec2, &src1[LU_IND0]);
      SIMD_MULPS(v4sf_vec0, v4sf_vec1, v4sf_vec2);
      SIMD_LOAD_V4SF(v4sf_vec8, &src2[LU_IND0]);
      SIMD_MULPS(v4sf_vec6, v4sf_vec3, v4sf_vec8);
      SIMD_ADDPS(v4sf_vec9, v4sf_vec0, v4sf_vec6);
      SIMD_SAVE_V4SF(v4sf_vec9, &result[LU_IND0]);
   }
   SIMD_SAVE_GENERIC_V4SF(v4sf_vec0, &F_03, &F_02, &F_01, &F_00);
   SIMD_SAVE_GENERIC_V4SF(v4sf_vec3, &F_13, &F_12, &F_11, &F_10);
   SIMD_SAVE_GENERIC_V4SF(v4sf_vec6, &F_23, &F_22, &F_21, &F_20);
   i = MAX0(LU_NUB0, 0);
   ;
}
