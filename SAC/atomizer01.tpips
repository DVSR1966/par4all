setenv WS="atomizer01"
setproperty ABORT_ON_USER_ERROR TRUE
delete $WS
create $WS $WS.c

echo original code
display PRINTED_FILE

echo atomized code
apply SIMD_ATOMIZER
display PRINTED_FILE

apply UNSPLIT
close

shell cc $WS.database/Src/$WS.c -o out
shell cc $WS.c -o ref
shell ( test `./out` = `./ref` && echo "ok" ) || echo "ko"
shell rm -f out ref

delete $WS
