void cadd(int dim1, int dim2, Cplfloat a[dim1][dim2], Cplfloat b[dim2][dim1])
{
   int i, j;
   for(i = 0; i <= dim1-1; i += 1)
      for(j = 0; j <= dim2-1; j += 1) {
         b[i][j].re = b[i][j].re+a[i][j].re;
         b[i][j].im = b[i][j].im+a[i][j].im;
      }
}
void cadd(int dim1, int dim2, Cplfloat a[dim1][dim2], Cplfloat b[dim2][dim1])
{
   int i, j;
   for(i = 0; i <= dim1-1; i += 1)
      for(j = 0; j <= dim2-1; j += 1) {
         b[i][j].re = b[i][j].re+a[i][j].re;
         b[i][j].im = b[i][j].im+a[i][j].im;
      }
}
