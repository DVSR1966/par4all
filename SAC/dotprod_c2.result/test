float dotprod(int n, float b[n], float c[n])
{
   float a;
   //PIPS generated variable
   float _return0;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0;
   LU_NUB0 = n;
   LU_IB0 = MOD(LU_NUB0, 4);
   for(LU_IND0 = 0; LU_IND0 <= LU_IB0-1; LU_IND0 += 1) {
      _return0 = b[LU_IND0]+c[LU_IND0];
      a = a+_return0;
   }
   for(LU_IND0 = LU_IB0; LU_IND0 <= LU_NUB0-1; LU_IND0 += 4) {
      //c SIMD_COMMENT_0
      _return0 = b[LU_IND0]+c[LU_IND0];
      //c SIMD_COMMENT_1
      a = a+_return0;
      //c SIMD_COMMENT_0
      _return0 = b[LU_IND0+1]+c[LU_IND0+1];
      //c SIMD_COMMENT_1
      a = a+_return0;
      //c SIMD_COMMENT_0
      _return0 = b[LU_IND0+2]+c[LU_IND0+2];
      //c SIMD_COMMENT_1
      a = a+_return0;
      //c SIMD_COMMENT_0
      _return0 = b[LU_IND0+3]+c[LU_IND0+3];
      //c SIMD_COMMENT_1
      a = a+_return0;
   }
   
   
   return a;
}
float dotprod(int n, float b[n], float c[n])
{
   float a;
   //PIPS generated variable
   float _return0;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0;
   //PIPS generated variable
   float RED0[1], RED1[4];
   LU_NUB0 = n;
   LU_IB0 = MOD(LU_NUB0, 4);
   RED0[0] = 0.000000;
   for(LU_IND0 = 0; LU_IND0 <= LU_IB0-1; LU_IND0 += 1) {
      _return0 = b[LU_IND0]+c[LU_IND0];
      RED0[0] = RED0[0]+_return0;
   }
   a = RED0[0]+a;
   RED1[0] = 0.000000;
   RED1[1] = 0.000000;
   RED1[2] = 0.000000;
   RED1[3] = 0.000000;
   for(LU_IND0 = LU_IB0; LU_IND0 <= LU_NUB0-1; LU_IND0 += 4) {
      //c SIMD_COMMENT_0
      _return0 = b[LU_IND0]+c[LU_IND0];
      //c SIMD_COMMENT_1
      RED1[0] = RED1[0]+_return0;
      //c SIMD_COMMENT_0
      _return0 = b[LU_IND0+1]+c[LU_IND0+1];
      //c SIMD_COMMENT_1
      RED1[1] = RED1[1]+_return0;
      //c SIMD_COMMENT_0
      _return0 = b[LU_IND0+2]+c[LU_IND0+2];
      //c SIMD_COMMENT_1
      RED1[2] = RED1[2]+_return0;
      //c SIMD_COMMENT_0
      _return0 = b[LU_IND0+3]+c[LU_IND0+3];
      //c SIMD_COMMENT_1
      RED1[3] = RED1[3]+_return0;
   }
   a = RED1[3]+RED1[2]+RED1[1]+RED1[0]+a;
   
   
   return a;
}
float dotprod(int n, float b[n], float c[n])
{
   float a;
   //PIPS generated variable
   float _return0;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0;
   //PIPS generated variable
   float RED0[1], RED1[4], a0, _return00, _return01, _return02, _return03, a1, _return04;
   //PIPS generated variable
   int LU_IB00, LU_NUB00;
   LU_NUB00 = n;
   LU_IB00 = MOD(LU_NUB00, 4);
   RED0[0] = 0.000000;
   for(LU_IND0 = 0; LU_IND0 <= LU_IB00-1; LU_IND0 += 1) {
      _return04 = b[LU_IND0]+c[LU_IND0];
      RED0[0] = RED0[0]+_return04;
   }
   a1 = RED0[0]+a;
   RED1[0] = 0.000000;
   RED1[1] = 0.000000;
   RED1[2] = 0.000000;
   RED1[3] = 0.000000;
   for(LU_IND0 = LU_IB00; LU_IND0 <= LU_NUB00-1; LU_IND0 += 4) {
      //c SIMD_COMMENT_0
      _return03 = b[LU_IND0]+c[LU_IND0];
      //c SIMD_COMMENT_1
      RED1[0] = RED1[0]+_return03;
      //c SIMD_COMMENT_0
      _return02 = b[LU_IND0+1]+c[LU_IND0+1];
      //c SIMD_COMMENT_1
      RED1[1] = RED1[1]+_return02;
      //c SIMD_COMMENT_0
      _return01 = b[LU_IND0+2]+c[LU_IND0+2];
      //c SIMD_COMMENT_1
      RED1[2] = RED1[2]+_return01;
      //c SIMD_COMMENT_0
      _return00 = b[LU_IND0+3]+c[LU_IND0+3];
      //c SIMD_COMMENT_1
      RED1[3] = RED1[3]+_return00;
   }
   a0 = RED1[3]+RED1[2]+RED1[1]+RED1[0]+a1;
   
   
   return a0;
}
float dotprod(int n, float b[n], float c[n])
{
   float a;
   //PIPS generated variable
   float _return0;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0;
   //PIPS generated variable
   float RED0[1], RED1[4], a0, _return00, _return01, _return02, _return03, a1, _return04;
   //PIPS generated variable
   int LU_IB00, LU_NUB00;
   //SAC generated temporary array
   float aligned[4] = {0, 0, 0, 0};
   LU_NUB00 = n;
   LU_IB00 = MOD(LU_NUB00, 4);
   RED0[0] = 0.000000;
   for(LU_IND0 = 0; LU_IND0 <= LU_IB00-1; LU_IND0 += 1) {
      _return04 = b[LU_IND0]+c[LU_IND0];
      RED0[0] = RED0[0]+_return04;
   }
   a1 = RED0[0]+a;
   RED1[0] = 0.000000;
   RED1[1] = 0.000000;
   RED1[2] = 0.000000;
   RED1[3] = 0.000000;
   for(LU_IND0 = LU_IB00; LU_IND0 <= LU_NUB00-1; LU_IND0 += 4) {
      //PIPS:SAC generated float vector(s)
      float v4sf_vec0[4], v4sf_vec1[4], v4sf_vec2[4], v4sf_vec3[4], v4sf_vec4[4];
      SIMD_LOAD_V4SF(v4sf_vec2, &c[LU_IND0]);
      SIMD_LOAD_V4SF(v4sf_vec1, &b[LU_IND0]);
      SIMD_ADDPS(v4sf_vec0, v4sf_vec1, v4sf_vec2);
      SIMD_STORE_V4SF(v4sf_vec0, &aligned[0]);
      SIMD_LOAD_V4SF(v4sf_vec3, &RED1[0]);
      SIMD_ADDPS(v4sf_vec3, v4sf_vec3, v4sf_vec0);
      SIMD_STORE_V4SF(v4sf_vec3, &RED1[0]);
   }
   a0 = RED1[3]+RED1[2]+RED1[1]+RED1[0]+a1;
   
   
   return a0;
}
float dotprod(int n, float b[n], float c[n])
{
   float a;
   //PIPS generated variable
   float _return0;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0;
   //PIPS generated variable
   float RED0[1], RED1[4], a0, _return00, _return01, _return02, _return03, a1, _return04;
   //PIPS generated variable
   int LU_IB00, LU_NUB00;
   //SAC generated temporary array
   float aligned[4] = {0, 0, 0, 0};
   LU_NUB00 = n;
   LU_IB00 = MOD(LU_NUB00, 4);
   RED0[0] = 0.000000;
   for(LU_IND0 = 0; LU_IND0 <= LU_IB00-1; LU_IND0 += 1) {
      _return04 = b[LU_IND0]+c[LU_IND0];
      RED0[0] = RED0[0]+_return04;
   }
   a1 = RED0[0]+a;
   RED1[0] = 0.000000;
   RED1[1] = 0.000000;
   RED1[2] = 0.000000;
   RED1[3] = 0.000000;
   {
      //PIPS:SAC generated float vector(s)
      float v4sf_vec0[4], v4sf_vec3[4];
      SIMD_LOAD_V4SF(v4sf_vec3, &RED1[0]);
      for(LU_IND0 = LU_IB00; LU_IND0 <= LU_NUB00-1; LU_IND0 += 4) {
         //PIPS:SAC generated float vector(s)
         float v4sf_vec1[4], v4sf_vec2[4], v4sf_vec4[4];
         SIMD_LOAD_V4SF(v4sf_vec2, &c[LU_IND0]);
         SIMD_LOAD_V4SF(v4sf_vec1, &b[LU_IND0]);
         SIMD_ADDPS(v4sf_vec0, v4sf_vec1, v4sf_vec2);
         SIMD_ADDPS(v4sf_vec3, v4sf_vec3, v4sf_vec0);
      }
      SIMD_STORE_V4SF(v4sf_vec0, &aligned[0]);
      SIMD_STORE_V4SF(v4sf_vec3, &RED1[0]);
   }
   a0 = RED1[3]+RED1[2]+RED1[1]+RED1[0]+a1;
   
   
   return a0;
}
