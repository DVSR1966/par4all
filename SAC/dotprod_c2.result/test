simdized code

float dotprod(int n, float b[n], float c[n])
{
   float a;
   //PIPS generated variable
   int LU_IND0;
   //PIPS generated variable
   float RED0[1], RED1[4], a0, a1, _return04;
   //PIPS generated variable
   int LU_IB00, LU_NUB00;
   //SAC generated temporary array
   float aligned[4] = {0, 0, 0, 0};
   LU_NUB00 = n;
   LU_IB00 = MOD(LU_NUB00, 4);
   RED0[0] = 0.000000;
   for(LU_IND0 = 0; LU_IND0 <= LU_IB00-1; LU_IND0 += 1) {
      _return04 = b[LU_IND0]+c[LU_IND0];
      RED0[0] = RED0[0]+_return04;
   }
   a1 = RED0[0]+a;
   RED1[0] = 0.000000;
   RED1[1] = 0.000000;
   RED1[2] = 0.000000;
   RED1[3] = 0.000000;
   {
      //PIPS:SAC generated float vector(s)
      float v4sf_vec0[4], v4sf_vec3[4];
      SIMD_LOAD_V4SF(v4sf_vec3, &RED1[0]);
      for(LU_IND0 = LU_IB00; LU_IND0 <= LU_NUB00-1; LU_IND0 += 4) {
         //PIPS:SAC generated float vector(s)
         float v4sf_vec1[4], v4sf_vec2[4], v4sf_vec4[4];
         SIMD_LOAD_V4SF(v4sf_vec2, &c[LU_IND0]);
         SIMD_LOAD_V4SF(v4sf_vec1, &b[LU_IND0]);
         SIMD_ADDPS(v4sf_vec0, v4sf_vec1, v4sf_vec2);
         SIMD_ADDPS(v4sf_vec3, v4sf_vec3, v4sf_vec0);
      }
      SIMD_STORE_V4SF(v4sf_vec0, &aligned[0]);
      SIMD_STORE_V4SF(v4sf_vec3, &RED1[0]);
   }
   a0 = RED1[3]+RED1[2]+RED1[1]+RED1[0]+a1;
   
   
   return a0;
}
