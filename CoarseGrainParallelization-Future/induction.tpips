delete induction

setproperty ABORT_ON_USER_ERROR TRUE

setproperty PRETTYPRINT_SEQUENTIAL_STYLE    "do"

create induction induction.c

apply PRIVATIZE_MODULE(%ALL)

apply COARSE_GRAIN_PARALLELIZATION
apply OMPIFY_CODE
display PRINTED_FILE [main]

apply UNSPLIT
shell gcc -fopenmp -c induction.database/Src/induction.c -o induction.database/Src/induction.o

close
delete induction
quit
