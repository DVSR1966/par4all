delete hello_world

setproperty ABORT_ON_USER_ERROR TRUE

create hello_world hello_world.f90

display PRINTED_FILE[%ALL]

apply UNSPLIT[%ALL]

echo
echo "compile sources produced by pips"
echo
shell gcc -c -o hello_world.database/Src/hello_world.o hello_world.database/Src/hello_world.f90

close
quit
