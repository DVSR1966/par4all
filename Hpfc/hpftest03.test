PIPS="${pips_version:-pips}"

echo "pips is ${PIPS}" >&2
echo "--- Hpfc Validation for hpftest03"
echo

file=${VDIR:-.}/hpftest03.f
stub=${VDIR:-.}/include/stub.f

#
# uses the validate command of the hpfc driver.

hpfc validate -p ${PIPS} ${file} ${stub}

# that is all
