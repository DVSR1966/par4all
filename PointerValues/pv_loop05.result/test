int main()
{

// Pointer values: none

   int i;

// Pointer values: none

   int *p, *q;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);
// q == *ANY_MODULE*:*UNDEFINED* (exact);


   p = (int *) malloc(10*sizeof(int));

// Pointer values:
// p == &*HEAP*_l_8[0] (exact);
// q == *ANY_MODULE*:*UNDEFINED* (exact);

   q = p;

// Pointer values:
// p == &*HEAP*_l_8[0] (exact);
// q == p (exact);


   for(i = 0; i <= 4; i += 1) {

// Pointer values:
// p == &*ANY_MODULE*:*ANYWHERE* (may);
// q == p (may);
// q == &*ANY_MODULE*:*ANYWHERE* (may);

      *q = i;

// Pointer values:
// p == &*ANY_MODULE*:*ANYWHERE* (may);
// q == p (may);
// q == &*ANY_MODULE*:*ANYWHERE* (may);

      q++;
   }

// Pointer values:
// p == &*ANY_MODULE*:*ANYWHERE* (may);
// q == p (may);
// q == &*ANY_MODULE*:*ANYWHERE* (may);


   free(p);

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);
// q == &*ANY_MODULE*:*ANYWHERE* (may);

   return 0;
}
