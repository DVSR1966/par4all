
Initial code for "suppress_dead_code05"

void suppress_dead_code05(int *x)
{
   int i = 1, j, k;

   if (i>0&&j>0)
      k = 2;
   else
      k = 3;

   if (j>0&&i>0)
      k = 2;
   else
      k = 3;

   if (i<0||j>0)
      k = 2;
   else
      k = 3;

   if (j>0||i<0)
      k = 2;
   else
      k = 3;

   return;
}

Initial code for "suppress_dead_code05" with proper effects

void suppress_dead_code05(int *x)
{
//               <must be written>: i
   int i = 1, j, k;
//               <must be read   >: i j

   if (i>0&&j>0)
//               <must be written>: k
      k = 2;
   else
//               <must be written>: k
      k = 3;
//               <must be read   >: i j

   if (j>0&&i>0)
//               <must be written>: k
      k = 2;
   else
//               <must be written>: k
      k = 3;
//               <must be read   >: i j

   if (i<0||j>0)
//               <must be written>: k
      k = 2;
   else
//               <must be written>: k
      k = 3;
//               <must be read   >: i j

   if (j>0||i<0)
//               <must be written>: k
      k = 2;
   else
//               <must be written>: k
      k = 3;

   return;
}

Initial code for "suppress_dead_code05" with cumulated effects

void suppress_dead_code05(int *x)
{
//               <must be written>: i
   int i = 1, j, k;
//               <must be read   >: i j
//               <must be written>: k

   if (i>0&&j>0)
//               <must be written>: k
      k = 2;
   else
//               <must be written>: k
      k = 3;
//               <must be read   >: i j
//               <must be written>: k

   if (j>0&&i>0)
//               <must be written>: k
      k = 2;
   else
//               <must be written>: k
      k = 3;
//               <must be read   >: i j
//               <must be written>: k

   if (i<0||j>0)
//               <must be written>: k
      k = 2;
   else
//               <must be written>: k
      k = 3;
//               <must be read   >: i j
//               <must be written>: k

   if (j>0||i<0)
//               <must be written>: k
      k = 2;
   else
//               <must be written>: k
      k = 3;

   return;
}

Code for "suppress_dead_code05" after dead code elimination

void suppress_dead_code05(int *x)
{
   int i = 1, j, k;

   if (j>0)
      k = 2;
   else
      k = 3;

   if (j>0)
      k = 2;
   else
      k = 3;

   if (j>0)
      k = 2;
   else
      k = 3;

   if (j>0)
      k = 2;
   else
      k = 3;

   return;
}
