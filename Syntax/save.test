#!/bin/sh

# Preconditions appear only if the SAVE declaration is processed

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

# Initialization:
# rm -f save.result/test
# tpips >save.result/test <<EOF

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete save
create save $PIPS_VALIDDIR/Syntax/save.f
activate INTERPROCEDURAL_SUMMARY_PRECONDITION
activate PRECONDITIONS_INTER_FULL 
activate TRANSFORMERS_INTER_FULL 
activate PRINT_CODE_PRECONDITIONS 
display PRINTED_FILE 
close
delete save
quit
EOF

