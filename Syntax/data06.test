#!/bin/sh

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete data06
create data06 ${PIPS_ROOT}/Validation/Syntax/data06.f

setproperty PARSER_DUMP_SYMBOL_TABLE TRUE
setproperty PARSER_ACCEPT_ANSI_EXTENSIONS FALSE

display PRINTED_FILE[DATA06]

close
delete data06
quit
EOF
