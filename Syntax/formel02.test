#!/bin/sh

# Detect functional formal parameters

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete formel02
create formel02 $PIPS_VALIDDIR/Syntax/formel02.f

echo
echo Listing for FOO
echo

display PRINTED_FILE[FOO]

echo
echo Listing for FORMEL02
echo

display PRINTED_FILE[FORMEL02]

echo
echo Listing for Q
echo

display PRINTED_FILE[Q]
close
delete formel02
quit
EOF

