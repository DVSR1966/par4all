      PROGRAM LENDDO

      REAL X(100), Y(100)

      DO I = 1, 100                                                     0001
         IF (Y(J).LT.0.) GOTO 1                                         0002
         X(I) = 0.                                                      0004
1        CONTINUE                                                       0005
      ENDDO

      DO 2 I = 1, 99                                                    0005
         X(I) = 1.                                                      0006
2        CONTINUE                                                       0007

      DO I = 1, 98                                                      0007
         X(I) = 2.                                                      0008
      ENDDO

c      do i = 1, 100
c         x(i) = 3.
c 4    continue

      DO 5 J = 1, 99                                                    0009
         DO 5 I = 1, 99                                                 0010
            X(I) = 1.+REAL(J)                                           0011
5           CONTINUE                                                    0012

      END
