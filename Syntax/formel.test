#!/bin/sh

# Detect functional formal parameters

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete formel
create formel ${PIPS_ROOT}/Validation/Syntax/formel.f

setproperty PARSER_DUMP_SYMBOL_TABLE TRUE
setproperty PARSER_ACCEPT_ANSI_EXTENSIONS FALSE

display PRINTED_FILE[FORMEL]
display PRINTED_FILE[FOO]
display PRINTED_FILE[Q]

close
delete formel
quit
EOF

