#!/bin/sh

# No core dump please when closing the database after a parser error
# Parser error for Hollerith constants

tpips=${tpips_version:-tpips}
export PIPS_SRCPATH=$(dirname $0)

# Initialization:
# rm -f alias.result/test
# tpips >alias.result/test <<EOF

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete alias
create alias alias.f
display PRINTED_FILE 
close
delete alias
quit
EOF
