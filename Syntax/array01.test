#!/bin/sh

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete array01
create array01 ${PIPS_ROOT}/Validation/Syntax/array01.f

# Make sure the array section is not parsed

module ARRAY01
display PRINTED_FILE[ARRAY01]
close
delete array01
quit
EOF
