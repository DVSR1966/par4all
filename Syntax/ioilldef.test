#!/bin/sh

# No core dump please when closing the database after a parser error

# Parser error for IO illegal default parameter

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

# Initialization:
# rm -f ioilldef.result/test
# tpips >ioilldef.result/test <<EOF

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete ioilldef
create ioilldef $PIPS_VALIDDIR/Syntax/ioilldef.f
display PRINTED_FILE 
close
delete ioilldef
quit
EOF

