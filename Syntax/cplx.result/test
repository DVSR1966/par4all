      PROGRAM CPLX
      COMPLEX A, B, C
      INTEGER L
      L = 10                                                            0001
10    A = (10, 20)                                                      0002
      L = 20                                                            0003
20    B = (20, 10)                                                      0004
30    IF (L.EQ.10) GOTO 10                                              0005
      IF (L.EQ.20) GOTO 20                                              0005
      STOP                                                              0005
      END

