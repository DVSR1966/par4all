Entry INCREMENT

      INTEGER*4 FUNCTION INCREMENT(Y, M)
      INTEGER M, INCREMENT
      CHARACTER Y(1:M)

99998 CONTINUE                                                          0001

      PRINT *, Y(2)                                                     0002

      M = M+1                                                           0003

      INCREMENT = 4                                                     0004

      END
