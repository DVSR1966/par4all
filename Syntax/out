      PROGRAM EDITED04

C     The parser must be able to process the same routine several times
C     after user edition steps

      CALL BAR(Y)                                                       0001

      PRINT *, IBAR(Z)                                                  0002

      END

      SUBROUTINE BAR(Y)

!!      integer x(3)
!%      real x(10)

      PRINT *, X                                                        0001

      END

      FUNCTION IBAR(Y)

!!      integer x(3)
!%      real x(10)

      PRINT *, X                                                        0001

      IBAR = 1                                                          0002

      END

New version of BAR with x as real[10]


      SUBROUTINE BAR(Y)

!!      integer x(3)
      REAL X(10)

      PRINT *, X                                                        0001

      END

New version of BAR with x as integer[3]


      SUBROUTINE BAR(Y)

!!      integer x(3)
!%      real x(10)

      PRINT *, X                                                        0001

      END
