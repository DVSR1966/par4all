#!/bin/sh

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

($tpips 2>&1 | sed '/file *\//d') <<EOF

delete external10
create external10 external10.f

setproperty PARSER_DUMP_SYMBOL_TABLE TRUE
setproperty PARSER_ACCEPT_ANSI_EXTENSIONS FALSE
setproperty PARSER_TYPE_CHECK_CALL_SITES

echo
echo Main parsed last
echo

display PRINTED_FILE[F]
display PRINTED_FILE[EXTERNAL10]

close
delete external10
quit
EOF
