      SUBROUTINE DESUGAR03(X, Y, *, *)

C     Check desugaring of labelled DO loops with a forward reference

      READ *, I                                                         0001

      IF (I.LT.0) THEN                                                  0002
      ENDIF
400   CONTINUE

c     labelled loop
      DO 500 I = 1, N                                                   0004
         PRINT *, I                                                     0005
500      CONTINUE                                                       0006

      I_PIPS_RETURN_CODE_ = 0                                           0007
      END

