c     passage de procedure formelle
C     abort "no source file for x"
      PROGRAM FORMEL
      EXTERNAL Q

C     Forbidden in Fortran 77:
c      p = q

      CALL FOO(Q)                                                       0009
      END
      SUBROUTINE FOO(X)
      REAL Y(100)

      DO 100 J = 1, 100                                                 0004
         Y(I) = X(I)                                                    0005
100      CONTINUE                                                       0006
      END
      FUNCTION Q(I)
      Q = FLOAT(I)                                                      0002
      END
