      IF (INIRR.NE.1) THEN                                              0002
C (unreachable)
C        BEGIN BLOCK
C (unreachable)
C        BEGIN BLOCK
C (unreachable)
1        READ (UNIT=NIRR,FMT=1000,END=2)                                0003
C (unreachable)
         GOTO 99999
         
C (unreachable)
         GOTO 2
99999    CONTINUE
C        END BLOCK
C (unreachable)
         GOTO 1
C        END BLOCK
      ELSE
C (unreachable)
C        BEGIN BLOCK
C        END BLOCK
      ENDIF
C (unreachable)
      INIRR = 0                                                         0004
C (unreachable)
2     CONTINUE                                                          0005
C     END BLOCK
      END
      PROGRAM MINI1
      INTEGER  NIRR
C (0,1)
C     BEGIN BLOCK
C (0,2)
      REWIND (NIRR)                                                     0001
C (0,3)
