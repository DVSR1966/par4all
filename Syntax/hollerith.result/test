      PROGRAM HOLLERITH
     
! this is not an hollerith;-) 
      REAL*8 HOLLERITH

      PRINT *, '"2hab"', '''"', "'2h  '"                                0001

      PRINT *, ' 2 45 78 0'                                             0002
      PRINT *, '1 3456789 '                                             0003
      PRINT *, '12 45678  '                                             0004
      PRINT *, '123 567   '                                             0005
      PRINT *, '1234 6    '                                             0006
      PRINT *, ' 2 45     '                                             0007
      PRINT *, '1  4      '                                             0008
      PRINT *, '  3       '                                             0009
      PRINT *, ' 2        '                                             0010
      PRINT *, '1         '                                             0011
      PRINT *, '          '                                             0012

      PRINT *, 'FAB', 'bien', '  '                                      0013

      PRINT *, '''    abcde'                                            0014

      PRINT *, '''1''2'' ''4''5'                                        0015

      PRINT *, 'foo', 'a', ' ', 'x x'                                   0016

      A2H = B                                                           0017

      PRINT *, 'foo a la ligne ', 'oui'                                 0018


      PRINT *, 'foo fin ', 'oui'                                        0019

! comments
      PRINT *, 'foo fin 2-', 'oui'                                      0020


      PRINT *, 'foo fin 3-', 'oui'                                      0021


      PRINT *, 'foo fin 4-', 'oui'                                      0022

      PRINT *, 'FAB', "-", 'bien'                                       0023

      PRINT *, 'abcdefghij          uvwxyz'                             0024

      PRINT *, "2ha b", '  ', '2ha b', 'a b'                            0025

      PRINT *, '0''''''''5''''''''0''''''''5''''''''0''''''''5''''''''0'
     &'''''''5''''''''0''''''''5''''''''0'''''''                        0026

! worst case dilatation
      PRINT *, '''''''''''''''''''''''''''''''''''''''''''''''''''''''''
     &''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
     &'''''                                                             0027

      PRINT *, "padding", '          ', 'ten blanks!'                   0028

! bang comments...

! mais la oui !
      PRINT *, '! pas un commentaire !'                                 0029

! hop !
      PRINT *, "! et avec ca aussi !"                                   0030

! c est tout !
      PRINT *, '!123456789'                                             0031

      END
