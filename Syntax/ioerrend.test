#!/bin/sh

# No core dump please when closing the database after a parser error

# Parser detection of non-supported ERR and END

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

# Initialization:
# rm -f ioerrend.result/test
# tpips >ioerrend.result/test <<EOF

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete ioerrend
create ioerrend $PIPS_VALIDDIR/Syntax/ioerrend.f
display PRINTED_FILE 
close
delete ioerrend
quit
EOF

