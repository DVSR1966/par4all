#!/bin/sh

# non-standard characters should ne nicely printed out

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete bughexa
create bughexa $PIPS_VALIDDIR/Syntax/bughexa.f
display PRINTED_FILE 
close
delete bughexa
quit
EOF

