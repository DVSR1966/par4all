#!/bin/sh

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete intrinsic01
create intrinsic01 ${PIPS_ROOT}/Validation/Syntax/intrinsic01.f 

setproperty PARSER_DUMP_SYMBOL_TABLE TRUE

display PRINTED_FILE[INTRINSIC01]

close
delete intrinsic01
EOF
