#!/bin/sh

if Pips -f lcart2.f -w lcart2 -m lcart2 -b CODE >&2
then
	echo Parser failed to detect unsupported substring in lcart2.f
	Delete lcart2 2>/dev/null
	exit 1
else
	echo Parser failed because of unsupported substring in lcart2.f
	Display -v -m lcart2 -w lcart2 code 2>&1 | grep "user error"
	Delete lcart2 2>/dev/null
fi
