      PROGRAM CLOSE56

c     Problems with IO statements

c     This one is not f77 compatible
c     print 56, i
      PRINT *, I                                                        0001

      WRITE (56) I                                                      0002

      CLOSE (56)                                                        0003

c     This one should be ok
c      close((56))

      CLOSE (I*J)                                                       0004

      END
