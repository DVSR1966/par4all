#!/bin/sh

tpips=${tpips_version:-tpips}
PIPS_VALIDDIR=${PIPS_VALIDDIR:-$PIPS_ROOT/Validation}

($tpips 2>&1 | sed '/file *\//d') <<EOF
delete common04
create common04 ${PIPS_ROOT}/Validation/Syntax/common04.f
module COMMON04
setproperty PARSER_DUMP_SYMBOL_TABLE TRUE
module BAR
display PRINTED_FILE[BAR]
module COMMON04
display PRINTED_FILE[COMMON04]
module FOO
display PRINTED_FILE[FOO]
close
delete common04
quit
EOF
