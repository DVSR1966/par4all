
Module ENTRY01

      SUBROUTINE ENTRY08(X, N)
      INTEGER N, L
      REAL*4 W, U
      COMPLEX*8 X(1:N)
      COMMON /ENTRY_ENTRY08/ L

      PRINT *, X(1), W                                                  0001


      END

Entry INCREMENT

      SUBROUTINE INCREMENT(Y, M)
      INTEGER M, L
      REAL*4 W, U
      CHARACTER Y(1:M)
      COMMON /ENTRY_ENTRY08/ L

99999 CONTINUE                                                          0001

      PRINT *, Y(2), U                                                  0002

      M = M+1                                                           0003

      END
