      PROGRAM ENTRY14
      INTEGER ENTRY, SUBROUTINE, PROGRAM, FUNCTION, COMMON
      INTEGER REAL, INTEGER, THEN, ENTRY FOO
      REAL = 6                                                          0001
      INTEGER = 7                                                       0002
      ENTRY = 1                                                         0003
      SUBROUTINE = 2                                                    0004
      PROGRAM = 3                                                       0005
      FUNCTION = 4                                                      0006
      COMMON = 5                                                        0007
      THEN = 8                                                          0008
      ENTRYFOO = 9                                                      0009
      PRINT *, 'entry = ', ENTRY                                        0010
      PRINT *, 'subroutine = ', SUBROUTINE                              0011
      PRINT *, 'program = ', PROGRAM                                    0012
      PRINT *, 'function = ', FUNCTION                                  0013
      PRINT *, 'common = ', COMMON                                      0014
      PRINT *, 'real = ', REAL                                          0015
      PRINT *, 'integer = ', INTEGER                                    0016
      PRINT *, 'then = ', THEN                                          0017
      PRINT *, 'entry foo = ', ENTRYFOO                                 0018
      END

