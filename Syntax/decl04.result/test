      PROGRAM P
      INTEGER I
      COMPLEX*16 DC1, DC2(1:3), DC3
      DC1 = (1.0D0, 1.0D0)                                              0001
      DC3 = 0                                                           0002
      DO 10 I = 1, 3                                                    0003
         DC2(I) = DC1+I                                                 0004
         DC3 = DC3+DC2(I)                                               0005
10       CONTINUE                                                       0006
      PRINT *, DC3                                                      0007
      END
