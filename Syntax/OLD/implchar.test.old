#!/bin/sh

# To display the memory layout, a property should be used
# instead of a debug level.

if [ -f properties.rc ]
then
    echo Incompatible with a local properties.rc file >&2
    exit 2
fi

echo PARSER_DUMP_SYMBOL_TABLE TRUE >properties.rc

if Pips -q -f $PIPS_ROOT/Validation/Syntax/implchar.f -w implchar -m implchar -b PARSED_PRINTED_FILE 2>&1 \
    | sed '/\//d'
then
	exit_code=0
else
	exit_code=1
fi
Delete implchar 2>/dev/null
rm properties.rc
exit $exit_code
