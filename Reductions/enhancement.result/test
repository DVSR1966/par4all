extern int foo1(int i, int j);
extern void foo2(int *i, int j);
extern int foo3(int i, int j);
extern int foo4(int i, int j);
extern int nontrivial(void);
extern int main(void);
//  summary sum[i],
int foo1(int i, int j)
{
//  call sum[i],
   return i += j;
}
void foo2(int *i, int j)
{
   *i += j;
}
int foo3(int i, int j)
{
   static int k = 0;
//  call sum[i],
   i += j;
   return k = i;
}
int foo4(int i, int j)
{
   int k = 0;
//  call sum[i],
   i += j;
   return k = i;
}
int main(void)
{
   int k = 0;
   int sum1 = 0;
   int sum2 = 0;
   int sum3 = 0;
   int sum4 = 0;
   int sum5 = 0;

   for(k = 0; k <= 99; k += 1)
      sum1 = foo1(sum1, k);

   for(k = 0; k <= 99; k += 1)
      foo2(&sum2, k);

   for(k = 0; k <= 99; k += 1)
      sum3 = foo3(sum3, k);

   for(k = 0; k <= 99; k += 1)
      sum4 = foo4(sum4, k);
//  loop sum[sum5],

   for(k = 0; k <= 99; k += 1)
//  call sum[sum5],
      sum5 += k;
   

   printf("sum1: %d\nsum2: %d\nsum3: %d\nsum4: %d\nsum5: %d\n", sum1, sum2, sum3, sum4, sum5);

   return 0;
}
int nontrivial(void)
{
   int i = 0;
   int b = 0;
   for(i = 0; i <= 99; i += 1)
      b = b+b+i;
   return b;
}
