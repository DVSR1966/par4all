
Print code with proper reductions

      PROGRAM PROPERS
      INTEGER P,I,J,K,L
      S = 0                                                             0003
      P = 1                                                             0004
      I = 2                                                             0005
      J = 3                                                             0006
      K = 4                                                             0007
      L = 5                                                             0008
!  call prod[P],
!
      P = P*2                                                           0010
!  call prod[P],
      P = 2*P                                                           0011
!  call prod[P],
      P = 2*L*K*P                                                       0012
!  call sum[S],
!
      S = S+1                                                           0014
!  call sum[S],
      S = 1+S                                                           0015
!  call sum[S],
      S = I+S+J                                                         0016
!  call sum[S],
      S = S+I+J                                                         0017
!  call sum[S],
      S = I+J+S                                                         0018
!  call sum[S],
      S = S-I                                                           0019
!  call sum[S],
      S = S+3-J                                                         0020
!  call sum[S],
      S = L+K+S+J                                                       0021
!
      P = 3*I*P/J                                                       0023
      P = 2*I/P                                                         0024
      P = P/L                                                           0025
      S = K-S                                                           0026
      END

Print code with cumulated reductions

      PROGRAM PROPERS
      INTEGER P,I,J,K,L
      S = 0                                                             0003
      P = 1                                                             0004
      I = 2                                                             0005
      J = 3                                                             0006
      K = 4                                                             0007
      L = 5                                                             0008
!  call prod[P],
!
      P = P*2                                                           0010
!  call prod[P],
      P = 2*P                                                           0011
!  call prod[P],
      P = 2*L*K*P                                                       0012
!  call sum[S],
!
      S = S+1                                                           0014
!  call sum[S],
      S = 1+S                                                           0015
!  call sum[S],
      S = I+S+J                                                         0016
!  call sum[S],
      S = S+I+J                                                         0017
!  call sum[S],
      S = I+J+S                                                         0018
!  call sum[S],
      S = S-I                                                           0019
!  call sum[S],
      S = S+3-J                                                         0020
!  call sum[S],
      S = L+K+S+J                                                       0021
!
      P = 3*I*P/J                                                       0023
      P = 2*I/P                                                         0024
      P = P/L                                                           0025
      S = K-S                                                           0026
      END
