
Print code with proper reductions

      PROGRAM REDUCTION
      INTEGER I, N
      PARAMETER (N=10)
      REAL S, A(N), P
      S = 1.0                                                           0005
!  call sum[S],
      S = S-2.0                                                         0006
      S = S+S                                                           0007
!  call sum[S],
      S = 3.0+S                                                         0008
      DO I = 1, N                                                       0009
         A(I) = REAL(I)                                                 0010
      ENDDO

      DO I = 1, N                                                       0013
!  call sum[S],
         S = S+A(I)                                                     0014
!  call sum[S],
         S = S-A(I)                                                     0015
      ENDDO
      PRINT *, 'sum is ', S                                             0017

      P = 1.0                                                           0019
      DO I = 1, N                                                       0020
         IF (MOD(I, 2).EQ.0) THEN                                       0021
!  call prod[P],
            P = A(I)*P                                                  0022
         ELSE
!  call prod[P],
            P = P/A(I)                                                  0024
         ENDIF
      ENDDO
      PRINT *, 'p is ', P                                               0027

      END

Print code with cumulated reductions

      PROGRAM REDUCTION
      INTEGER I, N
      PARAMETER (N=10)
      REAL S, A(N), P
      S = 1.0                                                           0005
!  call sum[S],
      S = S-2.0                                                         0006
      S = S+S                                                           0007
!  call sum[S],
      S = 3.0+S                                                         0008
      DO I = 1, N                                                       0009
         A(I) = REAL(I)                                                 0010
      ENDDO
!  loop sum[S],

      DO I = 1, N                                                       0013
!  call sum[S],
         S = S+A(I)                                                     0014
!  call sum[S],
         S = S-A(I)                                                     0015
      ENDDO
      PRINT *, 'sum is ', S                                             0017

      P = 1.0                                                           0019
!  loop prod[P],
      DO I = 1, N                                                       0020
!  test prod[P],
         IF (MOD(I, 2).EQ.0) THEN                                       0021
!  call prod[P],
            P = A(I)*P                                                  0022
         ELSE
!  call prod[P],
            P = P/A(I)                                                  0024
         ENDIF
      ENDDO
      PRINT *, 'p is ', P                                               0027

      END
