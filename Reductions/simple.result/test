
Print code with proper reductions

      PROGRAM REDUCTION
      INTEGER I, N
      PARAMETER (N=10)
      REAL S, A(N), P
      S = 1.0                                                           0001
!  call sum[S],
      S = S-2.0                                                         0002
      S = S+S                                                           0003
!  call sum[S],
      S = 3.0+S                                                         0004
      DO I = 1, N                                                       0005
         A(I) = REAL(I)                                                 0006
      ENDDO

      DO I = 1, N                                                       0007
!  call sum[S],
         S = S+A(I)                                                     0008
!  call sum[S],
         S = S-A(I)                                                     0009
      ENDDO
      PRINT *, 'sum is ', S                                             0010

      P = 1.0                                                           0011
      DO I = 1, N                                                       0012
         IF (MOD(I, 2).EQ.0) THEN                                       0013
!  call prod[P],
            P = A(I)*P                                                  0014
         ELSE
!  call prod[P],
            P = P/A(I)                                                  0015
         ENDIF
      ENDDO
      PRINT *, 'p is ', P                                               0016

      END

Print code with cumulated reductions

      PROGRAM REDUCTION
      INTEGER I, N
      PARAMETER (N=10)
      REAL S, A(N), P
      S = 1.0                                                           0001
!  call sum[S],
      S = S-2.0                                                         0002
      S = S+S                                                           0003
!  call sum[S],
      S = 3.0+S                                                         0004
      DO I = 1, N                                                       0005
         A(I) = REAL(I)                                                 0006
      ENDDO
!  loop sum[S],

      DO I = 1, N                                                       0007
!  call sum[S],
         S = S+A(I)                                                     0008
!  call sum[S],
         S = S-A(I)                                                     0009
      ENDDO
      PRINT *, 'sum is ', S                                             0010

      P = 1.0                                                           0011
!  loop prod[P],
      DO I = 1, N                                                       0012
!  test prod[P],
         IF (MOD(I, 2).EQ.0) THEN                                       0013
!  call prod[P],
            P = A(I)*P                                                  0014
         ELSE
!  call prod[P],
            P = P/A(I)                                                  0015
         ENDIF
      ENDDO
      PRINT *, 'p is ', P                                               0016

      END
