//  call sum[b],
      b = b+i;
//  loop prod[b],
   
   // b should be reduced here
   for(i = 0; i <= 99; i += 1)
//  call prod[b],
      b = b*i;
   
   // b should not be reduced here
   // because "/" on int is not accepted
   for(i = 0; i <= 99; i += 1)
      b = b/i;
//  loop bitwise_and[b],
   
   // b should be reduced here
   for(i = 0; i <= 99; i += 1)
//  call bitwise_and[b],
      b = b&i;
//  loop bitwise_xor[b],
   
   // b should be reduced here
   for(i = 0; i <= 99; i += 1)
//  call bitwise_xor[b],
      b = b^i;
//  loop bitwise_or[b],
   
   // b should be reduced here
   for(i = 0; i <= 99; i += 1)
//  call bitwise_or[b],
      b = b|i;
//  loop sum[b],
   
   // b should be reduced here
   for(i = 0; i <= 99; i += 1)
//  call sum[b],
      b += i;
//  loop prod[b],
   
   // b should be reduced here
   for(i = 0; i <= 99; i += 1)
//  call prod[b],
      b *= i;
   
   // b should not be reduced here
