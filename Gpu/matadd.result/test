/* C pretty print for module P4A_COPY_FROM_ACCEL. */
int
p4a_copy_from_accel (char host_address[N - 0], char accel_address[N - 0],
		     int n)
{
  int result;
  int i;

  for (i = 1; i <= n; i++)
    {
      host_address[i - 1] = accel_address[i - 1];
    }
  result = host_address;
  return result;
}
/* C pretty print for module P4A_COPY_TO_ACCEL. */
int
p4a_copy_to_accel (char host_address[N - 0], char accel_address[N - 0], int n)
{
  int result;
  int i;

  for (i = 1; i <= n; i++)
    {
      accel_address[i - 1] = host_address[i - 1];
    }
  result = accel_address;
  return result;
}
      PROGRAM MATADD
      INTEGER M
      INTEGER N
      PARAMETER (M = 402, N = 160)
      INTEGER I, J
      REAL A(M, N), B(M, N), C(M, N)


!$OMP PARALLEL DO PRIVATE(J)
      DO I = 0, M-1+1+-1

!$OMP    PARALLEL DO 
         DO J = 0, N-1+1+-1
            B(I+1,J+1) = I+1
            C(I+1,J+1) = J+1
         ENDDO
      ENDDO


!$OMP PARALLEL DO PRIVATE(J)
      DO I = 0, M-1+1+-1

!$OMP    PARALLEL DO 
         DO J = 0, N-1+1+-1
            A(I+1,J+1) = B(I+1,J+1)+C(I+1,J+1)
         ENDDO
      ENDDO

      END
      PROGRAM MATADD
      INTEGER M
      INTEGER N
      PARAMETER (M = 402, N = 160)
      INTEGER I, J
      REAL A(M, N), B(M, N), C(M, N)

      CALL P4A_KERNEL_LAUNCHER_0(M, N, B, C)

      CALL P4A_KERNEL_LAUNCHER_1(M, N, A, B, C)

      END
/* C pretty print for module MATADD. */
int
main (int argc, char *argv[])
{
  static const int m = 402;
  static const int n = 160;
  int i;
  int j;
  float a[N - 0][M - 0];
  float b[N - 0][M - 0];
  float c[N - 0][M - 0];

  p4a_kernel_launcher_0 (m, n, b, c);
  p4a_kernel_launcher_1 (m, n, a, b, c);
  return 0;
}
