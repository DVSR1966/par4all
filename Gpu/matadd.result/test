	CHARACTER ACCEL_ADDRESS(N)
	INTEGER I
	INTEGER P4A_COPY_TO_ACCEL

      DO I = 1, N
         ACCEL_ADDRESS(I) = HOST_ADDRESS(I)
      ENDDO
      P4A_COPY_TO_ACCEL = ACCEL_ADDRESS
      END
      PROGRAM MATADD
      INTEGER M
      INTEGER N
      PARAMETER (M = 402, N = 160)
      INTEGER I, J
      REAL A(M, N), B(M, N), C(M, N)

      DO I = 0, M-1+1+-1
         DO J = 0, N-1+1+-1
            B(I+1,J+1) = I+1
            C(I+1,J+1) = J+1
         ENDDO
      ENDDO

      DO I = 0, M-1+1+-1
         DO J = 0, N-1+1+-1
            A(I+1,J+1) = B(I+1,J+1)+C(I+1,J+1)
         ENDDO
      ENDDO

      END


