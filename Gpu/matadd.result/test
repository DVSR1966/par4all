  return result;
}
/* C pretty print for module P4A_KERNEL_0. */
void
p4a_kernel_0 (int m, int n, float b[N - 0][M - 0], float c[N - 0][M - 0],
	      int i, int j)
{
  ;

  b[(j + 1) - 1][(i + 1) - 1] = (i + 1);
  c[(j + 1) - 1][(i + 1) - 1] = (j + 1);
  return;
}
/* C pretty print for module P4A_KERNEL_1. */
void
p4a_kernel_1 (int m, int n, float a[N - 0][M - 0], float b[N - 0][M - 0],
	      float c[N - 0][M - 0], int i, int j)
{
  ;

  a[(j + 1) - 1][(i + 1) - 1] =
    (b[(j + 1) - 1][(i + 1) - 1] + c[(j + 1) - 1][(i + 1) - 1]);
  return;
}
/* C pretty print for module P4A_KERNEL_LAUNCHER_0. */
void
p4a_kernel_launcher_0 (int m, int n, float b[N - 0][M - 0],
		       float c[N - 0][M - 0])
{
  int j;
  int i;

  for (i = 0; i <= ((m - 1) + 1) + -1; i++)
    {
      for (j = 0; j <= ((n - 1) + 1) + -1; j++)
	{
	  p4a_kernel_wrapper_0 (m, n, b, c, i, j);
	}
    }
  return;
}
/* C pretty print for module P4A_KERNEL_LAUNCHER_1. */
void
p4a_kernel_launcher_1 (int m, int n, float a[N - 0][M - 0],
		       float b[N - 0][M - 0], float c[N - 0][M - 0])
{
  int j;
  int i;

  for (i = 0; i <= ((m - 1) + 1) + -1; i++)
    {
      for (j = 0; j <= ((n - 1) + 1) + -1; j++)
	{
	  p4a_kernel_wrapper_1 (m, n, a, b, c, i, j);
	}
    }
  return;
}
/* C pretty print for module P4A_KERNEL_WRAPPER_0. */
void
p4a_kernel_wrapper_0 (int m, int n, float b[N - 0][M - 0],
		      float c[N - 0][M - 0], int i, int j)
{
  ;

  p4a_kernel_0 (m, n, b, c, i, j);
  return;
}
/* C pretty print for module P4A_KERNEL_WRAPPER_1. */
void
p4a_kernel_wrapper_1 (int m, int n, float a[N - 0][M - 0],
		      float b[N - 0][M - 0], float c[N - 0][M - 0], int i,
		      int j)
{
  ;

  p4a_kernel_1 (m, n, a, b, c, i, j);
  return;
}
