#include <stdlib.h>
enum {N=100};

void give();
//PIPS generated variable
void p4a_kernel_give(int N, double array[N], int k), p4a_wrapper_give(int N, double array[N], int k), p4a_launcher_give(int N, double array[N], int *k);
void p4a_launcher_give(int N, double array[N], int *k)
{

   *k = 0;
   while (*k<=99) {
      // To be assigned to a call to P4A_vp_0: k
      p4a_wrapper_give(N, array, *k);
      *k += 1;
   }
}
void p4a_wrapper_give(int N, double array[N], int k)
{
   // To be assigned to a call to P4A_vp_0: k
   p4a_kernel_give(N, array, k);
}
void p4a_kernel_give(int N, double array[N], int k)
{
   array[k] = 0;
}
void give()
{
   int k;
   double array[N];

   p4a_launcher_give(N, array, &k);

   exit(k);
}
