
//  <save[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <save[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>
//  <space[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <space[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>

void compute(float_t space[501][501], float_t save[501][501])
{
   int t;                                                               /*0102*/

//  <save[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <save[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>
//  <space[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <space[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>

   
   /* Apply the relaxation T times: */
   for(t = 0; t <= 399; t += 1)

//  <save[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <save[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>
//  <space[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <space[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>

      iteration(space, save);                                           /*0106*/
}

//  <*ANY_MODULE*:*ANYWHERE*-R-MAY-{}>
//  <*ANY_MODULE*:*ANYWHERE*-W-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>

void get_data(char filename[])
{
   int i, j, nx, ny;                                                    /*0013*/
   unsigned char c;                                                     /*0014*/
   FILE *fp;                                                            /*0015*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <filename[PHI1]-R-MAY-{}>
//  <fp-W-EXACT-{}>
//  <stderr-R-MAY-{}>
//  <stderr[PHI1]-R-MAY-{PHI1==0}>
//  <stderr[PHI1]-W-MAY-{PHI1==0}>

   if ((fp = fopen(filename, "r"))==(void *) 0) {                       /*0017*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==2}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==2}>
//  <stderr-R-EXACT-{}>
//  <stderr[PHI1]-R-MAY-{PHI1==0}>
//  <stderr[PHI1]-W-MAY-{PHI1==0}>

      perror("Error loading file");                                     /*0018*/
      exit(0);                                                          /*0019*/
   }

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   
   /* Get *.pgm file type */
   c = fgetc(fp);                                                       /*0023*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   c = fgetc(fp);                                                       /*0024*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   do {                                                                 /*0029*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

      while ((c = fgetc(fp))!='\n')                                     /*0028*/
         ;                                                              /*0028*/
   }
   while ((c = fgetc(fp))=='#');                                        /*0029*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   
   /* Put back good char */
   ungetc(c, fp);                                                       /*0032*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   
   /* Get image dimensions */
   fscanf(fp, "%d %d\n", &nx, &ny);                                     /*0035*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   /* Get grey levels */
   fscanf(fp, "%d", &i);                                                /*0037*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   /* Get ONE carriage return */
   fgetc(fp);                                                           /*0039*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>
//  <stdout-R-EXACT-{}>
//  <stdout[PHI1]-R-MAY-{PHI1==0}>
//  <stdout[PHI1]-W-MAY-{PHI1==0}>

   printf("Input image  : x=%d y=%d grey=%d\n", nx, ny, i);             /*0040*/

//  <space[PHI1][PHI2]-W-MAY-{0<=PHI1, PHI1<=500, 0<=PHI2, PHI2<=500}>

   
   /* Erase the memory, in case the image is not big enough: */
   for(i = 0; i <= 500; i += 1)

//  <space[PHI1][PHI2]-W-MAY-{PHI1==i, 0<=PHI2, PHI2<=500, 0<=i,
//    i<=500}>

      for(j = 0; j <= 500; j += 1)

//  <space[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==j, 0<=i, i<=500, 0<=j,
//    j<=500}>

         space[i][j] = 0;                                               /*0045*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>
//  <space[PHI1][PHI2]-W-MAY-{0<=PHI1, PHI1<=500, PHI1+1<=nx, 0<=PHI2,
//    PHI2<=500, PHI2+1<=ny}>

   
   /* Read the pixel grey value: */
   for(j = 0; j <= -1+ny; j += 1)

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>
//  <space[PHI1][PHI2]-W-MAY-{PHI2==j, 0<=PHI1, PHI1<=500, PHI1+1<=nx,
//    0<=j, j<=500, j+1<=ny}>

      for(i = 0; i <= -1+nx; i += 1) {

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

         c = fgetc(fp);                                                 /*0050*/

//  <space[PHI1][PHI2]-W-MAY-{PHI1==i, PHI2==j, 0<=i, i<=500, i+1<=nx,
//    0<=j, j<=500, j+1<=ny}>

         /* Truncate the image if too big: */
         if (i<501&&j<501)                                              /*0052*/

//  <space[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==j, 0<=i, i<=500,
//    i+1<=nx, 0<=j, j<=500, j+1<=ny}>

            space[i][j] = c;                                            /*0053*/
      }

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>


   fclose(fp);                                                          /*0056*/
}

//  <save[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <save[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>
//  <space[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <space[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>

void iteration(float_t space[501][501], float_t save[501][501])
{
   int i, j;                                                            /*0083*/

//  <save[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>
//  <space[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>

   
   /* Use 2 arrays in flip-flop to have dataparallel forall semantics. I
        could use also a flip-flop dimension instead... */
   for(i = 0; i <= 498; i += 1)

//  <save[PHI1][PHI2]-W-MAY-{PHI1==i+1, 1<=PHI2, PHI2<=499, 0<=i,
//    i<=498}>
//  <space[PHI1][PHI2]-R-MAY-{i+1<=PHI1+PHI2, PHI2+i<=PHI1+499,
//    PHI1<=PHI2+i+1, PHI1+PHI2<=i+501, i<=PHI1, PHI1<=i+2, 0<=i,
//    i<=498}>

      for(j = 0; j <= 498; j += 1)

//  <save[PHI1][PHI2]-W-EXACT-{PHI1==i+1, PHI2==j+1, 0<=i, i<=498,
//    0<=j, j<=498}>
//  <space[PHI1][PHI2]-R-MAY-{i+j+1<=PHI1+PHI2, PHI2+i<=PHI1+j+1,
//    PHI1+j<=PHI2+i+1, PHI1+PHI2<=i+j+3, 0<=i, i<=498, 0<=j, j<=498}>


         save[i+1][j+1] = 0.25*(space[i+1-1][j+1]+space[i+1+1][j+1]+space[i+1][j+1-1]+space[i+1][j+1+1]);            /*0090*/

//  <save[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <space[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>


   for(i = 0; i <= 498; i += 1)

//  <save[PHI1][PHI2]-R-MAY-{i+1<=PHI1+PHI2, PHI2+i<=PHI1+499,
//    PHI1<=PHI2+i+1, PHI1+PHI2<=i+501, i<=PHI1, PHI1<=i+2, 0<=i,
//    i<=498}>
//  <space[PHI1][PHI2]-W-MAY-{PHI1==i+1, 1<=PHI2, PHI2<=499, 0<=i,
//    i<=498}>

      for(j = 0; j <= 498; j += 1)

//  <save[PHI1][PHI2]-R-MAY-{i+j+1<=PHI1+PHI2, PHI2+i<=PHI1+j+1,
//    PHI1+j<=PHI2+i+1, PHI1+PHI2<=i+j+3, 0<=i, i<=498, 0<=j, j<=498}>
//  <space[PHI1][PHI2]-W-EXACT-{PHI1==i+1, PHI2==j+1, 0<=i, i<=498,
//    0<=j, j<=498}>


         space[i+1][j+1] = 0.25*(save[i+1-1][j+1]+save[i+1+1][j+1]+save[i+1][j+1-1]+save[i+1][j+1+1]);               /*0096*/
}

//  <*ANY_MODULE*:*ANYWHERE*-R-MAY-{}>
//  <*ANY_MODULE*:*ANYWHERE*-W-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>

void write_data(char filename[])
{
   int i, j;                                                            /*0061*/
   unsigned char c;                                                     /*0062*/
   FILE *fp;                                                            /*0063*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <filename[PHI1]-R-MAY-{}>
//  <fp-W-EXACT-{}>
//  <stderr-R-MAY-{}>
//  <stderr[PHI1]-R-MAY-{PHI1==0}>
//  <stderr[PHI1]-W-MAY-{PHI1==0}>

   if ((fp = fopen(filename, "w"))==(void *) 0) {                       /*0065*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==2}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==2}>
//  <stderr-R-EXACT-{}>
//  <stderr[PHI1]-R-MAY-{PHI1==0}>
//  <stderr[PHI1]-W-MAY-{PHI1==0}>

      perror("Error opening file");                                     /*0066*/
      exit(0);                                                          /*0067*/
   }

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   
   /* Write the PGM header: */
   fprintf(fp, "P5\n%d %d\n255\n", 501, 501);                           /*0071*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>
//  <space[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 0<=PHI2, PHI2<=500}>


   for(j = 0; j <= 500; j += 1)

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>
//  <space[PHI1][PHI2]-R-MAY-{PHI2==j, 0<=PHI1, PHI1<=500, 0<=j,
//    j<=500}>

      for(i = 0; i <= 500; i += 1) {

//  <space[PHI1][PHI2]-R-EXACT-{PHI1==i, PHI2==j, 0<=i, i<=500, 0<=j,
//    j<=500}>

         c = space[i][j];                                               /*0075*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

         fputc(c, fp);                                                  /*0076*/
      }

//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>
//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
//  <fp[PHI1]-W-MAY-{PHI1==0}>

   fclose(fp);                                                          /*0078*/
}

//  <*ANY_MODULE*:*ANYWHERE*-R-MAY-{}>
//  <*ANY_MODULE*:*ANYWHERE*-W-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>

int main(int argc, char *argv[])
{
   int i;                                                               /*0112*/

//  <*ANY_MODULE*:*ANYWHERE*-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{PHI1==2}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{PHI1==2}>
//  <stderr[PHI1]-W-MAY-{PHI1==0}>


   if (argc!=2) {                                                       /*0114*/

//  <*ANY_MODULE*:*ANYWHERE*-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==2}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==2}>
//  <stderr-R-EXACT-{}>
//  <stderr[PHI1]-R-MAY-{PHI1==0}>
//  <stderr[PHI1]-W-MAY-{PHI1==0}>

      
      
      fprintf(stderr, "%s needs only one argument that is the PGM image input file\n", argv[0]);      /*0117*/
      exit(0);                                                          /*0118*/
   }

//  <*ANY_MODULE*:*ANYWHERE*-R-MAY-{}>
//  <*ANY_MODULE*:*ANYWHERE*-W-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>

   get_data(argv[1]);                                                   /*0120*/

//  <save[PHI1][PHI2]-W-MAY-{0<=PHI1, PHI1<=500, 0<=PHI2, PHI2<=500,
//    argc==2}>

   
   /* Initialize the border of the destination image, since it is used but
        never written to: */
   for(i = 0; i <= 500; i += 1)

//  <save[PHI1][PHI2]-W-MAY-{0<=PHI1, PHI1<=500, i<=PHI1+PHI2,
//    PHI1+PHI2<=i+500, 0<=PHI2, PHI2<=500, argc==2, 0<=i, i<=500}>

      save[i][0] = save[0][i] = save[i][501-1] = save[501-1][i] = 0;    /*0125*/

//  <save[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <save[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>
//  <space[PHI1][PHI2]-R-MAY-{0<=PHI1, PHI1<=500, 1<=PHI1+PHI2,
//    PHI2<=PHI1+499, PHI1<=PHI2+499, PHI1+PHI2<=999, 0<=PHI2,
//    PHI2<=500}>
//  <space[PHI1][PHI2]-W-MAY-{1<=PHI1, PHI1<=499, 1<=PHI2, PHI2<=499}>


   compute(space, save);                                                /*0127*/

//  <*ANY_MODULE*:*ANYWHERE*-R-MAY-{}>
//  <*ANY_MODULE*:*ANYWHERE*-W-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>


   write_data("output.pgm");                                            /*0129*/

   return 0;                                                            /*0131*/
}
void compute(float_t space[501][501], float_t save[501][501])
{
   int t;                                                               /*0102*/
   
   /* Apply the relaxation T times: */
   for(t = 0; t <= 399; t += 1)
      iteration(space, save);                                           /*0106*/
}
void get_data(char filename[])
{
   int i, j, nx, ny;                                                    /*0013*/
   unsigned char c;                                                     /*0014*/
   FILE *fp;                                                            /*0015*/
   if ((fp = fopen(filename, "r"))==(void *) 0) {                       /*0017*/
      perror("Error loading file");                                     /*0018*/
      exit(0);                                                          /*0019*/
   }
   
   /* Get *.pgm file type */
   c = fgetc(fp);                                                       /*0023*/
   c = fgetc(fp);                                                       /*0024*/
   do {                                                                 /*0029*/
      while ((c = fgetc(fp))!='\n')                                     /*0028*/
         ;                                                              /*0028*/
   }
   while ((c = fgetc(fp))=='#');                                        /*0029*/
   
   /* Put back good char */
   ungetc(c, fp);                                                       /*0032*/
   
   /* Get image dimensions */
   fscanf(fp, "%d %d\n", &nx, &ny);                                     /*0035*/
   /* Get grey levels */
   fscanf(fp, "%d", &i);                                                /*0037*/
   /* Get ONE carriage return */
   fgetc(fp);                                                           /*0039*/
   printf("Input image  : x=%d y=%d grey=%d\n", nx, ny, i);             /*0040*/
   
   /* Erase the memory, in case the image is not big enough: */
#pragma omp parallel for private(j)
   for(i = 0; i <= 500; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 500; j += 1)
         space[i][j] = 0;                                               /*0045*/
   
   /* Read the pixel grey value: */
   for(j = 0; j <= -1+ny; j += 1)
      for(i = 0; i <= -1+nx; i += 1) {
         c = fgetc(fp);                                                 /*0050*/
         /* Truncate the image if too big: */
         if (i<501&&j<501)                                              /*0052*/
            space[i][j] = c;                                            /*0053*/
      }

   fclose(fp);                                                          /*0056*/
}
void iteration(float_t space[501][501], float_t save[501][501])
{
   int i, j;                                                            /*0083*/
   
   /* Use 2 arrays in flip-flop to have dataparallel forall semantics. I
        could use also a flip-flop dimension instead... */
#pragma omp parallel for private(j)
   for(i = 0; i <= 498; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 498; j += 1)

         save[i+1][j+1] = 0.25*(space[i+1-1][j+1]+space[i+1+1][j+1]+space[i+1][j+1-1]+space[i+1][j+1+1]);            /*0090*/

#pragma omp parallel for private(j)
   for(i = 0; i <= 498; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 498; j += 1)

         space[i+1][j+1] = 0.25*(save[i+1-1][j+1]+save[i+1+1][j+1]+save[i+1][j+1-1]+save[i+1][j+1+1]);               /*0096*/
}
void write_data(char filename[])
{
   int i, j;                                                            /*0061*/
   unsigned char c;                                                     /*0062*/
   FILE *fp;                                                            /*0063*/
   if ((fp = fopen(filename, "w"))==(void *) 0) {                       /*0065*/
      perror("Error opening file");                                     /*0066*/
      exit(0);                                                          /*0067*/
   }
   
   /* Write the PGM header: */
   fprintf(fp, "P5\n%d %d\n255\n", 501, 501);                           /*0071*/

   for(j = 0; j <= 500; j += 1)
      for(i = 0; i <= 500; i += 1) {
         c = space[i][j];                                               /*0075*/
         fputc(c, fp);                                                  /*0076*/
      }
   fclose(fp);                                                          /*0078*/
}
int main(int argc, char *argv[])
{
   int i;                                                               /*0112*/

   if (argc!=2) {                                                       /*0114*/
      
      
      fprintf(stderr, "%s needs only one argument that is the PGM image input file\n", argv[0]);      /*0117*/
      exit(0);                                                          /*0118*/
   }
   get_data(argv[1]);                                                   /*0120*/
   
   /* Initialize the border of the destination image, since it is used but
        never written to: */
   for(i = 0; i <= 500; i += 1)
      save[i][0] = save[0][i] = save[i][501-1] = save[501-1][i] = 0;    /*0125*/

   compute(space, save);                                                /*0127*/

   write_data("output.pgm");                                            /*0129*/

   return 0;                                                            /*0131*/
}
void compute(float_t space[501][501], float_t save[501][501])
{
   int t;                                                               /*0102*/
   
   /* Apply the relaxation T times: */
   for(t = 0; t <= 399; t += 1)
      iteration(space, save);                                           /*0106*/
}
void get_data(char filename[])
{
   int i, j, nx, ny;                                                    /*0013*/
   unsigned char c;                                                     /*0014*/
   FILE *fp;                                                            /*0015*/
   if ((fp = fopen(filename, "r"))==(void *) 0) {                       /*0017*/
      perror("Error loading file");                                     /*0018*/
      exit(0);                                                          /*0019*/
   }
   
   /* Get *.pgm file type */
   c = fgetc(fp);                                                       /*0023*/
   c = fgetc(fp);                                                       /*0024*/
   do {                                                                 /*0029*/
      while ((c = fgetc(fp))!='\n')                                     /*0028*/
         ;                                                              /*0028*/
   }
   while ((c = fgetc(fp))=='#');                                        /*0029*/
   
   /* Put back good char */
   ungetc(c, fp);                                                       /*0032*/
   
   /* Get image dimensions */
   fscanf(fp, "%d %d\n", &nx, &ny);                                     /*0035*/
   /* Get grey levels */
   fscanf(fp, "%d", &i);                                                /*0037*/
   /* Get ONE carriage return */
   fgetc(fp);                                                           /*0039*/
   printf("Input image  : x=%d y=%d grey=%d\n", nx, ny, i);             /*0040*/
   
   /* Erase the memory, in case the image is not big enough: */
   p4a_kernel_launcher_0(space);
   
   /* Read the pixel grey value: */
   for(j = 0; j <= -1+ny; j += 1)
      for(i = 0; i <= -1+nx; i += 1) {
         c = fgetc(fp);                                                 /*0050*/
         /* Truncate the image if too big: */
         if (i<501&&j<501)                                              /*0052*/
            space[i][j] = c;                                            /*0053*/
      }

   fclose(fp);                                                          /*0056*/
}
void iteration(float_t space[501][501], float_t save[501][501])
{
   int i, j;                                                            /*0083*/
   
   /* Use 2 arrays in flip-flop to have dataparallel forall semantics. I
        could use also a flip-flop dimension instead... */
   p4a_kernel_launcher_1(save, space);

   p4a_kernel_launcher_2(save, space);
}
void write_data(char filename[])
{
   int i, j;                                                            /*0061*/
   unsigned char c;                                                     /*0062*/
   FILE *fp;                                                            /*0063*/
   if ((fp = fopen(filename, "w"))==(void *) 0) {                       /*0065*/
      perror("Error opening file");                                     /*0066*/
      exit(0);                                                          /*0067*/
   }
   
   /* Write the PGM header: */
   fprintf(fp, "P5\n%d %d\n255\n", 501, 501);                           /*0071*/

   for(j = 0; j <= 500; j += 1)
      for(i = 0; i <= 500; i += 1) {
         c = space[i][j];                                               /*0075*/
         fputc(c, fp);                                                  /*0076*/
      }
   fclose(fp);                                                          /*0078*/
}
int main(int argc, char *argv[])
{
   int i;                                                               /*0112*/

   if (argc!=2) {                                                       /*0114*/
      
      
      fprintf(stderr, "%s needs only one argument that is the PGM image input file\n", argv[0]);      /*0117*/
      exit(0);                                                          /*0118*/
   }
   get_data(argv[1]);                                                   /*0120*/
   
   /* Initialize the border of the destination image, since it is used but
        never written to: */
   for(i = 0; i <= 500; i += 1)
      save[i][0] = save[0][i] = save[i][501-1] = save[501-1][i] = 0;    /*0125*/

   compute(space, save);                                                /*0127*/

   write_data("output.pgm");                                            /*0129*/

   return 0;                                                            /*0131*/
}
void p4a_kernel_launcher_2(float_t save[501][501], float_t space[501][501])
{
   //PIPS generated variable
   int i, j;                                                            /*0004*/

   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         // To be assigned to a call to P4A_vp_0: i
         // To be assigned to a call to P4A_vp_1: j
         
         p4a_kernel_wrapper_2(i, j, save, space);                       /*0011*/
}
void p4a_kernel_launcher_0(float_t space[501][501])
{
   //PIPS generated variable
   int i, j;                                                            /*0004*/
   
   /* Erase the memory, in case the image is not big enough: */
   for(i = 0; i <= 500; i += 1)
      for(j = 0; j <= 500; j += 1)
         // To be assigned to a call to P4A_vp_0: i
         // To be assigned to a call to P4A_vp_1: j
         p4a_kernel_wrapper_0(space, i, j);                             /*0011*/
}
