void p4a_kernel_launcher_1(float_t save[501][501], float_t space[501][501])
{
   int i;
   int j;
   int k;
   float_t dest[501][501];

kernel2:
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         p4a_kernel_wrapper_1(save, space, i+1, j+1);

kernel3:
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         for(k = 0; k <= 498; k += 1)
            p4a_kernel_wrapper_2(save, space, dest, i+1, j+1, k+1);
}
void p4a_kernel_launcher_1(float_t save[501][501], float_t space[501][501])
{
   int i;
   int j;
   int k;
   float_t dest[501][501];
   // Loop nest P4A begin,2D(499,499)
kernel2:
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         // Loop nest P4A end
         if (i<=498&&j<=498)
            p4a_kernel_wrapper_1(save, space, i+1, j+1);
   // Loop nest P4A begin,3D(499,499,499)
kernel3:
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         for(k = 0; k <= 498; k += 1)
            // Loop nest P4A end
            if (i<=498&&j<=498&&k<=498)
               p4a_kernel_wrapper_2(save, space, dest, i+1, j+1, k+1);
}
