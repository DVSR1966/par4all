      PROGRAM MATADD
      INTEGER M
      INTEGER N
      PARAMETER (M = 402, N = 160)
      INTEGER I, J
      REAL A(M, N), B(M, N), C(M, N)

      CALL P4A_KERNEL_LAUNCHER_0(N, M, C, B)                            0008

      CALL P4A_KERNEL_LAUNCHER_1(N, M, C, B, A)                         0015

      END
      SUBROUTINE P4A_KERNEL_0(N, M, J, I, C, B)
      INTEGER N, M, J, I
      REAL*4 C(1:M, 1:N), B(1:M, 1:N)
      B(I,J) = I                                                        0004
      C(I,J) = J                                                        0005
      END
      SUBROUTINE P4A_KERNEL_1(N, M, J, I, C, B, A)
      INTEGER N, M, J, I
      REAL*4 C(1:M, 1:N), B(1:M, 1:N), A(1:M, 1:N)
      A(I,J) = B(I,J)+C(I,J)                                            0004
      END
      SUBROUTINE P4A_KERNEL_LAUNCHER_0(N, M, C, B)
      INTEGER J, I, N, M
      REAL*4 C(1:M, 1:N), B(1:M, 1:N)
C PIPS generated variable

      DO I = 1, M                                                       0006
         DO J = 1, N                                                    0007
C To be assigned to a call to P4A_vp_0: I
C To be assigned to a call to P4A_vp_1: J
            CALL P4A_KERNEL_WRAPPER_0(N, M, J, I, C, B)                 0010
         ENDDO
      ENDDO
      END
      SUBROUTINE P4A_KERNEL_LAUNCHER_1(N, M, C, B, A)
      INTEGER J, I, N, M
      REAL*4 C(1:M, 1:N), B(1:M, 1:N), A(1:M, 1:N)
C PIPS generated variable

      DO I = 1, M                                                       0006
         DO J = 1, N                                                    0007
C To be assigned to a call to P4A_vp_0: I
C To be assigned to a call to P4A_vp_1: J
            CALL P4A_KERNEL_WRAPPER_1(N, M, J, I, C, B, A)              0010
         ENDDO
      ENDDO
      END
      SUBROUTINE P4A_KERNEL_WRAPPER_0(N, M, J, I, C, B)
      INTEGER N, M, J, I
      REAL*4 C(1:M, 1:N), B(1:M, 1:N)
C To be assigned to a call to P4A_vp_0: I
C To be assigned to a call to P4A_vp_1: J
      CALL P4A_KERNEL_0(N, M, J, I, C, B)                               0006
      END
      SUBROUTINE P4A_KERNEL_WRAPPER_1(N, M, J, I, C, B, A)
      INTEGER N, M, J, I
      REAL*4 C(1:M, 1:N), B(1:M, 1:N), A(1:M, 1:N)
C To be assigned to a call to P4A_vp_0: I
C To be assigned to a call to P4A_vp_1: J
      CALL P4A_KERNEL_1(N, M, J, I, C, B, A)                            0006
      END
