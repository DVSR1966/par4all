PROPER  EFFECTS WITH POINTS TO

int foo()
{
//             <    is written>: x
   int x = 3;
//             <    is read   >: x
   return x*x*x/54;
}
void main()
{
   int x1, y, x2;
//             <    is read   >: x1 x2
   0<=x1&&1<=x2&&x1+x2<=2?(void) 0:__assert_fail("0<=x1 && 1<=x2 && x1+x2<=2", "$VDIR/schrammel03.c", 8, (const char *) 0);
//             <    is written>: y
   y = foo();
//             <    is read   >: x1 x2 y
   while (2*x1+2*x2<=7&&0<=y&&y<=1) {
//             <    is read   >: x1 y
//             <    is written>: x1
      x1 = x1+y+1;
//             <    is read   >: y
//             <    is written>: x2
      x2 = y;
//             <    is written>: y
      y = foo();
   }
//             <    is read   >: y
//             <    is written>: y
   // Expected result: 1<=x1+x2, 0<=x2, 2x1<=2x2+9, 2x1+11x2<=22, 0<=x1
   y = y;
}

