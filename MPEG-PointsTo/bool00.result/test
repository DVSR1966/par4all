
PROPER  EFFECTS 

int main(void)
{
   int bla;
   int *pbla;
//             <    is written>: b
   _Bool b = 1;
//             <    is written>: pbla

   pbla = &bla;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pbla
   // kill all preconditions
   *pbla = 1;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(void)
{

// Points To: none

   int bla;

// Points To: none

   int *pbla;

// Points To:
// pbla -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   _Bool b = 1;

// Points To:
// pbla -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   pbla = &bla;

// Points To:
// pbla -> bla , EXACT

   // kill all preconditions
   *pbla = 1;

// Points To:
// pbla -> bla , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main(void)
{
   int bla;
   int *pbla;
//             <    is written>: b
   _Bool b = 1;
//             <    is written>: pbla

   pbla = &bla;
//             <    is read   >: pbla
//             <    is written>: bla
   // kill all preconditions
   *pbla = 1;

   return 0;
}

