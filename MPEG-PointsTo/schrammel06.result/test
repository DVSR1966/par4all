
PROPER  EFFECTS 

int main()
{
//             <    is written>: j
   int i, j = 2, k;
//             <    is read   >: i

   while (i*i>1)
//             <    is read   >: k
      if (k>=3)
//             <    is written>: j
         j = 3;
//             <    is read   >: j
//             <    is written>: j
   j = j;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i, j = 2, k;

// Points To: none


   while (i*i>1)

// Points To: none

      if (k>=3)

// Points To: none

         j = 3;

// Points To: none

   j = j;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: j
   int i, j = 2, k;
//             <    is read   >: i

   while (i*i>1)
//             <    is read   >: k
      if (k>=3)
//             <    is written>: j
         j = 3;
//             <    is read   >: j
//             <    is written>: j
   j = j;
}

