
PROPER  EFFECTS 

int continue01()
{
   /* Compute transformers for continue statement used to represent
        declarations */
//             <    is written>: 0`i
   int i = 2;
   int k;
   {
//             <    is read   >: 0`i
//             <    is written>: j
      int j = i;
//             <    is written>: 0`1`i
      int i = 3;
//             <    is read   >: 0`1`i j
//             <    is written>: k

      k = i+j;
   }
//             <    is read   >: k
   return k;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int continue01()
{
   /* Compute transformers for continue statement used to represent
        declarations */

// Points To: none

   int i = 2;

// Points To: none

   int k;
   {

// Points To: none

      int j = i;

// Points To: none

      int i = 3;

// Points To: none


      k = i+j;
   }

// Points To: none

   return k;
}

PROPER  EFFECTS WITH POINTS TO

int continue01()
{
   /* Compute transformers for continue statement used to represent
        declarations */
//             <    is written>: 0`i
   int i = 2;
   int k;
   {
//             <    is read   >: 0`i
//             <    is written>: j
      int j = i;
//             <    is written>: 0`1`i
      int i = 3;
//             <    is read   >: 0`1`i j
//             <    is written>: k

      k = i+j;
   }
//             <    is read   >: k
   return k;
}

 *********************** Use-Def Chains *********************

	05 --> 08 with conflicts
		from <0`i-W-EXACT> to <0`i-R-EXACT>
	08 --> 11 with conflicts
		from <j-W-EXACT> to <j-R-EXACT>
	09 --> 11 with conflicts
		from <0`1`i-W-EXACT> to <0`1`i-R-EXACT>
	11 --> 13 with conflicts
		from <k-W-EXACT> to <k-R-EXACT>

****************** End of Dependence Graph ******************
