//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   shift05(i, 2);
}
int shift05(int i, int k)
{
//             <    is read   >: i
//             <    is written>: j
   int j = i;
//             <    is written>: l
   int l = -1;
   int m;
   int n;
   int o;
//             <    is read   >: i

   if (i>0) {
//             <    is read   >: i
//             <    is written>: j
      j = 2<<i;
//             <    is read   >: j
//             <    is written>: o
      o = j;
   }
   else if (i==0)
//             <    is read   >: i
//             <    is written>: j
      j = 2<<i;
   else
//             <    is read   >: i
//             <    is written>: j
      // i<0
      j = 2<<i;
//             <    is read   >: i
//             <    is written>: m

   m = 2<<i;
//             <    is read   >: i
//             <    is written>: n
   n = -2<<i;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j l m n o stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   
   // j=2097152, l=-1, m=2097152, n=-2097152, o=2097152
   printf("j=%d, l=%d, m=%d, n=%d, o=%d\n", j, l, m, n, o);
//             <    is read   >: j
   return j;
