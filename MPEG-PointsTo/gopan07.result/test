
PROPER  EFFECTS 

int main()
{

   int x, y, z, k;
//             <    is written>: x

   x = 0;
//             <    is written>: y
   y = 0;
//             <    is read   >: y

   while (y>=0) {
//             <    is read   >: x y
      while (y>=0&&x<=50) {
//             <    is read   >: x
//             <    is written>: x
         x++;
//             <    is read   >: y
//             <    is written>: y
         y++;
      }
//             <    is read   >: x y
      while (y>=0&&x>50) {
//             <    is read   >: y
//             <    is written>: y
         y--;
//             <    is read   >: x
//             <    is written>: x
         x++;
      }
   }
//             <    is read   >: x
   if (x==103)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property  verified\n");
   else
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property not found\n");
}

