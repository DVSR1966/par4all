
PROPER  EFFECTS 

int main()
{
//             <    is written>: j
   int i, j = 2, k;
   float x;
//             <    is read   >: i

   while (i*i>1) {
//             <    is read   >: k
      while (k*k>1)
//             <    is written>: j
         j = 1;
//             <    is written>: x
      // Oops, this generate an indentity transformer that is not identified
      //    j = j;
      x = 0.;
   }
//             <    is read   >: j
//             <    is written>: j
   j = j;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i, j = 2, k;

// Points To: none

   float x;

// Points To: none


   while (i*i>1) {

// Points To: none

      while (k*k>1)

// Points To: none

         j = 1;

// Points To: none

      // Oops, this generate an indentity transformer that is not identified
      //    j = j;
      x = 0.;
   }

// Points To: none

   j = j;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: j
   int i, j = 2, k;
   float x;
//             <    is read   >: i

   while (i*i>1) {
//             <    is read   >: k
      while (k*k>1)
//             <    is written>: j
         j = 1;
//             <    is written>: x
      // Oops, this generate an indentity transformer that is not identified
      //    j = j;
      x = 0.;
   }
//             <    is read   >: j
//             <    is written>: j
   j = j;
}

