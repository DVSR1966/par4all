
PROPER  EFFECTS 

int main()
{
   double z;
   unsigned char c;
//             <    is written>: z

   z = 2.0;
//             <    is read   >: z
//             <    is written>: c

   c = z>1.0?255:255*z;
   
   
   return 0;
}

