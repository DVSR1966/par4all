PROPER  EFFECTS WITH POINTS TO

int main(void)
{
//             <    is written>: x
   int x = 0;
//             <    is written>: d
   int d = 1;
   // infinite loop
   while (1) {
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: d stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      fprintf(stdout, "loop: x=%d d=%d\n", x, d);
//             <    is read   >: x
      if (x==0)
//             <    is written>: d
         d = 1;
//             <    is read   >: x
      if (x==1000)
//             <    is written>: d
         d = -1;
//             <    is read   >: d x
//             <    is written>: x
      x += d;
   }
   // obviously never reached:
   // there is no exit from the previous while loop.
   return 0;
}

