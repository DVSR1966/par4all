
PROPER  EFFECTS 

int main()
{
   int i;
//             <    is read   >: i
//             <    is written>: i

   i>1?(i = 2):(i = 3);
//             <    is read   >: i
//             <    is written>: i

   i = i>1?2:3;
//             <    is read   >: i

   i>1?(void) 0:__assert_fail("i>1", "$VDIR/assert01.c", 11, (const char *) 0);
//             <    is read   >: i

   return i>1;
}

