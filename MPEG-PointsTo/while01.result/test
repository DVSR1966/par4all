
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = -1;
   int a[20];
//             <    is read   >: i
//             <    is written>: i

   while (i++>0)
//             <    is read   >: i
//             <    is written>: a[i]
      a[i] = i;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", i);
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = -1;

// Points To: none

   int a[20];

// Points To: none


   while (i++>0)

// Points To: none

      a[i] = i;

// Points To: none

   printf("%d\n", i);
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = -1;
   int a[20];
//             <    is read   >: i
//             <    is written>: i

   while (i++>0)
//             <    is read   >: i
//             <    is written>: a[i]
      a[i] = i;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", i);
}

