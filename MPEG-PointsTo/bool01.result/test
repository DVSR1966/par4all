
PROPER  EFFECTS 

int main(void)
{
   int bla, *pbla;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pbla
   //  _Bool b = 1;
   
   //pbla = &bla;
   // kill all preconditions
   *pbla = 1;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(void)
{

// Points To: none

   int bla, *pbla;

// Points To:
// pbla -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   //  _Bool b = 1;
   
   //pbla = &bla;
   // kill all preconditions
   *pbla = 1;

// Points To:
// pbla -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main(void)
{
   int bla, *pbla;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pbla
   //  _Bool b = 1;
   
   //pbla = &bla;
   // kill all preconditions
   *pbla = 1;

   return 0;
}

