
PROPER  EFFECTS 

_Bool alea(void)
{
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   return rand()%2;
}
int main()
{
   int v, t, x, d;
//             <    is written>: d t v x
   v = t = x = d = 0;
   
   // The convex enveloppe of the guards is true
   while (1) {
//             <    is read   >: d x
      while (x<=4&&d<=9)
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
         if (alea())
//             <    is read   >: v x
//             <    is written>: v x
            x++, v++;
         else
//             <    is read   >: d t
//             <    is written>: d t
            d++, t++;
//             <    is read   >: d
      
      //if(x>4)
      /*x>4 &&*/
      while (d<=9)
//             <    is read   >: d t
//             <    is written>: d t
         d++, t++;
//             <    is read   >: x _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
      
      //else if(d>9)
      /*d>9 &&*/
      while (x<=4&&(alea()||x>=2))
//             <    is read   >: v x
//             <    is written>: v x
         x++, v++;
//             <    is read   >: d x
      
      /* && alea() */
      while (d==10&&x>=2)
//             <    is written>: d x
         x = d = 0;
   }
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

_Bool alea(void)
{

// Points To: none

   return rand()%2;
}

// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int v, t, x, d;

// Points To: none

   v = t = x = d = 0;

// Points To: none

   
   // The convex enveloppe of the guards is true
   while (1) {

// Points To: none

      while (x<=4&&d<=9)

// Points To: none

         if (alea())

// Points To: none

            x++, v++;
         else

// Points To: none

            d++, t++;

// Points To: none

      
      //if(x>4)
      /*x>4 &&*/
      while (d<=9)

// Points To: none

         d++, t++;

// Points To: none

      
      //else if(d>9)
      /*d>9 &&*/
      while (x<=4&&(alea()||x>=2))

// Points To: none

         x++, v++;

// Points To: none

      
      /* && alea() */
      while (d==10&&x>=2)

// Points To: none

         x = d = 0;
   }
}

PROPER  EFFECTS WITH POINTS TO

_Bool alea(void)
{
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   return rand()%2;
}
int main()
{
   int v, t, x, d;
//             <    is written>: d t v x
   v = t = x = d = 0;
   
   // The convex enveloppe of the guards is true
   while (1) {
//             <    is read   >: d x
      while (x<=4&&d<=9)
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
         if (alea())
//             <    is read   >: v x
//             <    is written>: v x
            x++, v++;
         else
//             <    is read   >: d t
//             <    is written>: d t
            d++, t++;
//             <    is read   >: d
      
      //if(x>4)
      /*x>4 &&*/
      while (d<=9)
//             <    is read   >: d t
//             <    is written>: d t
         d++, t++;
//             <    is read   >: x _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
      
      //else if(d>9)
      /*d>9 &&*/
      while (x<=4&&(alea()||x>=2))
//             <    is read   >: v x
//             <    is written>: v x
         x++, v++;
//             <    is read   >: d x
      
      /* && alea() */
      while (d==10&&x>=2)
//             <    is written>: d x
         x = d = 0;
   }
}

