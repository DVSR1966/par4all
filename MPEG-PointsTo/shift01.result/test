PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 20;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   shift01(i);
}
int shift01(int i)
{
//             <    is read   >: i
//             <    is written>: j
   int j = i<<2;
//             <    is written>: n
   int n = 1;
//             <    is read   >: j n
//             <    is written>: j

   j = j<<n;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   
   
   printf("j=%d\n", j);
//             <    is read   >: j
   return j;
}

