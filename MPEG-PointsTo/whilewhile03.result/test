// Points To: none

            i--;

// Points To: none

         // Never reached
         ;
      }

// Points To: none


   return;
}

PROPER  EFFECTS WITH POINTS TO

void whilewhile03()
{
//             <    is written>: i
   int i = -1;
//             <    is read   >: i

   while (i<=10)
      if (1) {
//             <    is read   >: i
         while (i<=10&&i>=0)
//             <    is read   >: i
//             <    is written>: i
            i++;
//             <    is read   >: i
         while (i<=10&&i<0)
//             <    is read   >: i
//             <    is written>: i
            i--;
         // Never reached
         ;
      }

   return;
}

