
PROPER  EFFECTS 

int main()
{
//             <    is written>: s0 s1 s2
   int s0 = 1, s1 = 0, s2 = 0;
   //int c0 = 1, c1 = 0, c2 = 0;
   
   while (1)
//             <    is read   >: s0
      if (s0==1) {
//             <    is read   >: s0
//             <    is written>: s0
         s0--;
//             <    is read   >: s1
//             <    is written>: s1
         s1++;
      }
      else if (s1==1) {
//             <    is read   >: s1
//             <    is written>: s1
         s1--;
//             <    is read   >: s2
//             <    is written>: s2
         s2++;
      }
      else if (s2==1) {
//             <    is read   >: s2
//             <    is written>: s2
         s2--;
//             <    is read   >: s0
//             <    is written>: s0
         s0++;
      }
   return 0;
}

