
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 4;
   int j, k;
//             <    is read   >: i
//             <    is written>: i j

   j = i++%2;
//             <    is read   >: i
//             <    is written>: i k
   k = ++i%2;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i j k stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("i=%d, j=%d, k=%d\n", i, j, k);

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 4;

// Points To: none

   int j, k;

// Points To: none


   j = i++%2;

// Points To: none

   k = ++i%2;

// Points To: none


   printf("i=%d, j=%d, k=%d\n", i, j, k);

// Points To: none


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 4;
   int j, k;
//             <    is read   >: i
//             <    is written>: i j

   j = i++%2;
//             <    is read   >: i
//             <    is written>: i k
   k = ++i%2;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i j k stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("i=%d, j=%d, k=%d\n", i, j, k);

   return 0;
}

 *********************** Use-Def Chains *********************

	08 --> 11 with conflicts
		from <i-W-EXACT> to <i-W-EXACT>
		from <i-W-EXACT> to <i-R-EXACT>
	11 --> 12 with conflicts
		from <i-W-EXACT> to <i-W-EXACT>
		from <i-W-EXACT> to <i-R-EXACT>
		from <i-R-EXACT> to <i-W-EXACT>
	11 --> 14 with conflicts
		from <j-W-EXACT> to <j-R-EXACT>
	12 --> 14 with conflicts
		from <k-W-EXACT> to <k-R-EXACT>
		from <i-W-EXACT> to <i-R-EXACT>

