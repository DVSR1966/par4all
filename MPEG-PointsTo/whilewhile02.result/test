
PROPER  EFFECTS 

void whilewhile02()
{
//             <    is written>: i
   int i = 0;
//             <    is read   >: i
   
   // The second internal while is not entered
   while (i<=10) {
//             <    is read   >: i
      while (i<=10&&i>=0)
//             <    is read   >: i
//             <    is written>: i
         i++;
//             <    is read   >: i
      while (i<=10&&i<0)
//             <    is read   >: i
//             <    is written>: i
         i--;
   }

   return;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void whilewhile02()
{

// Points To: none

   int i = 0;

// Points To: none

   
   // The second internal while is not entered
   while (i<=10) {

// Points To: none

      while (i<=10&&i>=0)

// Points To: none

         i++;

// Points To: none

      while (i<=10&&i<0)

// Points To: none

         i--;
   }

// Points To: none


   return;
}

PROPER  EFFECTS WITH POINTS TO

void whilewhile02()
{
//             <    is written>: i
   int i = 0;
//             <    is read   >: i
   
   // The second internal while is not entered
   while (i<=10) {
//             <    is read   >: i
      while (i<=10&&i>=0)
//             <    is read   >: i
//             <    is written>: i
         i++;
//             <    is read   >: i
      while (i<=10&&i<0)
//             <    is read   >: i
//             <    is written>: i
         i--;
   }

   return;
}

