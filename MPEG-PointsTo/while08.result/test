
PROPER  EFFECTS 

int main()
{
//             <    is written>: c
   int c = 0;
//             <    is written>: i
   int i = 0;
//             <    is written>: j
   int j = 0;
//             <    is read   >: j

   while (j<=10)
//             <    is read   >: c
      if (c==0)
//             <    is read   >: i j
//             <    is written>: i j
         i++, j++;
      else {
//             <    is written>: c
         c = 0;
//             <    is read   >: i j
//             <    is written>: i j
         i--, j++;
      }
   return;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int c = 0;

// Points To: none

   int i = 0;

// Points To: none

   int j = 0;

// Points To: none


   while (j<=10)

// Points To: none

      if (c==0)

// Points To: none

         i++, j++;
      else {

// Points To: none

         c = 0;

// Points To: none

         i--, j++;
      }

// Points To: none

   return;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: c
   int c = 0;
//             <    is written>: i
   int i = 0;
//             <    is written>: j
   int j = 0;
//             <    is read   >: j

   while (j<=10)
//             <    is read   >: c
      if (c==0)
//             <    is read   >: i j
//             <    is written>: i j
         i++, j++;
      else {
//             <    is written>: c
         c = 0;
//             <    is read   >: i j
//             <    is written>: i j
         i--, j++;
      }
   return;
}

