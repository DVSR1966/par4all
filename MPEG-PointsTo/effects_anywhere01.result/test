
PROPER  EFFECTS 

void effects_anywhere01(int i, int *p)
{
//             <    is read   >: i
   if (i!=1)
      exit(1);
   else {
      int k;
//             <    is read   >: i
//             <    is written>: k

      k = i;
      
      // If this statement is commented out, the information i==1 is
      // found after the call in the main programm
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: l
      int l = *p;
//             <    is read   >: k

      if (k!=1)
         print("error\n");
   }
   return;
}
int * foo()
{
   return 0;
}
int main()
{
//             <    is written>: p
   int *p = foo();
   int i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
   effects_anywhere01(i, p);
   return 0;
}
void print(char *s)
{
}

