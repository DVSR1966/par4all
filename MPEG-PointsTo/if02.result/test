
PROPER  EFFECTS 

void if02()
{
//             <    is written>: i
   int i = 0;
//             <    is written>: j
   int j = 0;
//             <    is read   >: i
//             <    is written>: i

   if (i++, i>0)
//             <    is written>: j
      // This branch is taken by gcc
      j = 1;
   else
//             <    is written>: j
      // Should not be reached
      j = 2;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("%d\n", j);
   return;
}
int main()
{
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   if02();
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void if02()
{

// Points To: none

   int i = 0;

// Points To: none

   int j = 0;

// Points To: none


   if (i++, i>0)

// Points To: none

      // This branch is taken by gcc
      j = 1;
   else

// Points To: none

      // Should not be reached
      j = 2;

// Points To: none


   printf("%d\n", j);

// Points To: none

   return;
}

// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   if02();
}

PROPER  EFFECTS WITH POINTS TO

void if02()
{
//             <    is written>: i
   int i = 0;
//             <    is written>: j
   int j = 0;
//             <    is read   >: i
//             <    is written>: i

   if (i++, i>0)
//             <    is written>: j
      // This branch is taken by gcc
      j = 1;
   else
//             <    is written>: j
      // Should not be reached
      j = 2;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("%d\n", j);
   return;
}
int main()
{
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   if02();
}

