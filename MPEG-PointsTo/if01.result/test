
PROPER  EFFECTS 

void if01()
{
//             <    is written>: i
   int i = 0;
//             <    is written>: j
   int j = 0;
//             <    is read   >: i
//             <    is written>: i

   if (i++)
//             <    is written>: j
      // Should not be reached
      j = 1;
//             <    is read   >: i
//             <    is written>: i

   if (i++)
//             <    is written>: j
      // Should be reached
      j = 2;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("%d ", j);
//             <    is written>: i

   i = 0;
//             <    is read   >: i
//             <    is written>: i

   if (i++>0)
//             <    is written>: j
      // Should not be reached
      j = 1;
//             <    is read   >: i
//             <    is written>: i

   if (i++>0)
//             <    is written>: j
      // Should be reached
      j = 2;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("%d ", j);
//             <    is written>: i

   i = 0;
//             <    is read   >: i
//             <    is written>: i

   if (i++, i>0)
//             <    is written>: j
      // Should be reached according to gcc
      j = 10;
//             <    is read   >: i
//             <    is written>: i

   if (i++, i>0)
//             <    is read   >: j
//             <    is written>: j
      // Should be reached
      j = 2*j+2;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("%d ", j);

   return;
}
int main()
{
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   if01();
}

