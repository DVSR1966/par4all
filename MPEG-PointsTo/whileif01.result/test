void whileif01()
{
//             <    is written>: i
   int i = 0;
//             <    is read   >: i

   while (i<=10)
//             <    is read   >: i
      if (i>=0)
//             <    is read   >: i
//             <    is written>: i
         i++;
      else
//             <    is read   >: i
//             <    is written>: i
         i--;
//             <    is written>: i

   i = -1;
//             <    is read   >: i
   while (i<=10)
//             <    is read   >: i
      if (i>=0)
//             <    is read   >: i
//             <    is written>: i
         i++;
      else
//             <    is read   >: i
//             <    is written>: i
         i--;
   return;
}

