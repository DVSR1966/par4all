//             <    is read   >: result
   return result;
}
int main(int argc, char **argv)
{
   // Parsing error because i is used in its own declaration statement
   //int i = 10, j = i+1, a[i], k = foo(i);
   
//             <    is written>: i
   int i = 10;
//             <    is read   >: i
