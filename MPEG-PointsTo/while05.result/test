
PROPER  EFFECTS 

_Bool alea(void)
{
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   return rand()%2;
}
int main()
{
//             <may be read   >: stdout[0] _RANDOM_EFFECTS:GEN_SEED
//             <may be written>: stdout[0] _RANDOM_EFFECTS:GEN_SEED
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   while05();
}
void while05()
{
//             <    is written>: x y
   int x = 0, y = 0;
//             <    is read   >: x y

   while (x<=4&&y<=8) {
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
      if (alea())
//             <    is read   >: x
//             <    is written>: x
         x++;
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
      if (alea())
//             <    is read   >: y
//             <    is written>: y
         y++;
   }
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout x y __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   // PIPS: {x<=5, 45<=9x+5y, x+y<=12, y<=9}
   fprintf(stdout, "x=%d y=%d\n", x, y);
}

