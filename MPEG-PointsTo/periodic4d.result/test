
PROPER  EFFECTS 

int main()
{

   int x;
   int new, old;
   int y, z, k;
//             <    is written>: x
   x = 0;
//             <    is written>: y
   y = 0;
//             <    is written>: z
   z = 0;
//             <    is written>: new
   new = 0;
//             <    is written>: old
   old = 1;
//             <    is read   >: x

   while (x<10) {
//             <    is read   >: new
      if (new==0)
//             <    is read   >: y
//             <    is written>: y
         y++;
      else
//             <    is read   >: z
//             <    is written>: z
         z++;
//             <    is read   >: new
//             <    is written>: new
      new = 1-new;
//             <    is read   >: old
//             <    is written>: old
      old = 1-old;
//             <    is read   >: x
//             <    is written>: x
      x++;
   }
//             <    is read   >: new old
   if (new==1&&old==0||new==0&&old==1)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property  verified\n");
   else
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property not found\n");
}

