
PROPER  EFFECTS 

int invariance01(int a)
{
   int b;
//             <    is read   >: a
//             <    is written>: a

   a = a+1;
//             <    is read   >: a
//             <    is written>: a
   a = a-1;
//             <    is read   >: a
//             <    is written>: b
   b = a;
//             <    is written>: a
   a = 2;
//             <    is read   >: b
   return b;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int invariance01(int a)
{

// Points To: none

   int b;

// Points To: none


   a = a+1;

// Points To: none

   a = a-1;

// Points To: none

   b = a;

// Points To: none

   a = 2;

// Points To: none

   return b;
}

PROPER  EFFECTS WITH POINTS TO

int invariance01(int a)
{
   int b;
//             <    is read   >: a
//             <    is written>: a

   a = a+1;
//             <    is read   >: a
//             <    is written>: a
   a = a-1;
//             <    is read   >: a
//             <    is written>: b
   b = a;
//             <    is written>: a
   a = 2;
//             <    is read   >: b
   return b;
}

 *********************** Use-Def Chains *********************

	13 --> 14 with conflicts
		from <a-W-EXACT> to <a-W-EXACT>
		from <a-W-EXACT> to <a-R-EXACT>
		from <a-R-EXACT> to <a-W-EXACT>
	14 --> 15 with conflicts
		from <a-W-EXACT> to <a-R-EXACT>
	14 --> 16 with conflicts
		from <a-W-EXACT> to <a-W-EXACT>
		from <a-R-EXACT> to <a-W-EXACT>
	15 --> 16 with conflicts
		from <a-R-EXACT> to <a-W-EXACT>
	15 --> 17 with conflicts
		from <b-W-EXACT> to <b-R-EXACT>

