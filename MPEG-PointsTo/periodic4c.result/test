
PROPER  EFFECTS 

int main()
{

   int x;
   int new, old;
//             <    is written>: x
   //int y,z,k;
   x = 0;
//             <    is written>: new
   //y=0;
   //z=0;
   new = 0;
//             <    is written>: old
   old = 1;
//             <    is read   >: x

   while (x<10) {
//             <    is read   >: new
//             <    is written>: new
      //if (new==0) y++;
      //else z++;
      new = 1-new;
//             <    is read   >: old
//             <    is written>: old
      old = 1-old;
//             <    is read   >: x
//             <    is written>: x
      x++;
   }
//             <    is read   >: new old
   if (new==1&&old==0||new==0&&old==1)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property  verified\n");
   else
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property not found\n");
}

