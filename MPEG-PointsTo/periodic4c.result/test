// Points To: none

   //y=0;
   //z=0;
   new = 0;

// Points To: none

   old = 1;

// Points To: none


   while (x<10) {

// Points To: none

      //if (new==0) y++;
      //else z++;
      new = 1-new;

// Points To: none

      old = 1-old;

// Points To: none

      x++;
   }

// Points To: none

   if (new==1&&old==0||new==0&&old==1)

// Points To: none

      printf("property  verified\n");
   else

// Points To: none

      printf("property not found\n");
}

PROPER  EFFECTS WITH POINTS TO

int main()
{

   int x;
   int new, old;
//             <    is written>: x
   //int y,z,k;
   x = 0;
//             <    is written>: new
   //y=0;
   //z=0;
   new = 0;
//             <    is written>: old
   old = 1;
//             <    is read   >: x

   while (x<10) {
//             <    is read   >: new
//             <    is written>: new
      //if (new==0) y++;
      //else z++;
      new = 1-new;
//             <    is read   >: old
//             <    is written>: old
      old = 1-old;
//             <    is read   >: x
//             <    is written>: x
      x++;
   }
//             <    is read   >: new old
   if (new==1&&old==0||new==0&&old==1)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property  verified\n");
   else
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property not found\n");
}

