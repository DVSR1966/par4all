
PROPER  EFFECTS 

int main()
{
//             <    is written>: i j
   int i = 0, j = 0;
   //float x = 2.12;
   float b[10][10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is written>: j
      for(j = 0; j <= 9; j += 1)
//             <    is read   >: i j
//             <    is written>: b[i][j]
         b[i][j] = 0.0;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0, j = 0;
   //float x = 2.12;

// Points To: none

   float b[10][10];

// Points To: none


   for(i = 0; i <= 9; i += 1)

// Points To: none

      for(j = 0; j <= 9; j += 1)

// Points To: none

         b[i][j] = 0.0;

// Points To: none


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i j
   int i = 0, j = 0;
   //float x = 2.12;
   float b[10][10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is written>: j
      for(j = 0; j <= 9; j += 1)
//             <    is read   >: i j
//             <    is written>: b[i][j]
         b[i][j] = 0.0;

   return 0;
}

 *********************** Use-Def Chains *********************

	13 --> 17 with conflicts
		from <i-W-EXACT> to <i-W-EXACT>
	13 --> 18 with conflicts
		from <j-W-EXACT> to <j-W-EXACT>
	18 --> 18 with conflicts
		from <j-W-EXACT> to <j-W-EXACT>
	19 --> 18 with conflicts
		from <j-R-EXACT> to <j-W-EXACT>
	19 --> 19 with conflicts
		from <b[i][j]-W-EXACT> to <b[i][j]-W-EXACT>

