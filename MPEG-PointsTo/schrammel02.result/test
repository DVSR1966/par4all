
PROPER  EFFECTS 

int foo()
{
//             <    is written>: x
   int x = 3;
//             <    is read   >: x
   return x*x*x/54;
}
void main()
{
   int x1, y, x2;
//             <    is read   >: x1 x2
   0<=x1&&x1<=x2&&x2<=1?(void) 0:__assert_fail("0<=x1 && x1<=x2 && x2<=1", "$VDIR/schrammel02.c", 8, (const char *) 0);
//             <    is written>: y
   y = foo();
//             <    is read   >: x1 x2 y
   while (x1+x2<=4&&1<=y&&y<=2) {
//             <    is read   >: x1 y
//             <    is written>: x1
      x1 = x1+2*y-1;
//             <    is read   >: x2 y
//             <    is written>: x2
      x2 = x2+y;
//             <    is written>: y
      y = foo();
   }
//             <    is read   >: y
//             <    is written>: y
   // Expected result: 0<=x1, x2<=x1+1, x1+x2<=9, 4x2<=2x1+9, 2x1<=3x2
   y = y;
}

