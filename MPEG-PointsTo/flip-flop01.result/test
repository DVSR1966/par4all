
PROPER  EFFECTS 

int main()
{
   int s[2][10];
   int i, j;
   int cur, next;
//             <    is written>: cur
   cur = 0;
//             <    is written>: next
   next = 1;
//             <    is written>: i
   for(i = 1; i <= 9; i += 1) {
//             <    is written>: j
      for(j = 1; j <= 9; j += 1) {
//             <    is read   >: cur j next s[next][j]
//             <    is written>: s[cur][j]
         s[cur][j] = s[next][j]+j;
//             <    is read   >: cur j next s[cur][j]
//             <    is written>: s[next][j]
         s[next][j] = s[cur][j];
      }
//             <    is read   >: next
//             <    is written>: cur
      cur = next;
//             <    is read   >: next
//             <    is written>: next
      next = 1-next;
   }
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int s[2][10];

// Points To: none

   int i, j;

// Points To: none

   int cur, next;

// Points To: none

   cur = 0;

// Points To: none

   next = 1;

// Points To: none

   for(i = 1; i <= 9; i += 1) {

// Points To: none

      for(j = 1; j <= 9; j += 1) {

// Points To: none

         s[cur][j] = s[next][j]+j;

// Points To: none

         s[next][j] = s[cur][j];
      }

// Points To: none

      cur = next;

// Points To: none

      next = 1-next;
   }

// Points To: none

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int s[2][10];
   int i, j;
   int cur, next;
//             <    is written>: cur
   cur = 0;
//             <    is written>: next
   next = 1;
//             <    is written>: i
   for(i = 1; i <= 9; i += 1) {
//             <    is written>: j
      for(j = 1; j <= 9; j += 1) {
//             <    is read   >: cur j next s[next][j]
//             <    is written>: s[cur][j]
         s[cur][j] = s[next][j]+j;
//             <    is read   >: cur j next s[cur][j]
//             <    is written>: s[next][j]
         s[next][j] = s[cur][j];
      }
//             <    is read   >: next
//             <    is written>: cur
      cur = next;
//             <    is read   >: next
//             <    is written>: next
      next = 1-next;
   }
   return 0;
}

