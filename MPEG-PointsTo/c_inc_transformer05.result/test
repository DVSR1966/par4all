
PROPER  EFFECTS 

int main(int j, char **unused)
{
   double A[10];
//             <    is written>: k
   int k = 2;
//             <    is read   >: k
//             <    is written>: A[1] k

   A[1] = (double) k++;
//             <    is read   >: j k
   return j+k;
}

POINTS-TO


// Points To IN:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT


// Points To OUT:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

int main(int j, char **unused)
{

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

   double A[10];

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

   int k = 2;

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT


   A[1] = (double) k++;

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

   return j+k;
}

PROPER  EFFECTS WITH POINTS TO

int main(int j, char **unused)
{
   double A[10];
//             <    is written>: k
   int k = 2;
//             <    is read   >: k
//             <    is written>: A[1] k

   A[1] = (double) k++;
//             <    is read   >: j k
   return j+k;
}

 *********************** Use-Def Chains *********************

	06 --> 08 with conflicts
		from <k-W-EXACT> to <k-W-EXACT>
		from <k-W-EXACT> to <k-R-EXACT>
	08 --> 09 with conflicts
		from <k-W-EXACT> to <k-R-EXACT>

****************** End of Dependence Graph ******************
