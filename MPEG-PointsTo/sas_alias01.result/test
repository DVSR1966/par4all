
PROPER  EFFECTS 

float alea(void)
{
   return 1.;
}
void sas_alias01(int m)
{
//             <    is read   >: m
//             <    is written>: x y
   int x = m, y = 0;
//             <    is read   >: x y
   while (x>=0&&y>=0) {
      if (alea()>=0.) {
//             <    is read   >: m y
         while (y<=m&&alea()>=0.)
//             <    is read   >: y
//             <    is written>: y
            y++;
//             <    is read   >: x
//             <    is written>: x
         x--;
      }
//             <    is read   >: y
//             <    is written>: y
      y--;
   }
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

float alea(void)
{

// Points To: none

   return 1.;
}

// Points To IN: none


// Points To OUT: none

void sas_alias01(int m)
{

// Points To: none

   int x = m, y = 0;

// Points To: none

   while (x>=0&&y>=0) {

// Points To: none

      if (alea()>=0.) {

// Points To: none

         while (y<=m&&alea()>=0.)

// Points To: none

            y++;

// Points To: none

         x--;
      }

// Points To: none

      y--;
   }
}

PROPER  EFFECTS WITH POINTS TO

float alea(void)
{
   return 1.;
}
void sas_alias01(int m)
{
//             <    is read   >: m
//             <    is written>: x y
   int x = m, y = 0;
//             <    is read   >: x y
   while (x>=0&&y>=0) {
      if (alea()>=0.) {
//             <    is read   >: m y
         while (y<=m&&alea()>=0.)
//             <    is read   >: y
//             <    is written>: y
            y++;
//             <    is read   >: x
//             <    is written>: x
         x--;
      }
//             <    is read   >: y
//             <    is written>: y
      y--;
   }
}

