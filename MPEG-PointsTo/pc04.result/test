
PROPER  EFFECTS 

int alea(void)
{
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   return rand()%2;
}
int main()
{
//             <    is written>: s0 s1 s2
   int s0 = 0, s1 = 1, s2 = 0;
   //int c0 = 1, c1 = 0, c2 = 0;
   
   while (1) {
//             <    is read   >: s0 _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
      if (s0==1&&alea()) {
//             <    is written>: s0
         s0 = 0;
//             <    is written>: s1
         s1 = 1;
      }
//             <    is read   >: s1 _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
      if (s1==1&&alea()) {
//             <    is written>: s1
         s1 = 0;
//             <    is written>: s2
         s2 = 1;
      }
//             <    is read   >: s2 _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
      if (s2==1&&alea()) {
//             <    is written>: s2
         s2 = 0;
//             <    is written>: s0
         s0 = 1;
      }
   }
   return 0;
}

