
PROPER  EFFECTS 

int main()
{
//             <    is written>: i j
   int i = 0, j = 0, n;
//             <    is read   >: n

   if (n<0)
      exit(1);
//             <    is read   >: i n

   while (i<=n)
//             <    is read   >: i j
//             <    is written>: i j
      i++, j += i;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

