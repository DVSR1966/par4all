// Points To: none


   if (n<0)

// Points To: none

      exit(1);

// Points To: none


   while (i<=n)

// Points To: none

      i++, j += i;

// Points To: none


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i j
   int i = 0, j = 0, n;
//             <    is read   >: n

   if (n<0)
      exit(1);
//             <    is read   >: i n

   while (i<=n)
//             <    is read   >: i j
//             <    is written>: i j
      i++, j += i;

   return 0;
}

 *********************** Use-Def Chains *********************

	-1 --> -1 with conflicts
		from <j-W-EXACT> to <j-W-EXACT>
		from <i-W-EXACT> to <i-W-EXACT>
		from <j-W-EXACT> to <j-R-EXACT>
		from <i-W-EXACT> to <i-R-EXACT>
		from <j-R-EXACT> to <j-W-EXACT>
		from <i-R-EXACT> to <i-W-EXACT>
	05 --> -1 with conflicts
		from <j-W-EXACT> to <j-W-EXACT>
		from <i-W-EXACT> to <i-W-EXACT>
		from <j-W-EXACT> to <j-R-EXACT>
		from <i-W-EXACT> to <i-R-EXACT>
	05 --> 09 with conflicts
		from <i-W-EXACT> to <i-R-EXACT>
	09 --> -1 with conflicts
		from <i-R-EXACT> to <i-W-EXACT>

