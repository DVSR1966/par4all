int main()
{

// Points To: none

   int s0 = 0, s1 = 1, s2 = 0;

// Points To: none

   //int c0 = 1, c1 = 0, c2 = 0;
   
   while (1) {

// Points To: none

      if (s0==1) {

// Points To: none

         s0 = 0;

// Points To: none

         s1 = 1;
      }

// Points To: none

      if (s1==1) {

// Points To: none

         s1 = 0;

// Points To: none

         s2 = 1;
      }

// Points To: none

      if (s2==1) {

// Points To: none

         s2 = 0;

// Points To: none

         s0 = 1;
      }
   }

// Points To: none

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: s0 s1 s2
   int s0 = 0, s1 = 1, s2 = 0;
   //int c0 = 1, c1 = 0, c2 = 0;
   
   while (1) {
//             <    is read   >: s0
      if (s0==1) {
//             <    is written>: s0
         s0 = 0;
//             <    is written>: s1
         s1 = 1;
      }
//             <    is read   >: s1
      if (s1==1) {
//             <    is written>: s1
         s1 = 0;
//             <    is written>: s2
         s2 = 1;
      }
//             <    is read   >: s2
      if (s2==1) {
//             <    is written>: s2
         s2 = 0;
//             <    is written>: s0
         s0 = 1;
      }
   }
   return 0;
}

