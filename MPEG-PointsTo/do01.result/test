PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 0;
   int a[20];
   int n;
   int m;
//             <    is read   >: m n
//             <    is written>: k
   int k = m<=n?m:n;
//             <may be read   >: stdin[0]
//             <may be written>: n stdin[0]
//             <    is read   >: stdin __IO_EFFECTS:LUNS[0]
//             <    is written>: __IO_EFFECTS:LUNS[0]

   scanf("%d", &n);
//             <may be read   >: stdin[0]
//             <may be written>: m stdin[0]
//             <    is read   >: stdin __IO_EFFECTS:LUNS[0]
//             <    is written>: __IO_EFFECTS:LUNS[0]
   scanf("%d", &m);
//             <    is read   >: m n
//             <    is written>: i

   for(i = 0; i <= n<=m?n:m; i += 1)
//             <    is read   >: i m n
//             <    is written>: a[i]
      a[i] = i;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", i);
}

