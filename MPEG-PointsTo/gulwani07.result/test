
PROPER  EFFECTS 

int main()
{

   int x, y, z;
//             <    is written>: x

   x = 0;
//             <    is written>: y
   y = 50;
//             <    is read   >: x

   while (x<100) {
//             <    is read   >: x
      while (x<50)
//             <    is read   >: x
//             <    is written>: x
         x++;
//             <    is read   >: x
      while (x<100&&x>=50) {
//             <    is read   >: x
//             <    is written>: x
         x++;
//             <    is read   >: y
//             <    is written>: y
         y++;
      }
   }
//             <    is read   >: x y

   if (x==100&&y==100)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("property  verified\n");
}

