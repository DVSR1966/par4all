// Points To IN: none


// Points To OUT: none

int main(void)
{

// Points To: none

   int x = 0;

// Points To: none

   int y = 0;

// Points To: none

   while (y>=0) {

// Points To: none

      fprintf(stdout, "loop: x=%d y=%d\n", x, y);

// Points To: none

      if (x<=50)

// Points To: none

         y++;
      else

// Points To: none

         y--;

// Points To: none

      x++;
   }

// Points To: none

   fprintf(stdout, "end: x=%d y=%d\n", x, y);

// Points To: none

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main(void)
{
//             <    is written>: x
   int x = 0;
//             <    is written>: y
   int y = 0;
//             <    is read   >: y
   while (y>=0) {
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout x y __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      fprintf(stdout, "loop: x=%d y=%d\n", x, y);
//             <    is read   >: x
      if (x<=50)
//             <    is read   >: y
//             <    is written>: y
         y++;
      else
//             <    is read   >: y
//             <    is written>: y
         y--;
//             <    is read   >: x
//             <    is written>: x
      x++;
   }
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout x y __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   fprintf(stdout, "end: x=%d y=%d\n", x, y);
   return 0;
}

