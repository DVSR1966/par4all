
PROPER  EFFECTS 

int main(int j, char **unused)
{
//             <    is written>: i
   int i = 0;
   int A[1];
//             <    is read   >: j
//             <    is written>: A[1] j

   A[1] = j++;
//             <    is read   >: j
//             <    is written>: j
   j++;
//             <    is read   >: j
//             <    is written>: j
   j = j+1;
//             <    is read   >: j
//             <    is written>: i j
   i = j++;
}

