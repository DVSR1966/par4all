int main(int j, char **unused)
{

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

   int i = 0;

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

   int A[1];

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT


   A[1] = j++;

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

   j++;

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

   j = j+1;

// Points To:
// _unused_2[0] -> __unused_2_2[0] , EXACT
// unused -> _unused_2[0] , EXACT

   i = j++;
}

PROPER  EFFECTS WITH POINTS TO

int main(int j, char **unused)
{
//             <    is written>: i
   int i = 0;
   int A[1];
//             <    is read   >: j
//             <    is written>: A[1] j

   A[1] = j++;
//             <    is read   >: j
//             <    is written>: j
   j++;
//             <    is read   >: j
//             <    is written>: j
   j = j+1;
//             <    is read   >: j
//             <    is written>: i j
   i = j++;
}

