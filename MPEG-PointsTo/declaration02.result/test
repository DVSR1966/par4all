
PROPER  EFFECTS 

int foo(int i)
{
//             <    is read   >: i
   return i+10;
}
int main(int argc, char **argv)
{
   long long int size[argc++][foo(argc)];
//             <    is read   >: size[0][0]
   return (int) size[0][0];
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int foo(int i)
{

// Points To: none

   return i+10;
}

// Points To IN:
// _argv_2[0] -> __argv_2_2[0] , EXACT
// argv -> _argv_2[0] , EXACT


// Points To OUT:
// _argv_2[0] -> __argv_2_2[0] , EXACT
// argv -> _argv_2[0] , EXACT

int main(int argc, char **argv)
{

// Points To:
// _argv_2[0] -> __argv_2_2[0] , EXACT
// argv -> _argv_2[0] , EXACT

   long long int size[argc++][foo(argc)];

// Points To:
// _argv_2[0] -> __argv_2_2[0] , EXACT
// argv -> _argv_2[0] , EXACT

   return (int) size[0][0];
}

PROPER  EFFECTS WITH POINTS TO

int foo(int i)
{
//             <    is read   >: i
   return i+10;
}
int main(int argc, char **argv)
{
   long long int size[argc++][foo(argc)];
//             <    is read   >: size[0][0]
   return (int) size[0][0];
}

