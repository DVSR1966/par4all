// Points To OUT: none

int main()
{

// Points To: none

   int r1, r2, r3, r4;
   //int a=3, b=2;

// Points To: none

   int a = 3, b = 2, c = 0;

// Points To: none


   r1 = a%b;

// Points To: none

   r2 = -a%b;

// Points To: none

   r3 = a%(-b);

// Points To: none

   r4 = -a%(-b);

// Points To: none

   
   // Expected result: r1=1, r2=-1, r3=1, r4=-1
   printf("r1=%d, r2=%d, r3=%d, r4=%d\n", r1, r2, r3, r4);

// Points To: none


   r1 = c%b;

// Points To: none

   // This causes a zero divide
   r2 = a%c;

// Points To: none

   
   // Expected result: r1=1, r2=-1
   printf("r1=%d, r2=%d\n", r1, r2);

// Points To: none


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int r1, r2, r3, r4;
   //int a=3, b=2;
//             <    is written>: a b c
   int a = 3, b = 2, c = 0;
//             <    is read   >: a b
//             <    is written>: r1

   r1 = a%b;
//             <    is read   >: a b
//             <    is written>: r2
   r2 = -a%b;
//             <    is read   >: a b
//             <    is written>: r3
   r3 = a%(-b);
//             <    is read   >: a b
//             <    is written>: r4
   r4 = -a%(-b);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: r1 r2 r3 r4 stdout 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   
   // Expected result: r1=1, r2=-1, r3=1, r4=-1
   printf("r1=%d, r2=%d, r3=%d, r4=%d\n", r1, r2, r3, r4);
//             <    is read   >: b c
//             <    is written>: r1

   r1 = c%b;
//             <    is read   >: a c
//             <    is written>: r2
   // This causes a zero divide
   r2 = a%c;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: r1 r2 stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   
   // Expected result: r1=1, r2=-1
   printf("r1=%d, r2=%d\n", r1, r2);

   return 0;
}

