
PROPER  EFFECTS 

float alea(void)
{
   return 1.;
}
void sas_alias03(int m)
{
//             <    is read   >: m
//             <    is written>: x y
   int x = m, y = m;
//             <    is read   >: x
   while (x>=2)
      if (1) {
//             <    is read   >: x y
//             <    is written>: x y
         x--, y += x;
//             <    is read   >: x y

         while (y>=x+1&&alea()>=0.)
            if (1) {
//             <    is read   >: y
//             <    is written>: y
               y--;
//             <    is read   >: x y
               while (y>=x+3&&alea()>=0.)
//             <    is read   >: x y
//             <    is written>: x y
                  x++, y -= 2;
//             <    is read   >: y
//             <    is written>: y

               y--;
            }
//             <    is read   >: x y
//             <    is written>: x y
         x--, y -= x;
      }
}

