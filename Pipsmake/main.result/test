"%MAIN before parsing"
      PROGRAM MONMAIN
      PRINT *, 'monmain'                                                0001
      CALL HELLO                                                        0002
      END
"%ALL"
      SUBROUTINE HELLO
      PRINT *, 'hello'                                                  0001
      END
      PROGRAM MONMAIN
      PRINT *, 'monmain'                                                0001
      CALL HELLO                                                        0002
      END
"%MAIN after parsing"
      PROGRAM MONMAIN
      PRINT *, 'monmain'                                                0001
      CALL HELLO                                                        0002
      END

