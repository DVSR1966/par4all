C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
C               <must be read   >: X Y

      SUBROUTINE B(X, Y)
C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
C               <must be read   >: X Y

      PRINT *, X+Y                                                      0001

      END
