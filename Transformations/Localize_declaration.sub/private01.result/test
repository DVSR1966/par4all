int main()
{
   int i, j;
   int tmp;
   int a[10][10];
   {
      //PIPS generated variable
      int i;
      
      // tmp must be declared as private for this loop
      // even if it belongs to the loop locals of the inner loop which is not parallel
      for(i = 0; i <= 9; i += 1) {
         //PIPS generated variable
         int j1, tmp1;
         a[i][0] = i;
         {
            //PIPS generated variable
            int j;
            for(j1 = 1; j1 <= 9; j1 += 1) {
               //PIPS generated variable
               int tmp;
               tmp1 = a[i][j1-1];
               a[i][j1] = tmp1+j1;
            }
         }
      }
   }
   return 0;
}
