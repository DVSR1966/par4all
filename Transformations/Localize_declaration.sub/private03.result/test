int main()
{
   int i, j;
   int tmp;
   int a[10][10];
   {
      //PIPS generated variable
      int i;
      
      // tmp must be declared as private only in the second loop
      for(i = 0; i <= 9; i += 1) {
         //PIPS generated variable
         int j, j, tmp;
         for(j = 0; j <= 9; j += 1) {
            //PIPS generated variable
            int tmp;
            tmp = a[i][j];
            a[i][j] = tmp+j;
         }
      }
   }
   return 0;
}
