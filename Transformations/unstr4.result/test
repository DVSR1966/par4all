
C  P() {}

      PROGRAM UNSTR4

C  P() {}


c     test of entry and exit node extraction by Ronan's restructurer
c     plus unstructured decomposition

      J = 2                                                             0001

C  P(J) {J==2}


100   CONTINUE                                                          0002

C  P(J) {J==2}

      PRINT *, J                                                        0003

C  P(J) {J==2}


      J = 2                                                             0004

C  P(J) {J==2}


200   CONTINUE                                                          0005

C  P(J) {J==2}

      J = 3                                                             0006

C  P(J) {J==3}

      PRINT *, J                                                        0007

C  P(J) {J==3}

      IF (J.LT.3) GOTO 200                                              0008

C  P(J) {J==3}


      IF (J.LT.2) GOTO 100                                              0010

C  P(J) {J==3}


      CONTINUE                                                          0012

C  P(J) {J==3}

      PRINT *, J                                                        0012

C  P(J) {J==3}


      RETURN
      END

C  P() {}

