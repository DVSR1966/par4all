void foo(int i)
{
   i = i+1;                                                             /*0004*/
   return;                                                              /*0005*/
}
void inlining20(int j)
{
   foo(2);                                                              /*0004*/
   foo(j);                                                              /*0005*/
}
void inlining20(int j)
{
   {
      int i_ = 2;
      i_ = i_+1;                                                        /*0005*/
      /*0004*/
      /*0005*/
   }
   {
      int i_ = j;
      i_ = i_+1;                                                        /*0011*/
      /*0004*/
      /*0005*/
   }
}
