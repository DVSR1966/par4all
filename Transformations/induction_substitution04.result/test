
Initial code for "induction04"

void induction04(double A[SIZE], double B[SIZE][SIZE])
{
   int i, j;
   int k = -1;
   for(i = 0; i <= SIZE-1; i += 1) {
      k = i;
      for(j = 0; j <= SIZE-1; j += 1)
         A[k++] = B[j-k][k]+A[k];
   }
}

Transformers for "induction04"


//  T() {}

void induction04(double A[SIZE], double B[SIZE][SIZE])
{

//  T(i,j) {}

   int i, j;

//  T(k) {k==-1}

   int k = -1;

//  T(i,j,k) {0<=i}

   for(i = 0; i <= SIZE-1; i += 1) {

//  T(k) {i==k}

      k = i;

//  T(j,k) {j+k#init==k, k#init<=k}

      for(j = 0; j <= SIZE-1; j += 1)

//  T(k) {k==k#init+1}

         A[k++] = B[j-k][k]+A[k];
   }
}

Preconditions for "induction04"


//  P() {SIZE==10}

void induction04(double A[SIZE], double B[SIZE][SIZE])
{

//  P() {SIZE==10}

   int i, j;

//  P(i,j) {SIZE==10}

   int k = -1;

//  P(i,j,k) {SIZE==10, k==-1}

   for(i = 0; i <= SIZE-1; i += 1) {

//  P(i,j,k) {SIZE==10, 0<=i, i<=9}

      k = i;

//  P(i,j,k) {SIZE==10, i==k, 0<=i, i<=9}

      for(j = 0; j <= SIZE-1; j += 1)

//  P(i,j,k) {SIZE==10, i+j==k, 0<=j, j<=9, k<=j+9, j<=k}

         A[k++] = B[j-k][k]+A[k];
   }
}

Code for "induction04" after induction substitution


//  P() {SIZE==10}

void induction04(double A[SIZE], double B[SIZE][SIZE])
{

//  P() {SIZE==10}

   int i, j;

//  P(i,j) {SIZE==10}

   int k = -1;

//  P(i,j,k) {SIZE==10, k==-1}

   for(i = 0; i <= SIZE-1; i += 1) {

//  P(i,j,k) {SIZE==10, 0<=i, i<=9}

      k = i;

//  P(i,j,k) {SIZE==10, i==k, 0<=i, i<=9}

      for(j = 0; j <= SIZE-1; j += 1)

//  P(i,j,k) {SIZE==10, 0<=i, i<=9, 0<=j, j<=9}

         A[k = -((-i)+(-j))+1, k-1] = B[j-k][k]+A[k];
   }
}
