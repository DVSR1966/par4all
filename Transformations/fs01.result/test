! forward substitution example 01
!
      PROGRAM FS01

      REAL X, Y, Z, W, U, V

      X = 0.2226473                                                     0001
      Y = X+X*X                                                         0002
      Z = Y+X                                                           0003
      W = Z*Z*Z+Y*X                                                     0004

      PRINT *, 'w = ', W                                                0005

      U = 0.3236433                                                     0006
      V = U+2*U                                                         0007

      END
