raw file with loop flagged and privated variables scanned
pop(int op)
{
   int i, j;
#pragma omp DO private(i,j)
l99999:
   for(i = i; i <= op-1; i += 1) {
      private(i,j)
      j = i*2;
      printf("%d", j);
   }
}
after loop expansion
pop(int op)
{
   int i, j;
   //PIPS generated variable
   int I_0;
   // PIPS test to adjust loop size to required loop expansion parameter
   if (MOD(-i+op, 128)==0)
      I_0 = op-1;
   else
      I_0 = op-1+128-MOD(-i+op, 128);
#pragma omp DO private(i,j)
l99999:
   for(i = i; i <= I_0; i += 1) {
      private(i,j)
      j = i*2;
      if (i<=op-1) 
         printf("%d", j);
   }
}
