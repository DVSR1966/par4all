
C  P() {}

      PROGRAM UNSTR3

C  P() {}


c     test of dead code elimination

      J = 2                                                             0001

C  P(J) {J==2}


100   CONTINUE                                                          0002

C  P(J) {J==2}

      PRINT *, J                                                        0003

C  P(J) {J==2}

      IF (J.LT.2) GOTO 100                                              0004

C  P(J) {J==2}



200   CONTINUE                                                          0009

C  P(J) {J==2}


      RETURN
      END

C  P() {}

