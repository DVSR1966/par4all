char * foo(void)
{
   const char *foo_fun = __FUNCTION__;
   return (char *) foo_fun;                                             /*0005*/
}
extern char *foo(void);
extern int main(void);
int main(void)
{
   const char *fun;
   const char *fun2;
   const char *fun3;
   fun = foo();                                                         /*0005*/
   fun2 = __FUNCTION__;                                                 /*0006*/
   fun3 = __func__;                                                     /*0007*/
   return 0;                                                            /*0008*/
}
int main(void)
{
   char *P_0;
   const char *fun;
   const char *fun2;
   const char *fun3;
   {
      const char *foo_fun = __FUNCTION__;
      P_0 = (char *) foo_fun;                                           /*0009*/
   }
   fun = P_0;                                                           /*0012*/
   fun2 = __FUNCTION__;                                                 /*0013*/
   /*0006*/
   fun3 = __func__;                                                     /*0014*/
   /*0007*/
   return 0;                                                            /*0015*/
}
