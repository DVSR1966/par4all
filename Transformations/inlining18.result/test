char * foo(void)
{
   const char *foo_fun = __FUNCTION__;
   return (char *) foo_fun;                                             /*0005*/
}
int main(void)
{
   char *fun = foo();
   fprintf(stdout, "fun: %s\n", fun);                                   /*0005*/
   fprintf(stdout, "function: %s\n", __FUNCTION__);                     /*0006*/
   return 0;                                                            /*0007*/
}
int main(void)
{
   char *fun = foo();
   fprintf(stdout, "fun: %s\n", fun);                                   /*0004*/
   /*0005*/
   fprintf(stdout, "function: %s\n", __FUNCTION__);                     /*0005*/
   /*0006*/
   return 0;                                                            /*0006*/
}
