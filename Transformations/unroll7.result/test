      PROGRAM UNROLL7
C
      PARAMETER (N=4)
      REAL T(N,N)
Cxxx
      DO 10 I = 1, N                                                    0001
Cxxx
         DO 10 J = 1, N                                                 0002
            T(I,J) = -N*I+J                                             0003
10          CONTINUE                                                    0004
C
      END
      PROGRAM UNROLL7
C
      PARAMETER (N=4)
      REAL T(N,N)
Cxxx
Cxxx
      T(1,1) = -N*1+1                                                   0003
      T(1,2) = -N*1+2                                                   0003
      T(1,3) = -N*1+3                                                   0003
      T(1,4) = -N*1+4                                                   0003
      J = 5
Cxxx
      T(2,1) = -N*2+1                                                   0003
      T(2,2) = -N*2+2                                                   0003
      T(2,3) = -N*2+3                                                   0003
      T(2,4) = -N*2+4                                                   0003
      J = 5
Cxxx
      T(3,1) = -N*3+1                                                   0003
      T(3,2) = -N*3+2                                                   0003
      T(3,3) = -N*3+3                                                   0003
      T(3,4) = -N*3+4                                                   0003
      J = 5
Cxxx
      T(4,1) = -N*4+1                                                   0003
      T(4,2) = -N*4+2                                                   0003
      T(4,3) = -N*4+3                                                   0003
      T(4,4) = -N*4+4                                                   0003
      J = 5
      I = 5
C
      END
