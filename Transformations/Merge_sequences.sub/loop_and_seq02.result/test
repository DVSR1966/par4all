int main()
{
   int i, j;
   for(i = 0; i <= 9; i += 1) {
      int k = i;
      {
         int l = 2;
         {
            int k = 3;
            l = l+k;
         }
         k = k+l;
      }
      j = j+k;
      {
         int l = 4;
         j = j+l;
      }
   }
   return j;
}
#
# Code after merge_sequences for module main
#
int main()
{
   int i, j;
   for(i = 0; i <= 9; i += 1) {
      int k = i;
      int l_0 = 2;
      int k_0 = 3;
      l_0 = l_0+k_0;
      k = k+l_0;
      j = j+k;
      int l = 4;
      j = j+l;
   }
   return j;
}
