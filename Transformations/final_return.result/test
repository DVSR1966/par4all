      SUBROUTINE FOO
      PRINT *, 'live'                                                   0001
      STOP                                                              0002
      RETURN
      END
c     Should not fade away the final return
      SUBROUTINE FOO
      PRINT *, 'live'                                                   0001
      STOP                                                              0002
      CONTINUE
      END
