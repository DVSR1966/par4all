### initial code
void until05()
{
   // BEGIN BLOCK
   int old, new;
   do {
      // BEGIN BLOCK
      old = new;
      foo(&new);
      // END BLOCK
   }
   while (old!=new);
   printf("%d", old);
   // END BLOCK
}
### Preconditions

//  P() {}

void until05()
{

//  P() {}

   // BEGIN BLOCK

//  P() {}

   int old, new;

//  P(new,old) {}

   do {

//  P(new,old) {}

      // BEGIN BLOCK

//  P(new,old) {}

      old = new;

//  P(new,old) {new==old}

      foo(&new);
      // END BLOCK
   }
   while (old!=new);

//  P(new,old) {new==old}

   printf("%d", old);
   // END BLOCK
}
### suppress dead code
void until05()
{
   // BEGIN BLOCK
   int old, new;
   do {
      // BEGIN BLOCK
      old = new;
      foo(&new);
      // END BLOCK
   }
   while (old!=new);
   printf("%d", old);
   // END BLOCK
}

