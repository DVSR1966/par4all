 main
     foo
         bla

before inlining

int foo(void)
{
   bla(kernel);                                                         /*0004*/
   return 0;                                                            /*0005*/
}
int main(void)
{
   foo();                                                               /*0003*/
   return 0;                                                            /*0004*/
}

after inlining

int main(void)
{
   int _return0;
   {
      extern void bla(int *f1);
      extern const  int kernel[3];
      bla(kernel);                                                      /*0007*/
      /*0004*/
      _return0 = 0;                                                     /*0008*/
   }
   _return0;                                                            /*0011*/
   return 0;                                                            /*0012*/
}

