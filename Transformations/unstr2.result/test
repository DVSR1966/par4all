
C  P() {}

      PROGRAM UNSTR2

C  P() {}


c     test of sequence restructuration by Ronan Keryell

      J = 2                                                             0001

C  P(J) {J==2}

100   CONTINUE                                                          0004

C  P(J) {J==2}

      PRINT *, J                                                        0005

C  P(J) {J==2}

200   CONTINUE                                                          0006

C  P(J) {J==2}

300   CONTINUE                                                          0002

C  P(J) {J==2}

      J = 3                                                             0003

C  P(J) {J==3}

400   CONTINUE                                                          0007

C  P(J) {J==3}

      PRINT *, J                                                        0008

C  P(J) {J==3}


      RETURN
      END

C  P() {}

