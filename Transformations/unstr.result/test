      PROGRAM UNSTR

C  P() {}


c     test of entry and exit node extraction by Ronan's restructurer

      J = 2                                                             0001

C  P(J) {J==2}


100   CONTINUE                                                          0002

C  P(J) {J==2}

      PRINT *, J                                                        0003

C  P(J) {J==2}

      IF (J.LT.2) GOTO 100                                              0004

C  P(J) {J==2}


      CONTINUE                                                          0006

C  P(J) {J==2}

      PRINT *, J                                                        0006

C  P(J) {J==2}


      RETURN
      END

