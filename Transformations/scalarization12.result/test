
Code after scalarization

int main(int argc, char **argv)
{
   // BEGIN BLOCK
   int d;
   int i;
   int n = 100;
   int x[n];
   int y[n];
   int t[n];

   t[0] = 0;
   for(i = 1; i <= n-1; i += 1) {
      // BEGIN BLOCK
      t[i] = x[i];
      x[i] = y[i];
      y[i] = t[i-1];
      // END BLOCK
   }
   // END BLOCK
}
