int use_def_elim13c()
{
//             <    is written>: r
   int r = 0;                                                           /*0006*/
//             <may be written>: r

   if (1)                                                               /*0008*/
//             <    is written>: r
      r = 1;                                                            /*0009*/
//             <may be written>: r
   if (0)                                                               /*0010*/
//             <    is written>: r
      r = 0;                                                            /*0011*/
//             <    is read   >: r

   return r;                                                            /*0013*/
}

