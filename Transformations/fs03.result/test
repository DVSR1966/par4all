!! Initial file
!
! forward substitution example 
!
      PROGRAM FS03

      INTEGER N, I
      PARAMETER (N=100)

      REAL A(N), X

      DO I = 1, N                                                       0001
         X = A(I)*A(I)                                                  0002
         A(I) = X+X                                                     0003
      ENDDO

      X = A(4)                                                          0004
      A(I-3) = X+X                                                      0005
      A(I-2) = X-X                                                      0006

      PRINT *, A(5)                                                     0007

      END
!! After forward substitution
!
! forward substitution example 
!
      PROGRAM FS03

      INTEGER N, I
      PARAMETER (N=100)

      REAL A(N), X

      DO I = 1, N                                                       0001
         X = A(I)*A(I)                                                  0002
         A(I) = A(I)*A(I)+A(I)*A(I)                                     0003
      ENDDO

      X = A(4)                                                          0004
      A(I-3) = A(4)+A(4)                                                0005
      A(I-2) = X-X                                                      0006

      PRINT *, A(5)                                                     0007

      END
!! After use-def elimination
!
! forward substitution example 
!
      PROGRAM FS03

      INTEGER N, I
      PARAMETER (N=100)

      REAL A(N), X

      DO I = 1, N                                                       0001
         A(I) = A(I)*A(I)+A(I)*A(I)                                     0003
      ENDDO

      X = A(4)                                                          0004
      A(I-3) = A(4)+A(4)                                                0005
      A(I-2) = X-X                                                      0006

      PRINT *, A(5)                                                     0007

      END
!! After cleaning declarations
      PROGRAM FS03
      INTEGER*4 N
      PARAMETER (N = 100)
      INTEGER I
      REAL*4 A(1:N), X

      DO I = 1, N                                                       0001
         A(I) = A(I)*A(I)+A(I)*A(I)                                     0003
      ENDDO

      X = A(4)                                                          0004
      A(I-3) = A(4)+A(4)                                                0005
      A(I-2) = X-X                                                      0006

      PRINT *, A(5)                                                     0007

      END
