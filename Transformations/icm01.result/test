
Initial code

int icm01()
{
   // BEGIN BLOCK
   int i;
   int j = 3;
   int k;
   int a[10];

   for(i = 0; i <= 9; i += 1) {                                         /*0009*/
      // BEGIN BLOCK
      k = j*j+1;                                                        /*0010*/
      a[i] = k;                                                         /*0011*/
      // END BLOCK
   }
   // END BLOCK
}

Code after Invariant Code Motion

int icm01()
{
   // BEGIN BLOCK
   int i;
   int j = 3;
   int k;
   int a[10];
   int I_0;


   I_0 = j*j+1;
   for(i = 0; i <= 9; i += 1) {
      // BEGIN BLOCK
      k = I_0;                                                          /*0010*/
      a[i] = k;                                                         /*0011*/
      // END BLOCK
   }
   // END BLOCK
}
