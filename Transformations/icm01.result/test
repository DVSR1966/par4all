
Initial code

int icm01()
{
   // BEGIN BLOCK
   int i;                                                               /*0004*/
   int j = 3;                                                           /*0005*/
   int k;                                                               /*0006*/
   int a[10];                                                           /*0007*/

   for(i = 0; i <= 9; i += 1) {
      // BEGIN BLOCK
      k = j*j+1;                                                        /*0010*/
      a[i] = k;                                                         /*0011*/
      // END BLOCK
   }
   // END BLOCK
}

Code after Invariant Code Motion

int icm01()
{
   // BEGIN BLOCK
   int i;                                                               /*0004*/
   int j = 3;                                                           /*0005*/
   int k;                                                               /*0006*/
   int a[10];                                                           /*0007*/
   //PIPS generated variable
   int I_0;

   I_0 = j*j+1;
   for(i = 0; i <= 9; i += 1) {
      // BEGIN BLOCK
      k = I_0;                                                          /*0010*/
      a[i] = k;                                                         /*0011*/
      // END BLOCK
   }
   // END BLOCK
}
