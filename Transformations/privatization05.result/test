C     Check that j is privatized

      SUBROUTINE PRIVATIZATION05
      INTEGER A(10, 10)


!$OMP DOPRIVATE(I,J,K)
      DO I = 1, 10                                                      0006

!$OMP    DOPRIVATE(J,K)
         DO K = 1, 10                                                   0007
         PRIVATE(I,J,K)
            PRIVATE(J,K)
            J = 0                                                       0008
            J = J+1                                                     0009
            A(I,K) = J                                                  0010
         ENDDO
      ENDDO
      END
