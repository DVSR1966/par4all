int compute(int n)
{
   int i = 1;
   while (i<n) {
      i <<= 1;
      if (rand())
         i++;
   }
   return i;
}
int compute(int n)
{
   int i = 1;
   //PIPS generated variable
   int _if_then_0 = 0, _if_else_0 = 0, _while_0 = 0;
   while (i<n) {
      _while_0 = _while_0+1;
      i <<= 1;
      if (rand()) {
         _if_then_0 = _if_then_0+1;
         i++;
      }
      else
         _if_else_0 = _if_else_0+1;
   }
   return i;
}

