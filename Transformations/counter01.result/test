int compute(int n)
{
   int i = 1;
   while (i<n) {
      i <<= 1;
      if (rand())
         i++;
   }
   return i;
}
int compute(int n)
{
   int i = 1;
   //PIPS generated variable
   int _if_then_00 = 0, _if_else_10 = 0, _while_20 = 0;
   while (i<n) {
      _while_20 = _while_20+1;
      i <<= 1;
      if (rand()) {
         _if_then_00 = _if_then_00+1;
         i++;
      }
      else
         _if_else_10 = _if_else_10+1;
   }
   return i;
}

