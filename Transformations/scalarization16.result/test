      for(j = 0; j <= n-1; j += 1) {
         // BEGIN BLOCK
         __scalar__1 = __scalar__0^2;
         __scalar__1 = __scalar__1+__scalar__0+j;
         y[i][j] = __scalar__1;
         // END BLOCK
