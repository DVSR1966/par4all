
Code after scalarization

int scalarization16(int n)
{
   // BEGIN BLOCK
   int __ld__1;
   int __ld__0;
   int x[n];
   int y[n][n];
   int i;
   int j;

   for(i = 0; i <= n-1; i += 1) {
      // BEGIN BLOCK
      __ld__0 = i;
      for(j = 0; j <= n-1; j += 1) {
         // BEGIN BLOCK
         __ld__1 = __ld__0^2;
         __ld__1 = __ld__1+__ld__0+j;
         y[i][j] = __ld__1;
         // END BLOCK
      }
      // END BLOCK
   }
   return y[n-1][n-1];
   // END BLOCK
}
