
Privatized regions

int func(int n)
{

//  <i-PRIVATE-EXACT-{}>
//  <j-PRIVATE-EXACT-{}>
//  <t[PHI1]-PRIVATE-EXACT-{0<=PHI1, PHI1<=99}>
//  <x[PHI1]-PRIVATE-EXACT-{0<=PHI1, PHI1<=99}>
//  <y[PHI1][PHI2]-PRIVATE-EXACT-{PHI2==100, 0<=PHI1, PHI1<=99}>

   // BEGIN BLOCK
   int x[100];
   int y[100][100];
   int t[100];
   int i;
   int j;

   for(i = 0; i <= 99; i += 1) {

//  <j-PRIVATE-EXACT-{}>
//  <t[PHI1]-PRIVATE-EXACT-{PHI1==i, 0<=i, i<=99}>
//  <x[PHI1]-PRIVATE-EXACT-{PHI1==i, 0<=i, i<=99}>
//  <y[PHI1][PHI2]-PRIVATE-EXACT-{PHI1==i, PHI2==100, 0<=i, i<=99}>

      // BEGIN BLOCK
      PRIVATE i,j,t[100],x[100],y[100][100]
      x[i] = i;
      for(j = 0; j <= 99; j += 1)
         PRIVATE j,t[100]

//  <t[PHI1]-PRIVATE-EXACT-{PHI1==i, 0<=i, i<=99, 0<=j, j<=99}>

         t[i] = x[i];
      y[i][j] = x[i]+j;
      // END BLOCK
   }
   return y[n][n];
   // END BLOCK
}

