### initial code
void until09()
{
   // BEGIN BLOCK
   int i = 1;
   do {
      i++;
   }
   while (i++<0);
   printf("%d", i);
   // END BLOCK
}
### Preconditions

//  P() {}

void until09()
{

//  P() {}

   // BEGIN BLOCK

//  P() {}

   int i = 1;

//  P(i) {i==1}

   do {

//  P(i) {i==1}

      i++;
   }
   while (i++<0);

//  P(i) {i==3}

   printf("%d", i);
   // END BLOCK
}
### suppress dead code
void until09()
{
   // BEGIN BLOCK
   int i = 1;
   do {
      i++;
   }
   while (i++<0);
   printf("%d", i);
   // END BLOCK
}

