      PROGRAM UNROLL8
C
      PARAMETER (N=4)
      REAL T(N,N)

C     Outer unrolling with different labels

Cxxx
      DO 10 I = 1, N                                                    0001
C
         DO 20 J = 1, N                                                 0002
            T(I,J) = -N*I+J                                             0003
20          CONTINUE                                                    0004
10       CONTINUE                                                       0005

C     Inner unrolling with different labels

      DO 30 I = 1, N                                                    0006
Cxxx
         DO 40 J = 1, N                                                 0007
            T(I,J) = -N*I+J                                             0008
40          CONTINUE                                                    0009
30       CONTINUE                                                       0010

C     Outer unrolling with same labels

Cxxx
      DO 11 I = 1, N                                                    0011
C
         DO 11 J = 1, N                                                 0012
            T(I,J) = -N*I+J                                             0013
11          CONTINUE                                                    0014

C     Inner unrolling with same labels

      DO 31 I = 1, N                                                    0015
Cxxx
         DO 31 J = 1, N                                                 0016
            T(I,J) = -N*I+J                                             0017
31          CONTINUE                                                    0018
C
      END
      PROGRAM UNROLL8
C
      PARAMETER (N=4)
      REAL T(N,N)

C     Outer unrolling with different labels

Cxxx
C
      DO J = 1, N                                                       0002
         T(1,J) = -N*1+J                                                0003
      ENDDO
C
      DO J = 1, N                                                       0002
         T(2,J) = -N*2+J                                                0003
      ENDDO
C
      DO J = 1, N                                                       0002
         T(3,J) = -N*3+J                                                0003
      ENDDO
C
      DO J = 1, N                                                       0002
         T(4,J) = -N*4+J                                                0003
      ENDDO
10    CONTINUE
      I = 5

C     Inner unrolling with different labels

      DO 30 I = 1, N                                                    0006
Cxxx
         T(I,1) = -N*I+1                                                0008
         T(I,2) = -N*I+2                                                0008
         T(I,3) = -N*I+3                                                0008
         T(I,4) = -N*I+4                                                0008
40       CONTINUE
         J = 5
30       CONTINUE                                                       0010

C     Outer unrolling with same labels

Cxxx
C
      DO J = 1, N                                                       0012
         T(1,J) = -N*1+J                                                0013
      ENDDO
C
      DO J = 1, N                                                       0012
         T(2,J) = -N*2+J                                                0013
      ENDDO
C
      DO J = 1, N                                                       0012
         T(3,J) = -N*3+J                                                0013
      ENDDO
C
      DO J = 1, N                                                       0012
         T(4,J) = -N*4+J                                                0013
      ENDDO
11    CONTINUE
      I = 5

C     Inner unrolling with same labels

      DO 31 I = 1, N                                                    0015
Cxxx
         T(I,1) = -N*I+1                                                0017
         T(I,2) = -N*I+2                                                0017
         T(I,3) = -N*I+3                                                0017
         T(I,4) = -N*I+4                                                0017
31       CONTINUE
         J = 5
C
      END
