Code after privatization12

int main(int argc, char *argv[])
{
   int i = 0;
   int j = i++;
   int k = 0;
   {
      int i1;
      for(i1 = 0; i1 <= 9; i1 += 1) {
         int j2;
         for(j2 = 0; j2 <= 9; j2 += 1)
            k = k+i1*j2;
      }
   }
   // This would prevent the privatization
   //printf("%d\n",k);
   return 0;
}

