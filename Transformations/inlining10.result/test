{
   int I_0 = c[0][2][4];
   int *P_2 = (int *) c[0][2];
   int (*P_1)[20] = (int (*)[20]) c[0][1];
   int (*P_0)[30][20] = (int (*)[30][20]) c[0];
   P_0[20][10][0] = P_1[10][0];
   P_2[0] = I_0;
