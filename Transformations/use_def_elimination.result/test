      SUBROUTINE TEST IF 3(B)
      INTEGER A, B, J

C  P() {}


      J = 3                                                             0005

C  P(J) {J==3}


      B = J                                                             0007

C  P(B,J) {B==3, J==3}

      AVIRER = J                                                        0008

C  P(B,J) {B==3, J==3}


      DO I = 3, 10                                                      0010

C  P(B,I,J) {B==3, J==3, 3<=I, I<=10}

         PRINT *, 'une boucle !'                                        0011
      ENDDO

C  P(B,I,J) {B==3, I==11, J==3}

C     BEGIN UNSTRUCTURED

C  P(B,I,J) {B==3, I==11, J==3}


c      if(.true.) then
      IF (J.EQ.5) GOTO 31                                               0015

C  P(B,I,J) {B==3, I==11, J==3}

      PRINT *, 'salut'                                                  0016

C  P(B,I,J) {B==3, I==11, J==3}

31    A = 9                                                             0017

C  P(A,B,I,J) {A==9, B==3, I==11, J==3}


c      endif
      AVIRERENCORE = B                                                  0020

C  P(A,B,I,J) {A==9, B==3, I==11, J==3}


      IF (A.EQ.I) THEN                                                  0022

C  P() {0==-1}

         PRINT *, 'vrai'                                                0023
      ELSE

C  P(A,B,I,J) {A==9, B==3, I==11, J==3}

         PRINT *, 'faux'                                                0025
      ENDIF

C  P(A,B,I,J) {A==9, B==3, I==11, J==3}


100   CONTINUE

C  P(A,B,I,J) {A==9, B==3, I==11, J==3}

200   CONTINUE                                                          0029
      GOTO 100
C     END UNSTRUCTURED
      END

C  P() {}


