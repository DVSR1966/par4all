      SUBROUTINE TEST IF 5(B)
c test the propagation of control usefulness through the predecesors      
      INTEGER A, B, J
      REAL X(10), Y(10)

C  P() {}


      J = 5                                                             0007

C  P(J) {J==5}

100   B = J                                                             0009

C  P(B,J) {B==5, J==5}

C     BEGIN UNSTRUCTURED

C  P(B,J) {B==5, J==5}

      IF (A.NE.0) GOTO 200                                              0010

C  P(B,J) {A==0, B==5, J==5}

      PRINT *, 'Well...'                                                0011

C  P(B,J) {B==5, J==5}

200   CONTINUE                                                          0012
C     END UNSTRUCTURED

C  P(B,J) {B==5, J==5}

      TOBEDISCARDED = 1                                                 0012

C  P(B,J) {B==5, J==5}

      YACCTOO = 2                                                       0013

C  P(B,J) {B==5, J==5}

      X(1) = Y(2)                                                       0014

C  P(B,J) {B==5, J==5}

      Y(2) = B                                                          0015

C  P(B,J) {B==5, J==5}


      RETURN
      END

C  P() {}


