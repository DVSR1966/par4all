      PROGRAM TEST IF 1
      INTEGER A, B, J

C  P() {}



C     Added to avoid empty summary preconditions
      IF (.FALSE.) THEN                                                 0006

C  P() {0==-1}

         CALL TESTIF2(B)                                                0007

C  P() {0==-1}

         CALL TESTIF3(B)                                                0008

C  P() {0==-1}

         CALL TESTIF4(B)                                                0009

C  P() {0==-1}

         CALL TESTIF5(B)                                                0010
      ELSE
      ENDIF

C  P() {}



      J = 3                                                             0014

C  P(J) {J==3}

C     BEGIN UNSTRUCTURED

C  P(J) {J==3}



      IF (J.EQ.5) GOTO 31                                               0017

C  P(J) {J==3}

      WRITE (*, 9001) 'salut'                                           0018

C  P(J) {J==3}

9001  FORMAT('j = ',I3)                                                 0019

C  P(J) {J==3}

31    A = 9                                                             0020

C  P(A,J) {A==9, J==3}


      IF (J.EQ.3) GOTO 1                                                0022

C  P() {0==-1}

2     PRINT *, 'Faux 2'                                                 0027

C  P(A,J) {A==9, J==3}

1     PRINT *, 'Vrai 1'                                                 0028

C  P(A,J) {A==9, J==3}

      IF (J.EQ.4) GOTO 3                                                0029

C  P(A,J) {A==9, J==3}

      PRINT *, 'Vrai'                                                   0032

C  P(A,J) {A==9, J==3}


3     PRINT *, 'Faux 3'                                                 0035

C  P(A,J) {A==9, J==3}

      IF (J.EQ.3) THEN                                                  0036
         GOTO 3
      ELSE
         GOTO 2
      ENDIF
C     END UNSTRUCTURED
      END

