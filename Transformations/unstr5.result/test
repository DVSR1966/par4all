
Preconditions computed for user code


C  P() {}

      PROGRAM UNSTR5
      INTEGER I

C  P() {}


      I = 4                                                             0001

C  P(I) {I==4}


      IF (I.GE.3) GOTO 99999                                            0002

C  P() {0==-1}

      I = 1                                                             0004
      GOTO 100
99999 CONTINUE

C  P(I) {I==4}

      I = 3                                                             0003

C  P(I) {I==3}

100   CONTINUE                                                          0005

C  P(I) {I==3}

      PRINT *, J                                                        0005

C  P(I) {I==3}


      RETURN
      END

