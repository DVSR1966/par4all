
       FUNCTION SDOT(N,X,INCX,Y,INCY)
       REAL*4 X(1),Y(1),SDOT
      SDOT = 0.0                                                        0004
      SDOT = SDOT+X(1)*Y(1)                                             0008
      SDOT = SDOT+X(2)*Y(2)                                             0008
      SDOT = SDOT+X(3)*Y(3)                                             0008
      SDOT = SDOT+X(4)*Y(4)                                             0008
      SDOT = SDOT+X(5)*Y(5)                                             0008
      SDOT = SDOT+X(6)*Y(6)                                             0008
      SDOT = SDOT+X(7)*Y(7)                                             0008
      SDOT = SDOT+X(8)*Y(8)                                             0008
      SDOT = SDOT+X(9)*Y(9)                                             0008
      SDOT = SDOT+X(10)*Y(10)                                           0008
      SDOT = SDOT+X(11)*Y(11)                                           0008
      SDOT = SDOT+X(12)*Y(12)                                           0008
      SDOT = SDOT+X(13)*Y(13)                                           0008
      SDOT = SDOT+X(14)*Y(14)                                           0008
      SDOT = SDOT+X(15)*Y(15)                                           0008
      SDOT = SDOT+X(16)*Y(16)                                           0008
      SDOT = SDOT+X(17)*Y(17)                                           0008
      SDOT = SDOT+X(18)*Y(18)                                           0008
      SDOT = SDOT+X(19)*Y(19)                                           0008
      SDOT = SDOT+X(20)*Y(20)                                           0008
      SDOT = SDOT+X(21)*Y(21)                                           0008
      SDOT = SDOT+X(22)*Y(22)                                           0008
      SDOT = SDOT+X(23)*Y(23)                                           0008
      SDOT = SDOT+X(24)*Y(24)                                           0008
      SDOT = SDOT+X(25)*Y(25)                                           0008
      SDOT = SDOT+X(26)*Y(26)                                           0008
      SDOT = SDOT+X(27)*Y(27)                                           0008
      SDOT = SDOT+X(28)*Y(28)                                           0008
      SDOT = SDOT+X(29)*Y(29)                                           0008
      SDOT = SDOT+X(30)*Y(30)                                           0008
      SDOT = SDOT+X(31)*Y(31)                                           0008
      SDOT = SDOT+X(32)*Y(32)                                           0008
      SDOT = SDOT+X(33)*Y(33)                                           0008
      SDOT = SDOT+X(34)*Y(34)                                           0008
      SDOT = SDOT+X(35)*Y(35)                                           0008
      SDOT = SDOT+X(36)*Y(36)                                           0008
      SDOT = SDOT+X(37)*Y(37)                                           0008
      SDOT = SDOT+X(38)*Y(38)                                           0008
      SDOT = SDOT+X(39)*Y(39)                                           0008
      SDOT = SDOT+X(40)*Y(40)                                           0008
      SDOT = SDOT+X(41)*Y(41)                                           0008
      SDOT = SDOT+X(42)*Y(42)                                           0008
      SDOT = SDOT+X(43)*Y(43)                                           0008
      SDOT = SDOT+X(44)*Y(44)                                           0008
      SDOT = SDOT+X(45)*Y(45)                                           0008
      SDOT = SDOT+X(46)*Y(46)                                           0008
      SDOT = SDOT+X(47)*Y(47)                                           0008
      SDOT = SDOT+X(48)*Y(48)                                           0008
      SDOT = SDOT+X(49)*Y(49)                                           0008
10    CONTINUE
      END
