void c_clean_declarations01(int x)
{
   int x;
   int y;
   extern  stderr();

   if (y) {
      int x = y++;
      int z;

      y = 1;
   }
   else {
      int x = 0;
      int z = y--;

      y = -1;
   }
   fprintf(stderr, "x=%dn", x);
}
void c_clean_declarations01(int x)
{
   int x;
   int y;
   extern  stderr();

   if (y) {
      int x = y++;
      int z;

      y = 1;
   }
   else {
      int x = 0;
      int z = y--;

      y = -1;
   }
   fprintf(stderr, "x=%dn", x);
