C     Check that j is privatized when there is a def-def arc

      SUBROUTINE PRIVATIZATION06
      INTEGER A(10, 10)

      DO I = 1, 10                                                      0006
         DO K = 1, 10                                                   0007
         PRIVATE I,J,K
            PRIVATE J,K
            J = 0                                                       0008
            J = 1                                                       0009
            A(I,K) = J                                                  0010
         ENDDO
      ENDDO
      END
