Code after privatization12

int main(int argc, char *argv[])
{
   int i = 0;
   int j = i++;
   int k = 0;
   {
      //PIPS generated variable
      int i;
      for(i = 0; i <= 9; i += 1) {
         //PIPS generated variable
         int j1, j;
         for(j = 0; j <= 9; j += 1)
            k = k+i*j;
      }
   }
   // This would prevent the privatization
   //printf("%d\n",k);
   return 0;
}

