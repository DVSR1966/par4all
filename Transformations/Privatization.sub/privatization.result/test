
Initial code

int main(int argc, char *argv[])
{
   int i = 0, j = i++, k = 0;
   for(i = 0; i <= 9; i += 1)
      for(j = 0; j <= 9; j += 1)
         k += i*j;
   printf("%d\n", k);
   return 0;
}

Code after privatization

int main(int argc, char *argv[])
{
   int i = 0, j = i++, k = 0;
   {
      //PIPS generated variable
      int i;
      for(i = 0; i <= 9; i += 1) {
         //PIPS generated variable
         int j1, j;
         for(j = 0; j <= 9; j += 1)
            k += i*j;
      }
   }
   printf("%d\n", k);
   return 0;
}

Code after declaration cleaning

int main(int argc, char *argv[])
{
   int i = 0, j = i++, k = 0;
   {
      //PIPS generated variable
      int i;
      for(i = 0; i <= 9; i += 1) {
         //PIPS generated variable
         int j;
         for(j = 0; j <= 9; j += 1)
            k += i*j;
      }
   }
   printf("%d\n", k);
   return 0;
}
