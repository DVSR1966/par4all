      SUBROUTINE CLEANDEC

      COMMON /FOO/I,J,C(2), M, N
      COMMON /UFOO/A,B,D
      INTEGER O,P,Q
      PARAMETER (K=10)
      PARAMETER (W=5)	
      EQUIVALENCE (O,P)
      IF (I.GT.J) CALL BAR                                              0001
      PRINT *, I, J, M, M, P                                            0003

      END

      SUBROUTINE BAR
      COMMON /FOO/I,J,K,L,M,N
      COMMON /UFOO/A,B,D
      PARAMETER (X=10)
      PARAMETER (Y=5*X)	
      IF (I.GT.J) THEN                                                  0001
         I = I+1                                                        0002
         J = J+2*M                                                      0003
         K = K+3                                                        0004
         L = L+4*Y                                                      0005
      ENDIF

      END
