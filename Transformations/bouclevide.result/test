## Code Initial
      PROGRAM TEST1

      INTEGER I,J,NC,JC
C      parameter (NC=100)
      REAL  ZG,V(100),W(100),G(100,100),GAMAI(100)

      NC = 50                                                           0001
      JC = 30                                                           0002
      DO 320 I = 1, NC                                                  0003
         V(I) = 0.                                                      0004
         W(I) = 0.                                                      0005
         IF (I.GT.10) GOTO 99999                                        0006
         DO J = 1, JC                                                   0009
            W(I) = W(I)/V(I)                                            0010
            GAMAI(I) = W(I)+G(I,J)                                      0011
         ENDDO
99996    CONTINUE
320      CONTINUE                                                       0012
         GOTO 99998
99999    CONTINUE
         IF (NC.LT.JC.AND.NC.GT.10) GOTO 99997                          0007
         GOTO 99996
99997    CONTINUE
         GOTO 320
99998    CONTINUE

      END
## apres restructuration
      PROGRAM TEST1

      INTEGER I,J,NC,JC
C      parameter (NC=100)
      REAL  ZG,V(100),W(100),G(100,100),GAMAI(100)

      NC = 50                                                           0001
      JC = 30                                                           0002
      DO 320 I = 1, NC                                                  0003
         V(I) = 0.                                                      0004
         W(I) = 0.                                                      0005
         IF (I.GT.10) THEN                                              0006
            IF (NC.LT.JC.AND.NC.GT.10) THEN                             0007
            ENDIF
         ELSE
            DO J = 1, JC                                                0009
               W(I) = W(I)/V(I)                                         0010
               GAMAI(I) = W(I)+G(I,J)                                   0011
            ENDDO
         ENDIF
320      CONTINUE                                                       0012

      END
