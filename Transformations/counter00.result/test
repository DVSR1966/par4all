void counter00(int n)
{
   int i;
   // simple while
   i = 0;
   while (i<n)
      i++;
   // while with sequence
   i = 0;
   while (i<n) {
      i++;
      fprintf(stdout, "%d\n", i);
   }
   // do loop
   for(i = 0; i <= n-1; i += 1)
      fprintf(stdout, "%d\n", i);
   // for loop
   i = 0;
   while (i<n&&n<1000) {
      fprintf(stdout, "%d\n", i);
      i++;
   }
   // do-while loop
   i = 0;
   do {
      i++;
      fprintf(stdout, "%d\n", i);
   }
   while (i<n);
   // simple test
   if (n%2==0)
      i++;
   else
      i--;
   // sequence test
   if (n%2==0) {
      i++;
      fprintf(stdout, "%d\n", i);
   }
   else {
      i--;
      fprintf(stdout, "%d\n", i);
   }
}
void counter00(int n)
{
   int i;
   //PIPS generated variable
   int _while_00 = 0, _while_10 = 0, _do_20 = 0, _while_30 = 0, _while_40 = 0, _if_then_50 = 0, _if_else_60 = 0, _if_then_70 = 0, _if_else_80 = 0;
   // simple while
   i = 0;
   while (i<n) {
      _while_00 = _while_00+1;
      i++;
   }
   // while with sequence
   i = 0;
   while (i<n) {
      _while_10 = _while_10+1;
      i++;
      fprintf(stdout, "%d\n", i);
   }
   // do loop
   for(i = 0; i <= n-1; i += 1) {
      _do_20 = _do_20+1;
      fprintf(stdout, "%d\n", i);
   }
   // for loop
   i = 0;
   while (i<n&&n<1000) {
      _while_30 = _while_30+1;
      fprintf(stdout, "%d\n", i);
      i++;
   }
   // do-while loop
   i = 0;
   do {
      _while_40 = _while_40+1;
      i++;
      fprintf(stdout, "%d\n", i);
   }
   while (i<n);
   // simple test
   if (n%2==0) {
      _if_then_50 = _if_then_50+1;
      i++;
   }
   else {
      _if_else_60 = _if_else_60+1;
      i--;
   }
   // sequence test
   if (n%2==0) {
      _if_then_70 = _if_then_70+1;
      i++;
      fprintf(stdout, "%d\n", i);
   }
   else {
      _if_else_80 = _if_else_80+1;
      i--;
      fprintf(stdout, "%d\n", i);
   }
}

