void counter00(int n)
{
   int i;
   // simple while
   i = 0;
   while (i<n)
      i++;
   // while with sequence
   i = 0;
   while (i<n) {
      i++;
      fprintf(stdout, "%d\n", i);
   }
   // empty while loop
   i = 0;
   while (i++<n)
      ;
   // do loop
   for(i = 0; i <= n-1; i += 1)
      fprintf(stdout, "%d\n", i);
   // empty do loop
   for(i = 0; i <= n-1; i += 1)
      ;
   // for loop
   i = 0;
   while (i<n&&n<1000) {
      fprintf(stdout, "%d\n", i);
      i++;
   }
   // empty for loop
   i = 0;
   while (i<n&&n<1000) {
      i++;
   }
   // do-while loop
   i = 0;
   do {
      i++;
      fprintf(stdout, "%d\n", i);
   }
   while (i<n);
   // simple test
   if (n%2==0)
      i++;
   else
      i--;
   // sequence test
   if (n%2==0) {
      i++;
      fprintf(stdout, "%d\n", i);
   }
   else {
      i--;
      fprintf(stdout, "%d\n", i);
   }
   // test with true branch only
   if (n%2==0)
      i++;
}
void counter00(int n)
{
   int i;
   //PIPS generated variable
   int _while_00 = 0, _while_10 = 0, _while_20 = 0, _do_30 = 0, _do_40 = 0, _while_50 = 0, _while_60 = 0, _while_70 = 0, _if_then_80 = 0, _if_else_90 = 0, _if_then_100 = 0, _if_else_110 = 0, _if_then_120 = 0, _if_else_130 = 0;
   // simple while
   i = 0;
   while (i<n) {
      _while_00 = _while_00+1;
      i++;
   }
   // while with sequence
   i = 0;
   while (i<n) {
      _while_10 = _while_10+1;
      i++;
      fprintf(stdout, "%d\n", i);
   }
   // empty while loop
   i = 0;
   while (i++<n) {
      _while_20 = _while_20+1;

   }
   // do loop
   for(i = 0; i <= n-1; i += 1) {
      _do_30 = _do_30+1;
      fprintf(stdout, "%d\n", i);
   }
   // empty do loop
   for(i = 0; i <= n-1; i += 1) {
      _do_40 = _do_40+1;

   }
   // for loop
   i = 0;
   while (i<n&&n<1000) {
      _while_50 = _while_50+1;
      fprintf(stdout, "%d\n", i);
      i++;
   }
   // empty for loop
   i = 0;
   while (i<n&&n<1000) {
      _while_60 = _while_60+1;
      i++;
   }
   // do-while loop
   i = 0;
   do {
      _while_70 = _while_70+1;
      i++;
      fprintf(stdout, "%d\n", i);
   }
   while (i<n);
   // simple test
   if (n%2==0) {
      _if_then_80 = _if_then_80+1;
      i++;
   }
   else {
      _if_else_90 = _if_else_90+1;
      i--;
   }
   // sequence test
   if (n%2==0) {
      _if_then_100 = _if_then_100+1;
      i++;
      fprintf(stdout, "%d\n", i);
   }
   else {
      _if_else_110 = _if_else_110+1;
      i--;
      fprintf(stdout, "%d\n", i);
   }
   // test with true branch only
   if (n%2==0) {
      _if_then_120 = _if_then_120+1;
      i++;
   }
   else
      _if_else_130 = _if_else_130+1;
}

