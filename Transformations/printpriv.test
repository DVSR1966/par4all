#!/bin/sh
#
# printpriv.f test script
#
# echo "GENERATE_NESTED_PARALLEL_LOOPS FALSE" > properties.rc
if Pips -s rice_cray -f $PIPS_ROOT/Validation/Transformations/printpriv.f -w printpriv -m printpriv -p priv >&2
then
	echo "--- Cray code for PRINTPRIV"
	if Display cray
	then
		echo "--- Cray code for FOO"
		Display -m foo cray
		exit_status=$?
	else
		exit_status=2
	fi
else
	exit_status=1
fi
Delete printpriv
exit $exit_status
