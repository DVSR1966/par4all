#
# testing source
#
delete source

set ABORT_ON_USER_ERROR TRUE

create source source.f
close
# sourcing
echo ! one 
source  ${VDIR:-.}/sourced.tpips
echo ! two
source  ${VDIR:-.}/sourced.tpips \
	${VDIR:-.}/sourced2.tpips
echo ! three
source  ${VDIR:-.}/sourced.tpips \
	${VDIR:-.}/sourced2.tpips \
	${VDIR:-.}/sourced3.tpips
echo ! already open
open source
source  ${VDIR:-.}/sourced.tpips
# echo # delete all
# setproperty PIPSDBM_RESOURCES_TO_DELETE="all"
# source sourced.tpips
shell echo ! done
delete source
quit
