}
int find_while(int i)
{
   while (i++)                                                                /*0006*/
      i = 8*i;                                                            /*0006*/
   i--;                                                               /*0010*/
   while (i++<9) 
      ;
   while (i<9)                                                                /*0018*/
      i = 8*i;                                                            /*0018*/
   return i;                                                               /*0021*/
}
