{
begin:   if (i++) goto l99999;
   i--;                                                               /*0010*/

begin3:   if (i++<9) goto begin3;

begin2:   if (i<9) {
   }
   else {
      goto l99998;
   }
   i = 8*i;                                                               /*0018*/
   goto begin2;
l99999:   ;
   i = 8*i;                                                               /*0006*/
   goto begin;
l99998:   ;
