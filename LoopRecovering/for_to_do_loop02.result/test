int main()
{
   int a[100];                                                          /*0003*/
   int i, j;                                                            /*0004*/
   
   // A do loop ! Should be parallel
   for (i = 2;i<=50;i = i+1)                                            /*0007*/
      a[i] = 2;                                                         /*0008*/
   
   /* The following code should not prevent the previous loop to be
        parallel: */
   // Should be parallel but for->do here not implemented yet...
   for (j = 60;j>10;j = j-3)                                            /*0013*/
      a[j] = 2;                                                         /*0014*/

   for (i = 2;i<=50;i = 2*i)                                            /*0016*/
      a[i] = 2;                                                         /*0017*/

   return 0;                                                            /*0019*/
}
int main()
{
   int a[100];                                                          /*0003*/
   int i, j;                                                            /*0004*/
   
   // A do loop ! Should be parallel
   for(i = 2; i <= 50; i += 1)
      a[i] = 2;                                                         /*0008*/
   
   /* The following code should not prevent the previous loop to be
        parallel: */
   // Should be parallel but for->do here not implemented yet...
   for (j = 60;j>10;j = j-3)                                            /*0013*/
      a[j] = 2;                                                         /*0014*/

   for (i = 2;i<=50;i = 2*i)                                            /*0016*/
      a[i] = 2;                                                         /*0017*/

   return 0;                                                            /*0019*/
}
int main()
{
   int a[100];                                                          /*0003*/
   int i, j;                                                            /*0004*/
   
   // A do loop ! Should be parallel
   ;
   for(i = 2; i <= 50; i += 1)
      a[i] = 2;                                                         /*0008*/
   
   /* The following code should not prevent the previous loop to be
        parallel: */
   // Should be parallel but for->do here not implemented yet...
   for (j = 60;j>10;j = j-3)                                            /*0013*/
      a[j] = 2;                                                         /*0014*/

   for (i = 2;i<=50;i = 2*i)                                            /*0016*/
      a[i] = 2;                                                         /*0017*/

   return 0;                                                            /*0019*/
}
