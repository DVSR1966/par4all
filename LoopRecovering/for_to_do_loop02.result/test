int main()
{
   int a[100];
   int i;
   int j;
   
   // A do loop ! Should be parallel
   for (i = 2;i<=50;i = i+1)                                            /*0007*/
      a[i] = 2;                                                         /*0008*/
   
   // Should be parallel but for->do here not implemented yet...
   for (j = 60;j>10;j = j-3)                                            /*0011*/
      a[j] = 2;                                                         /*0012*/

   for (i = 2;i<=50;i = 2+1)                                            /*0014*/
      a[i] = 2;                                                         /*0015*/

   return 0;                                                            /*0017*/
}
int main()
{
   int a[100];
   int i;
   int j;
   
   // A do loop ! Should be parallel
   for(i = 2; i <= 50; i += 1)                                          /*0007*/
      a[i] = 2;                                                         /*0008*/
   
   // Should be parallel but for->do here not implemented yet...
   for (j = 60;j>10;j = j-3)                                            /*0011*/
      a[j] = 2;                                                         /*0012*/

   for (i = 2;i<=50;i = 2+1)                                            /*0014*/
      a[i] = 2;                                                         /*0015*/

   return 0;                                                            /*0017*/
}
