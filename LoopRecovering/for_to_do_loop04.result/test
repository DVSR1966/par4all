int main()
{
   int a[1000];
   int j;
   
   // A do loop ! Should be parallel.
   for (j = 0;j<1000;j++)                                               /*0007*/
      a[j] = j;                                                         /*0008*/
   for (j = 0;j<1000;j++)                                               /*0009*/
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}

After for to do loop conversion

int main()
{
   int a[1000];
   int j;
   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)                                         /*0007*/
      a[j] = j;                                                         /*0008*/
   for(j = 0; j <= 999; j += 1)                                         /*0009*/
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}

Proper effects for "main"

int main()
{
   int a[1000];
   int j;
//               <must be written>: j
   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)                                         /*0007*/
//               <must be read   >: j
//               <must be written>: a[j]
      a[j] = j;                                                         /*0008*/
//               <must be written>: j
   for(j = 0; j <= 999; j += 1)                                         /*0009*/
//               <must be read   >: j
//               <must be written>: a[j]
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}

Cumulated effects for "main"

int main()
{
   int a[1000];
   int j;
//               <may be read    >: j
//               <may be written >: a[*]
//               <must be written>: j
   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)                                         /*0007*/
//               <may be written >: a[*]
//               <must be read   >: j
      a[j] = j;                                                         /*0008*/
//               <may be read    >: j
//               <may be written >: a[*]
//               <must be written>: j
   for(j = 0; j <= 999; j += 1)                                         /*0009*/
//               <may be written >: a[*]
//               <must be read   >: j
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}

Transformers for "main"


//  T(main) {main==0}

int main()
{
   int a[1000];
   int j;

//  T(j) {0<=j}

   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)                                         /*0007*/

//  T() {0<=j, j<=999}

      a[j] = j;                                                         /*0008*/

//  T(j) {j#init==1000, 0<=j}

   for(j = 0; j <= 999; j += 1)                                         /*0009*/

//  T() {0<=j, j<=999}

      a[j] = j;                                                         /*0010*/

//  T(main) {j==1000, main==0}


   return 0;                                                            /*0012*/
}

Preconditions for "main"


//  P() {}

int main()
{
   int a[1000];
   int j;

//  P() {}

   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)                                         /*0007*/

//  P(j) {0<=j, j<=999}

      a[j] = j;                                                         /*0008*/

//  P(j) {j==1000}

   for(j = 0; j <= 999; j += 1)                                         /*0009*/

//  P(j) {0<=j, j<=999}

      a[j] = j;                                                         /*0010*/

//  P(j) {j==1000}


   return 0;                                                            /*0012*/
}
int main()
{
   int a[1000];
   int j;
   
   // A do loop ! Should be parallel.
#pragma omp parallel for 
   for(j = 0; j <= 999; j += 1)                                         /*0007*/
      a[j] = j;                                                         /*0008*/
#pragma omp parallel for 
   for(j = 0; j <= 999; j += 1)                                         /*0009*/
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}
