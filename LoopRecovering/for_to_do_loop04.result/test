int main()
{
   int a[1000];                                                         /*0003*/
   int j;                                                               /*0004*/
   
   // A do loop ! Should be parallel.
   for (j = 0;j<1000;j++)                                               /*0007*/
      a[j] = j;                                                         /*0008*/
   for (j = 0;j<1000;j++)                                               /*0009*/
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}

After for to do loop conversion

int main()
{
   int a[1000];                                                         /*0003*/
   int j;                                                               /*0004*/
   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)
      a[j] = j;                                                         /*0008*/
   for(j = 0; j <= 999; j += 1)
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}

Proper effects for "main"

int main()
{
   int a[1000];                                                         /*0003*/
   int j;                                                               /*0004*/
//               <must be written>: j
   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)
//               <must be read   >: j
//               <must be written>: a[j]
      a[j] = j;                                                         /*0008*/
//               <must be written>: j
   for(j = 0; j <= 999; j += 1)
//               <must be read   >: j
//               <must be written>: a[j]
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}

Cumulated effects for "main"

int main()
{
   int a[1000];                                                         /*0003*/
   int j;                                                               /*0004*/
//               <may be read    >: j
//               <may be written >: a[*]
//               <must be written>: j
   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)
//               <may be written >: a[*]
//               <must be read   >: j
      a[j] = j;                                                         /*0008*/
//               <may be read    >: j
//               <may be written >: a[*]
//               <must be written>: j
   for(j = 0; j <= 999; j += 1)
//               <may be written >: a[*]
//               <must be read   >: j
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}

Transformers for "main"


//  T(main) {main==0}

int main()
{

//  T() {}

   int a[1000];                                                         /*0003*/

//  T() {}

   int j;                                                               /*0004*/

//  T(j) {0<=j}

   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)

//  T() {0<=j, j<=999}

      a[j] = j;                                                         /*0008*/

//  T(j) {j#init==1000, 0<=j}

   for(j = 0; j <= 999; j += 1)

//  T() {0<=j, j<=999}

      a[j] = j;                                                         /*0010*/

//  T(main) {j==1000, main==0}


   return 0;                                                            /*0012*/
}

Preconditions for "main"


//  P() {}

int main()
{

//  P() {}

   int a[1000];                                                         /*0003*/

//  P() {}

   int j;                                                               /*0004*/

//  P() {}

   
   // A do loop ! Should be parallel.
   for(j = 0; j <= 999; j += 1)

//  P(j) {0<=j, j<=999}

      a[j] = j;                                                         /*0008*/

//  P(j) {j==1000}

   for(j = 0; j <= 999; j += 1)

//  P(j) {0<=j, j<=999}

      a[j] = j;                                                         /*0010*/

//  P(j) {j==1000}


   return 0;                                                            /*0012*/
}
int main()
{
   int a[1000];                                                         /*0003*/
   int j;                                                               /*0004*/
   
   // A do loop ! Should be parallel.
   ;
#pragma omp parallel for 
   for(j = 0; j <= 999; j += 1)
      a[j] = j;                                                         /*0008*/
#pragma omp parallel for 
   for(j = 0; j <= 999; j += 1)
      a[j] = j;                                                         /*0010*/

   return 0;                                                            /*0012*/
}
