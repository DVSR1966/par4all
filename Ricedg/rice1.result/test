      PROGRAM RICE1      
      REAL A(1:101,1:101,1:101),X(1:101,1:101,1:101)

      DO I = 1, 100                                                     0001
         DOALL J = 1, 100                                               0002
            DOALL K = 1, 100                                            0003
               X(I,J+1,K) = A(I,J,K)+10                                 0004
            ENDDO
         ENDDO
         DOALL J = 1, 100                                               0002
            DOALL L = 1, 50                                             0006
               A(I+1,J,L) = X(I,J,L)+5                                  0007
            ENDDO
         ENDDO
      ENDDO
      END
