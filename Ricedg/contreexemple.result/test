      PROGRAM CONTREEXEMPLE
      INTEGER A, C
      READ *, C                                                         0001
      CALL CONTREEXEMPLEFOO(A, A, C)                                    0002
      PRINT *, A, C                                                     0003
      END
      SUBROUTINE CONTREEXEMPLEFOO(A, B ,C)
      INTEGER A, B, C
      A = 2                                                             0001
      B = 3*A                                                           0002
C	Attention: impact alias MUST at (CONTREEXEMPLE:(0,3))  between B and A
C	New flow-dependence with statement
C	B = 3*A
      C = A*B                                                           0003
      END
