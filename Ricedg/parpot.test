#!/bin/sh
#

tpips=${tpips_version:-tpips}


$tpips <<EOF
delete parpot
create parpot ${PIPS_VALIDDIR}/Ricedg/parpot.f
set PRINT_DEPENDENCE_GRAPH TRUE
module PARPOT
activate PRECONDITIONS_INTER_FULL 
activate TRANSFORMERS_INTRA_FULL 
activate MUST_REGIONS 
apply PRIVATIZE_MODULE
activate REGION_CHAINS 
display DG_FILE
display PARALLELPRINTED_FILE 
close
delete parpot
EOF
