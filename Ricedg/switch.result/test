// OMP style
void main(int argc, char **argv)
{
   int i;
   int j;
   int k;
   int a[10];
   i = 0;                                                               /*0005*/
   j = 0;                                                               /*0006*/
   k = 0;                                                               /*0007*/
   if (argc==2) goto switch_1_case_2;
   if (argc==6) goto switch_1_case_6;
switch_1_default:   ;
   k;
   while (k<10) {
      a[k] = k;                                                         /*0021*/
      k++;
   }
   goto break_1;
switch_1_case_6:   ;
#pragma omp parallel for 
   for(j = 6; j <= 9; j += 1)                                           /*0015*/
      a[j] = j;                                                         /*0016*/
   goto break_1;
switch_1_case_2:   ;
#pragma omp parallel for 
   for(i = 2; i <= 9; i += 1)                                           /*0010*/
      a[i] = i;                                                         /*0011*/
break_1:   ;
   return 0;                                                            /*0025*/
}
extern void main(int argc, char **argv);
