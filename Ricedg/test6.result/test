      PROGRAM TEST6
      REAL MAT(10,10)

      DOALL I = 1, 10                                                   0001
         MAT(I,I) = 0.0                                                 0002
      ENDDO
      DOALL I = 1, 10                                                   0001
         DOALL J = 1, I-1                                               0003
            MAT(I,J) = 1.0                                              0004
         ENDDO
      ENDDO
      DOALL I = 1, 10                                                   0001
         DOALL J = 1, I-1                                               0003
            MAT(J,I) = -1.0                                             0005
         ENDDO
      ENDDO
      END
