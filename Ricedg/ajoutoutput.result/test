      PROGRAM AJOUTOUTPUT
      INTEGER A, C
      READ *, C                                                         0001
      CALL AJOUTOUTPUTFOO(A, A, C)                                      0002
      PRINT *, A, C                                                     0003
      END
      SUBROUTINE AJOUTOUTPUTFOO(A, B, C)
      INTEGER A, B, C
      A = 2*C                                                           0001
      C = 2                                                             0002
C	Attention: impact alias MUST at (AJOUTOUTPUT:(0,3))  between B and A
C	New output-dependence with statement
C	A = 2*C
      B = C*5                                                           0003
      END
