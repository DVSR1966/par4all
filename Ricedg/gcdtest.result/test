c     test for GCD     
      PROGRAM GCDTEST
      REAL A(100)

      DO I = 1, 10                                                      0001
         DOALL J = 1, 10                                                0002
            A(2*I+4*J) = A(2*I+2*J+1)                                   0003
         ENDDO
      ENDDO
      END
