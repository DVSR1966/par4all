
      SUBROUTINE FOO(X,Y)
      COMMON /COM/ T
      X = 0                                                             0001
      IF (T.GT.0) THEN                                                  0002
         X = 1                                                          0003
      ELSE
C	Attention: impact alias MUST at (IMPACT:(0,5))  between Y and X
C	New output-dependence with statement
C	X = 0
         Y = 2                                                          0004
      ENDIF
C	Attention: impact alias MUST at (IMPACT:(0,5))  between Y and X
C	New output-dependence with statement
C	X = 1
C	Attention: impact alias MUST at (IMPACT:(0,5))  between Y and X
C	New output-dependence with statement
C	X = 0
      Y = 3                                                             0005
      END

      SUBROUTINE FOO1(X, Y)
      INTEGER X(4), Y(3)
      X(1) = 1                                                          0001
      X(2) = X(2)+T                                                     0002
      Y(1) = Y(1)+T                                                     0003
      END
      PROGRAM IMPACT
      COMMON /COM/ T
      INTEGER A, B, C(5)
      A = 0                                                             0001
      B = 1                                                             0002
      CALL FOO(A, B)                                                    0003
      CALL FOO(A, A)                                                    0004
C      CALL FOO(T,A)
C      CALL FOO(A,T)
C      CALL FOO1(C(1), C(2))
      END
