int jacobi2(int nb, int** a, int** b)
{
   int indice_i;
   int indice_j;
   indice_i = 0;                                                        /*0004*/
   indice_j = 0;                                                        /*0005*/

#pragma omp parallel for private(indice_j)
   for(indice_i = 1; indice_i <= nb-1-1; indice_i += 1)                 /*0007*/
#pragma omp parallel for 
      for(indice_j = 1; indice_j <= nb-1-1; indice_j += 1)              /*0008*/
         
         
         
         

         a[indice_i][indice_j] = (b[indice_i-1][indice_j]+b[indice_i+1][indice_j]+b[indice_i][indice_j-1]+b[indice_i][indice_j+1])/4;              /*0014*/
   

   return 0;                                                            /*0019*/
}

