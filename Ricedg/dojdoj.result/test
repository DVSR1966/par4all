c     Test pour la reutilisation des indices des boucles
      PROGRAM DOJDOJ
      REAL *8 T(11,11)

      DO I = 1, 10                                                      0001
         DOALL J = 1, 5                                                 0002
            T(I,J) = T(I-1,J-1)                                         0003
         ENDDO
         DOALL J = 6, 10                                                0005
            T(I+1,J+1) = T(I,J)                                         0006
         ENDDO
      ENDDO
      END
