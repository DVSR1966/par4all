int main()
{

// Points To: none

   int d1 = 4;

// Points To: none

   int d2 = 4;

// Points To: none

   int y[d1][d2];

// Points To: none

   int i, j;

// Points To: none


   call14(d1, d2, (int *) y);

// Points To: none


   for(i = 0; i <= d1-1; i += 1) {

// Points To: none

      for(j = 0; j <= d2-1; j += 1)

// Points To: none

         printf("y[%d][%d] = %d\t", i, j, y[i][j]);

// Points To: none

      printf("\n");
   }

// Points To: none

   return 0;
}
