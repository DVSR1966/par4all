void recurrence03(list l)
{

// Points To:
// l -> &_l_1[0] , EXACT

   l->next->next->next = l->next;

// Points To:
// *ANY_MODULE*:*NOWHERE*_b0 -> &*ANY_MODULE*:*ANYWHERE*_b0 , EXACT
