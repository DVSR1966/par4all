
POINTS TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   static int *p;

// Points To:
// p -> *NULL_POINTER* , EXACT

   int *q;

// Points To:
// p -> *NULL_POINTER* , EXACT
// q -> undefined , EXACT


   q = p;

// Points To:
// p -> *NULL_POINTER* , EXACT
// q -> *NULL_POINTER* , EXACT


   return 0;
}
