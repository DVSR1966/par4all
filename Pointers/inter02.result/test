
POINTS TO

//  {(p,_p_1,-Exact-)}
void foo(int *p)
{
//  {(p,_p_1,-Exact-)}
   *p = 4;
//  {(p,_p_1,-Exact-)}
   return;
}

POINTS TO

//  {}
int main()
{
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int *x;
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int tab[10];
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int *tab2[10];
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int tab3[10][10];
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int **tab4;
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int y;
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   
   
   foo(x);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   foo(tab);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   foo(tab2[4]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   foo(tab3[5]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   foo(tab4[6]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   foo(&y);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   foo(&tab[1]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}

   printf("%d\n", *x);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   printf("%d\n", tab[0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   printf("%d\n", tab2[4][0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   printf("%d\n", tab3[5][0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   printf("%d\n", tab4[6][0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   printf("%d\n", y);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   printf("%d\n", tab[1]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*,-Exact-);(x,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   return 1;
}
