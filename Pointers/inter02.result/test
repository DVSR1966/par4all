
PROPER POINTER EFFECTS

int main()
{
   int *x;
   int tab[10];
   int *tab2[10];
   int tab3[10][10];
   int **tab4;
   int y;
//             <    is read   >: x
   
   
   foo(x);
   foo(tab);
//             <    is read   >: tab2[4]
   foo(tab2[4]);
   foo(tab3[5]);
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
   foo(tab4[6]);
   foo(&y);
   foo(&tab[1]);
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("%d\n", *x);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout tab[0] __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab[0]);
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab2[4][0]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout tab3[5][0] 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab3[5][0]);
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab4[6][0]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout y __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", y);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout tab[1] __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab[1]);
   return 1;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int *x;
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int tab[10];
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int *tab2[10];
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int tab3[10][10];
//  {(x,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int **tab4;
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int y;
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   
   
   foo(x);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(tab);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(tab2[4]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(tab3[5]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(tab4[6]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(&y);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   foo(&tab[1]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   printf("%d\n", *x);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%d\n", tab[0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%d\n", tab2[4][0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%d\n", tab3[5][0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%d\n", tab4[6][0]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%d\n", y);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   printf("%d\n", tab[1]);
//  {(tab4,*ANY_MODULE*:*NOWHERE*_b1,-Exact-);(x,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   return 1;
}
