void exit01()
{
   // BEGIN BLOCK
   int i;
   int *j;
//               <must be written>: j

   j = &i;
   exit(0);
//               <must be written>: i
   i = 2;
   // END BLOCK
}
POINTS TO
//  {}
void exit01()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int i;
//  points to = {}
   int *j;
//  points to = {}

   j = &i;
//  {(j,i,-Exact-)}
   exit(0);
//  points to = {}
   i = 2;
   // END BLOCK
}
