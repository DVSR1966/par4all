
PROPER EFFECTS

int main()
{
//               <must be written>: j
   int j = 0, k, tab[5];
//               <must be written>: p
   int *p = &j;
//               <must be read   >: tab[1]
//               <must be written>: q
   int *q = tab[1];
//               <must be written>: k

   k = 1;
//               <must be read   >: k
//               <must be written>: j
   j = k;
//               <must be written>: p
   p = &k;

   return 0;
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   int j = 0, k, tab[5];
//  {(p,j,-Exact-)}
   int *p = &j;
//  {(p,j,-Exact-);(q,tab[1][0],-Exact-)}
   int *q = tab[1];
//  {(p,j,-Exact-);(q,tab[1][0],-Exact-)}

   k = 1;
//  {(p,j,-Exact-);(q,tab[1][0],-Exact-)}
   j = k;
//  {(p,j,-Exact-);(q,tab[1][0],-Exact-)}
   p = &k;
//  {(p,k,-Exact-);(q,tab[1][0],-Exact-)}

   return 0;
}
