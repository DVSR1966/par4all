
PROPER  EFFECTS

int main()
{
   int x, tab1[10], tab2[10], *p;
//             <    is read   >: x
//             <    is written>: x
   x++;
//             <    is read   >: x
//             <    is written>: x
   x += 3;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p tab1[tab2[x]] tab2[x] x
   *(p+tab1[tab2[x]]) = 0;
   return 1;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int x, tab1[10], tab2[10], *p;

// Points To:
// p -> undefined , EXACT

   x++;

// Points To:
// p -> undefined , EXACT

   x += 3;

// Points To:
// p -> undefined , EXACT

   *(p+tab1[tab2[x]]) = 0;

// Points To:
// p -> undefined , EXACT

   return 1;
}
