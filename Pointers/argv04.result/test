
POINTS-TO


// Points To IN:
// argv -> *NULL_POINTER* , MAY
// argv -> _argv_2[0] , MAY


// Points To OUT: none

int main(int argc, char **argv)
{

// Points To: none

   int i = 0;

// Points To: none

   //char * s0 = *argv;
   for(i = 0; i <= argc-1; i += 1) {

// Points To:
// argv -> _argv_2[*] , MAY
// argv -> _argv_2[1] , MAY
// argv -> _argv_2[2] , MAY
// argv -> _argv_2[3] , MAY

      printf("%s\n", *argv);

// Points To:
// argv -> _argv_2[*] , MAY
// argv -> _argv_2[0] , MAY
// argv -> _argv_2[1] , MAY
// argv -> _argv_2[2] , MAY
// argv -> _argv_2[3] , MAY

      argv++;
   }

// Points To:
// argv -> _argv_2[*] , MAY
// argv -> _argv_2[2] , MAY
// argv -> _argv_2[3] , MAY

   return 0;
}
