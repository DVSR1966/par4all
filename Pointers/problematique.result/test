
PROPER  EFFECTS

int main()
{
   int *a, *b, *c;
   int i;
//             <    is written>: i
   i = 0;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: a _MALLOC_EFFECTS:_MALLOC_
   /* Allocation dynamiques des tableaux */
   a = (int *) malloc(20*sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: b _MALLOC_EFFECTS:_MALLOC_
   b = (int *) malloc(20*sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: c _MALLOC_EFFECTS:_MALLOC_
   c = (int *) malloc(20*sizeof(int));
//             <    is read   >: b
//             <    is written>: a
   /* Creation d'alias entre a et b */
   a = b;
//             <    is read   >: c
//             <    is written>: a
   
   /* Suppression de l'ancien alias entre a et b, creation d'un nouveau
    * entre a et c */
   a = c;
//             <    is written>: i
   
   /* Initialisation du tableau b */
   for(i = 0; i <= 19; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: b i
      b[i] = i;
//             <    is written>: i
   
   /* Initialisation du tableau c */
   for(i = 0; i <= 19; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: c i
      c[i] = 1;
//             <    is written>: i
   /* Initialisation du tableau a */
   for(i = 0; i <= 19; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      a[i] = b[i];
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int *a, *b, *c;

// Points To:
// a -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// a -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   i = 0;

// Points To:
// a -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   /* Allocation dynamiques des tableaux */
   a = (int *) malloc(20*sizeof(int));

// Points To:
// a -> &*HEAP*_l_9[0] , EXACT
// b -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   b = (int *) malloc(20*sizeof(int));

// Points To:
// a -> &*HEAP*_l_9[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*ANY_MODULE*:*NOWHERE*_b0 , EXACT

   c = (int *) malloc(20*sizeof(int));

// Points To:
// a -> &*HEAP*_l_9[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

   /* Creation d'alias entre a et b */
   a = b;

// Points To:
// a -> &*HEAP*_l_10[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

   
   /* Suppression de l'ancien alias entre a et b, creation d'un nouveau
    * entre a et c */
   a = c;

// Points To:
// a -> &*HEAP*_l_11[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

   
   /* Initialisation du tableau b */
   for(i = 0; i <= 19; i += 1)

// Points To:
// a -> &*HEAP*_l_11[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

      b[i] = i;

// Points To:
// a -> &*HEAP*_l_11[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

   
   /* Initialisation du tableau c */
   for(i = 0; i <= 19; i += 1)

// Points To:
// a -> &*HEAP*_l_11[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

      c[i] = 1;

// Points To:
// a -> &*HEAP*_l_11[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

   /* Initialisation du tableau a */
   for(i = 0; i <= 19; i += 1)

// Points To:
// a -> &*HEAP*_l_11[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

      a[i] = b[i];

// Points To:
// a -> &*HEAP*_l_11[0] , EXACT
// b -> &*HEAP*_l_10[0] , EXACT
// c -> &*HEAP*_l_11[0] , EXACT

   return 0;
}
