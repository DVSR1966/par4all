
Pointer proper effects

int main()
{
   // BEGIN BLOCK
   int i;
   int *p;
   int **q;
//             <    is written>: p

   p = 0;
//             <    is read   >: q q[0]
//             <    is written>: q[0][0]
   **q = 0;
//             <    is read   >: p
//             <    is written>: p[0]

   *p = 1;
//             <    is read   >: q
//             <    is written>: q[0]
   *q = &i;

   return 0;
   // END BLOCK
}

Points-to

//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int i;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   int *p;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int **q;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}

   p = 0;
//  {(p,*NULL_POINTER*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   **q = 0;
//  {(p,*NULL_POINTER*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}

   *p = 1;
//  {(p,*NULL_POINTER*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   *q = &i;
//  {(*ANY_MODULE*:*ANYWHERE*,i,-Exact-);(p,*NULL_POINTER*,
//   -Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}

   return 0;
   // END BLOCK
}
