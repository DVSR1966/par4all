//  {}
int main(void)
{
//  points to = {}
   int i1;
//  points to = {}
   int *pi1 = &i1;
//  {(pi1,i1,-Exact-)}
   int **pp;
//  {(pi1,i1,-Exact-);(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   srand(time((void *) 0));
//  {(pi1,i1,-Exact-);(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   if (alea())
//  {(pi1,i1,-Exact-);(pp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
      pp = &pi1;
//  {(pi1,i1,-Exact-);(pp,pi1,-MAY-);(pp,*ANY_MODULE*:*NOWHERE*_b0,
//   -MAY-)}

   *pp = (void *) 0;
//  {(*ANY_MODULE*:*ANYWHERE*_b0,*NULL_POINTER*,-MAY-);(pi1,
//   *NULL_POINTER*,-MAY-);(pi1,i1,-MAY-);(pp,pi1,-MAY-);(pp,
//   *ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
   printf("%p %p \n", pp, pi1);
//  {(*ANY_MODULE*:*ANYWHERE*_b0,*NULL_POINTER*,-MAY-);(pi1,
//   *NULL_POINTER*,-MAY-);(pi1,i1,-MAY-);(pp,pi1,-MAY-);(pp,
//   *ANY_MODULE*:*NOWHERE*_b0,-MAY-)}
   return 0;
}
//  {}
static int alea(void)
{
//  points to = {}
   int r = rand();
//  points to = {}
   printf("%d\n", r);
//  points to = {}
   return rand()%2==1;
}

