
Points-to for "call28"


// Points To IN:
// ppp -> _ppp_1 , EXACT


// Points To OUT:
// _ppp_1 -> *NULL_POINTER* , EXACT

void call28(int ***ppp)
{

// Points To:
// ppp -> _ppp_1 , EXACT

   *ppp = (void *) 0;

// Points To:
// _ppp_1 -> *NULL_POINTER* , EXACT
// ppp -> _ppp_1 , EXACT

   return;
}

Points-to for "main"


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p = &i;

// Points To:
// p -> i , EXACT

   int **pp = &p;

// Points To:
// p -> i , EXACT
// pp -> p , EXACT

   int ***ppp = &pp;

// Points To:
// p -> i , EXACT
// pp -> p , EXACT
// ppp -> pp , EXACT

   call28(ppp);

// Points To:
// p -> i , EXACT
// pp -> *NULL_POINTER* , EXACT
// ppp -> pp , EXACT

   return 0;
}

Proper effects with points-to for "main"

int main()
{
   int i;
//             <    is written>: p
   int *p = &i;
//             <    is written>: pp
   int **pp = &p;
//             <    is written>: ppp
   int ***ppp = &pp;
//             <    is read   >: ppp
//             <    is written>: pp
   call28(ppp);
   return 0;
}
