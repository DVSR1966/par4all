
PROPER  EFFECTS

void dereferencing05()
{
   double *p;
   int i;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p++ = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p-- = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *++p = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *--p = 0.;
}

POINTS-TO


// Points To: none

void dereferencing05()
{

// Points To: none

   double *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   *p++ = 0.;

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   *p-- = 0.;

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   *++p = 0.;

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   *--p = 0.;
}
