void assign03()
{
   char *malloc(int);
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ p
   double *p = (double *) malloc(10*sizeof(double));
//               <must be written>: j
   int j = 2;
//               <must be read   >: j p
//               <must be written>: p[j]

   p[j] = 1.;
}

