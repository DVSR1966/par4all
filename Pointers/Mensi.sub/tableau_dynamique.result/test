// Points To IN:
// pa -> *NULL* , MAY
// pa -> _pa_1 , MAY


// Points To OUT:
// pa -> _pa_1 , EXACT

void init_array(parray_t pa)
{

// Points To: none

   int i;

// Points To: none

   for(i = 0; i <= pa->dim-1; i += 1)

// Points To:
// pa -> *NULL* , MAY
// pa -> _pa_1 , MAY

      (pa->data)[i] = (float) i;

// Points To:
// pa -> _pa_1 , EXACT


   return;
}

