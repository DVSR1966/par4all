void allocate_array(parray_t pa)
{

// Points To:
// pa -> *NULL* , MAY
// pa -> _pa_1 , MAY

   pa->data = (float *) malloc(pa->dim*sizeof(float));

// Points To:
// _pa_1.data -> *HEAP*_l_7[0] , MAY
// pa -> _pa_1 , EXACT

   return;
}

