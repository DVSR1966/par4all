int main()
{
//             <    is written>: i j pi pj
   int i = 1, j = 2, *pi = &i, *pj = &j;
//             <    is read   >: pj
//             <    is written>: pi pj
   swap(&pi, &pj);

   return 0;
}
void swap(int **p, int **q)
{
//             <    is read   >: _p_1 p
//             <    is written>: pt
   int *pt = *p;
//             <    is read   >: _q_2 p q
//             <    is written>: _p_1
   *p = *q;
//             <    is read   >: pt q
//             <    is written>: _q_2
   *q = pt;
   return;
}
void swap(int **p, int **q);

