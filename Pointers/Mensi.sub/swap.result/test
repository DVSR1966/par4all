// Points To IN:
// _p_1 -> _p_1_1 , EXACT
// _q_2 -> _q_2_2 , EXACT
// p -> _p_1 , EXACT
// q -> _q_2 , EXACT


// Points To OUT:
// _p_1 -> _q_2_2 , EXACT
// _q_2 -> _p_1_1 , EXACT

void swap(int **p, int **q)
{

// Points To:
// _p_1 -> _p_1_1 , EXACT
// p -> _p_1 , EXACT

   int *pt = *p;

// Points To:
// _p_1 -> _p_1_1 , EXACT
// _q_2 -> _q_2_2 , EXACT
// p -> _p_1 , EXACT
// pt -> _p_1_1 , EXACT
// q -> _q_2 , EXACT

   *p = *q;

// Points To:
// _p_1 -> _q_2_2 , EXACT
// _q_2 -> _q_2_2 , EXACT
// p -> _p_1 , EXACT
// pt -> _p_1_1 , EXACT
// q -> _q_2 , EXACT

   *q = pt;

// Points To:
// _p_1 -> _q_2_2 , EXACT
// _q_2 -> _p_1_1 , EXACT
// p -> _p_1 , EXACT
// pt -> _p_1_1 , EXACT
// q -> _q_2 , EXACT

   return;
}

