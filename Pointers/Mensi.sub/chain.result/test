
POINTS TO for chain


// Points To IN:
// c1 -> _c1_1 , EXACT
// c2 -> _c2_2 , EXACT


// Points To OUT:
// _c1_1.next -> _c2_2 , EXACT

void chain(ilp_t c1, ilp_t c2)
{

// Points To:
// c1 -> _c1_1 , EXACT
// c2 -> _c2_2 , EXACT

   c1->next = c2;

// Points To:
// _c1_1.next -> _c2_2 , EXACT
// c1 -> _c1_1 , EXACT
// c2 -> _c2_2 , EXACT


   return;
}

