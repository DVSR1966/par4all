POINTS-TO for "main"


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   ilp_t x1 = (ilp_t) malloc(sizeof(il_t));

// Points To:
// x1 -> *HEAP*_l_17 , MAY

   ilp_t x2 = (ilp_t) malloc(sizeof(il_t));

// Points To:
// x1 -> *HEAP*_l_17 , MAY
// x2 -> *HEAP*_l_18 , MAY

   x1->next = (void *) 0;

// Points To:
// *HEAP*_l_17.next -> *NULL_POINTER* , MAY
// x1 -> *HEAP*_l_17 , MAY
// x2 -> *HEAP*_l_18 , MAY

   x2->next = (void *) 0;

// Points To:
// *HEAP*_l_17.next -> *NULL_POINTER* , MAY
// *HEAP*_l_18.next -> *NULL_POINTER* , MAY
// x1 -> *HEAP*_l_17 , MAY
// x2 -> *HEAP*_l_18 , MAY


   chain(x1, x2);

// Points To:
// *HEAP*_l_17.next -> *HEAP*_l_18 , MAY
// *HEAP*_l_17.next -> *NULL_POINTER* , MAY
// *HEAP*_l_18.next -> *NULL_POINTER* , MAY
// x1 -> *HEAP*_l_17 , MAY
// x2 -> *HEAP*_l_18 , MAY


   return 0;
}
