// Points To IN:
// _p_1 -> _p_1_1 , EXACT
// _q_2 -> _q_2_2 , EXACT
// p -> _p_1 , EXACT
// q -> _q_2 , EXACT


// Points To OUT:
// _p_1 -> _q_2_2 , EXACT
// _q_2 -> _p_1_1 , EXACT

void swap01(int **p, int **q)
{

// Points To:
// _p_1 -> _p_1_1 , EXACT
// p -> _p_1 , EXACT

   int *pt = *p;

// Points To:
// _p_1 -> _p_1_1 , EXACT
// _q_2 -> _q_2_2 , EXACT
// p -> _p_1 , EXACT
// pt -> _p_1_1 , EXACT
// q -> _q_2 , EXACT

   *p = *q;

// Points To:
// _p_1 -> _q_2_2 , EXACT
// _q_2 -> _q_2_2 , EXACT
// p -> _p_1 , EXACT
// pt -> _p_1_1 , EXACT
// q -> _q_2 , EXACT

   *q = pt;

// Points To:
// _p_1 -> _q_2_2 , EXACT
// _q_2 -> _p_1_1 , EXACT
// p -> _p_1 , EXACT
// pt -> _p_1_1 , EXACT
// q -> _q_2 , EXACT

   return;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i j pi pj ppi ppj
   int i = 1, j = 2, *pi = &i, *pj = &j, **ppi = &pi, **ppj = &pj;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: pi pj
   swap01(ppi, ppj);

   return 0;
}
void swap01(int **p, int **q)
{
//             <    is read   >: _p_1 p
//             <    is written>: pt
   int *pt = *p;
//             <    is read   >: _q_2 p q
//             <    is written>: _p_1
   *p = *q;
//             <    is read   >: pt q
//             <    is written>: _q_2
   *q = pt;
   return;
}
void swap01(int **p, int **q);

