//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
int main()
{
//             <    is written>: i j pi pj ppi ppj
   int i = 1, j = 2, *pi = &i, *pj = &j, **ppi = &pi, **ppj = &pj;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: pi pj
   swap01(ppi, ppj);

   return 0;
}
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: _p_1 _q_2
void swap01(int **p, int **q)
{
//             <    is read   >: _p_1 p
//             <    is written>: pt
   int *pt = *p;
//             <    is read   >: _q_2 p q
//             <    is written>: _p_1
   *p = *q;
//             <    is read   >: pt q
//             <    is written>: _q_2
   *q = pt;
   return;
}
void swap01(int **p, int **q);

