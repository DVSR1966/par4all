void global01()
{
   // BEGIN BLOCK
//               <must be written>: TOP-LEVEL:i
   i = &k;
//               <must be read   >: TOP-LEVEL:i
//               <must be written>: TOP-LEVEL:j
   j = i;
//               <must be read   >: TOP-LEVEL:i __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("i = %p", i);
   // END BLOCK
}
POINTS TO
//  {}
void global01()
{
   // BEGIN BLOCK
   i = &k;
//  {(i,k,-Exact-)}
   j = i;
//  {(j,k,-Exact-);(i,k,-Exact-)}
   printf("i = %p", i);
   // END BLOCK
}
