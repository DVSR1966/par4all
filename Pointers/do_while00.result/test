int main()
{
   // BEGIN BLOCK
   int *a, *b, c, d, i;
//               <must be written>: c
   c = 0;
//               <must be written>: d
   d = 1;
//               <must be written>: i
   i = 2;
//               <must be written>: b
   b = &c;
//               <must be read   >: i
   do {
      // BEGIN BLOCK
//               <must be read   >: i
//               <must be written>: i
      i = i+1;
//               <must be read   >: b
//               <must be written>: a
      a = b;
//               <must be written>: b
      b = &d;
      // END BLOCK
   }
   while (i<5);

   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int *a, *b, c, d, i;
//  points to = {}
   c = 0;
//  points to = {}
   d = 1;
//  points to = {}
   i = 2;
//  points to = {}
   b = &c;
//  {(b,c,-Exact-)}
   do {
//  {(a,c,-MAY-);(b,d,-Exact-);(a,d,-MAY-)}
      // BEGIN BLOCK
//  points to = {}
      i = i+1;
//  points to = {}
      a = b;
//  points to = {}
      b = &d;
      // END BLOCK
   }
   while (i<5);
//  {(a,c,-MAY-);(b,d,-Exact-);(a,d,-MAY-)}

   return 0;
   // END BLOCK
}
