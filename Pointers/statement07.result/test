int main()
{
   // BEGIN BLOCK
   int *x, *a, i;
//               <must be written>: i
   i = 1;
//               <must be written>: a
   a = &i;
//               <must be read   >: a
//               <must be written>: x
   x = a;

   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int *x, *a, i;
//  points to = {}
   i = 1;
//  points to = {}
   a = &i;
//  {(a,i,-Exact-)}
   x = a;
//  {(x,i,-Exact-);(a,i,-Exact-)}

   return 0;
   // END BLOCK
}
