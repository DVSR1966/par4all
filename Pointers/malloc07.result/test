
PROPER  EFFECTS

int main(int argc, char *argv[])
{
   int *pi;
   int *qi;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: pi _MALLOC_EFFECTS:_MALLOC_

   pi = (int *) malloc(sizeof(int));
//             <    is read   >: pi
//             <    is written>: qi
   qi = pi;
//             <    is read   >: pi _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   free(pi);

   return 0;
}

