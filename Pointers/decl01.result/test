
Proper effects for "decl01"

int decl01()
{
//               <must be written>: i
   int i = 2;
//               <must be read   >: i
//               <must be written>: j
   int j = i;
   int a[sizeof(i)];
//               <must be written>: i

   i = 2;
//               <must be read   >: i
//               <must be written>: j
   j = i;
//               <must be written>: j
   j = sizeof(i);
   return 0;
}

POINTS TO

//  {}
int decl01()
{
//  points to = {}
   int i = 2;
//  points to = {}
   int j = i;
//  points to = {}
   int a[sizeof(i)];
//  points to = {}

   i = 2;
//  points to = {}
   j = i;
//  points to = {}
   j = sizeof(i);
//  points to = {}
   return 0;
}
