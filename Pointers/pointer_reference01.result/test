
POINTS-TO


// Points To IN:
// _p_1[3] -> *NULL_POINTER* , MAY
// _p_1[3] -> _p_1_1[3][0] , MAY
// _p_1_1[3][0] -> *NULL_POINTER* , MAY
// _p_1_1[3][0] -> _p_1_1_1[3][0][0] , MAY
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY


// Points To OUT:
// _p_1[3] -> _p_1_1[3][0] , MAY
// _p_1_1[3][0] -> *NULL_POINTER* , MAY
// _p_1_1[3][0] -> _p_1_1_1[3][0][0] , MAY

int pointer_reference01(char ***p)
{

// Points To:
// _p_1[3] -> *NULL_POINTER* , MAY
// _p_1[3] -> _p_1_1[3][0] , MAY
// _p_1_1[3][0] -> *NULL_POINTER* , MAY
// _p_1_1[3][0] -> _p_1_1_1[3][0][0] , MAY
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY

   char *q = *p[3];

// Points To:
// _p_1[3] -> _p_1_1[3][0] , MAY
// _p_1_1[3][0] -> *NULL_POINTER* , MAY
// _p_1_1[3][0] -> _p_1_1_1[3][0][0] , MAY
// p -> _p_1[0] , EXACT
// q -> *NULL_POINTER* , MAY
// q -> _p_1_1_1[3][0][0] , MAY

   char *r;

// Points To:
// _p_1[3] -> _p_1_1[3][0] , MAY
// _p_1_1[3][0] -> *NULL_POINTER* , MAY
// _p_1_1[3][0] -> _p_1_1_1[3][0][0] , MAY
// p -> _p_1[0] , EXACT
// q -> *NULL_POINTER* , MAY
// q -> _p_1_1_1[3][0][0] , MAY
// r -> undefined , EXACT

   r = *p[3];

// Points To:
// _p_1[3] -> _p_1_1[3][0] , MAY
// _p_1_1[3][0] -> *NULL_POINTER* , MAY
// _p_1_1[3][0] -> _p_1_1_1[3][0][0] , MAY
// p -> _p_1[0] , EXACT
// q -> *NULL_POINTER* , MAY
// q -> _p_1_1_1[3][0][0] , MAY
// r -> *NULL_POINTER* , MAY
// r -> _p_1_1_1[3][0][0] , MAY

   return q-*p[3];
}
