
PROPER  EFFECTS

void assignment03()
{
   int i;
   int *p;
   int *q;
//             <    is written>: p

   p = &i;
//             <    is read   >: q
//             <    is written>: p
   p = q;

   return;
}

POINTS-TO


// Points To IN: none


// Points To OUT: unreachable

void assignment03()
{

// Points To: none

   int i;

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT

   int *q;

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT


   p = &i;

// Points To:
// p -> i , EXACT
// q -> undefined , EXACT

   p = q;

// Points To: unreachable


   return;
}
