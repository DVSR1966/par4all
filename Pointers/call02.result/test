
PROPER POINTER EFFECTS

int main()
{
   int a;
   int b;
   int x[10];
   int *ap[10];
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <may be read   >: ap[*]
//             <may be written>: ap[*][0] sp[0].two[*] x[*]
//             <    is read   >: a b sp
//             <    is written>: sp[0].one

   call02(a, b, x, ap, sp);
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int a;
//  points to = {}
   int b;
//  points to = {}
   int x[10];
//  points to = {}
   int *ap[10];
//  points to = {}
   tf_t s;
//  points to = {}
   tf_t *sp = &s;
//  {(sp,s,-Exact-)}

   call02(a, b, x, ap, sp);
}
