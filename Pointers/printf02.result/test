
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 5;
//             <    is written>: j
   int j = 6;
   float x;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i j stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("i=%d, %d, %f\n", i, i+j, x);
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int i = 5;
//  points to = {}
   int j = 6;
//  points to = {}
   float x;
//  points to = {}
   printf("i=%d, %d, %f\n", i, i+j, x);
}
