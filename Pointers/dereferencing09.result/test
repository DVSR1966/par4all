
Proper effects for "main"

int main()
{
   int a, aa, *b, *bb, **c;                                             /*0003*/
//             <    is written>: a

   a = 1;                                                               /*0005*/
//             <    is written>: b
   b = &a;                                                              /*0006*/
//             <    is written>: c
   c = &b;                                                              /*0007*/
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: bb

   bb = *c;                                                             /*0009*/
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: aa
   aa = **c;                                                            /*0010*/
   return 0;                                                            /*0011*/
}

POINTS TO for "main"

//  {}
int main()
{
//  points to = {}
   int a, aa, *b, *bb, **c;                                             /*0003*/
//  {(b,*ANY_MODULE*:*NOWHERE*,-Exact-);(bb,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(c,*ANY_MODULE*:*NOWHERE*,-Exact-)}

   a = 1;                                                               /*0005*/
//  {(b,*ANY_MODULE*:*NOWHERE*,-Exact-);(bb,*ANY_MODULE*:*NOWHERE*,
//   -Exact-);(c,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   b = &a;                                                              /*0006*/
//  {(b,a,-Exact-);(bb,*ANY_MODULE*:*NOWHERE*,-Exact-);(c,
//   *ANY_MODULE*:*NOWHERE*,-Exact-)}
   c = &b;                                                              /*0007*/
//  {(b,a,-Exact-);(bb,*ANY_MODULE*:*NOWHERE*,-Exact-);(c,b,-Exact-)}

   bb = *c;                                                             /*0009*/
//  {(b,a,-Exact-);(bb,a,-Exact-);(c,b,-Exact-)}
   aa = **c;                                                            /*0010*/
//  {(b,a,-Exact-);(bb,a,-Exact-);(c,b,-Exact-)}
   return 0;                                                            /*0011*/
}
