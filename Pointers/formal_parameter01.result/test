// pi -> _pi_1[0] , MAY


// Points To OUT:
// _pi_1[*] -> undefined , MAY
// _pi_1[1] -> undefined , MAY
// _pi_1[*] -> *NULL_POINTER* , MAY
// _pi_1[1] -> *NULL_POINTER* , MAY
// _pi_1[*] -> _pi_1_1[*][0] , MAY
// _pi_1[*] -> _pi_1_1[1][0] , MAY
// _pi_1[1] -> _pi_1_1[1][0] , MAY
// pi -> undefined , MAY

int formal_parameter01(int **pi)
{

// Points To:
// _pi_1[*] -> *NULL_POINTER* , MAY
// _pi_1[*] -> _pi_1_1[*][0] , MAY
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY

   /* FI: I need the summary for the sequence */
   if (1) {

// Points To: none

      int **q;

// Points To:
// q -> undefined , EXACT

      int *i;

// Points To:
// i -> undefined , EXACT
// q -> undefined , EXACT

      int j;

// Points To:
// i -> undefined , EXACT
// q -> undefined , EXACT


      i = 0;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> undefined , EXACT

      q = pi;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _pi_1[0] , MAY

      q++;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> _pi_1[1] , EXACT

      pi = &i;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> i , EXACT
// q -> _pi_1[1] , EXACT

      *pi = &j;

// Points To:
// i -> j , EXACT
// pi -> i , EXACT
// q -> _pi_1[1] , EXACT

      *q = &j;
   }

// Points To:
// _pi_1[*] -> undefined , MAY
// _pi_1[1] -> undefined , MAY
// _pi_1[*] -> *NULL_POINTER* , MAY
// _pi_1[1] -> *NULL_POINTER* , MAY
// _pi_1[*] -> _pi_1_1[*][0] , MAY
// _pi_1[*] -> _pi_1_1[1][0] , MAY
// _pi_1[1] -> _pi_1_1[1][0] , MAY
// pi -> undefined , MAY
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY

   return 0;
}

Cumulated effects for "formal_parameter01"

//             <may be read   >: pi
//             <may be written>: _pi_1[1]
int formal_parameter01(int **pi)
{
//             <may be read   >: pi
//             <may be written>: _pi_1[1] pi
   /* FI: I need the summary for the sequence */
   if (1) {
      int **q;
      int *i;
      int j;
//             <    is written>: i

      i = 0;
//             <    is read   >: pi
//             <    is written>: q
      q = pi;
//             <    is read   >: q
//             <    is written>: q
      q++;
//             <    is written>: pi
      pi = &i;
//             <    is read   >: pi
//             <    is written>: i
      *pi = &j;
//             <    is read   >: q
//             <    is written>: _pi_1[1]
      *q = &j;
   }
   return 0;
}

