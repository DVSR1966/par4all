
Points-to for "formal_parameter01"


// Points To IN:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY


// Points To OUT:
// _pi_1[*] -> *ANY_MODULE*:*NOWHERE*_b1 , MAY
// _pi_1[1] -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// pi -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

int formal_parameter01(int **pi)
{

// Points To: none

   /* FI: I need the summary for the sequence */
   if (1) {

// Points To: none

      int **q;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      int *i;

// Points To:
// i -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      int j;

// Points To:
// i -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


      i = 0;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      q = pi;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _pi_1[0] , MAY

      q++;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> _pi_1[1] , EXACT

      pi = &i;

// Points To:
// i -> *NULL_POINTER* , EXACT
// pi -> i , EXACT
// q -> _pi_1[1] , EXACT

      *pi = &j;

// Points To:
// i -> j , EXACT
// pi -> i , EXACT
// q -> _pi_1[1] , EXACT

      *q = &j;
   }

// Points To:
// _pi_1[*] -> *ANY_MODULE*:*NOWHERE*_b1 , MAY
// _pi_1[1] -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// pi -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return 0;
}

Cumulated effects for "formal_parameter01"

//             <may be read   >: pi
//             <may be written>: _pi_1[1]
int formal_parameter01(int **pi)
{
//             <may be read   >: pi
//             <may be written>: _pi_1[1] pi
   /* FI: I need the summary for the sequence */
   if (1) {
      int **q;
      int *i;
      int j;
//             <    is written>: i

      i = 0;
//             <    is read   >: pi
//             <    is written>: q
      q = pi;
//             <    is read   >: q
//             <    is written>: q
      q++;
//             <    is written>: pi
      pi = &i;
//             <    is read   >: pi
//             <    is written>: i
      *pi = &j;
//             <    is read   >: q
//             <    is written>: _pi_1[1]
      *q = &j;
   }
   return 0;
}

