int main()
{
   // BEGIN BLOCK
   int *x, *y, k[5], h;
   k = 1;
//             <    is written>: h
   h = 2;
//             <    is written>: x

   x = &k[2];
//             <    is written>: y
   y = &h;
//             <    is read   >: y
//             <    is written>: x

   x = y;

   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int *x, *y, k[5], h;
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   k = 1;
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   h = 2;
//  {(x,*ANY_MODULE*:*NOWHERE*,-Exact-);(y,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}

   x = &k[2];
//  {(x,k[2],-Exact-);(y,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   y = &h;
//  {(x,k[2],-Exact-);(y,h,-Exact-)}

   x = y;
//  {(x,h,-Exact-);(y,h,-Exact-)}

   return 0;
   // END BLOCK
}
