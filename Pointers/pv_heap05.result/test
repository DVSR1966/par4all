
PROPER  EFFECTS

int main()
{
   int ***p;
   int **q;
//             <    is written>: a
   int a = 0;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: q _MALLOC_EFFECTS:_MALLOC_
   q = (int **) malloc(sizeof(int *));
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   q[0] = &a;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int ***) malloc(sizeof(int * *));
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p q
   p[0] = q;
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   free(p);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT:
// *HEAP*_l_9 -> a , MAY

int main()
{

// Points To: none

   int ***p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int **q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int a = 0;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   q = (int **) malloc(sizeof(int *));

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *HEAP*_l_9 , MAY

   q[0] = &a;

// Points To:
// *HEAP*_l_9 -> a , MAY
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *HEAP*_l_9 , MAY

   p = (int ***) malloc(sizeof(int * *));

// Points To:
// *HEAP*_l_9 -> a , MAY
// p -> *HEAP*_l_11 , MAY
// q -> *HEAP*_l_9 , MAY

   p[0] = q;

// Points To:
// *HEAP*_l_11[0] -> *HEAP*_l_9 , MAY
// *HEAP*_l_9 -> a , MAY
// p -> *HEAP*_l_11 , MAY
// q -> *HEAP*_l_9 , MAY

   free(p);

// Points To:
// *HEAP*_l_9 -> a , MAY
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *HEAP*_l_9 , MAY

   return 0;
}
