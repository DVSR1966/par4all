
PROPER POINTER EFFECTS

int main()
{
   mys *p, *q, **r, *s[5];
//             <may be read   >: p[0].tab1[*]
//             <may be written>: p[0].tab1[*] p[0].tab2[*]
//             <    is read   >: p p[0].tab2 _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p[0].num p[0].tab1[0] p[0].tab2 
//                               p[0].tab2[0] _MALLOC_EFFECTS:_MALLOC_

   init(p);
//             <may be read   >: q[0].tab1[*]
//             <    is read   >: q q[0].num q[0].tab2 
//                               _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   init2(q);
//             <may be read   >: r[*] r[*][0].tab1[0] r[*][0].tab2
//             <may be written>: r[*] r[*][0].num r[*][0].tab1[0] 
//                               r[*][0].tab2 r[*][0].tab2[0]
//             <    is read   >: r r[0] r[0][0].tab2 
//                               _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: r[0][0].num r[0][0].tab1[0] 
//                               r[0][0].tab2[0] 
//                               _MALLOC_EFFECTS:_MALLOC_
   init3(r);
//             <may be read   >: s[*] s[*][0].tab1[0] s[*][0].tab2 
//                               _MALLOC_EFFECTS:_MALLOC_
//             <may be written>: s[*] s[*][0].num s[*][0].tab1[0] 
//                               s[*][0].tab2 s[*][0].tab2[0] 
//                               _MALLOC_EFFECTS:_MALLOC_
//             <    is read   >: s[0] s[0][0].tab2
//             <    is written>: s[0][0].num s[0][0].tab1[0] 
//                               s[0][0].tab2[0]
   init4(s);

   return 1;
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   mys *p, *q, **r, *s[5];
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}

   init(p);
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   init2(q);
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   init3(r);
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}
   init4(s);
//  {(p,*ANY_MODULE*:*NOWHERE*_b0,-Exact-);(q,
//   *ANY_MODULE*:*NOWHERE*_b0,-Exact-);(r,*ANY_MODULE*:*NOWHERE*_b1,
//   -Exact-)}

   return 1;
}
