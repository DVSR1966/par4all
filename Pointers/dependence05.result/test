// Points To OUT:
// dependence05 -> *HEAP*_l_10[0] , MAY

int * dependence05()
{

// Points To: none

   int *a;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int *b;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int *c;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   b = (int *) malloc(10*sizeof(int));

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *HEAP*_l_10[0] , MAY
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   c = (int *) malloc(10*sizeof(int));

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// b -> *HEAP*_l_10[0] , MAY
// c -> *HEAP*_l_11[0] , MAY


   a = b;

// Points To:
// a -> *HEAP*_l_10[0] , MAY
// b -> *HEAP*_l_10[0] , MAY
// c -> *HEAP*_l_11[0] , MAY

   // a and b are aliased, a points_to b[0]
   *a = 0;

// Points To:
// a -> *HEAP*_l_10[0] , MAY
// b -> *HEAP*_l_10[0] , MAY
// c -> *HEAP*_l_11[0] , MAY

   // write effect on *a may be visible from caller, because b escape
   
   a = c;

// Points To:
// a -> *HEAP*_l_11[0] , MAY
// b -> *HEAP*_l_10[0] , MAY
// c -> *HEAP*_l_11[0] , MAY

   // a and b are aliased, a points_to b[0]
   *a = 0;

// Points To:
// a -> *HEAP*_l_11[0] , MAY
// b -> *HEAP*_l_10[0] , MAY
// c -> *HEAP*_l_11[0] , MAY

   // write effect won't be visible from caller, because c doesn't escape
   
   free(c);

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// b -> *HEAP*_l_10[0] , MAY
// c -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   return b;
}

