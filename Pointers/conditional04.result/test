
POINTS-TO


// Points To IN:
// x -> "a" , EXACT
// y -> "b" , EXACT
// z -> "c" , EXACT


// Points To OUT:
// conditional04 -> "a" , MAY
// conditional04 -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// x -> "a" , EXACT
// y -> "b" , EXACT
// z -> "c" , EXACT

char * conditional04(int i)
{

// Points To: none

   char *p[3];

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p[0] = x;

// Points To:
// p[0] -> "a" , EXACT
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// x -> "a" , EXACT

   p[1] = y;

// Points To:
// p[0] -> "a" , EXACT
// p[1] -> "b" , EXACT
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// x -> "a" , EXACT
// y -> "b" , EXACT

   p[2] = z;

// Points To:
// p[0] -> "a" , EXACT
// p[1] -> "b" , EXACT
// p[2] -> "c" , EXACT
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// x -> "a" , EXACT
// y -> "b" , EXACT
// z -> "c" , EXACT

   char *r = i<0||i>2?p[0]:p[i];

// Points To:
// p[0] -> "a" , EXACT
// p[1] -> "b" , EXACT
// p[2] -> "c" , EXACT
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> "a" , MAY
// r -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// x -> "a" , EXACT
// y -> "b" , EXACT
// z -> "c" , EXACT

   return r;
}
