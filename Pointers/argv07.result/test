// p -> _ap_2_2[0][1][0] , MAY

   printf("\"%s\"\n", p);

// Points To:
// _ap_2[0][1] -> *NULL_POINTER* , MAY
// _ap_2[0][1] -> _ap_2_2[0][1][0] , MAY
// ap -> _ap_2[0][0] , EXACT
