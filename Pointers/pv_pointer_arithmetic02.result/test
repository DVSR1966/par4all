
PROPER  EFFECTS

int main()
{
   int a[10];
   int *p;
//             <    is written>: b c
   int b = 0, c = 1;
//             <    is read   >: b c
   if (b==c)
//             <    is written>: p
      p = &a[0];
   else
//             <    is written>: p
      p = &a[1];
//             <    is read   >: p
//             <    is written>: p
   p = p+1;
//             <    is read   >: p
//             <    is written>: p
   p = p+3;
//             <    is read   >: p
//             <    is written>: p
   p = p-1;
//             <    is read   >: p
//             <    is written>: p
   p = p-2;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a[10];

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b = 0, c = 1;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if (b==c)

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &a[0];
   else

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      p = &a[1];

// Points To:
// p -> a[0] , MAY
// p -> a[1] , MAY

   p = p+1;

// Points To:
// p -> a[1] , MAY
// p -> a[2] , MAY

   p = p+3;

// Points To:
// p -> a[4] , MAY
// p -> a[5] , MAY

   p = p-1;

// Points To:
// p -> a[3] , MAY
// p -> a[4] , MAY

   p = p-2;

// Points To:
// p -> a[1] , MAY
// p -> a[2] , MAY

   return 0;
}
