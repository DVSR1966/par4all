
PROPER EFFECTS

void fclose01()
{
   FILE *f;
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: __IO_EFFECTS:LUNS[*]
//             <    is written>: f
   f = fopen("toto", "r");
//             <may be read   >: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: *ANY_MODULE*:*ANYWHERE* 
//                               __IO_EFFECTS:LUNS[*]
   fclose(f);
   return;
}

PROPER POINTER EFFECTS

void fclose01()
{
   FILE *f;
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: __IO_EFFECTS:LUNS[*]
//             <    is written>: f
   f = fopen("toto", "r");
//             <may be read   >: f[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: f[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: f
   fclose(f);
   return;
}

POINTS TO


// Points To IN: none


// Points To OUT: none

void fclose01()
{

// Points To: none

   FILE *f;

// Points To:
// f -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   f = fopen("toto", "r");

// Points To:
// f -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   fclose(f);

// Points To:
// f -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return;
}
