
PROPER POINTER EFFECTS

int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: __IO_EFFECTS:LUNS[*]
//             <    is written>: fp
   fp = fopen("file.txt", "rb");
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: buf[*] fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
//             <    is written>: n
   n = fread(&buf[0], sizeof(double), 50, fp);
//             <    is read   >: n
   if (n==-1)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("fread failed");
//             <may be read   >: fp[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: fp[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fp
   fclose(fp);
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   FILE *fp;
//  {(fp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   size_t n;
//  {(fp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   char buf[200];
//  {(fp,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   fp = fopen("file.txt", "rb");
//  {(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   n = fread(&buf[0], sizeof(double), 50, fp);
//  {(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   if (n==-1)
//  {(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
      printf("fread failed");
//  {(fp,*ANY_MODULE*:*ANYWHERE*,-Exact-)}
   fclose(fp);
}
