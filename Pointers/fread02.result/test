int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
   fp = fopen("file.txt", "rb");
   n = fread(&buf[0], sizeof(double), 50, fp);
   if (n==-1) 
      printf("fread failed");
   fclose(fp);
}
//               <may be read    >: *ANY_MODULE*:*ANYWHERE* 
//                                  __IO_EFFECTS:LUNS[1] 
//                                  __IO_EFFECTS:LUNS[*]
//               <may be written >: *ANY_MODULE*:*ANYWHERE* 
//                                  __IO_EFFECTS:LUNS[1] 
//                                  __IO_EFFECTS:LUNS[*]
int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
//               <must be written>: fp
   fp = fopen("file.txt", "rb");
//               <may be read    >: __IO_EFFECTS:LUNS[*] fp[*]
//               <may be written >: __IO_EFFECTS:LUNS[*] buf[*] fp[*]
//               <must be read   >: fp
//               <must be written>: n
   n = fread(&buf[0], sizeof(double), 50, fp);
//               <may be read    >: __IO_EFFECTS:LUNS[1]
//               <may be written >: __IO_EFFECTS:LUNS[1]
//               <must be read   >: n
   if (n==-1) 
//               <must be read   >: __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
      printf("fread failed");
//               <may be read    >: __IO_EFFECTS:LUNS[*] fp[*]
//               <may be written >: __IO_EFFECTS:LUNS[*] fp[*]
//               <must be read   >: fp
   fclose(fp);
}

POINTS TO

//  {}
int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
   fp = fopen("file.txt", "rb");
   n = fread(&buf[0], sizeof(double), 50, fp);
   if (n==-1) 
      printf("fread failed");
   fclose(fp);
}
