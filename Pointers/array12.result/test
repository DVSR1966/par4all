
POINTS-TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY


// Points To OUT:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _p_1[0] , MAY

int array12(int *p, int *q)
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY

   int b[*(q = p)];

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _p_1[0] , MAY

   return b[0];
}
