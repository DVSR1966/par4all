int main()
{
   // BEGIN BLOCK
   int *a, *b, c, d, i;
//               <must be written>: c
   c = 0;
//               <must be written>: d
   d = 1;
//               <must be written>: b
   b = &c;
//               <must be read   >: i
//               <must be written>: i

   for (i = 1;i<5;i++)
//               <must be written>: c
      c = 5;

   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
   // BEGIN BLOCK
   int *a, *b, c, d, i;
   c = 0;
   d = 1;
   b = &c;
//  {(b,c,-Exact-)}

   for (i = 1;i<5;i++)
//  {(b,c,-Exact-)}
      c = 5;
//  {(b,c,-Exact-)}

   return 0;
   // END BLOCK
}
