int conditional_01()
{
   // BEGIN BLOCK
   int *q;
   int *p;
//               <must be written>: i j
   int i = 0, j = 1;
//               <must be written>: q
   q = &i;
//               <may be read    >: q
//               <must be read   >: i
//               <must be written>: p
   p = i>0?&j:q;
   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int conditional_01()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int *q;
//  points to = {}
   int *p;
//  points to = {}
   int i = 0, j = 1;
//  points to = {}
   q = &i;
//  {(q,i,-Exact-)}
   p = i>0?&j:q;
//  {(q,i,-Exact-)}
   return 0;
   // END BLOCK
}
