
PROPER  EFFECTS

void formal_parameter00(int *pi)
{
   int *q;
   int i;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is written>: pi
   pi = &i;
   return 0;
}

POINTS-TO

//  {(pi,_pi_1,-Exact-)}
void formal_parameter00(int *pi)
{
//  {(pi,_pi_1,-Exact-)}
   int *q;
//  {(pi,_pi_1,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   int i;
//  {(pi,_pi_1,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}

   i = 0;
//  {(pi,_pi_1,-Exact-);(q,*ANY_MODULE*:*NOWHERE*_b0,-Exact-)}
   q = pi;
//  {(pi,_pi_1,-Exact-);(q,_pi_1,-Exact-)}
   pi = &i;
//  {(pi,i,-Exact-);(q,_pi_1,-Exact-)}
   return 0;
}
