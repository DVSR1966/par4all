void array02()
{
   // BEGIN BLOCK
   float x[11][2];
   int i;
//               <must be written>: i

   for(i = 1; i <= 9; i += 1)
//               <must be read   >: i
//               <must be written>: x[i][1]
      x[i][1] = 0.;
   // END BLOCK
}
POINTS TO
//  {}
void array02()
{
   // BEGIN BLOCK
   float x[11][2];
   int i;

   for(i = 1; i <= 9; i += 1)
      x[i][1] = 0.;
   // END BLOCK
}
