int main()
{
   // BEGIN BLOCK
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
   my_str *m, *n;
   int j;
//               <must be written>: j
   j = 2;
//               <must be read   >: _MALLOC_EFFECTS:_MALLOC_
//               <must be written>: _MALLOC_EFFECTS:_MALLOC_ m
   m = (my_str *) malloc(sizeof(my_str));
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: m
   m->i = 1;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: m
   m->p = &j;
//               <must be read   >: m
//               <must be written>: n
   n = m;

   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
   // BEGIN BLOCK
   typedef struct my_struct {
      int i;
      int *p;
   } my_str;
   my_str *m, *n;
   int j;
   j = 2;
   m = (my_str *) malloc(sizeof(my_str));
//  {(m,*ANYWHERE*,-MAY-)}
   m->i = 1;
//  {(m,*ANYWHERE*,-MAY-)}
   m->p = &j;
//  {(m[0][2],j,-Exact-)}
   n = m;
//  {(n,j,-Exact-);(m[0][2],j,-Exact-)}

   return 0;
   // END BLOCK
}
