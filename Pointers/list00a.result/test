
Points-to for "list_next"


// Points To IN:
// _l_1[0].next -> *NULL_POINTER* , MAY
// _l_1[0].next -> _l_1_2__1[0][0] , MAY
// l -> *NULL_POINTER* , MAY
// l -> _l_1[0] , MAY
// nil -> *NULL_POINTER* , EXACT


// Points To OUT:
// _l_1[0].next -> *NULL_POINTER* , MAY
// _l_1[0].next -> _l_1_2__1[0][0] , MAY
// l -> *NULL_POINTER* , MAY
// l -> _l_1[0] , MAY
// list_next -> *NULL_POINTER* , MAY
// list_next -> _l_1_2__1[0][0] , MAY
// nil -> *NULL_POINTER* , EXACT

list list_next(list l)
{

// Points To: none

   list n;

// Points To:
// _l_1[0].next -> *NULL_POINTER* , MAY
// _l_1[0].next -> _l_1_2__1[0][0] , MAY
// l -> *NULL_POINTER* , MAY
// l -> _l_1[0] , MAY
// n -> undefined , EXACT
// nil -> *NULL_POINTER* , EXACT

   if (l==nil)

// Points To:
// l -> *NULL_POINTER* , EXACT
// n -> undefined , EXACT
// nil -> *NULL_POINTER* , EXACT

      n = nil;
   else

// Points To:
// _l_1[0].next -> *NULL_POINTER* , MAY
// _l_1[0].next -> _l_1_2__1[0][0] , MAY
// l -> _l_1[0] , EXACT
// n -> undefined , EXACT
// nil -> *NULL_POINTER* , EXACT

      n = l->next;

// Points To:
// _l_1[0].next -> *NULL_POINTER* , MAY
// _l_1[0].next -> _l_1_2__1[0][0] , MAY
// l -> *NULL_POINTER* , MAY
// l -> _l_1[0] , MAY
// n -> *NULL_POINTER* , MAY
// n -> _l_1_2__1[0][0] , MAY
// nil -> *NULL_POINTER* , EXACT

   return n;
}

Proper effects for "list_next"

list list_next(list l)
{
   list n;
//             <    is read   >: l nil
   if (l==nil)
//             <    is read   >: nil
//             <    is written>: n
      n = nil;
   else
//             <    is read   >: _l_1[0].next l
//             <    is written>: n
      n = l->next;
//             <    is read   >: n
   return n;
}

