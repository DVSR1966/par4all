
PROPER POINTER EFFECTS

int struct01()
{
   struct one {
      int first;
      int second;
   } x;
   int y[10];
//             <    is written>: x.first

   x.first = 1;
//             <    is written>: x.second
   x.second = 2;
//             <    is written>: y[0]

   y[0] = 1;
}

POINTS-TO

//  {}
int struct01()
{
//  points to = {}
   struct one {
      int first;
      int second;
   } x;
//  points to = {}
   int y[10];
//  points to = {}

   x.first = 1;
//  points to = {}
   x.second = 2;
//  points to = {}

   y[0] = 1;
}
