
PROPER  EFFECTS

void dereferencing03()
{
   double *p;
   int i;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *(p+1) = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
   *(p+i) = 0.;
}

POINTS-TO


// Points To: none

void dereferencing03()
{

// Points To: none

   double *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   *(p+1) = 0.;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   *(p+i) = 0.;
}
