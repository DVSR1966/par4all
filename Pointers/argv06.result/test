
POINTS-TO


// Points To IN:
// _argv_2[2][1] -> *NULL_POINTER* , MAY
// _argv_2[2][1] -> _argv_2_2[2][1][0] , MAY
// argv -> *NULL_POINTER* , MAY
// argv -> _argv_2[0][0] , MAY


// Points To OUT:
// _argv_2[2][1] -> *NULL_POINTER* , MAY
// _argv_2[2][1] -> _argv_2_2[2][1][0] , MAY

int argv06(int argc, char *(*argv)[argc])
{

// Points To: none

   char *p = (void *) 0;

// Points To:
// p -> *NULL_POINTER* , EXACT

   argv++;

// Points To:
// argv -> _argv_2[0][1] , EXACT
// p -> *NULL_POINTER* , EXACT

   p = (*argv)[2];

// Points To:
// _argv_2[2][1] -> *NULL_POINTER* , MAY
// _argv_2[2][1] -> _argv_2_2[2][1][0] , MAY
// argv -> _argv_2[0][1] , EXACT
// p -> *NULL_POINTER* , MAY
// p -> _argv_2_2[2][1][0] , MAY

   return p==p;
}
