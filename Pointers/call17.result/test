void call17(int *x)
{
   /* check aliases */
   int *y = x;
   y[0] = 1;
}

Proper effects for "call17"

void call17(int *x)
{
   /* check aliases */
//             <    is read   >: x
//             <    is written>: y
   int *y = x;
//             <    is read   >: y
//             <    is written>: y[0]
   y[0] = 1;
}

