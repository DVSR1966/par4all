
PROPER  EFFECTS

void arithmetic03(int *p)
{
   int *q;
//             <    is read   >: p
//             <    is written>: q

   q = p;
//             <    is read   >: q
//             <    is written>: q
   // not standard compliant as p does not points towards an array, but
   // gcc -c -Wall -std=c99 does not mention it... I guess it would
   // kill -Werror
   q++;
   return;
}

POINTS-TO


// Points To IN:
// p -> _p_1[0] , EXACT


// Points To OUT: none

void arithmetic03(int *p)
{

// Points To:
// p -> _p_1[0] , EXACT

   int *q;

// Points To:
// p -> _p_1[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   q = p;

// Points To:
// p -> _p_1[0] , EXACT
// q -> _p_1[0] , EXACT

   // not standard compliant as p does not points towards an array, but
   // gcc -c -Wall -std=c99 does not mention it... I guess it would
   // kill -Werror
   q++;

// Points To:
// p -> _p_1[0] , EXACT
// q -> _p_1[*] , EXACT

   return;
}
