int main()
{
   // BEGIN BLOCK
   int *x, x1, **y, *y1, y2;
//               <must be written>: x1

   x1 = 0;
//               <must be written>: y2
   y2 = 1;
//               <must be written>: x
   x = &x1;
//               <must be written>: y1
   y1 = &y2;
//               <must be written>: y
   y = &y1;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <must be written>: x
   x = *y;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: x y
   *y = x;
   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   int *x, x1, **y, *y1, y2;
//  points to = {}

   x1 = 0;
//  points to = {}
   y2 = 1;
//  points to = {}
   x = &x1;
//  {(x,x1,-Exact-)}
   y1 = &y2;
//  {(y1,y2,-Exact-);(x,x1,-Exact-)}
   y = &y1;
//  {(y,y1,-Exact-);(y1,y2,-Exact-);(x,x1,-Exact-)}
   x = *y;
//  {(y,y1,-Exact-);(y1,y2,-Exact-);(x,y2,-Exact-)}
   *y = x;
//  {(y,y1,-Exact-);(x,y2,-Exact-);(y1,y2,-Exact-)}
   return 0;
   // END BLOCK
}
