
POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   FILE *pFile;

// Points To:
// pFile -> undefined , EXACT

   int n;

// Points To:
// pFile -> undefined , EXACT

   char name[100];

// Points To:
// pFile -> undefined , EXACT


   pFile = fopen("myfile.txt", "w");

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

   for(n = 0; n <= 2; n += 1) {

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

      puts("please, enter a name: ");

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

      gets(name);

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

      fprintf(pFile, "Name %d %s\n", n, name);
   }

// Points To:
// pFile -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY

   fclose(pFile);

// Points To:
// pFile -> undefined , EXACT

   return 0;
}
