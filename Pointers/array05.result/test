
Proper effects for "main"

int main()
{
   int t[100][10][3];                                                   /*0005*/

   int (*p)[3];                                                         /*0007*/
   int *pu;                                                             /*0008*/

   int (*q)[10][3];                                                     /*0010*/
   int (*qu)[3];                                                        /*0011*/

   int (*r)[100][10][3];                                                /*0013*/
   int (*ru)[10][3];                                                    /*0014*/
//               <must be written>: p
   
   
   p = &t[1][2];                                                        /*0017*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: p
   (*p)[1] = 0;                                                         /*0018*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: p
   p[1][1] = 0;                                                         /*0019*/
//               <must be written>: pu

   pu = &t[1][2][0];                                                    /*0021*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: pu
   *pu = 0;                                                             /*0022*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: pu
   pu[2] = 0;                                                           /*0023*/
//               <must be written>: q

   q = &t[1];                                                           /*0025*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: q
   ((*q)[2])[3] = 0;                                                    /*0026*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: q
   q[1][2][3] = 0;                                                      /*0027*/
//               <must be written>: qu

   qu = &t[1][0];                                                       /*0029*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: qu
   (*qu)[1] = 0;                                                        /*0030*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: qu
   qu[1][1] = 0;                                                        /*0031*/
//               <must be written>: r

   r = &t;                                                              /*0033*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: r
   (((*r)[0])[0])[0] = 0;                                               /*0034*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: r
   (((*r)[1])[2])[3] = 0;                                               /*0035*/
//               <must be written>: ru

   ru = &t[0];                                                          /*0037*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ru
   ((*ru)[0])[0] = 0;                                                   /*0038*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ru
   ru[0][0][0] = 0;                                                     /*0039*/
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ru
   ru[1][2][1] = 0;                                                     /*0040*/

   return 0;                                                            /*0042*/
}

POINTS TO for "main"

//  {}
int main()
{
//  points to = {}
   int t[100][10][3];                                                   /*0005*/

//  points to = {}
   int (*p)[3];                                                         /*0007*/
//  points to = {}
   int *pu;                                                             /*0008*/

//  points to = {}
   int (*q)[10][3];                                                     /*0010*/
//  points to = {}
   int (*qu)[3];                                                        /*0011*/

//  points to = {}
   int (*r)[100][10][3];                                                /*0013*/
//  points to = {}
   int (*ru)[10][3];                                                    /*0014*/
//  points to = {}
   
   
   p = &t[1][2];                                                        /*0017*/
//  {(p,t[1][2],-Exact-)}
   (*p)[1] = 0;                                                         /*0018*/
//  {(p,t[1][2],-Exact-)}
   p[1][1] = 0;                                                         /*0019*/
//  {(p,t[1][2],-Exact-)}

   pu = &t[1][2][0];                                                    /*0021*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-)}
   *pu = 0;                                                             /*0022*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-)}
   pu[2] = 0;                                                           /*0023*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-)}

   q = &t[1];                                                           /*0025*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-)}
   ((*q)[2])[3] = 0;                                                    /*0026*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-)}
   q[1][2][3] = 0;                                                      /*0027*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-)}

   qu = &t[1][0];                                                       /*0029*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-)}
   (*qu)[1] = 0;                                                        /*0030*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-)}
   qu[1][1] = 0;                                                        /*0031*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-)}

   r = &t;                                                              /*0033*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-)}
   (((*r)[0])[0])[0] = 0;                                               /*0034*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-)}
   (((*r)[1])[2])[3] = 0;                                               /*0035*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-)}

   ru = &t[0];                                                          /*0037*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-);(ru,t[0],-Exact-)}
   ((*ru)[0])[0] = 0;                                                   /*0038*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-);(ru,t[0],-Exact-)}
   ru[0][0][0] = 0;                                                     /*0039*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-);(ru,t[0],-Exact-)}
   ru[1][2][1] = 0;                                                     /*0040*/
//  {(p,t[1][2],-Exact-);(pu,t[1][2][0],-Exact-);(q,t[1],-Exact-);(qu,
//   t[1][0],-Exact-);(r,t,-Exact-);(ru,t[0],-Exact-)}

   return 0;                                                            /*0042*/
}

Proper effects using points_to

int main()
{
   int t[100][10][3];                                                   /*0005*/

   int (*p)[3];                                                         /*0007*/
   int *pu;                                                             /*0008*/

   int (*q)[10][3];                                                     /*0010*/
   int (*qu)[3];                                                        /*0011*/

   int (*r)[100][10][3];                                                /*0013*/
   int (*ru)[10][3];                                                    /*0014*/
//               <must be written>: p
   
   
   p = &t[1][2];                                                        /*0017*/
//               <must be read   >: p
//               <must be written>: t[1][2][1]
   (*p)[1] = 0;                                                         /*0018*/
//               <must be read   >: p
//               <must be written>: t[1][3][1]
   p[1][1] = 0;                                                         /*0019*/
//               <must be written>: pu

   pu = &t[1][2][0];                                                    /*0021*/
//               <must be read   >: pu
//               <must be written>: t[1][2][0]
   *pu = 0;                                                             /*0022*/
//               <must be read   >: pu
//               <must be written>: t[1][2][2]
   pu[2] = 0;                                                           /*0023*/
//               <must be written>: q

   q = &t[1];                                                           /*0025*/
//               <must be read   >: q
//               <must be written>: t[1][2][3]
   ((*q)[2])[3] = 0;                                                    /*0026*/
//               <must be read   >: q
//               <must be written>: t[2][2][3]
   q[1][2][3] = 0;                                                      /*0027*/
//               <must be written>: qu

   qu = &t[1][0];                                                       /*0029*/
//               <must be read   >: qu
//               <must be written>: t[1][0][1]
   (*qu)[1] = 0;                                                        /*0030*/
//               <must be read   >: qu
//               <must be written>: t[1][1][1]
   qu[1][1] = 0;                                                        /*0031*/
//               <must be written>: r

   r = &t;                                                              /*0033*/
//               <must be read   >: r
//               <must be written>: t[0][0][0]
   (((*r)[0])[0])[0] = 0;                                               /*0034*/
//               <must be read   >: r
//               <must be written>: t[1][2][3]
   (((*r)[1])[2])[3] = 0;                                               /*0035*/
//               <must be written>: ru

   ru = &t[0];                                                          /*0037*/
//               <must be read   >: ru
//               <must be written>: t[0][0][0]
   ((*ru)[0])[0] = 0;                                                   /*0038*/
//               <must be read   >: ru
//               <must be written>: t[0][0][0]
   ru[0][0][0] = 0;                                                     /*0039*/
//               <must be read   >: ru
//               <must be written>: t[1][2][1]
   ru[1][2][1] = 0;                                                     /*0040*/

   return 0;                                                            /*0042*/
}
