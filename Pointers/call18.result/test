void call18(int i, ...)
{
//               <must be read   >: i
//               <must be written>: i
   i++;
//               <must be read   >: i
   if (i) {
      va_list pa;
      int r;
//               <must be read   >: pa
//               <must be written>: pa

      va_start(pa, fmt);
//               <may be written >: pa
//               <must be read   >: pa

      va_end(pa);
   }
}
int main()
{
//               <must be written>: i
   int i = 1;
//               <must be written>: j
   int j = 2;
//               <must be written>: k
   int k = 3;
//               <must be read   >: i j k

   call18(i, i, j, k, 0);
}

POINTS TO

//  {}
void call18(int i, ...)
{
//  points to = {}
   i++;
//  points to = {}
   if (i) {
//  points to = {}
      va_list pa;
//  points to = {}
      int r;
//  points to = {}

      va_start(pa, fmt);
//  points to = {}

      va_end(pa);
   }
}
//  {}
int main()
{
//  points to = {}
   int i = 1;
//  points to = {}
   int j = 2;
//  points to = {}
   int k = 3;
//  points to = {}

   call18(i, i, j, k, 0);
}
