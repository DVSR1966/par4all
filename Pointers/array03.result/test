int foo(float (*b)[3])
{
//  {(b,_b_1,-Exact-)}
   float c;                                                             /*0011*/
   (*b)[3] = 2.0;                                                       /*0012*/
   c = (*b)[3];                                                         /*0013*/
   b[1][3] = 2.0;                                                       /*0014*/
   c = b[1][3];                                                         /*0015*/
   (*b)[3]++;                                                           /*0017*/
   (*b)[3] += 5.0;                                                      /*0018*/
   b[1][3]++;                                                           /*0019*/
   b[1][3] += 5.0;                                                      /*0020*/
   return 1;                                                            /*0022*/
}

