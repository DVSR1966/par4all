int main()
{
//               <must be written>: b
   int b = 1;
//               <must be written>: i
   int i = 10;
//               <must be read   >: b i
//               <must be written>: b

   b = b+i;
//               <must be read   >: b i
//               <must be written>: b
   b = b*i;
//               <must be read   >: b i
//               <must be written>: b
   b = b/i;
//               <must be read   >: b i
//               <must be written>: b
   b = b&i;
//               <must be read   >: b i
//               <must be written>: b
   b = b^i;
//               <must be read   >: b i
//               <must be written>: b
   b = b|i;
//               <must be read   >: b i
//               <must be written>: b
   b += i;
//               <must be read   >: b i
//               <must be written>: b
   b *= i;
//               <must be read   >: b i
//               <must be written>: b
   b /= i;
//               <must be read   >: b i
//               <must be written>: b
   b -= i;
//               <must be read   >: b i
//               <must be written>: b
   b &= i;
//               <must be read   >: b i
//               <must be written>: b
   b |= i;
//               <must be read   >: b i
//               <must be written>: b
   b ^= i;
//               <must be read   >: b
//               <must be written>: b
   b++;
//               <must be read   >: b
//               <must be written>: b
   b--;

   return 0;
}

POINTS TO

//  {}
int main()
{
   int b = 1;
   int i = 10;

   b = b+i;
   b = b*i;
   b = b/i;
   b = b&i;
   b = b^i;
   b = b|i;
   b += i;
   b *= i;
   b /= i;
   b -= i;
   b &= i;
   b |= i;
   b ^= i;
   b++;
   b--;

   return 0;
}
