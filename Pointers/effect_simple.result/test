
PROPER EFFECTS

int main()
{
   int *p, *q;
   int i, j;
//             <    is written>: p
   p = &i;
//             <    is written>: q
   q = &j;
//             <    is read   >: q
//             <    is written>: p
   p = q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p = 1;
   return 0;
}
POINTS TO
//  {}
int main()
{
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int *p, *q;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   int i, j;
//  {(p,*ANY_MODULE*:*NOWHERE*,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,
//   -Exact-)}
   p = &i;
//  {(p,i,-Exact-);(q,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   q = &j;
//  {(p,i,-Exact-);(q,j,-Exact-)}
   p = q;
//  {(p,j,-Exact-);(q,j,-Exact-)}
   *p = 1;
//  {(p,j,-Exact-);(q,j,-Exact-)}
   return 0;
}
