int struct02()
{
   // BEGIN BLOCK
   struct two {
      int first;
      int second;
   } x[10];
//               <must be written>: i
   int i = 0;
//               <must be read   >: i
//               <must be written>: i

   for (i = 0;i<10;i++)
      if (1) {
         // BEGIN BLOCK
//               <must be read   >: i
//               <must be written>: x[i][.first]
         x[i].first = 1;
//               <must be read   >: i
//               <must be written>: x[i][.second]
         x[i].second = 0;
         // END BLOCK
      }
      else {
         // BEGIN BLOCK
//               <must be read   >: i
//               <must be written>: x[i][.first]
         x[i].first = 0;
//               <must be read   >: i
//               <must be written>: x[i][.second]
         x[i].second = 1;
         // END BLOCK
      }
   // END BLOCK
}
POINTS TO
//  {}
int struct02()
{
//  points to = {}
   // BEGIN BLOCK
//  points to = {}
   struct two {
      int first;
      int second;
   } x[10];
//  points to = {}
   int i = 0;
//  points to = {}

   for (i = 0;i<10;i++)
//  points to = {}
      if (1) {
//  points to = {}
         // BEGIN BLOCK
//  points to = {}
         x[i].first = 1;
//  points to = {}
         x[i].second = 0;
         // END BLOCK
      }
      else {
//  points to = {}
         // BEGIN BLOCK
//  points to = {}
         x[i].first = 0;
//  points to = {}
         x[i].second = 1;
         // END BLOCK
      }
   // END BLOCK
}
