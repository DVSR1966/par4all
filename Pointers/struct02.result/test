int struct02()
{
   // BEGIN BLOCK
   struct two {
      int first;
      int second;
   } x[10];
//               <must be written>: i
   int i = 0;
//               <must be read   >: i
//               <must be written>: i

   for (i = 0;i<10;i++)
      if (1) {
         // BEGIN BLOCK
//               <must be read   >: i
//               <must be written>: x[i][1]
         x[i].first = 1;
//               <must be read   >: i
//               <must be written>: x[i][2]
         x[i].second = 0;
         // END BLOCK
      }
      else {
         // BEGIN BLOCK
//               <must be read   >: i
//               <must be written>: x[i][1]
         x[i].first = 0;
//               <must be read   >: i
//               <must be written>: x[i][2]
         x[i].second = 1;
         // END BLOCK
      }
   // END BLOCK
}
POINTS TO
//  {}
int struct02()
{
   // BEGIN BLOCK
   struct two {
      int first;
      int second;
   } x[10];
   int i = 0;

   for (i = 0;i<10;i++)
      if (1) {
         // BEGIN BLOCK
         x[i].first = 1;
         x[i].second = 0;
         // END BLOCK
      }
      else {
         // BEGIN BLOCK
         x[i].first = 0;
         x[i].second = 1;
         // END BLOCK
      }
   // END BLOCK
}
