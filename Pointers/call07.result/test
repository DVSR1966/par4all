
PROPER POINTER EFFECTS

int main()
{
//             <    is written>: d1
   int d1 = 4;
//             <    is written>: d2
   int d2 = 4;
   int y[d1][d2];
   int i, j;
//             <    is read   >: d1
//             <    is written>: i

   for(i = 0; i <= d1-1; i += 1)
//             <    is read   >: d1 d2 i
//             <    is written>: y[i][4]
      call07(i, d2, &y[i][0]);
}

POINTS-TO

//  {}
int main()
{
//  points to = {}
   int d1 = 4;
//  points to = {}
   int d2 = 4;
//  points to = {}
   int y[d1][d2];
//  points to = {}
   int i, j;
//  points to = {}

   for(i = 0; i <= d1-1; i += 1)
//  points to = {}
      call07(i, d2, &y[i][0]);
}
