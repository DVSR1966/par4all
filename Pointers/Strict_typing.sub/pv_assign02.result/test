
POINTS TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a[2];

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b = 0;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int c = 1;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   a[0] = &b;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a[*] -> b , MAY
// a[0] -> b , EXACT

   a[1] = &b;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a[*] -> b , MAY
// a[0] -> b , EXACT
// a[1] -> b , EXACTLY

   a[1] = &c;

// Points To:
// a[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// a[*] -> b , MAY
// a[0] -> b , EXACT
// a[1] -> c , EXACT

   return 0;
}
