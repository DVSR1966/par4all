Points-to for "f3"


// Points To IN:
// stderr -> *NULL* , MAY
// stderr -> _stderr_ , MAY
// stdout -> *NULL* , MAY
// stdout -> _stdout_ , MAY


// Points To OUT:
// stderr -> _stderr_ , EXACT
// stdout -> _stdout_ , EXACT

void f3(int n_ptrs, int f4_after, ...)
{

// Points To: none

   va_list ap, ap_save;

// Points To: none

   char *array[31];

// Points To:
// array[*] -> undefined , EXACT

   int ptr_no = 0;

// Points To:
// array[*] -> undefined , EXACT

   if (n_ptrs>31)

// Points To:
// array[*] -> undefined , EXACT

      n_ptrs = 31;

// Points To:
// array[*] -> undefined , EXACT

   va_start(ap, f4_after);

// Points To:
// array[*] -> undefined , EXACT

   for(ptr_no = 0; ptr_no <= n_ptrs-1; ptr_no += 1) {

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY

      array[ptr_no] = va_arg(ap, char *);

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY

      if (ptr_no==f4_after)

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY

         va_copy(ap_save, ap);
   }

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY

   va_end(ap);

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY
// stderr -> *NULL* , MAY
// stderr -> _stderr_ , MAY

   f2(n_ptrs, array);

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY
// stderr -> _stderr_ , EXACT

   // Now process the saved copy.
   n_ptrs -= f4_after;

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY
// stderr -> _stderr_ , EXACT

   for(ptr_no = 0; ptr_no <= n_ptrs-1; ptr_no += 1)

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY
// stderr -> _stderr_ , EXACT

      array[ptr_no] = va_arg(ap_save, char *);

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY
// stderr -> _stderr_ , EXACT

   va_end(ap_save);

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY
// stderr -> _stderr_ , EXACT
// stdout -> *NULL* , MAY
// stdout -> _stdout_ , MAY

   f4(n_ptrs, array);

// Points To:
// array[*] -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// array[*] -> undefined , MAY
// stderr -> _stderr_ , EXACT
// stdout -> _stdout_ , EXACT

   return;
}

