void f2(int n_ptrs, char *array[31])
{

// Points To: none

   int ptr_no;

// Points To: none

   for(ptr_no = 0; ptr_no <= n_ptrs-1; ptr_no += 1)

// Points To:
// array[*] -> *NULL* , MAY
// array[*] -> _array_2[*] , MAY
// stderr -> *NULL* , MAY
// stderr -> _stderr_ , MAY

      fprintf(stderr, "%s\n", array[ptr_no]);

// Points To:
// array[*] -> *NULL* , MAY
// array[*] -> _array_2[*] , MAY
// stderr -> _stderr_ , EXACT

   return;
}

