int main()
{
   // BEGIN BLOCK
   int **x, **y, *z, *t;
   int i, j;
//               <must be written>: i
   i = 1;
//               <must be written>: j
   j = 2;
//               <must be written>: z

   z = &i;
//               <must be written>: t
   t = &j;
//               <must be read   >: t
//               <must be written>: z

   z = t;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: x z
   *x = z;
//               <must be read   >: __IO_EFFECTS:LUNS[1] x
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf(" x = %p\n", x);
   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
   // BEGIN BLOCK
   int **x, **y, *z, *t;
   int i, j;
   i = 1;
   j = 2;

   z = &i;
//  {(z,i,-Exact-)}
   t = &j;
//  {(t,j,-Exact-);(z,i,-Exact-)}

   z = t;
//  {(t,j,-Exact-);(z,j,-Exact-)}
   *x = z;
//  {(t,j,-Exact-);(z,j,-Exact-)}
   printf(" x = %p\n", x);
   return 0;
   // END BLOCK
}
