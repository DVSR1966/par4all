
PROPER  EFFECTS

int ptr_to_field()
{
   typedef struct {
      int *q;
   } my_str;

   my_str *m, *n;
//             <    is written>: i j
   int i = 0, j = 1;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: m _MALLOC_EFFECTS:_MALLOC_
   m = (my_str *) malloc(sizeof(my_str));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: n _MALLOC_EFFECTS:_MALLOC_
   n = (my_str *) malloc(sizeof(my_str));
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: m
   m->q = &i;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   n->q = m->q;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT:
// *HEAP*_l_14.q -> undefined , MAY
// *HEAP*_l_15.q -> undefined , MAY

int ptr_to_field()
{

// Points To: none

   typedef struct {
      int *q;
   } my_str;


// Points To: none

   my_str *m, *n;

// Points To:
// m -> undefined , EXACT
// n -> undefined , EXACT

   int i = 0, j = 1;

// Points To:
// m -> undefined , EXACT
// n -> undefined , EXACT

   m = (my_str *) malloc(sizeof(my_str));

// Points To:
// *HEAP*_l_14.q -> undefined , EXACT
// m -> *HEAP*_l_14 , MAY
// n -> undefined , EXACT

   n = (my_str *) malloc(sizeof(my_str));

// Points To:
// *HEAP*_l_14.q -> undefined , EXACT
// *HEAP*_l_15.q -> undefined , EXACT
// m -> *HEAP*_l_14 , MAY
// n -> *HEAP*_l_15 , MAY

   m->q = &i;

// Points To:
// *HEAP*_l_14.q -> i , MAY
// *HEAP*_l_15.q -> undefined , EXACT
// m -> *HEAP*_l_14 , MAY
// n -> *HEAP*_l_15 , MAY

   n->q = m->q;

// Points To:
// *HEAP*_l_14.q -> i , MAY
// *HEAP*_l_15.q -> i , MAY
// m -> *HEAP*_l_14 , MAY
// n -> *HEAP*_l_15 , MAY


   return 0;
}
