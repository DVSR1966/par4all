int main()
{
   // BEGIN BLOCK
   int **x, *y, k, h;
//               <must be written>: k
   k = 1;
//               <must be written>: h
   h = 2;
//               <must be written>: y
   y = &h;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: x y
   *x = y;
//               <must be read   >: __IO_EFFECTS:LUNS[1] x
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("x = %p \n", x);
   return 0;
   // END BLOCK
}
POINTS TO
//  {}
int main()
{
   // BEGIN BLOCK
   int **x, *y, k, h;
   k = 1;
   h = 2;
   y = &h;
//  {(y,h,-Exact-)}
   *x = y;
//  {(y,h,-Exact-)}
   printf("x = %p \n", x);
   return 0;
   // END BLOCK
}
