int main()
{
   // BEGIN BLOCK
   int a, b, c, d;
   int *pa, *pb, *pc, *pd;
   int **ppa, **ppb, **ppc, **ppd;
//               <must be written>: a
   a = 1;
//               <must be written>: b
   b = 2;
//               <must be written>: c
   c = 3;
//               <must be written>: d
   d = 4;
//               <must be written>: pa

   pa = &a;
//               <must be written>: ppa
   ppa = &b;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppa
   *ppa = &pa;
//               <must be written>: pb

   pb = &b;
//               <must be written>: ppb
   ppb = &a;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppb
   *ppb = &pb;
//               <must be written>: pc

   pc = &c;
//               <must be written>: ppc
   ppc = &d;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppc
   *ppc = &pc;
//               <must be written>: pd

   pd = &d;
//               <must be written>: ppd
   ppd = &c;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppd
   *ppd = &pd;
//               <must be read   >: pd
//               <must be written>: pc

   pc = pd;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: ppc

   *ppc = &a;
//               <may be written >: *ANY_MODULE*:*ANYWHERE*
//               <must be read   >: pc ppa

   *ppa = pc;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <must be written>: pa

   pa = *ppd;
//               <may be read    >: *ANY_MODULE*:*ANYWHERE*
//               <may be written >: *ANY_MODULE*:*ANYWHERE*

   *ppa = *ppd;

   return 0;
   // END BLOCK
}
POINTS TO
//  {}
