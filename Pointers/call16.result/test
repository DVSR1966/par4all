
PROPER EFFECTS

void call16(int _i, int *_a, int *_b, int *_c)
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   _a[_i] = _b[_i]+_c[_i];
}
void dotprod(int *a, int *b, int *c)
{
   int i;
//             <    is written>: i
   for(i = 0; i <= 999999; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      call16(i, a, b, c);
   
   
   return;
}

PROPER POINTER EFFECTS

void dotprod(int *a, int *b, int *c)
{
   int i;
//             <    is written>: i
   for(i = 0; i <= 999999; i += 1)
//             <may be read   >: b[*] c[*]
//             <may be written>: a[*]
//             <    is read   >: a b c i
      call16(i, a, b, c);
   
   
   return;
}

POINTS TO

//  {(_a,__a_1,-Exact-);(_b,__b_2,-Exact-);(_c,__c_3,-Exact-)}
void call16(int _i, int *_a, int *_b, int *_c)
{
//  {(_a,__a_1,-Exact-);(_b,__b_2,-Exact-);(_c,__c_3,-Exact-)}
   _a[_i] = _b[_i]+_c[_i];
}
//  {(a,_a_4,-Exact-);(b,_b_5,-Exact-);(c,_c_6,-Exact-)}
void dotprod(int *a, int *b, int *c)
{
//  {(a,_a_4,-Exact-);(b,_b_5,-Exact-);(c,_c_6,-Exact-)}
   int i;
//  {(a,_a_4,-Exact-);(b,_b_5,-Exact-);(c,_c_6,-Exact-)}
   for(i = 0; i <= 999999; i += 1)
//  {(a,_a_4,-Exact-);(b,_b_5,-Exact-);(c,_c_6,-Exact-)}
      call16(i, a, b, c);
//  {(a,_a_4,-Exact-);(b,_b_5,-Exact-);(c,_c_6,-Exact-)}
   
   
   return;
}
