
PROPER EFFECTS

void call16(int _i, int *_a, int *_b, int *_c)
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   _a[_i] = _b[_i]+_c[_i];
   return;
}
void dotprod(int *a, int *b, int *c)
{
   int i;
//             <    is written>: i
   for(i = 0; i <= 999999; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      call16(i, a, b, c);

   return;
}

PROPER POINTER EFFECTS

void dotprod(int *a, int *b, int *c)
{
   int i;
//             <    is written>: i
   for(i = 0; i <= 999999; i += 1)
//             <may be read   >: b[*] c[*]
//             <may be written>: a[*]
//             <    is read   >: a b c i
      call16(i, a, b, c);

   return;
}

POINTS TO


// Points To IN:
// _a -> *NULL_POINTER* , MAY
// _a -> __a_2[0] , MAY
// _b -> *NULL_POINTER* , MAY
// _b -> __b_3[0] , MAY
// _c -> *NULL_POINTER* , MAY
// _c -> __c_4[0] , MAY


// Points To OUT: none

void call16(int _i, int *_a, int *_b, int *_c)
{

// Points To: none

   _a[_i] = _b[_i]+_c[_i];

// Points To:
// _a -> __a_2[0] , EXACT
// _b -> __b_3[0] , EXACT
// _c -> __c_4[0] , EXACT

   return;
}

// Points To IN: none


// Points To OUT: none

void dotprod(int *a, int *b, int *c)
{

// Points To: none

   int i;

// Points To: none

   for(i = 0; i <= 999999; i += 1)

// Points To: none

      call16(i, a, b, c);

// Points To: none


   return;
}
