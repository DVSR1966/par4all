
TRANSFORMERS


//  T() {}

void run(void)
{

//  T(i,m,n) {}

   int i, n, m;

//  T(i) {i<=1}

   i = rand();

//  T(n) {i<=1, n<=1}

   n = rand();

//  T(m) {i<=1, m<=1, n<=1}

   m = rand();

//  T() {i<=1, m<=1, n<=1}


   if (m>=0&&n>=1&&i<=m)

//  T() {n==1, i<=1, i<=m, 0<=m, m<=1}

      if (i>n*m)

//  T() {0==-1}

         "unreachable";
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int i, n, m;

//  P(i,m,n) {}

   i = rand();

//  P(i,m,n) {i<=1}

   n = rand();

//  P(i,m,n) {i<=1, n<=1}

   m = rand();

//  P(i,m,n) {i<=1, m<=1, n<=1}


   if (m>=0&&n>=1&&i<=m)

//  P(i,m,n) {n==1, i<=1, i<=m, 0<=m, m<=1}

      if (i>n*m)

//  P() {0==-1}

         "unreachable";
}
