
WITHOUT TRANSFORMER LISTS
=========================


TRANSFORMERS


//  T() {}

void run(void)
{

//  T(s,x,y) {}

   int s, x, y;

//  T(s) {s==1}

   s = 1;

//  T(x,y) {s==1, x==0, y==0}

   x = y = 0;

//  T(x,y) {s==1, x#init+y<=x+y#init, y<=x, 0<=y, y#init<=y}

   while (flip()) {

//  T(x,y) {s==1, x+y#init==x#init+y, y#init<=y}

      while (flip())

//  T(x,y) {s==1, x==x#init+1, y==y#init+1, 0<=x, x<=100, y<=x,
//    x+y<=202, 0<=y}

         if (s==1&&x<=99) {

//  T(x) {s==1, x==x#init+1, x<=100}

            x++;

//  T(y) {s==1, y==y#init+1, x<=100}

            y++;

//  T() {s==1, 0<=x, x<=100, y<=x, x+y<=202, 0<=y}

            if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 100<=x}

            while (1)

//  T() {s==1, 100<=x}

               ;

//  T(x) {s==1, x#init<=x}

      while (flip())

//  T(x) {s==1, x==x#init+2, 0<=x, x<=100, y<=x, x+y<=202, 0<=y}

         if (s==1&&x<=98) {

//  T(x) {s==1, x==x#init+2, x<=100}

            x += 2;

//  T() {s==1, 0<=x, x<=100, y<=x, x+y<=202, 0<=y}

            if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 99<=x}

            while (1)

//  T() {s==1, 99<=x}

               ;
   }

//  T(x,y) {s==1, x+y==x#init+y#init+2, 101<=x, x<=x#init+2,
//    x#init+2<=x+y, x+y<=2x#init+2, x#init<=100}

   if (flip())

//  T(x,y) {s==1, x==101, x#init==100, y==y#init+1, 1<=y, y<=101}

      if (s==1&&x==100) {

//  T(x) {s==1, x==101, x#init==100, 0<=y, y<=100}

         x++;

//  T(y) {s==1, x==101, y==y#init+1, 1<=y, y<=101}

         y++;

//  T() {s==1, x==101, 1<=y, y<=101}

         if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {s==1, y<=x, 0<=y}

         while (1)

//  T() {s==1, y<=x, 0<=y}

            ;
   else if (s==1&&x<=100&&x>98) {

//  T(x) {s==1, x==x#init+2, 101<=x, x<=102, y+2<=x, 0<=y}

      x += 2;

//  T() {s==1, 101<=x, x<=102, y+2<=x, x+y<=202, 0<=y}

      if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

         checking_error();
   }
   else

//  T() {s==1, y<=x, 0<=y}

      while (1)

//  T() {s==1, y<=x, 0<=y}

         ;

//  T(s) {s==2, s#init==1, 101<=x, x<=102, 2<=x+y, y<=x, x+y<=202,
//    0<=y}

   if (s==1) {

//  T(s) {s==2, s#init==1, 101<=x, x<=102, 2<=x+y, x+y<=202, 0<=y}

      s = 2;

//  T() {s==2, 101<=x, x<=102, 2<=x+y, y<=x, x+y<=202, 0<=y}

      if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

         checking_error();
   }
   else

//  T() {0==-1}

      while (1)

//  T() {0==-1}

         ;
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int s, x, y;

//  P(s,x,y) {}

   s = 1;

//  P(s,x,y) {s==1}

   x = y = 0;

//  P(s,x,y) {s==1, x==0, y==0}

   while (flip()) {

//  P(s,x,y) {s==1, y<=x, 0<=y}

      while (flip())

//  P(s,x,y) {s==1, y<=x, 0<=y}

         if (s==1&&x<=99) {

//  P(s,x,y) {s==1, x<=99, y<=x, 0<=y}

            x++;

//  P(s,x,y) {s==1, x<=100, y+1<=x, 0<=y}

            y++;

//  P(s,x,y) {s==1, x<=100, y<=x, 1<=y}

            if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(s,x,y) {s==1, 100<=x, y<=x, 0<=y}

            while (1)

//  P(s,x,y) {s==1, 100<=x, y<=x, 0<=y}

               ;

//  P(s,x,y) {s==1, y<=x, 0<=y}

      while (flip())

//  P(s,x,y) {s==1, y<=x, 0<=y}

         if (s==1&&x<=98) {

//  P(s,x,y) {s==1, x<=98, y<=x, 0<=y}

            x += 2;

//  P(s,x,y) {s==1, x<=100, y+2<=x, 0<=y}

            if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(s,x,y) {s==1, 99<=x, y<=x, 0<=y}

            while (1)

//  P(s,x,y) {s==1, 99<=x, y<=x, 0<=y}

               ;
   }

//  P(s,x,y) {s==1, y<=x, 0<=y}

   if (flip())

//  P(s,x,y) {s==1, y<=x, 0<=y}

      if (s==1&&x==100) {

//  P(s,x,y) {s==1, x==100, 0<=y, y<=100}

         x++;

//  P(s,x,y) {s==1, x==101, 0<=y, y<=100}

         y++;

//  P(s,x,y) {s==1, x==101, 1<=y, y<=101}

         if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

            checking_error();
      }
      else

//  P(s,x,y) {s==1, y<=x, 0<=y}

         while (1)

//  P(s,x,y) {s==1, y<=x, 0<=y}

            ;
   else if (s==1&&x<=100&&x>98) {

//  P(s,x,y) {s==1, 99<=x, x<=100, y<=x, 0<=y}

      x += 2;

//  P(s,x,y) {s==1, 101<=x, x<=102, y+2<=x, 0<=y}

      if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

         checking_error();
   }
   else

//  P(s,x,y) {s==1, y<=x, 0<=y}

      while (1)

//  P(s,x,y) {s==1, y<=x, 0<=y}

         ;

//  P(s,x,y) {s==1, 101<=x, x<=102, x+y<=202, 0<=y}

   if (s==1) {

//  P(s,x,y) {s==1, 101<=x, x<=102, x+y<=202, 0<=y}

      s = 2;

//  P(s,x,y) {s==2, 101<=x, x<=102, 2<=x+y, x+y<=202, 0<=y}

      if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

         checking_error();
   }
   else

//  P() {0==-1}

      while (1)

//  P() {0==-1}

         ;
}

WITH TRANSFORMER LISTS
======================


TRANSFORMERS


//  T() {}

void run(void)
{

//  T(s,x,y) {}

   int s, x, y;

//  T(s) {s==1}

   s = 1;

//  T(x,y) {s==1, x==0, y==0}

   x = y = 0;

//  T(x,y) {s==1, x#init+y<=x+y#init, y<=x, 0<=y, y#init<=y}

   while (flip()) {

//  T(x,y) {s==1, x+y#init==x#init+y, y#init<=y}

      while (flip())

//  T(x,y) {s==1, x==x#init+1, y==y#init+1, 0<=x, x<=100, y<=x,
//    x+y<=202, 0<=y}

         if (s==1&&x<=99) {

//  T(x) {s==1, x==x#init+1, x<=100}

            x++;

//  T(y) {s==1, y==y#init+1, x<=100}

            y++;

//  T() {s==1, 0<=x, x<=100, y<=x, x+y<=202, 0<=y}

            if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 100<=x}

            while (1)

//  T() {s==1, 100<=x}

               ;

//  T(x) {s==1, x#init<=x}

      while (flip())

//  T(x) {s==1, x==x#init+2, 0<=x, x<=100, y<=x, x+y<=202, 0<=y}

         if (s==1&&x<=98) {

//  T(x) {s==1, x==x#init+2, x<=100}

            x += 2;

//  T() {s==1, 0<=x, x<=100, y<=x, x+y<=202, 0<=y}

            if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {s==1, 99<=x}

            while (1)

//  T() {s==1, 99<=x}

               ;
   }

//  T(x,y) {s==1, x+y==x#init+y#init+2, 101<=x, x<=x#init+2,
//    x#init+2<=x+y, x+y<=2x#init+2, x#init<=100}

   if (flip())

//  T(x,y) {s==1, x==101, x#init==100, y==y#init+1, 1<=y, y<=101}

      if (s==1&&x==100) {

//  T(x) {s==1, x==101, x#init==100, 0<=y, y<=100}

         x++;

//  T(y) {s==1, x==101, y==y#init+1, 1<=y, y<=101}

         y++;

//  T() {s==1, x==101, 1<=y, y<=101}

         if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {s==1, y<=x, 0<=y}

         while (1)

//  T() {s==1, y<=x, 0<=y}

            ;
   else if (s==1&&x<=100&&x>98) {

//  T(x) {s==1, x==x#init+2, 101<=x, x<=102, y+2<=x, 0<=y}

      x += 2;

//  T() {s==1, 101<=x, x<=102, y+2<=x, x+y<=202, 0<=y}

      if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

         checking_error();
   }
   else

//  T() {s==1, y<=x, 0<=y}

      while (1)

//  T() {s==1, y<=x, 0<=y}

         ;

//  T(s) {s==2, s#init==1, 101<=x, x<=102, 2<=x+y, y<=x, x+y<=202,
//    0<=y}

   if (s==1) {

//  T(s) {s==2, s#init==1, 101<=x, x<=102, 2<=x+y, x+y<=202, 0<=y}

      s = 2;

//  T() {s==2, 101<=x, x<=102, 2<=x+y, y<=x, x+y<=202, 0<=y}

      if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  T() {0==-1}

         checking_error();
   }
   else

//  T() {0==-1}

      while (1)

//  T() {0==-1}

         ;
}

PRECONDITIONS


//  P() {}

void run(void)
{

//  P() {}

   int s, x, y;

//  P(s,x,y) {}

   s = 1;

//  P(s,x,y) {s==1}

   x = y = 0;

//  P(s,x,y) {s==1, x==0, y==0}

   while (flip()) {

//  P(s,x,y) {s==1, x<=100, y<=x, 0<=y, y<=100}

      while (flip())

//  P(s,x,y) {s==1, x<=100, y<=x, 0<=y}

         if (s==1&&x<=99) {

//  P(s,x,y) {s==1, x<=99, y<=x, 0<=y}

            x++;

//  P(s,x,y) {s==1, x<=100, y+1<=x, 0<=y}

            y++;

//  P(s,x,y) {s==1, x<=100, y<=x, 1<=y}

            if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(s,x,y) {s==1, x==100, 0<=y, y<=100}

            while (1)

//  P(s,x,y) {s==1, x==100, 0<=y, y<=100}

               ;

//  P(s,x,y) {s==1, x<=100, y<=x, 0<=y}

      while (flip())

//  P(s,x,y) {s==1, x<=100, y<=x, 0<=y}

         if (s==1&&x<=98) {

//  P(s,x,y) {s==1, x<=98, y<=x, 0<=y}

            x += 2;

//  P(s,x,y) {s==1, x<=100, y+2<=x, 0<=y}

            if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

               checking_error();
         }
         else

//  P(s,x,y) {s==1, 99<=x, x<=100, y<=x, 0<=y}

            while (1)

//  P(s,x,y) {s==1, 99<=x, x<=100, y<=x, 0<=y}

               ;
   }

//  P(s,x,y) {s==1, x<=100, y<=x, 0<=y}

   if (flip())

//  P(s,x,y) {s==1, x<=100, y<=x, 0<=y}

      if (s==1&&x==100) {

//  P(s,x,y) {s==1, x==100, 0<=y, y<=100}

         x++;

//  P(s,x,y) {s==1, x==101, 0<=y, y<=100}

         y++;

//  P(s,x,y) {s==1, x==101, 1<=y, y<=101}

         if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

            checking_error();
      }
      else

//  P(s,x,y) {s==1, x<=99, y<=x, 0<=y}

         while (1)

//  P(s,x,y) {s==1, x<=99, y<=x, 0<=y}

            ;
   else if (s==1&&x<=100&&x>98) {

//  P(s,x,y) {s==1, 99<=x, x<=100, y<=x, 0<=y}

      x += 2;

//  P(s,x,y) {s==1, 101<=x, x<=102, y+2<=x, 0<=y}

      if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

         checking_error();
   }
   else

//  P(s,x,y) {s==1, x<=98, y<=x, 0<=y}

      while (1)

//  P(s,x,y) {s==1, x<=98, y<=x, 0<=y}

         ;

//  P(s,x,y) {s==1, 101<=x, x<=102, x+y<=202, 0<=y}

   if (s==1) {

//  P(s,x,y) {s==1, 101<=x, x<=102, x+y<=202, 0<=y}

      s = 2;

//  P(s,x,y) {s==2, 101<=x, x<=102, 2<=x+y, x+y<=202, 0<=y}

      if (!(0<=y&&y<=x&&y<=-x+202&&0<=x&&x<=102))

//  P() {0==-1}

         checking_error();
   }
   else

//  P() {0==-1}

      while (1)

//  P() {0==-1}

         ;
}
