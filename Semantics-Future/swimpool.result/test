
TRANSFORMERS


//  T() {}

void run(void)
{


//  T(x1,x2,x3,x4,x5,x6,x7) {}

   int x1, x2, x3, x4, x5, x6, x7;

//  T(x1,x2,x3,x4,x5) {x1==0, x2==0, x3==0, x4==0, x5==0}

   x1 = x2 = x3 = x4 = x5 = 0;

//  T(x6) {x1==0, x2==0, x3==0, x4==0, x5==0}

   x6 = rand();

//  T(x7) {x1==0, x2==0, x3==0, x4==0, x5==0}

   x7 = rand();

//  T() {x1==0, x2==0, x3==0, x4==0, x5==0}


   if (x6>=1&&x7>=0)

//  T(x1,x2,x3,x4,x5,x6,x7) {x1+x2+x4+x5+x6==x6#init, x1#init==0,
//    x2+x3+x4+x7==x7#init, x2#init==0, x3#init==0, x4#init==0,
//    x5#init==0, 1<=x6#init, 0<=x7#init}

      while (1) {

//  T(x1,x6) {x1+x6==x1#init+x6#init, 1<=x6, x6<=x6#init, 1<=x6#init}

         while (flip()&&x6>=1) {

//  T(x1) {x1==x1#init+1, 1<=x6}

            x1++;

//  T(x6) {x6==x6#init-1, 0<=x6}

            x6--;
         }

//  T() {}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  T() {0==-1}

            error();

//  T(x1,x2,x7) {x1+x7#init==x1#init+x7, x2+x7==x2#init+x7#init,
//    1<=x1, 1<=x1#init, 1<=x7, x7<=x7#init, 1<=x7#init}

         while (flip()&&x1>=1&&x7>=1) {

//  T(x2) {x2==x2#init+1, 1<=x1, 1<=x7}

            x2++;

//  T(x1) {x1==x1#init-1, 0<=x1, 1<=x7}

            x1--;

//  T(x7) {x7==x7#init-1, 0<=x1, 0<=x7}

            x7--;
         }

//  T() {}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  T() {0==-1}

            error();

//  T(x2,x3,x6) {x2+x3==x2#init+x3#init, x2+x6==x2#init+x6#init,
//    1<=x2, x2<=x2#init, 1<=x2#init}

         while (flip()&&x2>=1) {

//  T(x6) {x6==x6#init+1, 1<=x2}

            x6++;

//  T(x3) {x3==x3#init+1, 1<=x2}

            x3++;

//  T(x2) {x2==x2#init-1, 0<=x2}

            x2--;
         }

//  T() {}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  T() {0==-1}

            error();

//  T(x3,x4,x6) {x3+x6#init==x3#init+x6, x4+x6==x4#init+x6#init,
//    1<=x3, 1<=x3#init, 1<=x6, x6<=x6#init, 1<=x6#init}

         while (flip()&&x3>=1&&x6>=1) {

//  T(x4) {x4==x4#init+1, 1<=x3, 1<=x6}

            x4++;

//  T(x3) {x3==x3#init-1, 0<=x3, 1<=x6}

            x3--;

//  T(x6) {x6==x6#init-1, 0<=x3, 0<=x6}

            x6--;
         }

//  T() {}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  T() {0==-1}

            error();

//  T(x4,x5,x7) {x4+x5==x4#init+x5#init, x4+x7==x4#init+x7#init,
//    1<=x4, x4<=x4#init, 1<=x4#init}

         while (flip()&&x4>=1) {

//  T(x5) {x5==x5#init+1, 1<=x4}

            x5++;

//  T(x7) {x7==x7#init+1, 1<=x4}

            x7++;

//  T(x4) {x4==x4#init-1, 0<=x4}

            x4--;
         }

//  T() {}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  T() {0==-1}

            error();

//  T(x5,x6) {x5+x6==x5#init+x6#init, x5<=x5#init, 1<=x6, 1<=x6#init}

         while (flip()&&x6>=1) {

//  T(x6) {x6==x6#init+1, 2<=x6}

            x6++;

//  T(x5) {x5==x5#init-1, 2<=x6}

            x5--;
         }

//  T() {}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  T() {0==-1}

            error();
      }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int x1, x2, x3, x4, x5, x6, x7;

//  P(x1,x2,x3,x4,x5,x6,x7) {}

   x1 = x2 = x3 = x4 = x5 = 0;

//  P(x1,x2,x3,x4,x5,x6,x7) {x1==0, x2==0, x3==0, x4==0, x5==0}

   x6 = rand();

//  P(x1,x2,x3,x4,x5,x6,x7) {x1==0, x2==0, x3==0, x4==0, x5==0}

   x7 = rand();

//  P(x1,x2,x3,x4,x5,x6,x7) {x1==0, x2==0, x3==0, x4==0, x5==0}


   if (x6>=1&&x7>=0)

//  P(x1,x2,x3,x4,x5,x6,x7) {x1==0, x2==0, x3==0, x4==0, x5==0, 1<=x6,
//    0<=x7}

      while (1) {

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         while (flip()&&x6>=1) {

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7, 1<=x6}

            x1++;

//  P(x1,x2,x3,x4,x5,x6,x7) {2<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7, 1<=x6}

            x6--;
         }

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  P() {0==-1}

            error();

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         while (flip()&&x1>=1&&x7>=1) {

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1, 1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7,
//    1<=x7}

            x2++;

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1, 2<=x1+x2+x4+x5+x6, 1<=x2+x3+x4+x7,
//    1<=x7}

            x1--;

//  P(x1,x2,x3,x4,x5,x6,x7) {0<=x1, 1<=x1+x2+x4+x5+x6, 1<=x2+x3+x4+x7,
//    1<=x7}

            x7--;
         }

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  P() {0==-1}

            error();

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         while (flip()&&x2>=1) {

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 1<=x2, 0<=x2+x3+x4+x7}

            x6++;

//  P(x1,x2,x3,x4,x5,x6,x7) {2<=x1+x2+x4+x5+x6, 1<=x2, 0<=x2+x3+x4+x7}

            x3++;

//  P(x1,x2,x3,x4,x5,x6,x7) {2<=x1+x2+x4+x5+x6, 1<=x2, 1<=x2+x3+x4+x7}

            x2--;
         }

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  P() {0==-1}

            error();

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         while (flip()&&x3>=1&&x6>=1) {

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7, 1<=x3,
//    1<=x6}

            x4++;

//  P(x1,x2,x3,x4,x5,x6,x7) {2<=x1+x2+x4+x5+x6, 1<=x2+x3+x4+x7, 1<=x3,
//    1<=x6}

            x3--;

//  P(x1,x2,x3,x4,x5,x6,x7) {2<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7, 0<=x3,
//    1<=x6}

            x6--;
         }

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  P() {0==-1}

            error();

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         while (flip()&&x4>=1) {

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7, 1<=x4}

            x5++;

//  P(x1,x2,x3,x4,x5,x6,x7) {2<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7, 1<=x4}

            x7++;

//  P(x1,x2,x3,x4,x5,x6,x7) {2<=x1+x2+x4+x5+x6, 1<=x2+x3+x4+x7, 1<=x4}

            x4--;
         }

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  P() {0==-1}

            error();

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         while (flip()&&x6>=1) {

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7, 1<=x6}

            x6++;

//  P(x1,x2,x3,x4,x5,x6,x7) {2<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7, 2<=x6}

            x5--;
         }

//  P(x1,x2,x3,x4,x5,x6,x7) {1<=x1+x2+x4+x5+x6, 0<=x2+x3+x4+x7}

         if (x1==0&&x2==0&&x4==0&&5==0&&x6==0)

//  P() {0==-1}

            error();
      }
}
