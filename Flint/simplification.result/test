      PROGRAM SIMPLIF
      INTEGER I
      REAL*4 R
      REAL*8 D
      COMPLEX*8 C
      COMPLEX*16 DC

!-5
      I = -5                                                            0001
!-5
      I = -5                                                            0002
!-5
      I = -5                                                            0003
!I
      I = I                                                             0004
!INT(R)
      I = INT(R)                                                        0005

C     R -> I
!5
      I = 5                                                             0006
!590
      I = 590                                                           0007
!5
      I = 5                                                             0008
!-5
      I = -5                                                            0009
!-590
      I = -590                                                          0010
!-5
      I = -5                                                            0011

C     I -> R
      R = R+5.0E0+5.9+REAL(I)                                           0012

C     D -> R
!590.9E-2 
      R = 590.9E-2                                                      0013
!-590.9E-2
      R = -590.9E-2                                                     0014

C     R -> D
!5.9D2
      D = 5.9D2                                                         0015
!-5.9D2
      D = -5.9D2                                                        0016

C     D -> I
!590
      I = 590                                                           0017
!5
      I = 5                                                             0018
!-590
      I = -590                                                          0019
!-5
      I = -5                                                            0020

      I = INT(-R)                                                       0021
      R = REAL(-I)                                                      0022
      I = INT(-D)                                                       0023
      D = DBLE(-I)                                                      0024

C     R -> C
!-(5.9E2, 0.0)
      C = -CMPLX(5.9E2, 0.0E0)                                          0025
!(590E-2, 0.0)
      C = CMPLX(590E-2, 0.0E0)                                          0026

c     D -> C
!-(5.9E2, 0.0)
      C = -CMPLX(5.9E2, 0.0E0)                                          0027
!(590E-2, 0.0)
      C = CMPLX(590E-2, 0.0E0)                                          0028

C     I -> C
!-(5.0, 0.0)
      C = -CMPLX(5.0E0, 0.0E0)                                          0029
!(5.0, 0.0)
      C = CMPLX(5.0E0, 0.0E0)                                           0030

C     C -> R
!5.9E2
      R = 5.9E2                                                         0031
!5.9E2
      R = 5.9E2                                                         0032
!-590.9E-1
      R = -590.9E-1                                                     0033
!-590.9E-1
      R = -590.9E-1                                                     0034

C     C -> D
!5.9D2
      D = 5.9D2                                                         0035
!590D-2
      D = 590D-2                                                        0036
!-590D-2
      D = -590D-2                                                       0037

C     C -> I
!590
      I = 590                                                           0038
!5
      I = 5                                                             0039
!-5
      I = -5                                                            0040

      R = SIN(R)                                                        0041
      D = DSIN(D)                                                       0042
      C = CSIN(C)                                                       0043

C     DC -> C
!(-5.9E2, 3.9E2)
      C = CMPLX(-5.9E2, 3.9E2)                                          0044
!(-5.9E2, 3.9E2)
      C = CMPLX(-5.9E2, 3.9E2)                                          0045
!(-5.9E2, 3.9E2)
      DC = DCMPLX(-5.9D2, 3.9D2)                                        0046

C     C -> DC
!(-5.9D2, -3.9D2)
      DC = DCMPLX(-5.9D2, -3.9D2)                                       0047
!(5.9D2, -3.9D2)
      DC = DCMPLX(5.9D2, -3.9D2)                                        0048
!(5.9D2, -3.9D2)
      DC = DCMPLX(5.9D2, -3.9D2)                                        0049

C     DC -> D
!-5.9D2
      D = -5.9D2                                                        0050

C     DC -> R
!-5.9E2
      R = -5.9E2                                                        0051

C     DC -> I
!-590
      I = -590                                                          0052

C     I -> DC
!(5.0, 0.0)
      DC = DCMPLX(5.0D0, 0.0D0)                                         0053
!-(5.9D2, 0.0)
      DC = -DCMPLX(5.9D2, 0.0D0)                                        0054
!-(5.9D2, 0.0)
      DC = -DCMPLX(5.9D2, 0.0D0)                                        0055

C others...

      I = 10                                                            0056

      END

