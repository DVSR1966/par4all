delete c
setenv src ${VDIR:.}/Flint
echo compile and execute initial file
! f77 ${src}/constants.F
! ./a.out > out.1
! rm ./a.out
create c constants.F
apply TYPE_CHECKER[%ALL]
display PRINTED_FILE
apply UNSPLIT
