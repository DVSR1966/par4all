
Source code with proper effects

void assign01()
{
   int i;
   int j;
//               <must be written>: i j

   i = 2+(j = 1)+3;                                                     /*0007*/

   foo(i);                                                              /*0009*/
}

Source code with cumulated effects

void assign01()
{
   int i;
   int j;
//               <must be written>: i j

   i = 2+(j = 1)+3;                                                     /*0007*/

   foo(i);                                                              /*0009*/
}

Source code with transformers


//  T() {}

void assign01()
{
   int i;
   int j;

//  T(i,j) {i==6, j==1}


   i = 2+(j = 1)+3;                                                     /*0007*/

//  T() {}


   foo(i);                                                              /*0009*/
}
