C  T(I) {I==I#init+1, I+1<=N}

C (1,1)
C     BEGIN BLOCK

C  T() {}

C (1,2)

100   CONTINUE                                                          0002

C  T(I) {I==I#init+1}

C (1,3)
      I = I+1                                                           0003

C  T() {I+1<=N}

C (1,4)
c      if(i.eq.n) stop
      IF (I.GE.N) THEN                                                  0004

C  T() {0==-1}

C (1,5)
         STOP                                                           0005
      ELSE

C  T() {}

C (1,6)
C        BEGIN BLOCK
C        END BLOCK
      ENDIF

C  T() {}

C (1,7)
      PRINT *, I                                                        0006
C     END BLOCK
      GOTO 100
C     END UNSTRUCTURED
C     END BLOCK
      END

C  P() {}

      PROGRAM UNSTRUC01

C  P() {}

C (0,1)
C     BEGIN BLOCK

C  P() {}

C (0,2)

C     Show bug on non-exiting unstructured by Nga Nguyen

      I = 0                                                             0001

C  P(I) {I==0}

C (0,3)
C     BEGIN UNSTRUCTURED

C  P(I) {0<=I}

C (1,1)
C     BEGIN BLOCK

C  P(I) {0<=I}

C (1,2)

100   CONTINUE                                                          0002

C  P(I) {0<=I}

C (1,3)
      I = I+1                                                           0003

C  P(I) {1<=I}

C (1,4)
c      if(i.eq.n) stop
      IF (I.GE.N) THEN                                                  0004

C  P(I) {1<=I, N<=I}

C (1,5)
         STOP                                                           0005
      ELSE

C  P(I) {1<=I, I+1<=N}

C (1,6)
C        BEGIN BLOCK
C        END BLOCK
      ENDIF

C  P(I) {1<=I, I+1<=N}

C (1,7)
      PRINT *, I                                                        0006
