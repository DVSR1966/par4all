      I = 0                                                             0001
C (0,3)
C     BEGIN UNSTRUCTURED
C (1,1)
C     BEGIN BLOCK
C (1,2)

100   CONTINUE                                                          0002
C (1,3)
      I = I+1                                                           0003
C (1,4)
c      if(i.eq.n) stop
      IF (I.GE.N) THEN                                                  0004
C (1,5)
         STOP                                                           0005
      ELSE
C (1,6)
C        BEGIN BLOCK
C        END BLOCK
      ENDIF
C (1,7)
      PRINT *, I                                                        0006
C     END BLOCK
      GOTO 100
C     END UNSTRUCTURED
C     END BLOCK
      END

C  T() {0==-1}

      PROGRAM UNSTRUC01

C  T() {0==-1}

C (0,1)
C     BEGIN BLOCK

C  T(I) {I==0}

C (0,2)

C     Show bug on non-exiting unstructured by Nga Nguyen

      I = 0                                                             0001

C  T() {0==-1}

