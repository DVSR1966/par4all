
Source code with proper effects

int call04(int i)
{
//               <must be read   >: i
//               <must be written>: i
   i++;                                                                 /*0004*/
//               <must be read   >: TOP-LEVEL:j
//               <must be written>: TOP-LEVEL:j
   j++;                                                                 /*0005*/
}

Source code with cumulated effects

//               <must be read   >: TOP-LEVEL:j i
//               <must be written>: TOP-LEVEL:j
int call04(int i)
{
//               <must be read   >: i
//               <must be written>: i
   i++;                                                                 /*0004*/
//               <must be read   >: TOP-LEVEL:j
//               <must be written>: TOP-LEVEL:j
   j++;                                                                 /*0005*/
}

Source code with transformers for "call04"


//  T(TOP-LEVEL:j) {TOP-LEVEL:j==j#init+1}

int call04(int i)
{

//  T(i) {i==i#init+1}

   i++;                                                                 /*0004*/

//  T(TOP-LEVEL:j) {TOP-LEVEL:j==j#init+1}

   j++;                                                                 /*0005*/
}

Source code with transformers for "main"


//  T(TOP-LEVEL:j) {TOP-LEVEL:j==j#init+1}

main()
{
   int ai = 3;
   float ax = 4.;

//  T(TOP-LEVEL:j,ai) {TOP-LEVEL:j==j#init+1}


   ai = call04(ai++);                                                   /*0016*/

//  T(ai) {ai==0}


   ai = 0;                                                              /*0018*/
}

