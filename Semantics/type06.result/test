
C  T() {}

      PROGRAM TYPE06

C     Goal: check the extension of the semantics analysis to string
C     scalar variables in tests

      CHARACTER*4 C1

C  T(C1) {"TITI"==C1}


      C1 = "TITI"                                                       0001

C  T(C1) {}


      IF (C1.EQ."TOTO") THEN                                            0002

C  T(C1) {}

         READ *, C1                                                     0003

C  T() {}

         PRINT *, C1                                                    0004
      ENDIF

      END

C  P() {}

      PROGRAM TYPE06

C     Goal: check the extension of the semantics analysis to string
C     scalar variables in tests

      CHARACTER*4 C1

C  P() {}


      C1 = "TITI"                                                       0001

C  P(C1) {"TITI"==C1}


      IF (C1.EQ."TOTO") THEN                                            0002

C  P() {0==-1}

         READ *, C1                                                     0003

C  P() {0==-1}

         PRINT *, C1                                                    0004
      ENDIF

      END

