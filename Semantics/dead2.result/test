
C  P() {}

C     Semantics analysis and dead code

C     Partial evaluation may generate dead code and trivial tests

      PROGRAM DEAD2

C  P() {}


      I = 1                                                             0001

C  P(I) {I==1}

      J = 1                                                             0002

C  P(I,J) {I==1, J==1}

      IF (I.EQ.J) THEN                                                  0003

C  P(I,J) {I==1, J==1}

         K = 2                                                          0004
      ELSE

C  P() {0==-1}

         K = 4                                                          0005
      ENDIF

C  P(I,J,K) {I==1, J==1, K==2}

      IF (1.EQ.1) THEN                                                  0006

C  P(I,J,K) {I==1, J==1, K==2}

         K = 2                                                          0007
      ELSE

C  P() {0==-1}

         K = 4                                                          0008
      ENDIF

C  P(I,J,K) {I==1, J==1, K==2}

      PRINT *, I, J, K                                                  0009
      END
