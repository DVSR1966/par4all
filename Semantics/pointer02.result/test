C  P() {}

      PROGRAM POINTER02

C     Check the syntactic analysis of pointers

      POINTER (I, X(1))

C  P() {}

C (0,1)
C     BEGIN BLOCK

C  P() {}

C (0,2)

      I = MALLOC(10)

C  P(I) {}

C (0,3)

      PRINT *, X(1)

C  P(I) {}

C (0,4)

      RETURN
C     END BLOCK
      END
