C (0,17)
c      TM=DENS(XX,YY)*DSIGN(1.0D0,QSPEC(KSP))
               IF (NPX(KSP)*NPY(KSP).EQ.0) TM = 0.D0                    0013

C  P(I,J,KSP,L,NX1,NX2,NX2NY2,NY1,NY2) {NX==NX1-1, NX==NX2-2,
