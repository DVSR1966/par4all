
C  T() {}

      PROGRAM FUNCSIDE5
      INTEGER FOO5
      EXTERNAL FOO5

C  T(I1) {I1==10}


C     Non compliant: definition of a variable in an expression using
C     this variable

      I1 = 10                                                           0001

C  T(I1) {I1==I1#init+2}


      I1 = I1+FOO5(I1)                                                  0002

C  T() {}


      PRINT *, I1                                                       0003

C  T(I1) {I1==I1#init+3}


      I1 = FOO5(I1)+I1                                                  0004

C  T() {}


      PRINT *, I1                                                       0005

      END
