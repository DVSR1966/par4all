      PROGRAM UNSTRUC05
C (0,1)
C     BEGIN BLOCK
C (0,2)

C     Check that scc's are found

      READ *, I                                                         0001
C (0,3)
C     BEGIN UNSTRUCTURED
C (1,1)

      IF (I.LE.5) GOTO 100                                              0002
C (6,1)
      J = 6                                                             0004
C (4,1)
200   IF (I.LE.2) THEN                                                  0009
      ELSE
         GOTO 99998
      ENDIF
C (3,1)
C     BEGIN BLOCK
C (3,2)
300   CONTINUE                                                          0006
C (3,3)
      I = 4                                                             0007
C     END BLOCK
      GOTO 200
C (2,1)
100   J = 7                                                             0005
      GOTO 300
99998 CONTINUE

C     END UNSTRUCTURED
C (0,4)
      PRINT *, I, J                                                     0011
C (0,5)

      RETURN
C     END BLOCK
      END

C  T() {}

      PROGRAM UNSTRUC05

C  T(I,J) {18<=I+2J, 6<=J, J<=7}

C (0,1)
C     BEGIN BLOCK

C  T(I) {}

C (0,2)

C     Check that scc's are found

      READ *, I                                                         0001

C  T(I,J) {I#init+6<=I+J, 18<=I+2J, 6<=J, J<=7}

C (0,3)
C     BEGIN UNSTRUCTURED

C  T() {}

C (1,1)

      IF (I.LE.5) GOTO 100                                              0002

C  T(J) {J==6}

C (6,1)
      J = 6                                                             0004

C  T() {}

C (4,1)
200   IF (I.LE.2) THEN                                                  0009
      ELSE
         GOTO 99997
      ENDIF

C  T(I) {I==4}

C (3,1)
C     BEGIN BLOCK

C  T() {}

C (3,2)
300   CONTINUE                                                          0006

C  T(I) {I==4}

C (3,3)
      I = 4                                                             0007
C     END BLOCK
      GOTO 200

C  T(J) {J==7}

C (2,1)
100   J = 7                                                             0005
      GOTO 300
99997 CONTINUE

C     END UNSTRUCTURED

C  T() {}

C (0,4)
      PRINT *, I, J                                                     0011

C  T() {}

C (0,5)

      RETURN
C     END BLOCK
      END

C  P() {}

      PROGRAM UNSTRUC05

C  P() {}

C (0,1)
C     BEGIN BLOCK

C  P() {}

C (0,2)

C     Check that scc's are found

      READ *, I                                                         0001

C  P(I) {}

C (0,3)
C     BEGIN UNSTRUCTURED

C  P(I) {}

C (1,1)

      IF (I.LE.5) GOTO 100                                              0002

C  P(I) {6<=I}

C (6,1)
      J = 6                                                             0004

C  P(I,J) {18<=I+2J, 6<=J, J<=7}

C (4,1)
200   IF (I.LE.2) THEN                                                  0009
      ELSE
         GOTO 99996
      ENDIF

C  P(I,J) {J==7, I<=5}

C (3,1)
C     BEGIN BLOCK

C  P(I,J) {J==7, I<=5}

C (3,2)
300   CONTINUE                                                          0006

C  P(I,J) {J==7, I<=5}

C (3,3)
      I = 4                                                             0007
C     END BLOCK
      GOTO 200

C  P(I) {I<=5}

C (2,1)
100   J = 7                                                             0005
      GOTO 300
99996 CONTINUE

C     END UNSTRUCTURED

C  P(I,J) {18<=I+2J, 6<=J, J<=7}

C (0,4)
      PRINT *, I, J                                                     0011

C  P(I,J) {18<=I+2J, 6<=J, J<=7}

C (0,5)

      RETURN
C     END BLOCK
      END

