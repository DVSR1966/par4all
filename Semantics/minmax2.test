#!/bin/sh

tpips=${tpips_version:-tpips}

# Initialization:
# rm -f minmax2.result/test
# tpips >minmax2.result/test <<EOF

$tpips <<EOF
# Check effect translation problems destroy semantics information

delete minmax2
create minmax2 $PIPS_VALIDDIR/Semantics/minmax2.f

module MINMAX2
activate PRINT_CODE_TRANSFORMERS
display PRINTED_FILE

module MINMAX2
activate PRINT_CODE_PRECONDITIONS
display PRINTED_FILE

close
delete minmax2

quit
EOF

