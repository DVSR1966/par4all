
C  P() {}

      PROGRAM EQUIV01

C     Check that unusable information is not used

      EQUIVALENCE (I,J), (K,X)

C  P() {I==J}


      READ *, I, J, K, X                                                0001

C  P(I) {I==J}


      IF (I.GT.N) THEN                                                  0002

C  P(I) {I==J, N+1<=I}

         I = I-1                                                        0003

C  P(I) {I==J, N<=I}

         PRINT *, I                                                     0004
      ENDIF

C  P(I) {I==J}


      IF (J.GT.N) THEN                                                  0005

C  P(I) {I==J, N+1<=I}

         J = J-1                                                        0006

C  P(I) {I==J, N<=I}

         PRINT *, J                                                     0007
      ENDIF

C  P(I) {I==J}


      IF (K.GT.N) THEN                                                  0008

C  P(I) {I==J}

         K = K-1                                                        0009

C  P(I) {I==J}

         PRINT *, K                                                     0010
      ENDIF

C  P(I) {I==J}


      PRINT *, I, J, K, X                                               0011

      END

