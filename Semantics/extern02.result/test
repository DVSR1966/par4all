
Cumulated effects for "main"

//               <must be read   >: TOP-LEVEL:delta TOP-LEVEL:delta2
main()
{
//               <must be read   >: TOP-LEVEL:delta TOP-LEVEL:delta2 i
//               <must be written>: i
   // BEGIN BLOCK
   int i = 0;
//               <must be read   >: TOP-LEVEL:delta i
//               <must be written>: i

   i = i+delta;
//               <must be read   >: TOP-LEVEL:delta2 i
//               <must be written>: i
   i = i+delta2;
   // END BLOCK
}

Transformers for "main"


//  T() {TOP-LEVEL:delta==1, TOP-LEVEL:delta2==3}

main()
{

//  T() {TOP-LEVEL:delta==1, TOP-LEVEL:delta2==3}

   // BEGIN BLOCK

//  T() {TOP-LEVEL:delta==1, TOP-LEVEL:delta2==3, i==0}

   int i = 0;

//  T(i) {TOP-LEVEL:delta==1, TOP-LEVEL:delta2==3, i==1, i#init==0}


   i = i+delta;

//  T(i) {TOP-LEVEL:delta==1, TOP-LEVEL:delta2==3, i==4, i#init==1}

   i = i+delta2;
   // END BLOCK
}

