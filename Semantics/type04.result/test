
C  T() {}

      PROGRAM TYPE04

C     Goal: check the extension of the semantics analysis to logical
C     scalar variables

      LOGICAL L1, L2

C  T(L1) {}


      READ *, L1                                                        0001

C  T(L2) {0<=L1, L1<=1, L2<=L1, L1+L2<=1, 0<=L2, L2<=1}


      L2 = L1.AND..NOT.L1                                               0002

C  T(L1) {}


      READ *, L1                                                        0003

C  T() {}


      PRINT *, L1, L2                                                   0004

      END

C  P() {}

      PROGRAM TYPE04

C     Goal: check the extension of the semantics analysis to logical
C     scalar variables

      LOGICAL L1, L2

C  P() {}


      READ *, L1                                                        0001

C  P(L1) {}


      L2 = L1.AND..NOT.L1                                               0002

C  P(L1,L2) {0<=L1, L1<=1, L2<=L1, L1+L2<=1, 0<=L2, L2<=1}


      READ *, L1                                                        0003

C  P(L1,L2) {L2==0}


      PRINT *, L1, L2                                                   0004

      END

