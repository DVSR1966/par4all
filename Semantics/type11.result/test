
C  T() {}

      PROGRAM TYPE11

C  T(I) {}


C     Check integer behavior after extensions to non-integer types

      READ *, I                                                         0001

C  T(I) {I==0}


! known-in

      I = 0                                                             0002

C  T(R) {0.5==R}

      R = 0.5                                                           0003

C  T(D) {0.5D0==D}

      D = 0.5D0                                                         0004

C  T() {}


! int gt lt

      IF (I.GT.0) THEN                                                  0005

C  T() {}

         PRINT *, 'int gt int'                                          0006

C  T() {}

         PRINT *, I                                                     0007
      ENDIF

      END

C  P() {}

      PROGRAM TYPE11

C  P() {}


C     Check integer behavior after extensions to non-integer types

      READ *, I                                                         0001

C  P(I) {}


! known-in

      I = 0                                                             0002

C  P(I) {I==0}

      R = 0.5                                                           0003

C  P(I,R) {0.5==R, I==0}

      D = 0.5D0                                                         0004

C  P(D,I,R) {0.5==R, 0.5D0==D, I==0}


! int gt lt

      IF (I.GT.0) THEN                                                  0005

C  P() {0==-1}

         PRINT *, 'int gt int'                                          0006

C  P() {0==-1}

         PRINT *, I                                                     0007
      ENDIF

      END

