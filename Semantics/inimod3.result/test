
C  P() {}

      PROGRAM INIMOD3

C     Same as inimod.f, but let's analyze the floating point variables!

      IMPLICIT REAL*8 (A-H, O-Z)
*
*
      PARAMETER  ( MP   =      402 ,   NP   =       160 )
      PARAMETER  ( M1   =   MP - 1 ,   N1   =   NP - 1 )
*
*
*
      PARAMETER  ( XLZ =       30.0D0 )
      PARAMETER  ( XLR =        8.0D0 )
*
C     PARAMETER  ( XCZ =       30.0D0 )
      PARAMETER  ( XCZ =       15.0D0 )
      PARAMETER  ( XSZ =       30.0D0 )
*
      PARAMETER  ( RO0 =        1.0D0 )
      PARAMETER  ( PR0 =        0.6D0 )
*
      PARAMETER  ( XLJET =      1.0D0 )
      PARAMETER  ( ROJET =      0.1D0 )
      PARAMETER  ( PRJET =      0.6D0 )
      PARAMETER  (  VJET =    18.97D0 )

      COMMON /PRMT/   TS, DT, GAM, CFL,VCT,
     &                TZ,TR, FTZ,FTR, BSZ,BSR, QTZ,QTR, IS
      COMMON /GRID/   Z(MP), ZB(0:MP), DZ(MP), DBZ(0:MP), FZ(M1),
     &                R(NP), RB(0:NP), DR(NP), DBR(0:NP), FR(N1),
     &                DUM(8)
* Dum is a dummy             a.w.
      COMMON /VAR1/   RO(MP,NP), EN(MP,NP),
     &                GZ(MP,NP), GR(MP,NP)

C  P() {}

C (0,1)
C     BEGIN BLOCK

C  P() {}

C (0,2)
*
*
***********************************************************************
*     OPEN MODEL INPUT
***********************************************************************
      OPEN (UNIT=11,FILE='HYDRO2D.MODEL',STATUS='OLD')                  0001

C  P() {}

C (0,3)
***********************************************************************
*     SET BASIC PARAMETERS
***********************************************************************
*
*
      GAM = DBLE(5.0D0)/DBLE(3.0D0)                                     0002

C  P(GAM) {}

C (0,4)
*
      TZ = XLZ/(MP-2)                                                   0003

C  P(GAM,TZ) {}

C (0,5)
      TR = XLR/NP                                                       0004

C  P(GAM,TR,TZ) {}

C (0,6)
      FTZ = DBLE(-2.0D0)*TZ                                             0005

C  P(FTZ,GAM,TR,TZ) {}

C (0,7)
      FTR = 0.0D0                                                       0006

C  P(FTR,FTZ,GAM,TR,TZ) {0.0D0==FTR}

C (0,8)
      BSZ = 1.0D0                                                       0007

C  P(BSZ,FTR,FTZ,GAM,TR,TZ) {0.0D0==FTR, 1.0D0==BSZ}

C (0,9)
      BSR = 1.0D0                                                       0008

C  P(BSR,BSZ,FTR,FTZ,GAM,TR,TZ) {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ}

C (0,10)
      QTZ = 1.0D0                                                       0009

C  P(BSR,BSZ,FTR,FTZ,GAM,QTZ,TR,TZ) {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ,
C    BSR==QTZ}

C (0,11)
      QTR = 1.0D0                                                       0010

C  P(BSR,BSZ,FTR,FTZ,GAM,QTR,QTZ,TR,TZ) {0.0D0==FTR, 1.0D0==BSR,
C    BSR==BSZ, BSR==QTR, BSR==QTZ}

C (0,12)
*
*
***********************************************************************
*     SET VARIABLES
***********************************************************************
*
*
      CON = GAM-1.0D0                                                   0011

C  P(BSR,BSZ,CON,FTR,FTZ,GAM,QTR,QTZ,TR,TZ) {0.0D0==FTR, 1.0D0==BSR,
C    BSR==BSZ, BSR+CON==GAM, BSR==QTR, BSR==QTZ}

C (0,13)
      EN0 = PR0/CON                                                     0012

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,QTR,QTZ,TR,TZ) {0.0D0==FTR,
C    1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR, BSR==QTZ}

C (0,14)
*
      MPCON = NINT(MP*XCZ/XLZ)                                          0013

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,MPCON,QTR,QTZ,TR,TZ) {0.0D0==FTR,
C    1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR, BSR==QTZ}

C (0,15)
      MPSTR = NINT(MP*XSZ/XLZ)                                          0014

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ}

C (0,16)
      J = 1                                                             0015
C (0,17)
99995 CONTINUE

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=J, J<=161}

C (1,1)
*
C     DO loop 5 with exit had to be desugared
      IF (J.GT.NP) GOTO 99998                                           0015

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=J, J<=160}

C (10,1)
      I = 1                                                             0016
99996 CONTINUE

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=403, 1<=J, J<=160}

C (7,1)
C     DO loop 5 with exit had to be desugared
      IF (I.GT.MP) GOTO 99997                                           0016

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, 1<=J, J<=160}

C (9,1)
      READ (UNIT=11,FMT=1000,END=99) RO(I,J)                            0017

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, 1<=J, J<=160}

C (6,1)
C     BEGIN BLOCK

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, 1<=J, J<=160}

C (6,2)
5     CONTINUE                                                          0018

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, 1<=J, J<=160}

C (6,3)
      I = I+1                                                           0016
C     END BLOCK
      GOTO 99996

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, 1<=J, J<=160}

C (4,1)
C     BEGIN BLOCK

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, 1<=J, J<=160}

C (4,2)
99    WRITE (6, 1099)                                                   0039

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, 1<=J, J<=160}

C (4,3)
      STOP                                                              0040

C  P() {0==-1}

C (4,4)
1000  FORMAT(E20.14)                                                    0041

C  P() {0==-1}

C (4,5)
1099  FORMAT(' NO FURTHER MODEL DATA AVAILABLE, STOP')                  0042
C     END BLOCK
99997 CONTINUE

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, I==403, 1<=J, J<=160}

C (8,1)
      J = J+1                                                           0015
      GOTO 99995
99998 CONTINUE

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, J==161}

C (2,1)
C     BEGIN BLOCK

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, J==161}

C (2,2)
      DO 10 J = 1, NP                                                   0019

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=J, J<=160}

C (2,3)
         DO 10 I = 1, MPCON                                             0020

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=MPCON, 1<=J, J<=160}

C (2,4)
C           BEGIN BLOCK

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, I<=MPCON, 1<=J, J<=160}

C (2,5)
C        RO(I,J) =  RO0
            EN(I,J) = EN0                                               0021

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, I<=MPCON, 1<=J, J<=160}

C (2,6)
            GZ(I,J) = 0.0D0                                             0022

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, I<=MPCON, 1<=J, J<=160}

C (2,7)
            GR(I,J) = 0.0D0                                             0023

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, I<=MPCON, 1<=J, J<=160}

C (2,8)
10          CONTINUE                                                    0024
C           END BLOCK

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, J==161, 1<=I, I<=403, MPCON+1<=I}

C (2,9)
*
      DO 12 J = 1, NP                                                   0025

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=J, J<=160, MPCON<=402}

C (2,10)
         DO 12 I = MPCON+1, MP                                          0026

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, I<=402, MPCON+1<=I, 1<=J, J<=160, MPCON<=402}

C (2,11)
C           BEGIN BLOCK

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, MPCON+1<=I, 1<=J, J<=160, MPCON<=402}

C (2,12)
*        RO(I,J) =  RO0  *  10 ** ( REAL(MPCON-I) / REAL(MPSTR) )
C        RO(I,J) =  RO0  *  10 ** ( DBLE(MPCON-I) / DBLE(MPSTR) )
            EN(I,J) = EN0                                               0027

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, MPCON+1<=I, 1<=J, J<=160, MPCON<=402}

C (2,13)
            GZ(I,J) = 0.0D0                                             0028

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, MPCON+1<=I, 1<=J, J<=160, MPCON<=402}

C (2,14)
            GR(I,J) = 0.0D0                                             0029

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, 1<=I, I<=402, MPCON+1<=I, 1<=J, J<=160, MPCON<=402}

C (2,15)
12          CONTINUE                                                    0030
C           END BLOCK

C  P(BSR,BSZ,CON,EN0,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, I==403, J==161, MPCON<=402}

C (2,16)
*
*
      ENJET = PRJET/CON+0.5D0*ROJET*VJET**2                             0031

C  P(BSR,BSZ,CON,EN0,ENJET,FTR,FTZ,GAM,I,J,MPCON,MPSTR,QTR,QTZ,TR,TZ)
C    {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, I==403, J==161, MPCON<=401}

C (2,17)
      GZJET = ROJET*VJET                                                0032

C  P(BSR,BSZ,CON,EN0,ENJET,FTR,FTZ,GAM,GZJET,I,J,MPCON,MPSTR,QTR,QTZ,
C    TR,TZ) {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM, BSR==QTR,
C    BSR==QTZ, I==403, J==161, MPCON<=401}

C (2,18)
*
      NPJET = NINT(NP*XLJET/XLR)                                        0033

C  P(BSR,BSZ,CON,EN0,ENJET,FTR,FTZ,GAM,GZJET,I,J,MPCON,MPSTR,NPJET,
C    QTR,QTZ,TR,TZ) {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM,
C    BSR==QTR, BSR==QTZ, I==403, J==161, MPCON<=401}

C (2,19)
*
      DO 20 J = 1, NPJET                                                0034

C  P(BSR,BSZ,CON,EN0,ENJET,FTR,FTZ,GAM,GZJET,I,J,MPCON,MPSTR,NPJET,
C    QTR,QTZ,TR,TZ) {0.0D0==FTR, 1.0D0==BSR, BSR==BSZ, BSR+CON==GAM,
