
Source code with proper effects

void complete02()
{
   // BEGIN BLOCK
   int i;
   {
      // BEGIN BLOCK
      int j = 0;
//               <must be read   >: i
//               <must be written>: i
      for (i = 0;i<10;i++) {                                            /*0009*/
         // BEGIN BLOCK
//               <must be read   >: j
//               <must be written>: j
         j++;                                                           /*0010*/
//               <must be read   >: i
//               <must be written>: i
         i--;                                                           /*0011*/
         // END BLOCK
      }
      // END BLOCK
   }
//               <must be read   >: i

   foo(i);                                                              /*0015*/
   // END BLOCK
}

Source code with cumulated effects

void complete02()
{
//               <must be read   >: i j
//               <must be written>: i j
   // BEGIN BLOCK
   int i;
   {
//               <must be read   >: i j
//               <must be written>: i j
      // BEGIN BLOCK
      int j = 0;
//               <must be read   >: i j
//               <must be written>: i j
      for (i = 0;i<10;i++) {                                            /*0009*/
//               <must be read   >: i j
//               <must be written>: i j
         // BEGIN BLOCK
//               <must be read   >: j
//               <must be written>: j
         j++;                                                           /*0010*/
//               <must be read   >: i
//               <must be written>: i
         i--;                                                           /*0011*/
         // END BLOCK
      }
      // END BLOCK
   }
//               <must be read   >: i

   foo(i);                                                              /*0015*/
   // END BLOCK
}

Source code with transformers


//  T() {0==-1}

void complete02()
{

//  T() {0==-1}

   // BEGIN BLOCK
   int i;
   {

//  T() {0==-1}

      // BEGIN BLOCK
      int j = 0;

//  T(i,j) {i==0, j#init<=j}

      for (i = 0;i<10;i++) {                                            /*0009*/

//  T(i,j) {i==i#init-1, j==j#init+1}

         // BEGIN BLOCK

//  T(j) {j==j#init+1}

         j++;                                                           /*0010*/

//  T(i) {i==i#init-1}

         i--;                                                           /*0011*/
         // END BLOCK
      }
      // END BLOCK
   }

//  T() {}


   foo(i);                                                              /*0015*/
   // END BLOCK
}

Source code with preconditions


//  P() {}

void complete02()
{

//  P() {}

   // BEGIN BLOCK
   int i;
   {

//  P() {}

      // BEGIN BLOCK
      int j = 0;

//  P() {j==0}

      for (i = 0;i<10;i++) {                                            /*0009*/

//  P(i,j) {i==0, 0<=j}

         // BEGIN BLOCK

//  P(i,j) {i==0, 0<=j}

         j++;                                                           /*0010*/

//  P(i,j) {i==0, 1<=j}

         i--;                                                           /*0011*/
         // END BLOCK
      }
      // END BLOCK
   }

//  P() {0==-1}


   foo(i);                                                              /*0015*/
   // END BLOCK
}

