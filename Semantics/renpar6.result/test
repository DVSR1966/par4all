
      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0001
      DO I = 1, N                                                       0002
         DO J = 1, N                                                    0003
            WORK(J,K) = J+K                                             0004
         ENDDO

         CALL INC1(K)                                                   0005

         DO J = 1, N                                                    0006
            WORK(J,K) = J*J-K*K                                         0007
            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0008
         ENDDO
      ENDDO
      END

C  T(K) {0<=K, N<=K}


      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)

C  T(K) {0<=K}

      K = M*M                                                           0001

C  T(I,J,K) {I+K#init==K+1, K#init<=K}

      DO I = 1, N                                                       0002

C  T(J) {1<=J}

         DO J = 1, N                                                    0003

C  T() {}

            WORK(J,K) = J+K                                             0004
         ENDDO

C  T(K) {K==K#init+1}


         CALL INC1(K)                                                   0005

C  T(J) {1<=J}


         DO J = 1, N                                                    0006

C  T() {}

            WORK(J,K) = J*J-K*K                                         0007

C  T() {}

            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0008
         ENDDO
      ENDDO
      END

C  <A(PHI1)-R-EXACT-{1<=PHI1, PHI1<=N}>
C  <A(PHI1)-W-EXACT-{1<=PHI1, PHI1<=N}>


      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0001

C  <A(PHI1)-R-EXACT-{1<=PHI1, PHI1<=N, 0<=K}>
C  <A(PHI1)-W-EXACT-{1<=PHI1, PHI1<=N, 0<=K}>
C  <WORK(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=N, K<=PHI2, PHI2<=K+N,
C    0<=K}>
C  <WORK(PHI1,PHI2)-W-EXACT-{1<=PHI1, PHI1<=N, K<=PHI2, PHI2<=K+N,
C    0<=K}>

      DO I = 1, N                                                       0002

C  <WORK(PHI1,PHI2)-W-EXACT-{PHI2==K, 1<=PHI1, PHI1<=N, 1<=I, I<=1+K,
C    I<=N}>

         DO J = 1, N                                                    0003

C  <WORK(PHI1,PHI2)-W-EXACT-{PHI1==J, PHI2==K, 1<=I, I<=1+K, I<=N,
C    1<=J, J<=N}>

            WORK(J,K) = J+K                                             0004
         ENDDO

         CALL INC1(K)                                                   0005

C  <A(PHI1)-R-EXACT-{PHI1==I, 1<=I, I<=K, I<=N}>
C  <A(PHI1)-W-EXACT-{PHI1==I, 1<=I, I<=K, I<=N}>
C  <WORK(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=N, K<=PHI2+1, PHI2<=K,
C    1<=I, I<=K, I<=N}>
C  <WORK(PHI1,PHI2)-W-EXACT-{PHI2==K, 1<=PHI1, PHI1<=N, 1<=I, I<=K,
C    I<=N}>


         DO J = 1, N                                                    0006

C  <WORK(PHI1,PHI2)-W-EXACT-{PHI1==J, PHI2==K, 1<=I, I<=K, I<=N, 1<=J,
C    J<=N}>

            WORK(J,K) = J*J-K*K                                         0007

C  <A(PHI1)-R-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=J, J<=N}>
C  <A(PHI1)-W-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=J, J<=N}>
C  <WORK(PHI1,PHI2)-R-EXACT-{PHI1==J, 1<=PHI1, PHI1<=N, K<=PHI2+1,
C    PHI2<=K, 1<=I, I<=K, I<=N}>

            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0008
         ENDDO
      ENDDO
      END

C  <A(PHI1)-IN-EXACT-{1<=PHI1, PHI1<=N}>


      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0001

C  <A(PHI1)-IN-EXACT-{1<=PHI1, PHI1<=N, 0<=K}>

      DO I = 1, N                                                       0002
         DO J = 1, N                                                    0003
            WORK(J,K) = J+K                                             0004
         ENDDO

         CALL INC1(K)                                                   0005

C  <A(PHI1)-IN-EXACT-{I==PHI1, 1<=I, I<=K, I<=N, 1<=N}>
C  <WORK(PHI1,PHI2)-IN-EXACT-{K==PHI2+1, 1<=PHI1, PHI1<=N, 1<=I, I<=K,
C    I<=N}>


         DO J = 1, N                                                    0006
            WORK(J,K) = J*J-K*K                                         0007

C  <A(PHI1)-IN-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=J, J<=N}>
C  <WORK(PHI1,PHI2)-IN-EXACT-{PHI1==J, 1<=PHI1, PHI1<=N, K<=PHI2+1,
C    PHI2<=K, 1<=I, I<=K, I<=N}>

            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0008
         ENDDO
      ENDDO
      END

      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0001
      DO I = 1, N                                                       0002

C  <WORK(PHI1,PHI2)-OUT-EXACT-{K==PHI2, 1<=PHI1, PHI1<=N, 1<=I,
C    I<=1+K, I<=N}>

         DO J = 1, N                                                    0003

C  <WORK(PHI1,PHI2)-OUT-EXACT-{J==PHI1, PHI2==K, 1<=I, I<=1+K, I<=N,
C    1<=J, J<=N}>

            WORK(J,K) = J+K                                             0004
         ENDDO

         CALL INC1(K)                                                   0005

         DO J = 1, N                                                    0006

C  <WORK(PHI1,PHI2)-OUT-EXACT-{PHI1==J, PHI2==K, 1<=I, I<=K, I<=N,
C    1<=J, J<=N}>

            WORK(J,K) = J*J-K*K                                         0007

C  <A(PHI1)-OUT-EXACT-{I==PHI1, 1<=I, I<=K, I<=N, 1<=J, 1+J<=N}>

            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0008
         ENDDO
      ENDDO
      END

      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0001
      DO I = 1, N                                                       0002
         DOALL J = 1, N                                                 0003
            WORK(J,K) = J+K                                             0004
         ENDDO

         CALL INC1(K)                                                   0005
         DOALL J = 1, N                                                 0006
            WORK(J,K) = J*J-K*K                                         0007
         ENDDO
         DO J = 1, N                                                    0006
            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0008
         ENDDO
      ENDDO
      END


      SUBROUTINE RENPAR6_2(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K0 = M*M                                                          0001
      DO I = 1, N                                                       0002
         K = K0+I-1                                                     0003
         DO J = 1, N                                                    0004
            WORK(J,K) = J+K                                             0005
         ENDDO

         CALL INC1(K)                                                   0006

         DO J = 1, N                                                    0007
            WORK(J,K) = J*J-K*K                                         0008
            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0009
         ENDDO
      ENDDO
      END


      SUBROUTINE RENPAR6_2(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K0 = M*M                                                          0001
      DOALL I = 1, N                                                    0002
         PRIVATE WORK(1:100, 1:100),K,J
         K = K0+I-1                                                     0003
         DOALL J = 1, N                                                 0004
            WORK(J,K) = J+K                                             0005
         ENDDO

         CALL INC1(K)                                                   0006
         DOALL J = 1, N                                                 0007
            WORK(J,K) = J*J-K*K                                         0008
         ENDDO
         DO J = 1, N                                                    0007
            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0009
         ENDDO
      ENDDO
      END

