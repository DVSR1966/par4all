
C  P() {}

      INTEGER FUNCTION ABSVAL(N)

C  P() {}

      IF (N.LT.0) THEN                                                  0001

C  P() {N+1<=0}

         ABSVAL = -N                                                    0002
      ELSE

C  P() {0<=N}

         ABSVAL = N                                                     0003
      ENDIF
      END
