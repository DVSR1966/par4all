#!/bin/sh
#
# Script to test pips on a simple program
#
module=convgfm
if Pips -f $PIPS_ROOT/Validation/Semantics/$module.f -s tffull -w $module -m $module \
	 -p prct >&2
then
	exit_code=0
	echo "--- Transformers for module `echo $module | tr '[a-z]' '[A-Z]'`"
	Display -l tran
else
	exit_code=1
fi
Delete $module >&2
exit $exit_code
