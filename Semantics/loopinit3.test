#!/bin/sh

uppercase () {
tr '[a-z]' '[A-Z]' <<%
$*
%
}

echo "PRETTYPRINT_BLOCK_IF_ONLY TRUE" >properties.rc
if Pips -f $PIPS_ROOT/Validation/Semantics/loopinit3.f -w loopinit3 -m loopinit3 \
	-s pcinterfull -s tfinterfull \
	-s prcp -b PF >&2
then
	echo "--- Preconditions for" `uppercase loopinit3`
	if Display -l prec
	then
	    exit_status=0
	else
	    exit_status=1
	fi
else
	exit_status=1
fi
Delete loopinit3 
rm properties.rc
exit $exit_status

