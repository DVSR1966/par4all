
TRANSFORMERS


C  T() {}

      PROGRAM W02
      INTEGER I, N
      REAL T(10)

C  T(I,N) {N==10, 10<=I}

C (0,1)
C     BEGIN BLOCK

C  T(I) {I==1}

C (0,2)

      I = 1                                                             0001

C  T(N) {N==10}

C (0,3)
      N = 10                                                            0002

C  T(I) {I#init<=I}

C (0,4)

      DO WHILE (I.LT.N)                                                 0003

C  T(I) {I==I#init+1}

C (0,5)
C        BEGIN BLOCK

C  T() {}

C (0,6)
         PRINT *, T(I)                                                  0004

C  T(I) {I==I#init+1}

C (0,7)
         I = I+1                                                        0005
C        END BLOCK
      ENDDO

C  T() {}

C (0,8)

      PRINT *, I                                                        0006

C     END BLOCK
      END

PRECONDITIONS


C  P() {}

      PROGRAM W02
      INTEGER I, N
      REAL T(10)

C  P() {}

C (0,1)
C     BEGIN BLOCK

C  P() {}

C (0,2)

      I = 1                                                             0001

C  P(I) {I==1}

C (0,3)
      N = 10                                                            0002

C  P(I,N) {I==1, N==10}

C (0,4)

      DO WHILE (I.LT.N)                                                 0003

C  P(I,N) {N==10, 1<=I, I<=9}

C (0,5)
C        BEGIN BLOCK

C  P(I,N) {N==10, 1<=I, I<=9}

C (0,6)
         PRINT *, T(I)                                                  0004

C  P(I,N) {N==10, 1<=I, I<=9}

C (0,7)
         I = I+1                                                        0005
C        END BLOCK
      ENDDO

C  P(I,N) {I==10, N==10}

C (0,8)

      PRINT *, I                                                        0006

C     END BLOCK
      END

REGIONS


C  <__IO_EFFECTS:LUNS(PHI1)-R-MAY-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-MAY-{PHI1==6}>

      PROGRAM W02
      INTEGER I, N
      REAL T(10)

C  <__IO_EFFECTS:LUNS(PHI1)-R-MAY-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-MAY-{PHI1==6}>
C  <T(PHI1)-R-MAY-{1<=PHI1, PHI1<=9}>

C (0,1)
C     BEGIN BLOCK
C (0,2)

      I = 1                                                             0001
C (0,3)
      N = 10                                                            0002

C  <__IO_EFFECTS:LUNS(PHI1)-R-MAY-{PHI1==6, N==10}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-MAY-{PHI1==6, N==10}>
C  <T(PHI1)-R-MAY-{1<=PHI1, PHI1<=9, N==10}>

C (0,4)

      DO WHILE (I.LT.N)                                                 0003

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6, N==10, 1<=I, I<=9}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6, N==10, 1<=I, I<=9}>
C  <T(PHI1)-R-EXACT-{PHI1==I, N==10, 1<=I, I<=9}>

C (0,5)
C        BEGIN BLOCK

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6, N==10, 1<=I, I<=9}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6, N==10, 1<=I, I<=9}>
C  <T(PHI1)-R-EXACT-{PHI1==I, N==10, 1<=I, I<=9}>

C (0,6)
         PRINT *, T(I)                                                  0004
C (0,7)
         I = I+1                                                        0005
C        END BLOCK
      ENDDO

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6, I==10, N==10}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6, I==10, N==10}>

C (0,8)

      PRINT *, I                                                        0006

C     END BLOCK
      END
