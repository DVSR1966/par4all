
C  T() {}

      PROGRAM TYPE03

C     Goal: check the extension of the semantics analysis to logical
C     scalar variables

      LOGICAL L1, L2, L3, L4, L5, L6

C  T(L3) {0<=L1, L1<=1, L3<=L1+L2, L1<=L3, 0<=L2, L2<=1, L2<=L3,
C    0<=L3, L3<=1}


      L3 = L1.OR.L2                                                     0001

C  T(L4) {0<=L1, L1<=1, L1+L2<=L4+1, L4<=L1, 0<=L2, L2<=1, L4<=L2,
C    0<=L4, L4<=1}

      L4 = L1.AND.L2                                                    0002

C  T(L5) {0<=L1, L1<=1, 1<=L1+L2+L5, L1+L2<=L5+1, 0<=L2, L2<=1, 0<=L5,
C    L5<=1}

      L5 = L1.EQ.L2                                                     0003

C  T(L6) {0<=L1, L1<=1, L6<=L1+L2, L1+L2+L6<=2, 0<=L2, L2<=1, 0<=L6,
C    L6<=1}

      L6 = L1.NE.L2                                                     0004

C  T() {}


      PRINT *, L1, L2, L3, L4, L5, L6                                   0005

      END

