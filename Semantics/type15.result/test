
C  T() {}

      PROGRAM TYPE15
      REAL A, B

C  T(A) {}


      READ *, A                                                         0001

C  T(A) {1.+A#init<=A, 4.<=A, 5.<=A}


      A = AMAX1(A+1., 4., 5.)                                           0002

C  T() {}


      IF (A.LT.3.) THEN                                                 0003

C  T() {}

         PRINT *, 'impossible'                                          0004
      ELSE

C  T() {}

         PRINT *, 'of course'                                           0005
      ENDIF

      END

C  P() {}

      PROGRAM TYPE15
      REAL A, B

C  P() {}


      READ *, A                                                         0001

C  P(A) {}


      A = AMAX1(A+1., 4., 5.)                                           0002

C  P(A) {4.<=A, 5.<=A}


      IF (A.LT.3.) THEN                                                 0003

C  P() {0==-1}

         PRINT *, 'impossible'                                          0004
      ELSE

C  P(A) {3.<=A, 4.<=A, 5.<=A}

         PRINT *, 'of course'                                           0005
      ENDIF

      END
      PROGRAM TYPE15
      REAL A, B

      READ *, A                                                         0001

      A = AMAX1(A+1., 4., 5.)                                           0002

      PRINT *, 'of course'                                              0005

      END

