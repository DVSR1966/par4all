
C  P() {}

      SUBROUTINE TRUSTED_DECL01

      REAL A(100)

C  P() {}


      READ *, N                                                         0001

C  P(N) {}


      CALL INIT(A, N)                                                   0002

C  P(N) {1<=N}


      PRINT *, N                                                        0003

      END

