--- Preconditions for REALINDEX

C  P() {}

      PROGRAM REALINDEX

C     Check that index with no semantics values are ignored

      REAL T(10)

C  P() {}


      DO X = 1, N                                                       0001

C  P() {}

         T(X) = 0.                                                      0002
      ENDDO

C  P() {}


      IF (N.GE.1) THEN                                                  0003

C  P() {1<=N}

         DO X = 1, N                                                    0004

C  P() {1<=N}

            T(X) = 0.                                                   0005
         ENDDO
      ELSE

C  P() {N<=0}

         DO X = 1, N                                                    0006

C  P() {0==-1}

            T(X) = 0.                                                   0007
         ENDDO
      ENDIF

      END
