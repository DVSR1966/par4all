main()
{
   unsigned int i = 2;
   int j = 4;
   int k = 6;

   i++;
   i += j;
   i = k;
   return;
}

//  P() {i==2, j==4, k==6}

main()
{

//  P() {i==2, j==4, k==6}

   unsigned int i = 2;

//  P() {i==2, j==4, k==6}

   int j = 4;

//  P() {i==2, j==4, k==6}

   int k = 6;

//  P() {i==2, j==4, k==6}


   i++;

//  P(i) {i==3, j==4, k==6}

   i += j;

//  P(i) {i==7, j==4, k==6}

   i = k;

//  P(i) {i==6, j==4, k==6}

   return;
}
