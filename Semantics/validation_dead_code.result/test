--- Preconditions for module VALIDATION_DEAD_CODE

C  P() {}

      PROGRAM VALIDATION_DEAD_CODE
      INTEGER A, B

C  P() {}


      J = 3                                                             0001

C  P(J) {J==3}


c     if inutile qui doit disparai^tre No 1 (vide de`s le source) :
      IF (B.GE.A) THEN                                                  0002
      ELSE
      ENDIF

C  P(J) {J==3}


c     if inutile qui doit disparai^tre No 2 (devient vide par e'limination ) :
      IF (B.GE.A) THEN                                                  0003

C  P(J) {J==3, A<=B}

c     if faux :
         IF (1.EQ.0) THEN                                               0004

C  P() {0==-1}

            I = 1                                                       0005
         ELSE
         ENDIF
      ELSE

C  P(J) {J==3, B+1<=A}

c     if faux :
         IF (J.EQ.5) THEN                                               0006

C  P() {0==-1}

            I = 1                                                       0007
         ELSE
         ENDIF

C  P(J) {J==3, B+1<=A}


         CONTINUE
      ENDIF

C  P(J) {J==3}


c     Teste les optimisations imbrique'es
c     Doit e^tre re'e'crit :
c     i = -9 (devrait i = 1 et i = -9)
      IF (J.NE.5) THEN                                                  0008

C  P(J) {J==3}

         DO I = 1, 0                                                    0009

C  P() {0==-1}

            RIEN = J+0                                                  0010
         ENDDO

C  P(I,J) {I==1, J==3}

         IF (J.EQ.6) THEN                                               0011

C  P() {0==-1}

            I = -8                                                      0012
         ELSE

C  P(I,J) {I==1, J==3}

            I = -9                                                      0013
         ENDIF
      ELSE

C  P() {0==-1}

         J = 6                                                          0014
      ENDIF

C  P(I,J) {I==-9, J==3}


      DO I = 1, 0                                                       0015

C  P() {0==-1}

         J = J+0                                                        0016
      ENDDO

C  P(I,J) {I==1, J==3}

c     Bizarre : la pre'condition sur j est ici impre'cise !
c     Devrait avoir i = -2 car j = 3...
      IF (J.EQ.6) THEN                                                  0017

C  P() {0==-1}

         I = -1                                                         0018
      ELSE

C  P(I,J) {I==1, J==3}

         I = -2                                                         0019
      ENDIF

C  P(I,J) {I==-2, J==3}


c     doit e^tre re'e'crit :
c     j = j + 2
      IF (A.GT.10.AND.A.LT.10) THEN                                     0020

C  P() {0==-1}

         J = J+1                                                        0021

C  P() {0==-1}

c Si je veux compliquer :-)
c         goto 18
         CONTINUE
      ELSE

C  P(I,J) {I==-2, J==3}

         J = J+2                                                        0022
      ENDIF

C  P(I,J) {I==-2, J==5}


      IF (A.GT.3.AND.B.EQ.5) THEN                                       0023

C  P(I,J) {B==5, I==-2, J==5, 4<=A}

         RIEN = 2                                                       0024
      ELSE

C  P(I,J) {I==-2, J==5}

         RIEN = 3                                                       0025
      ENDIF

C  P(I,J) {I==-2, J==5}

      PRINT *, J                                                        0026

C  P(I,J) {I==-2, J==5}


c     devrait e^tre re'e'crit :
c     rien = rien + 1
c     i = 1
      DO 40 I = 0, 1                                                    0027

C  P(I,J) {J==5, 0<=I, I<=1}

         RIEN = RIEN+1                                                  0028

C  P(I,J) {J==5, 0<=I, I<=1}

40       CONTINUE                                                       0029

C  P(I,J) {I==2, J==5}


c     devrait e^tre supprime' :
      DO 20 J = I, 0, 2                                                 0030

C  P() {0==-1}

         RIEN = RIEN+1                                                  0031

C  P() {0==-1}

20       CONTINUE                                                       0032

C  P(I,J) {I==2, J==2}


18    I = 2                                                             0033

C  P(I,J) {I==2, J==2}


c     doit e^tre re'e'crit :
c19   i = j + 1
19    DO 30 I = J+1, J, 5                                               0034

C  P() {0==-1}

c     commentaire sur un goto :
10       CONTINUE                                                       0036

C  P() {0==-1}

30       CONTINUE                                                       0037

C  P(I,J) {I==3, J==2}


c     doit e^tre re'e'crit :
c     j = 1
      IF (I.EQ.J+1) THEN                                                0038

C  P(I,J) {I==3, J==2}

         J = 1                                                          0039
      ELSE

C  P() {0==-1}

         J = 2                                                          0040
      ENDIF

C  P(I,J) {I==3, J==1}

      PRINT *, J                                                        0041

C  P(I,J) {I==3, J==1}


c     doit e^tre supprime' :
      DO J = 3, 2                                                       0042

C  P() {0==-1}

         RIEN = RIEN+1                                                  0043

C  P() {0==-1}

         I = 3                                                          0044
      ENDDO

C  P(I,J) {I==3, J==3}


c     doit e^tre supprime' :
      DO J = 2, 7, -5                                                   0045

C  P() {0==-1}

         RIEN = RIEN+1                                                  0046

C  P() {0==-1}

         I = 7                                                          0047
      ENDDO

C  P(I,J) {I==3, J==2}


c     inchange' :
      I = 4                                                             0048

C  P(I,J) {I==4, J==2}

c     doit e^tre re'e'crit :
c     j = 4
      IF (I.EQ.5) THEN                                                  0049

C  P() {0==-1}

         J = 3                                                          0050
      ELSE

C  P(I,J) {I==4, J==2}

         J = 4                                                          0051
      ENDIF

C  P(I,J) {I==4, J==4}


c     doit e^tre re'e'crit :
c     j = 5
      IF (.NOT.I.EQ.5) THEN                                             0052

C  P(I,J) {I==4, J==4}

         J = 5                                                          0053
      ELSE

C  P() {0==-1}

         J = 6                                                          0054
      ENDIF

C  P(I,J) {I==4, J==5}

C     BEGIN UNSTRUCTURED

C  P(I,J) {I==4, J==5}


      IF (I.EQ.5) GOTO 31                                               0055

C  P(I,J) {I==4, J==5}

      PRINT *, 'salut'                                                  0057

C  P(I,J) {I==4, J==5}

31    A = 9                                                             0058

C  P(A,I,J) {A==9, I==4, J==5}


c     Ce test est en soit infaisable :
c     doit e^tre re'e'crit :
c     j = 8
      IF (I.GT.5.AND.3.GT.I) THEN                                       0059

C  P() {0==-1}

         J = 7                                                          0060
      ELSE

C  P(A,I,J) {A==9, I==4, J==5}

         J = 8                                                          0061
      ENDIF

C  P(A,I,J) {A==9, I==4, J==8}


c     de'pendance sur ce qui pre'ce`de :
c     doit e^tre re'e'crit :
c     k = 1
c     goto 100
      IF (J.GE.3) GOTO 99998                                            0062

C  P() {0==-1}

      K = 1                                                             0064
      GOTO 100
99998 CONTINUE

C  P(A,I,J) {A==9, I==4, J==8}

      K = 3                                                             0063

C  P(A,I,J,K) {A==9, I==4, J==8, K==3}

c     doit e^tre conserve' :
100   CONTINUE                                                          0070
C     END UNSTRUCTURED

C  P(A,I,J,K) {A==9, I==4, J==8, K==3}

      I = I+1                                                           0070

C  P(A,I,J,K) {A==9, I==5, J==8, K==3}


      STOP                                                              0071

C  P() {0==-1}


c     doit e^tre supprime' :
      J = A                                                             0072

C  P() {0==-1}


      RETURN
      END
