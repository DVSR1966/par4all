//  P() {}

int repeat02()
{
   int i;
   int j;

//  P() {}

//:   BEGIN BLOCK

//  P() {}


   i = 0;

//  P(i) {i==0}

   j = 1;
   {
      int n = 10;

//  P(i,j) {i==0, j==1}

//:      BEGIN BLOCK

//  P(i,j) {i==0, j==1, n==10}

      do {

//  P(i,j) {2i==j-1, n==10, 0<=i}

//:         BEGIN BLOCK

//  P(i,j) {2i==j-1, n==10, 0<=i}

         i++;

//  P(i,j) {2i==j+1, n==10, 1<=i}

         j += 2;
//:         END BLOCK
      }
      while (j>n);
//:      END BLOCK
   }

//  P(i,j) {i==1, j==3}

   return i+j;
//:   END BLOCK
}
