
Transformers for "repeat02"


C  T(repeat02) {repeat02==4}

int repeat02()
{

//  T(repeat02) {repeat02==4}

   // BEGIN BLOCK
   int i;
   int j;
   int n = 10;

//  T(i) {i==0, n==10}


   i = 0;

//  T(j) {i==0, j==1, n==10}

   j = 1;

//  T(i,j) {2i==j-1, 2i#init==j#init-1, n==10, 0<=i, i#init<=i}

   do {

//  T(i,j) {i==i#init+1, j==j#init+2, n==10, 3<=j}

      // BEGIN BLOCK

//  T(i) {i==i#init+1, n==10, 1<=j}

      i++;

//  T(j) {j==j#init+2, n==10, 3<=j}

      j += 2;
      // END BLOCK
   }
   while (j>n);

//  T(repeat02) {i==1, j==3, n==10, repeat02==4}

   //}
   return i+j;
   // END BLOCK
}

Preconditions for "repeat02"


//  P() {}

int repeat02()
{

//  P() {}

   // BEGIN BLOCK
   int i;
   int j;
   int n = 10;

//  P() {n==10}


   i = 0;

//  P(i) {i==0, n==10}

   j = 1;

//  P(i,j) {i==0, j==1, n==10}

   do {

//  P(i,j) {2i==j-1, n==10, 0<=i}

      // BEGIN BLOCK

//  P(i,j) {2i==j-1, n==10, 0<=i}

      i++;

//  P(i,j) {2i==j+1, n==10, 1<=i}

      j += 2;
      // END BLOCK
   }
   while (j>n);

//  P(i,j) {i==1, j==3, n==10}

   //}
   return i+j;
   // END BLOCK
}
