main()
{
   unsigned int i = 2;
   int j = 4;
   int k = 6;

   i++;
   i += j;
   i = foo(k);
   return;
}

//  P() {}

main()
{
   unsigned int i = 2;
   int j = 4;
   int k = 6;

//  P() {i==2, j==4, k==6}


   i++;

//  P(i) {i==3, j==4, k==6}

   i += j;

//  P(i) {i==7, j==4, k==6}

   i = foo(k);

//  P(i) {i==6, j==4, k==6}

   return;
}
