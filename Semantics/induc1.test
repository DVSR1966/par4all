#!/bin/sh
#
# Script to test pips on the simple program
#
# Use of the rule transformers_intra_full
#
if Pips -f $PIPS_ROOT/Validation/Semantics/induc1.f -w induc1 -m induc1 \
	-s preconditions_intra -s transformers_intra_full -p prcp >&2
then
	exit_code=0
	echo "--- Preconditions for module INDUC1"
	Display -l prec
else
	exit_code=1
fi
Delete induc1 >&2
exit $exit_code
