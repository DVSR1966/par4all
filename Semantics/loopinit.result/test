# proper effects
      
      INTEGER FUNCTION INC(K)
C               <must be read   >: K
C               <must be written>: K
      K = K+1                                                           0001
C               <must be read   >: K
C               <must be written>: INC
      INC = K                                                           0002
      END
      PROGRAM LOOPINIT

c     Check that loop index initialization is performed correctly

      REAL T(10)
C               <must be written>: J

      J = 2                                                             0001
C               <must be read   >: J N
C               <must be written>: I J

      DO I = INC(J), N                                                  0002
C               <must be read   >: I J N
C               <must be written>: J T(I)
         T(I) = 0.                                                      0003
C               <must be read   >: J N
C               <must be written>: J
         J = J+2                                                        0004
      ENDDO
C               <must be read   >: I J __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)

      PRINT *, I, J                                                     0005

      END
# cumulated effects
C               <must be read   >: K
C               <must be written>: K
      
      INTEGER FUNCTION INC(K)
C               <must be read   >: K
C               <must be written>: K
      K = K+1                                                           0001
C               <must be read   >: K
C               <must be written>: INC
      INC = K                                                           0002
      END
C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
      PROGRAM LOOPINIT

c     Check that loop index initialization is performed correctly

      REAL T(10)
C               <must be written>: J

      J = 2                                                             0001
C               <may be read    >: I
C               <may be written >: T(*)
C               <must be read   >: J N
C               <must be written>: I J

      DO I = INC(J), N                                                  0002
C               <may be written >: T(*)
C               <must be read   >: I J N
C               <must be written>: J
         T(I) = 0.                                                      0003
C               <must be read   >: J N
C               <must be written>: J
         J = J+2                                                        0004
      ENDDO
C               <may be read    >: __IO_EFFECTS:LUNS(*)
C               <may be written >: __IO_EFFECTS:LUNS(*)
C               <must be read   >: I J

      PRINT *, I, J                                                     0005

      END
# preconditions

C  P() {}

      PROGRAM LOOPINIT

c     Check that loop index initialization is performed correctly

      REAL T(10)

C  P() {}


      J = 2                                                             0001

C  P(J) {J==2}


      DO I = INC(J), N                                                  0002

C  P(I,J) {I<=N}

         T(I) = 0.                                                      0003

C  P(I,J) {I<=N}

         J = J+2                                                        0004
      ENDDO

C  P(I,J) {}


      PRINT *, I, J                                                     0005

      END

