//  T(TOP-LEVEL:j,call08) {TOP-LEVEL:j==j#init+1, call08==i+10}

int call08(int i)
{

//  T(i) {i==i#init+10}

   i += 10;                                                             /*0007*/

//  T(TOP-LEVEL:j) {TOP-LEVEL:j==j#init+1}

