#!/bin/sh

tpips=${tpips_version:-tpips}

# Initialization:
# rm -f dummy.result/test
# tpips >dummy.result/test <<EOF

$tpips <<EOF
# Check that tests are correctly used to generate transformers

delete dummy
create dummy $PIPS_VALIDDIR/Semantics/dummy.f

activate TRANSFORMERS_INTER_FAST
activate PRECONDITIONS_INTER_FAST

module BAR
activate PRINT_CODE_TRANSFORMERS
display PRINTED_FILE

module DUMMY
activate PRINT_CODE_PRECONDITIONS
display PRINTED_FILE
close
delete dummy
quit
EOF

