      PROGRAM STRDATA02
      CHARACTER*12 C
      DATA C /'hello world'/
      INTEGER I
      DATA I /12/
      REAL*4 R
      DATA R /1.0/
      LOGICAL L
      DATA L /.TRUE./
      PRINT *, C, I, R, L                                               0010
      C = 'bye world'                                                   0011
      I = 3                                                             0012
      R = 2.0                                                           0013
      L = .FALSE.                                                       0014
      PRINT *, C, I, R, L                                               0015
      END

C  P() {'hello world'==C, 1.0==R, I==12, L==1}


C  P() {'hello world'==C, 1.0==R, I==12, L==1}


C  T(C,I,L,R) {'bye world'==C, 2.0==R, I==3, L==0}

      PROGRAM STRDATA02
      CHARACTER*12 C
      DATA C /'hello world'/
      INTEGER I
      DATA I /12/
      REAL*4 R
      DATA R /1.0/
      LOGICAL L
      DATA L /.TRUE./

C  T() {}

      PRINT *, C, I, R, L                                               0010

C  T(C) {'bye world'==C}

      C = 'bye world'                                                   0011

C  T(I) {I==3}

      I = 3                                                             0012

C  T(R) {2.0==R}

      R = 2.0                                                           0013

C  T(L) {L==0}

      L = .FALSE.                                                       0014

C  T() {}

      PRINT *, C, I, R, L                                               0015
      END

C  P() {'hello world'==C, 1.0==R, I==12, L==1}

      PROGRAM STRDATA02
      CHARACTER*12 C
      DATA C /'hello world'/
      INTEGER I
      DATA I /12/
      REAL*4 R
      DATA R /1.0/
      LOGICAL L
      DATA L /.TRUE./

C  P() {'hello world'==C, 1.0==R, I==12, L==1}

      PRINT *, C, I, R, L                                               0010

C  P() {'hello world'==C, 1.0==R, I==12, L==1}

      C = 'bye world'                                                   0011

C  P(C) {'bye world'==C, 'hello world'==C#init, 1.0==R, I==12, L==1}

      I = 3                                                             0012

C  P(C,I) {'bye world'==C, 'hello world'==C#init, 1.0==R, I==3,
C    I#init==12, L==1}

      R = 2.0                                                           0013

C  P(C,I,R) {'bye world'==C, 'hello world'==C#init, 1.0==R#init,
C    2.0==R, I==3, I#init==12, L==1}

      L = .FALSE.                                                       0014

C  P(C,I,L,R) {'bye world'==C, 'hello world'==C#init, 1.0==R#init,
C    2.0==R, I==3, I#init==12, L==0, L#init==1}

      PRINT *, C, I, R, L                                               0015
      END
