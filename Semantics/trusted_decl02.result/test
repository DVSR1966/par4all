
C  T(N) {N==0, 1<=N#init}


      SUBROUTINE INIT(A, N)
      REAL A(N)
      REAL B(1, 4)

C  T(I) {1<=N}


      DO I = 1, N                                                       0001

C  T() {1<=I, I<=N, 1<=N}

         A(I) = 0.                                                      0002
      ENDDO

C  T(N) {I==N#init+1, N==0, 2<=I}


      N = 0                                                             0003

      END

C  P() {}

      SUBROUTINE TRUSTED_DECL02

      REAL A(100)

C  P() {}


      READ *, N                                                         0001

C  P(N) {}


      CALL INIT(A, N)                                                   0002

C  P(N) {N==0}


      PRINT *, N                                                        0003

      END

