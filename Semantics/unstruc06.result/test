
C  P() {}

      PROGRAM UNSTRUC06

C     Check that desugared loop might be handled

      REAL A(100)

C  P() {}


      READ *, N                                                         0001

C  P(N) {}


      I = 1                                                             0002

C  P(I,N) {1<=I}

100   CONTINUE                                                          0003

C  P(I,N) {1<=I}

      IF (I.GT.N) GOTO 200                                              0004

C  P(I,N) {1<=I, I<=N}

      A(I) = 0                                                          0006

C  P(I,N) {1<=I, I<=N}

      I = I+1                                                           0007
      GOTO 100

C  P(I,N) {1<=I, N+1<=I}


200   CONTINUE                                                          0008

C  P(I,N) {1<=I, N+1<=I}

      PRINT *, I, N                                                     0009

      END

