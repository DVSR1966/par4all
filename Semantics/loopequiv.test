#!/bin/sh

tpips=${tpips_version:-tpips}

$tpips <<EOF

# Check that meaningless preconditions do not appear about
# loop indices

delete loopequiv
create loopequiv $PIPS_ROOT/Validation/Semantics/loopequiv.f
module loopequiv
activate PRINT_CODE_PRECONDITIONS
display PRINTED_FILE

close
delete loopequiv
quit
EOF

