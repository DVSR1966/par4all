
C  T() {1<=N}

      SUBROUTINE TRUSTED_REF09(A, N)

      REAL A(N)

C  T(I,M) {1<=I, M+1<=I, I<=N+1, 1<=N}

C     BEGIN BLOCK

C  T(M) {1<=N}


      READ *, M                                                         0001

C  T(I) {1<=I, M+1<=I, I<=N+1, 1<=N}


      IF (.TRUE.) THEN                                                  0002

C  T(I) {1<=I, 1<=N}

         DO I = 1, M                                                    0003

C  T() {1<=I, I<=M, I<=N, 1<=N}

            A(I) = 0                                                    0004
         ENDDO
      ENDIF

C     END BLOCK
      END

