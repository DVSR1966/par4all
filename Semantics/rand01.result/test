
PRINT CODE for main

int main()
{
   int entier;
   int graine;
   double virgule;
   graine = 50;
   srandom(graine);
   /* generator initialization */
   virgule = (double) rand()/(2147483647+1);
   entier = 1+rand();
}

Print cumulated effects for main

//               <must be read   >: _RANDOM_EFFECTS:GEN_SEED
//               <must be written>: _RANDOM_EFFECTS:GEN_SEED
int main()
{
   int entier;
   int graine;
   double virgule;
//               <must be written>: graine
   graine = 50;
//               <must be read   >: graine
//               <must be written>: _RANDOM_EFFECTS:GEN_SEED
   srandom(graine);
//               <must be read   >: _RANDOM_EFFECTS:GEN_SEED
//               <must be written>: _RANDOM_EFFECTS:GEN_SEED virgule
   /* generator initialization */
   virgule = (double) rand()/(2147483647+1);
//               <must be read   >: _RANDOM_EFFECTS:GEN_SEED
//               <must be written>: _RANDOM_EFFECTS:GEN_SEED entier
   entier = 1+rand();
}

Transformers for main


//  T() {}

int main()
{
   int entier;
   int graine;
   double virgule;

//  T(graine) {graine==50}

   graine = 50;

//  T() {}

   srandom(graine);

//  T() {}

   /* generator initialization */
   virgule = (double) rand()/(2147483647+1);

//  T(entier) {}

   entier = 1+rand();
}
