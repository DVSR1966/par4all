#!/bin/sh
#
# Script to test pips on the simple program
#
# Use of the rule transformers_inter_fast
#
if Pips -f $PIPS_ROOT/Validation/Semantics/dead2.f -w dead2 -m dead2 \
	-s pcinter -s tfinter -p prcp >&2
then
	exit_code=0
	echo "--- Preconditions for module DEAD2"
	Display -l prec
else
	exit_code=1
fi
Delete dead2 >&2
exit $exit_code
