
C  T() {}

      PROGRAM CHAINE03
      CHARACTER*3 S1, S2

C  T(S1) {'hi'==S1}

!
      S1 = 'hi'                                                         0001

C  T(S2) {'hi '==S2}

      S2 = 'hi '                                                        0002

C  T() {}

      PRINT *, '-', S1, '-', S2, '-'                                    0003

C  T() {}

      IF (S1.EQ.S2) THEN                                                0004

C  T() {}

         PRINT *, 's1 == s2'                                            0005
      ENDIF

C  T() {}

      IF (S1.NE.S2) THEN                                                0006

C  T() {}

         PRINT *, 's1 != s2'                                            0007
      ENDIF
      END

C  P() {}

      PROGRAM CHAINE03
      CHARACTER*3 S1, S2

C  P() {}

!
      S1 = 'hi'                                                         0001

C  P(S1) {'hi'==S1}

      S2 = 'hi '                                                        0002

C  P(S1,S2) {'hi '==S2, 'hi'==S1}

      PRINT *, '-', S1, '-', S2, '-'                                    0003

C  P(S1,S2) {'hi '==S2, 'hi'==S1}

      IF (S1.EQ.S2) THEN                                                0004

C  P(S1,S2) {'hi '==S1, 'hi'==S1, S1==S2}

         PRINT *, 's1 == s2'                                            0005
      ENDIF

C  P(S1,S2) {'hi '==S2, 'hi'==S1}

      IF (S1.NE.S2) THEN                                                0006

C  P(S1,S2) {'hi '==S2, 'hi'==S1}

         PRINT *, 's1 != s2'                                            0007
      ENDIF
      END
      PROGRAM CHAINE03
      CHARACTER*3 S1, S2
!
      S1 = 'hi'                                                         0001
      S2 = 'hi '                                                        0002
      PRINT *, '-', S1, '-', S2, '-'                                    0003
      IF (S1.EQ.S2) THEN                                                0004
         PRINT *, 's1 == s2'                                            0005
      ENDIF
      IF (S1.NE.S2) THEN                                                0006
         PRINT *, 's1 != s2'                                            0007
      ENDIF
      END

