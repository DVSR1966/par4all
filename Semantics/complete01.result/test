
Source code with proper effects

void complete01()
{
   // BEGIN BLOCK
   int i;
   {
      // BEGIN BLOCK
      int j = 0;
//               <must be read   >: i
//               <must be written>: i
      for (i = 0;i<10;i++)                                              /*0009*/
//               <must be read   >: j
//               <must be written>: j
         j++;                                                           /*0010*/
      // END BLOCK
   }

   foo(i);                                                              /*0015*/
   // END BLOCK
}

Source code with cumulated effects

void complete01()
{
//               <must be read   >: i j
//               <must be written>: i j
   // BEGIN BLOCK
   int i;
   {
//               <must be read   >: i j
//               <must be written>: i j
      // BEGIN BLOCK
      int j = 0;
//               <must be read   >: i j
//               <must be written>: i j
      for (i = 0;i<10;i++)                                              /*0009*/
//               <must be read   >: j
//               <must be written>: j
         j++;                                                           /*0010*/
      // END BLOCK
   }

   foo(i);                                                              /*0015*/
   // END BLOCK
}

Source code with transformers


//  T() {}

void complete01()
{

//  T() {}

   // BEGIN BLOCK
   int i;
   {

//  T(i) {i==10}

      // BEGIN BLOCK
      int j = 0;

//  T(i,j) {i+j#init==j, 0<=i, i<=9, j#init<=j}

      for (i = 0;i<10;i++)                                              /*0009*/

//  T(j) {j==j#init+1}

         j++;                                                           /*0010*/
      // END BLOCK
   }

//  T() {}


   foo(i);                                                              /*0015*/
   // END BLOCK
}

Source code with preconditions


//  P() {}

void complete01()
{

//  P() {}

   // BEGIN BLOCK
   int i;
   {

//  P() {}

      // BEGIN BLOCK
      int j = 0;

//  P() {j==0}

      for (i = 0;i<10;i++)                                              /*0009*/

//  P(i,j) {i==j, 0<=i, i<=9}

         j++;                                                           /*0010*/
      // END BLOCK
   }

//  P(i) {i==10}


   foo(i);                                                              /*0015*/
   // END BLOCK
}

