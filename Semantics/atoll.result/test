// OMP style

//  P() {}

int main(int argc, char **argv)
{

//  P() {}

   long long size = test_intr(argv);

//  P(size) {}

   return (int) size;
}

//  P() {}

long long test_intr(char **argv)
{

//  P() {}

   long long result = 0;

//  P(result) {result==0}

   result = atoll(argv[1]);

//  P(result) {}

   return result;
}
