#!/bin/sh

tpips=${tpips_version:-tpips}

$tpips <<EOF

# Check that meaningless preconditions do not appear about
# loop indices

delete loopequiv2
create loopequiv2 $PIPS_ROOT/Validation/Semantics/loopequiv2.f
module loopequiv2
activate PRINT_CODE_PRECONDITIONS
display PRINTED_FILE

close
delete loopequiv2
quit
EOF

