Source code with transformers


//  T(main) {main==2}

int main()
{

//  T() {}

   int i, j, k;                                                         /*0003*/

//  T() {}

   int a[500];                                                          /*0004*/

//  T(i) {i==0}


   i = 0;                                                               /*0006*/

//  T(j) {j==1}

   j = 1;                                                               /*0007*/

//  T(i,j) {i+j#init==j, j#init<=j}


   for(i = 0; i <= 499; i += 1) {

//  T(j) {j==j#init+1}

      j++;                                                              /*0010*/

//  T() {}

      // Cumulated effects should be a[i] here and not a[*]
      a[i] = i;                                                         /*0012*/
   }

//  T(k) {k==2}

   /* We would have j==501 in the preconditions here... if we'd use
      // the proper activate and properties: the fast analysis exclude
      // fix points (see for01.tpips) */
   k = 2;                                                               /*0017*/

//  T(main) {k==main}

   return k;                                                            /*0018*/
}

Source code with preconditions


//  P() {}

int main()
{

//  P() {}

   int i, j, k;                                                         /*0003*/

//  P() {}

   int a[500];                                                          /*0004*/

//  P() {}


   i = 0;                                                               /*0006*/

//  P(i) {i==0}

   j = 1;                                                               /*0007*/

//  P(i,j) {i==0, j==1}


   for(i = 0; i <= 499; i += 1) {

//  P(i,j) {i==j-1, 0<=i, i<=499}

      j++;                                                              /*0010*/

//  P(i,j) {i==j-2, 0<=i, i<=499}

      // Cumulated effects should be a[i] here and not a[*]
      a[i] = i;                                                         /*0012*/
   }

//  P(i,j) {i==500, j==501}

   /* We would have j==501 in the preconditions here... if we'd use
      // the proper activate and properties: the fast analysis exclude
      // fix points (see for01.tpips) */
   k = 2;                                                               /*0017*/

//  P(i,j,k) {i==500, j==501, k==2}

   return k;                                                            /*0018*/
}

