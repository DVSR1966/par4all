
C  T() {}

      SUBROUTINE NON_LINEAR07(T, M)

C     Check rules for multiplication and loop bounds

      REAL T(M)

C  T(I,N) {2<=I}

C (0,1)
C     BEGIN BLOCK

C  T(N) {}

C (0,2)

      READ *, N                                                         0001

C  T(I) {2<=I}

C (0,3)
      DO I = N*N+2, M                                                   0002

C  T() {1<=I, I<=M}

C (0,4)
         T(I) = 0.                                                      0003
      ENDDO

C  T() {}

C (0,5)

      PRINT *, I                                                        0004

C     END BLOCK
      END

C  P() {}

      SUBROUTINE NON_LINEAR07(T, M)

C     Check rules for multiplication and loop bounds

      REAL T(M)

C  P() {1<=M}

C (0,1)
C     BEGIN BLOCK

C  P() {1<=M}

C (0,2)

      READ *, N                                                         0001

C  P(N) {1<=M}

C (0,3)
      DO I = N*N+2, M                                                   0002

C  P(I,N) {2<=I, I<=M, 1<=M}

C (0,4)
         T(I) = 0.                                                      0003
      ENDDO

C  P(I,N) {2<=I, 1<=M}

C (0,5)

      PRINT *, I                                                        0004

C     END BLOCK
      END

