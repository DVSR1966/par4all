--- Preconditions for SIMPLE

C  P() {}

C     Information generated by the test was not exploited 

      SUBROUTINE SIMPLE(N)
      INTEGER N, I, J
      REAL A(100), B(100)
      REAL M

C  P() {}

      M = SQRT(0.37*N)                                                  0001

C  P() {}

      DO I = 1, N                                                       0002

C  P(I) {1<=I, I<=N}

         IF (J.LT.5) THEN                                               0003

C  P(I) {1<=I, I<=N, J<=4}

            A(I) = B(I)*3+5                                             0004
         ENDIF
      ENDDO
      END
