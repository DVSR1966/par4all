
Cumulated effects for "main"

//               <must be read   >: TOP-LEVEL:delta TOP-LEVEL:delta2
main()
{
//               <must be read   >: TOP-LEVEL:delta TOP-LEVEL:delta2 i
//               <must be written>: i
   // BEGIN BLOCK
   int i = 0;
//               <must be read   >: TOP-LEVEL:delta i
//               <must be written>: i

   i = i+delta;
//               <must be read   >: TOP-LEVEL:delta2 i
//               <must be written>: i
   i = i+delta2;
   // END BLOCK
}

Transformers for "main"


//  T() {TOP-LEVEL:delta==2, TOP-LEVEL:delta2==4}

main()
{

