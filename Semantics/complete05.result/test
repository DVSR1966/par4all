
Source code with proper effects

void complete05()
{
   // BEGIN BLOCK
   int i;                                                               /*0010*/
   int n;                                                               /*0011*/
   {
      // BEGIN BLOCK
//             <    is written>: j
      int j = 0;                                                        /*0015*/
//             <    is read   >: i n
//             <    is written>: i
      for (i = 0;i<n;i++) {                                             /*0016*/
         // BEGIN BLOCK
//             <    is read   >: j
//             <    is written>: j
         j++;                                                           /*0017*/
//             <    is read   >: i
//             <    is written>: i
         i--;                                                           /*0018*/
         // END BLOCK
      }
      // END BLOCK
   }
//             <    is read   >: i

   foo(i);                                                              /*0022*/
   // END BLOCK
}

Source code with cumulated effects

void complete05()
{
   // BEGIN BLOCK
   int i;                                                               /*0010*/
   int n;                                                               /*0011*/
   {
//             <    is read   >: i n
//             <    is written>: i
      // BEGIN BLOCK
//             <    is written>: j
      int j = 0;                                                        /*0015*/
//             <may be read   >: j
//             <may be written>: j
//             <    is read   >: i n
//             <    is written>: i
      for (i = 0;i<n;i++) {                                             /*0016*/
//             <    is read   >: i j
//             <    is written>: i j
         // BEGIN BLOCK
//             <    is read   >: j
//             <    is written>: j
         j++;                                                           /*0017*/
//             <    is read   >: i
//             <    is written>: i
         i--;                                                           /*0018*/
         // END BLOCK
      }
      // END BLOCK
   }
//             <    is read   >: i

   foo(i);                                                              /*0022*/
   // END BLOCK
}

Source code with transformers


//  T() {}

void complete05()
{

//  T() {}

   // BEGIN BLOCK

//  T(i) {}

   int i;                                                               /*0010*/

//  T(n) {}

   int n;                                                               /*0011*/
   {

//  T(i) {i==0, n<=0}

      // BEGIN BLOCK

//  T(j) {j==0}

      int j = 0;                                                        /*0015*/

//  T(i,j) {i==0, j#init<=j, 1<=n}

      for (i = 0;i<n;i++) {                                             /*0016*/

//  T(i,j) {i==i#init-1, j==j#init+1}

         // BEGIN BLOCK

//  T(j) {j==j#init+1}

         j++;                                                           /*0017*/

//  T(i) {i==i#init-1}

         i--;                                                           /*0018*/
         // END BLOCK
      }
      // END BLOCK
   }

//  T() {}


   foo(i);                                                              /*0022*/
   // END BLOCK
}

Source code with preconditions


//  P() {}

void complete05()
{

//  P() {}

   // BEGIN BLOCK

//  P() {}

   int i;                                                               /*0010*/

//  P(i) {}

   int n;                                                               /*0011*/
   {

//  P(i,n) {}

      // BEGIN BLOCK

//  P(i,n) {}

      int j = 0;                                                        /*0015*/

//  P(i,j,n) {j==0}

      for (i = 0;i<n;i++) {                                             /*0016*/

//  P(i,j,n) {i==0, 0<=j, 1<=n}

         // BEGIN BLOCK

//  P(i,j,n) {i==0, 0<=j, 1<=n}

         j++;                                                           /*0017*/

//  P(i,j,n) {i==0, 1<=j, 1<=n}

         i--;                                                           /*0018*/
         // END BLOCK
      }
      // END BLOCK
   }

//  P(i,n) {i==0, n<=0}


   foo(i);                                                              /*0022*/
   // END BLOCK
}

