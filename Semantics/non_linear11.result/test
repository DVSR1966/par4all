
C  T() {}

      PROGRAM NON_LINEAR11

C  T(I1) {1<=I1}


C     Check the linearization of integer products

C     Both subexpressions are affine and equal

      I1 = (N+1)*(N+1)+1                                                0001

C  T(I1) {1<=I1#init}

      READ *, I1                                                        0002

C  T(I2) {0<=I2}


C     Both expressions are equal, i2 >=0

      I2 = N*M*N*M                                                      0003

C  T(I2) {0<=I2#init}

      READ *, I2                                                        0004

C  T(L1,L2) {2L1==L2, 1<=L1, L1<=10}


C     For intervals, there 9 different sign cases

C     ++++

      IF (X.GT.0) THEN                                                  0005

C  T(L1) {L1==1}

         L1 = 1                                                         0006

C  T(L2) {L1==1, L2==2}

         L2 = 2                                                         0007
      ELSE

C  T(L1) {L1==10}

         L1 = 10                                                        0008

C  T(L2) {L1==10, L2==20}

         L2 = 20                                                        0009
      ENDIF

C  T(K1) {2L1==L2, 2<=K1, K1<=200, 1<=L1, L1<=10}


      K1 = L1*L2                                                        0010

C  T(K1) {2L1==L2, 2<=K1#init, K1#init<=200, 1<=L1, L1<=10}

      READ *, K1                                                        0011

C  T(L1,L2) {22L1==9L2+40, 2L1#init==L2#init, 1<=L1#init, L1#init<=10,
C    0<=L2+2, L2<=20}


C     ++-+

      IF (X.GT.0) THEN                                                  0012

C  T(L1) {L1==1, 2L1#init==L2, 1<=L1#init, L1#init<=10}

         L1 = 1                                                         0013

C  T(L2) {L1==1, L2==-2, 2<=L2#init, L2#init<=20}

         L2 = -2                                                        0014
      ELSE

C  T(L1) {L1==10, 2L1#init==L2, 1<=L1#init, L1#init<=10}

         L1 = 10                                                        0015

C  T(L2) {L1==10, L2==20, 2<=L2#init, L2#init<=20}

         L2 = 20                                                        0016
      ENDIF

C  T(K2) {22L1==9L2+40, 0<=K2+20, K2<=200, 0<=L2+2, L2<=20}


      K2 = L1*L2                                                        0017

C  T(K2) {22L1==9L2+40, 0<=K2#init+20, K2#init<=200, 0<=L2+2, L2<=20}

      READ *, K2                                                        0018

C  T(L1,L2) {18L1==11L2-40, 22L1#init==9L2#init+40, 2<=L2, L2<=20,
C    0<=L2#init+2, L2#init<=20}


C     -+++

      IF (X.GT.0) THEN                                                  0019

C  T(L1) {L1==-1, 22L1#init==9L2+40, 0<=L2+2, L2<=20}

         L1 = -1                                                        0020

C  T(L2) {L1==-1, L2==2, 0<=L2#init+2, L2#init<=20}

         L2 = 2                                                         0021
      ELSE

C  T(L1) {L1==10, 22L1#init==9L2+40, 0<=L2+2, L2<=20}

         L1 = 10                                                        0022

C  T(L2) {L1==10, L2==20, 0<=L2#init+2, L2#init<=20}

         L2 = 20                                                        0023
      ENDIF

C  T(K3) {18L1==11L2-40, 0<=K3+20, K3<=200, 2<=L2, L2<=20}


      K3 = L1*L2                                                        0024

C  T(K3) {18L1==11L2-40, 0<=K3#init+20, K3#init<=200, 2<=L2, L2<=20}

      READ *, K3                                                        0025

C  T(L1,L2) {22L1==9L2+202, 18L1#init==11L2#init-40, 0<=L2+20, L2<=2,
C    2<=L2#init, L2#init<=20}


C     ++--

      IF (X.GT.0) THEN                                                  0026

C  T(L1) {L1==1, 18L1#init==11L2-40, 2<=L2, L2<=20}

         L1 = 1                                                         0027

C  T(L2) {L1==1, L2==-20, 2<=L2#init, L2#init<=20}

         L2 = -20                                                       0028
      ELSE

C  T(L1) {L1==10, 18L1#init==11L2-40, 2<=L2, L2<=20}

         L1 = 10                                                        0029

C  T(L2) {L1==10, L2==2, 2<=L2#init, L2#init<=20}

         L2 = 2                                                         0030
      ENDIF

C  T(K4) {22L1==9L2+202, 0<=K4+200, K4<=20, 0<=L2+20, L2<=2}


      K4 = L1*L2                                                        0031

C  T(K4) {22L1==9L2+202, 0<=K4#init+200, K4#init<=20, 0<=L2+20, L2<=2}

      READ *, K4                                                        0032

C  T(L1,L2) {2L1==L2-22, 22L1#init==9L2#init+202, 0<=L1+10, L1+1<=0,
C    0<=L2#init+20, L2#init<=2}


C     --++

      IF (X.GT.0) THEN                                                  0033

C  T(L1) {L1==-10, 22L1#init==9L2+202, 0<=L2+20, L2<=2}

         L1 = -10                                                       0034

C  T(L2) {L1==-10, L2==2, 0<=L2#init+20, L2#init<=2}

         L2 = 2                                                         0035
      ELSE

C  T(L1) {L1==-1, 22L1#init==9L2+202, 0<=L2+20, L2<=2}

         L1 = -1                                                        0036

C  T(L2) {L1==-1, L2==20, 0<=L2#init+20, L2#init<=2}

         L2 = 20                                                        0037
      ENDIF

C  T(K5) {2L1==L2-22, 0<=K5+200, K5+2<=0, 0<=L1+10, L1+1<=0}


      K5 = L1*L2                                                        0038

C  T(K5) {2L1==L2-22, 0<=K5#init+200, K5#init+2<=0, 0<=L1+10, L1+1<=0}

      READ *, K5                                                        0039

C  T(L1,L2) {18L1==11L2+202, 2L1#init==L2#init-22, 0<=L1#init+10,
C    L1#init+1<=0, 0<=L2+20, L2+2<=0}


C     -+--

      IF (X.GT.0) THEN                                                  0040

C  T(L1) {L1==-1, 2L1#init==L2-22, 0<=L1#init+10, L1#init+1<=0}

         L1 = -1                                                        0041

C  T(L2) {L1==-1, L2==-20, 2<=L2#init, L2#init<=20}

         L2 = -20                                                       0042
      ELSE

C  T(L1) {L1==10, 2L1#init==L2-22, 0<=L1#init+10, L1#init+1<=0}

         L1 = 10                                                        0043

C  T(L2) {L1==10, L2==-2, 2<=L2#init, L2#init<=20}

         L2 = -2                                                        0044
      ENDIF

C  T(K6) {18L1==11L2+202, 0<=K6+200, K6<=20, 0<=L2+20, L2+2<=0}


      K6 = L1*L2                                                        0045

C  T(K6) {18L1==11L2+202, 0<=K6#init+200, K6#init<=20, 0<=L2+20,
C    L2+2<=0}

      READ *, K6                                                        0046

C  T(L1,L2) {22L1==9L2-202, 18L1#init==11L2#init+202, 0<=L2+2, L2<=20,
C    0<=L2#init+20, L2#init+2<=0}


C     ---+

      IF (X.GT.0) THEN                                                  0047

C  T(L1) {L1==-10, 18L1#init==11L2+202, 0<=L2+20, L2+2<=0}

         L1 = -10                                                       0048

C  T(L2) {L1==-10, L2==-2, 0<=L2#init+20, L2#init+2<=0}

         L2 = -2                                                        0049
      ELSE

C  T(L1) {L1==-1, 18L1#init==11L2+202, 0<=L2+20, L2+2<=0}

         L1 = -1                                                        0050

C  T(L2) {L1==-1, L2==20, 0<=L2#init+20, L2#init+2<=0}

         L2 = 20                                                        0051
      ENDIF

C  T(K7) {22L1==9L2-202, 0<=K7+200, K7<=20, 0<=L2+2, L2<=20}


      K7 = L1*L2                                                        0052

C  T(K7) {22L1==9L2-202, 0<=K7#init+200, K7#init<=20, 0<=L2+2, L2<=20}

      READ *, K7                                                        0053

C  T(L1,L2) {2L1==L2, 22L1#init==9L2#init-202, 0<=L1+10, L1+1<=0,
C    0<=L2#init+2, L2#init<=20}


C     ----

      IF (X.GT.0) THEN                                                  0054

C  T(L1) {L1==-10, 22L1#init==9L2-202, 0<=L2+2, L2<=20}

         L1 = -10                                                       0055

C  T(L2) {L1==-10, L2==-20, 0<=L2#init+2, L2#init<=20}

         L2 = -20                                                       0056
      ELSE

C  T(L1) {L1==-1, 22L1#init==9L2-202, 0<=L2+2, L2<=20}

         L1 = -1                                                        0057

C  T(L2) {L1==-1, L2==-2, 0<=L2#init+2, L2#init<=20}

         L2 = -2                                                        0058
      ENDIF

C  T(K8) {2L1==L2, 2<=K8, K8<=200, 0<=L1+10, L1+1<=0}


      K8 = L1*L2                                                        0059

C  T(K8) {2L1==L2, 2<=K8#init, K8#init<=200, 0<=L1+10, L1+1<=0}

      READ *, K8                                                        0060

C  T(L1,L2) {2L1==L2, 2L1#init==L2#init, 0<=L1+1, L1<=10,
C    0<=L1#init+10, L1#init+1<=0}


C     -+-+ (this case further splits down according to magnitudes)

      IF (X.GT.0) THEN                                                  0061

C  T(L1) {L1==-1, 2L1#init==L2, 0<=L1#init+10, L1#init+1<=0}

         L1 = -1                                                        0062

C  T(L2) {L1==-1, L2==-2, 0<=L2#init+20, L2#init+2<=0}

         L2 = -2                                                        0063
      ELSE

C  T(L1) {L1==10, 2L1#init==L2, 0<=L1#init+10, L1#init+1<=0}

         L1 = 10                                                        0064

C  T(L2) {L1==10, L2==20, 0<=L2#init+20, L2#init+2<=0}

         L2 = 20                                                        0065
      ENDIF

C  T(K9) {2L1==L2, 0<=K9+20, K9<=200, 0<=L1+1, L1<=10}


      K9 = L1*L2                                                        0066

C  T(K9) {2L1==L2, 0<=K9#init+20, K9#init<=200, 0<=L1+1, L1<=10}

      READ *, K9                                                        0067

C  T(L1,L2) {2L1==L2, 2L1#init==L2#init, 0<=L1+1000000, L1<=10000000,
C    0<=L1#init+1, L1#init<=10}


C     -+-+ Same as above, but with overflows

      IF (X.GT.0) THEN                                                  0068

C  T(L1) {L1==-1000000, 2L1#init==L2, 0<=L1#init+1, L1#init<=10}

         L1 = -1000000                                                  0069

C  T(L2) {L1==-1000000, L2==-2000000, 0<=L2#init+2, L2#init<=20}

         L2 = -2000000                                                  0070
      ELSE

C  T(L1) {L1==10000000, 2L1#init==L2, 0<=L1#init+1, L1#init<=10}

         L1 = 10000000                                                  0071

C  T(L2) {L1==10000000, L2==20000000, 0<=L2#init+2, L2#init<=20}

         L2 = 20000000                                                  0072
      ENDIF

C  T(K10) {2L1==L2, 0<=L1+1000000, L1<=10000000}


      K10 = L1*L2                                                       0073

C  T(K10) {2L1==L2, 0<=L1+1000000, L1<=10000000}

      READ *, K10                                                       0074

      END

C  P() {}

      PROGRAM NON_LINEAR11

C  P() {}


C     Check the linearization of integer products

C     Both subexpressions are affine and equal

      I1 = (N+1)*(N+1)+1                                                0001

C  P(I1) {1<=I1}

      READ *, I1                                                        0002

C  P() {}


C     Both expressions are equal, i2 >=0

      I2 = N*M*N*M                                                      0003

C  P(I2) {0<=I2}

      READ *, I2                                                        0004

C  P() {}


C     For intervals, there 9 different sign cases

C     ++++

      IF (X.GT.0) THEN                                                  0005

C  P() {}

         L1 = 1                                                         0006

C  P() {}

         L2 = 2                                                         0007
      ELSE

C  P() {}

         L1 = 10                                                        0008

C  P() {}

         L2 = 20                                                        0009
      ENDIF

C  P(L1,L2) {2L1==L2, 1<=L1, L1<=10}


      K1 = L1*L2                                                        0010

C  P(K1) {2<=K1, K1<=200}

      READ *, K1                                                        0011

C  P(L1,L2) {2L1==L2, 1<=L1, L1<=10}


C     ++-+

      IF (X.GT.0) THEN                                                  0012

C  P(L1) {2L1==L2, 1<=L1, L1<=10}

         L1 = 1                                                         0013

C  P(L2) {2<=L2, L2<=20}

         L2 = -2                                                        0014
      ELSE

C  P(L1) {2L1==L2, 1<=L1, L1<=10}

         L1 = 10                                                        0015

C  P(L2) {2<=L2, L2<=20}

         L2 = 20                                                        0016
      ENDIF

C  P(L1,L2) {22L1==9L2+40, 1<=L1, L1<=10}


      K2 = L1*L2                                                        0017

C  P(K2) {0<=K2+20, K2<=200}

      READ *, K2                                                        0018

C  P(L1,L2) {22L1==9L2+40, 1<=L1, L1<=10, 0<=L2+2, L2<=20}


C     -+++

      IF (X.GT.0) THEN                                                  0019

C  P(L1) {22L1==9L2+40, 0<=L2+2, L2<=20}

         L1 = -1                                                        0020

C  P(L2) {0<=L2+2, L2<=20}

         L2 = 2                                                         0021
      ELSE

C  P(L1) {22L1==9L2+40, 0<=L2+2, L2<=20}

         L1 = 10                                                        0022

C  P(L2) {0<=L2+2, L2<=20}

         L2 = 20                                                        0023
      ENDIF

C  P(L1,L2) {18L1==11L2-40, 0<=L1+1, L1<=10}


      K3 = L1*L2                                                        0024

C  P(K3) {0<=K3+20, K3<=200}

      READ *, K3                                                        0025

C  P(L1,L2) {18L1==11L2-40, 0<=L1+1, L1<=10, 2<=L2, L2<=20}


C     ++--

      IF (X.GT.0) THEN                                                  0026

C  P(L1) {18L1==11L2-40, 2<=L2, L2<=20}

         L1 = 1                                                         0027

C  P(L2) {2<=L2, L2<=20}

         L2 = -20                                                       0028
      ELSE

C  P(L1) {18L1==11L2-40, 2<=L2, L2<=20}

         L1 = 10                                                        0029

C  P(L2) {2<=L2, L2<=20}

         L2 = 2                                                         0030
      ENDIF

C  P(L1,L2) {22L1==9L2+202, 1<=L1, L1<=10}


      K4 = L1*L2                                                        0031

C  P(K4) {0<=K4+200, K4<=20}

      READ *, K4                                                        0032

C  P(L1,L2) {22L1==9L2+202, 1<=L1, L1<=10, 0<=L2+20, L2<=2}


C     --++

      IF (X.GT.0) THEN                                                  0033

C  P(L1) {22L1==9L2+202, 0<=L2+20, L2<=2}

         L1 = -10                                                       0034

C  P(L2) {0<=L2+20, L2<=2}

         L2 = 2                                                         0035
      ELSE

C  P(L1) {22L1==9L2+202, 0<=L2+20, L2<=2}

         L1 = -1                                                        0036

C  P(L2) {0<=L2+20, L2<=2}

         L2 = 20                                                        0037
      ENDIF

C  P(L1,L2) {2L1==L2-22, 0<=L1+10, L1+1<=0}


      K5 = L1*L2                                                        0038

C  P(K5) {0<=K5+200, K5+2<=0}

      READ *, K5                                                        0039

C  P(L1,L2) {2L1==L2-22, 0<=L1+10, L1+1<=0}


C     -+--

      IF (X.GT.0) THEN                                                  0040

C  P(L1) {2L1==L2-22, 0<=L1+10, L1+1<=0}

         L1 = -1                                                        0041

C  P(L2) {2<=L2, L2<=20}

         L2 = -20                                                       0042
      ELSE

C  P(L1) {2L1==L2-22, 0<=L1+10, L1+1<=0}

         L1 = 10                                                        0043

C  P(L2) {2<=L2, L2<=20}

         L2 = -2                                                        0044
      ENDIF

C  P(L1,L2) {18L1==11L2+202, 0<=L1+1, L1<=10}


      K6 = L1*L2                                                        0045

C  P(K6) {0<=K6+200, K6<=20}

      READ *, K6                                                        0046

C  P(L1,L2) {18L1==11L2+202, 0<=L1+1, L1<=10, 0<=L2+20, L2+2<=0}


C     ---+

      IF (X.GT.0) THEN                                                  0047

C  P(L1) {18L1==11L2+202, 0<=L2+20, L2+2<=0}

         L1 = -10                                                       0048

C  P(L2) {0<=L2+20, L2+2<=0}

         L2 = -2                                                        0049
      ELSE

C  P(L1) {18L1==11L2+202, 0<=L2+20, L2+2<=0}

         L1 = -1                                                        0050

C  P(L2) {0<=L2+20, L2+2<=0}

         L2 = 20                                                        0051
      ENDIF

C  P(L1,L2) {22L1==9L2-202, 0<=L1+10, L1+1<=0}


      K7 = L1*L2                                                        0052

C  P(K7) {0<=K7+200, K7<=20}

      READ *, K7                                                        0053

C  P(L1,L2) {22L1==9L2-202, 0<=L1+10, L1+1<=0, 0<=L2+2, L2<=20}


C     ----

      IF (X.GT.0) THEN                                                  0054

C  P(L1) {22L1==9L2-202, 0<=L2+2, L2<=20}

         L1 = -10                                                       0055

C  P(L2) {0<=L2+2, L2<=20}

         L2 = -20                                                       0056
      ELSE

C  P(L1) {22L1==9L2-202, 0<=L2+2, L2<=20}

         L1 = -1                                                        0057

C  P(L2) {0<=L2+2, L2<=20}

         L2 = -2                                                        0058
      ENDIF

C  P(L1,L2) {2L1==L2, 0<=L1+10, L1+1<=0}


      K8 = L1*L2                                                        0059

C  P(K8) {2<=K8, K8<=200}

      READ *, K8                                                        0060

C  P(L1,L2) {2L1==L2, 0<=L1+10, L1+1<=0}


C     -+-+ (this case further splits down according to magnitudes)

      IF (X.GT.0) THEN                                                  0061

C  P(L1) {2L1==L2, 0<=L1+10, L1+1<=0}

         L1 = -1                                                        0062

C  P(L2) {0<=L2+20, L2+2<=0}

         L2 = -2                                                        0063
      ELSE

C  P(L1) {2L1==L2, 0<=L1+10, L1+1<=0}

         L1 = 10                                                        0064

C  P(L2) {0<=L2+20, L2+2<=0}

         L2 = 20                                                        0065
      ENDIF

C  P(L1,L2) {2L1==L2, 0<=L1+1, L1<=10}


      K9 = L1*L2                                                        0066

C  P(K9) {0<=K9+20, K9<=200}

      READ *, K9                                                        0067

C  P(L1,L2) {2L1==L2, 0<=L1+1, L1<=10}


C     -+-+ Same as above, but with overflows

      IF (X.GT.0) THEN                                                  0068

C  P(L1) {2L1==L2, 0<=L1+1, L1<=10}

         L1 = -1000000                                                  0069

C  P(L2) {0<=L2+2, L2<=20}

         L2 = -2000000                                                  0070
      ELSE

C  P(L1) {2L1==L2, 0<=L1+1, L1<=10}

         L1 = 10000000                                                  0071

C  P(L2) {0<=L2+2, L2<=20}

         L2 = 20000000                                                  0072
      ENDIF

C  P(L1,L2) {2L1==L2, 0<=L1+1000000, L1<=10000000}


      K10 = L1*L2                                                       0073

C  P(K10) {}

      READ *, K10                                                       0074

      END

