      PROGRAM UNSTRUC03
C (0,1)
C     BEGIN BLOCK
C (0,2)

C     Check that postconditions are properly propagated forward in
C     unstructured

      READ *, I                                                         0001
C (0,3)
C     BEGIN UNSTRUCTURED
C (1,1)

      IF (I.LE.5) GOTO 100                                              0002
C (4,1)
      J = 6                                                             0004
      GOTO 200
C (2,1)
C     BEGIN BLOCK
C (2,2)
100   J = 7                                                             0005
C (2,3)
      I = 4                                                             0006
C     END BLOCK
C (3,1)
200   CONTINUE                                                          0007
C     END UNSTRUCTURED
C (0,4)

      PRINT *, I, J                                                     0008
C (0,5)

      RETURN
C     END BLOCK
      END

C  P() {}

      PROGRAM UNSTRUC03

C  P() {}

C (0,1)
C     BEGIN BLOCK

C  P() {}

C (0,2)

C     Check that postconditions are properly propagated forward in
C     unstructured

      READ *, I                                                         0001

C  P(I) {}

C (0,3)
C     BEGIN UNSTRUCTURED

C  P(I) {}

C (1,1)

      IF (I.LE.5) GOTO 100                                              0002

C  P(I) {6<=I}

C (4,1)
      J = 6                                                             0004
      GOTO 200

C  P(I) {I<=5}

C (2,1)
C     BEGIN BLOCK

C  P(I) {I<=5}

C (2,2)
100   J = 7                                                             0005

C  P(I,J) {J==7, I<=5}

C (2,3)
      I = 4                                                             0006
C     END BLOCK

C  P(I,J) {18<=I+2J, 6<=J, J<=7}

C (3,1)
200   CONTINUE                                                          0007
C     END UNSTRUCTURED

C  P(I,J) {18<=I+2J, 6<=J, J<=7}

C (0,4)

      PRINT *, I, J                                                     0008

C  P(I,J) {18<=I+2J, 6<=J, J<=7}

C (0,5)

      RETURN
C     END BLOCK
      END

