#!/bin/sh
#
# Script to test pips on the simple program
#
# Use of the rule transformers_intra_full
#
echo "PRETTYPRINT_BLOCK_IF_ONLY TRUE" >properties.rc
if Pips -f $PIPS_ROOT/Validation/Semantics/andne.f -w andne -m andne \
	-s transformers_intra_full -p prcp >&2
then
	exit_code=0
	echo "--- Preconditions for module ANDNE"
	Display -l prec
else
	exit_code=1
fi
Delete andne >&2
rm properties.rc
exit $exit_code
