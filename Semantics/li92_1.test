#!/bin/sh
#
# Script to test pips on the li92_1 program
#
# Use of the rule transformers_inter_full
#
if Pips -f $PIPS_ROOT/Validation/Semantics/li92_1.f -w li92_1 -m li921 \
	-s pcinter -s tfinterfull -p prcp >&2
then
	exit_code=0
	echo "--- Preconditions for module LI921"
	Display -l prec
else
	exit_code=1
fi
Delete li92_1 >&2
exit $exit_code
