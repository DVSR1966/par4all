#!/bin/sh
#
# Script to test pips on the simple program
#
# Use of the rule transformers_inter_fast
#
echo "PRETTYPRINT_BLOCK_IF_ONLY TRUE" >properties.rc
if Pips -f $PIPS_ROOT/Validation/Semantics/negand2.f -w negand2 -m negand2 \
	-s pcinter -s tfinter -p prcp >&2
then
	exit_code=0
	echo "--- Preconditions for module NEGAND2"
	Display -l prec
else
	exit_code=1
fi
Delete negand2 >&2
rm properties.rc
exit $exit_code
