
Proper effects

void decl04(mystruct s)
{
//               <must be read   >: s[.champ1] s[.champ2]
//               <must be written>: s_loc[.champ1] s_loc[.champ2]
   mystruct s_loc = s;                                                  /*0006*/
//               <must be read   >: s_loc[.champ1]
//               <must be written>: s_loc[.champ1]

   s_loc.champ1 = s_loc.champ1-1;                                       /*0008*/
}

Cumulated effects 

//               <must be read   >: s[.champ1] s[.champ2]
void decl04(mystruct s)
{
//               <must be read   >: s[.champ1] s[.champ2]
//               <must be written>: s_loc[.champ1] s_loc[.champ2]
   mystruct s_loc = s;                                                  /*0006*/
//               <must be read   >: s_loc[.champ1]
//               <must be written>: s_loc[.champ1]

   s_loc.champ1 = s_loc.champ1-1;                                       /*0008*/
}
