
Proper effects for "foo"

int foo(float (*b)[3])
{
   float c;
//               <must be read   >: b
//               <must be written>: b[0][3]
   (*b)[3] = 2.0;                                                       /*0005*/
//               <must be read   >: b b[0][3]
//               <must be written>: c
   c = (*b)[3];                                                         /*0006*/
//               <must be read   >: b
//               <must be written>: b[1][3]
   b[1][3] = 2.0;                                                       /*0007*/
//               <must be read   >: b b[1][3]
//               <must be written>: c
   c = b[1][3];                                                         /*0008*/
//               <must be read   >: b b[0][3]
//               <must be written>: b[0][3]
   (*b)[3]++;                                                           /*0010*/
//               <must be read   >: b b[0][3]
//               <must be written>: b[0][3]
   (*b)[3] += 5.0;                                                      /*0011*/
//               <must be read   >: b b[1][3]
//               <must be written>: b[1][3]
   b[1][3]++;                                                           /*0012*/
//               <must be read   >: b b[1][3]
//               <must be written>: b[1][3]
   b[1][3] += 5.0;                                                      /*0013*/
   return 1;                                                            /*0015*/
}

Proper effects for "foo2"

int foo2(float b[5][3])
{
   float c;
//               <must be read   >: b
//               <must be written>: b[0][3]
   (*b)[3] = 2.0;                                                       /*0028*/
//               <must be read   >: b b[0][3]
//               <must be written>: c
   c = (*b)[3];                                                         /*0029*/
//               <must be read   >: b
//               <must be written>: b[1][3]
   b[1][3] = 2.0;                                                       /*0030*/
//               <must be read   >: b b[1][3]
//               <must be written>: c
   c = b[1][3];                                                         /*0031*/
//               <must be read   >: b b[0][3]
//               <must be written>: b[0][3]
   (*b)[3]++;                                                           /*0033*/
//               <must be read   >: b b[0][3]
//               <must be written>: b[0][3]
   (*b)[3] += 5.0;                                                      /*0034*/
//               <must be read   >: b b[1][3]
//               <must be written>: b[1][3]
   b[1][3]++;                                                           /*0035*/
//               <must be read   >: b b[1][3]
//               <must be written>: b[1][3]
   b[1][3] += 5.0;                                                      /*0036*/

   return 1;                                                            /*0038*/
}

Proper effects for "foo3"

int foo3()
{
   float c;
//               <must be written>: TOP-LEVEL:d[0][3]
   (*d)[3] = 2.0;                                                       /*0044*/
//               <must be read   >: TOP-LEVEL:d[0][3]
//               <must be written>: c
   c = (*d)[3];                                                         /*0045*/
//               <must be written>: TOP-LEVEL:d[1][3]
   d[1][3] = 2.0;                                                       /*0046*/
//               <must be read   >: TOP-LEVEL:d[1][3]
//               <must be written>: c
   c = d[1][3];                                                         /*0047*/
//               <must be read   >: TOP-LEVEL:d[0][3]
//               <must be written>: TOP-LEVEL:d[0][3]
   (*d)[3]++;                                                           /*0049*/
//               <must be read   >: TOP-LEVEL:d[0][3]
//               <must be written>: TOP-LEVEL:d[0][3]
   (*d)[3] += 5.0;                                                      /*0050*/
//               <must be read   >: TOP-LEVEL:d[1][3]
//               <must be written>: TOP-LEVEL:d[1][3]
   d[1][3]++;                                                           /*0051*/
//               <must be read   >: TOP-LEVEL:d[1][3]
//               <must be written>: TOP-LEVEL:d[1][3]
   d[1][3] += 5.0;                                                      /*0052*/

   return 1;                                                            /*0054*/
}

Proper regions for "foo"

int foo(float (*b)[3])
{
   float c;

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3] = 2.0;                                                       /*0005*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b-R-EXACT-{}>
//  <c-W-EXACT-{}>

   c = (*b)[3];                                                         /*0006*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3] = 2.0;                                                       /*0007*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <c-W-EXACT-{}>

   c = b[1][3];                                                         /*0008*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3]++;                                                           /*0010*/

//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3] += 5.0;                                                      /*0011*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3]++;                                                           /*0012*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3] += 5.0;                                                      /*0013*/
   return 1;                                                            /*0015*/
}

Proper regions for "foo2"

int foo2(float b[5][3])
{
   float c;

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3] = 2.0;                                                       /*0028*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <c-W-EXACT-{}>

   c = (*b)[3];                                                         /*0029*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3] = 2.0;                                                       /*0030*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <c-W-EXACT-{}>

   c = b[1][3];                                                         /*0031*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3]++;                                                           /*0033*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*b)[3] += 5.0;                                                      /*0034*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3]++;                                                           /*0035*/

//  <b-R-EXACT-{}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <b[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   b[1][3] += 5.0;                                                      /*0036*/

   return 1;                                                            /*0038*/
}

Proper regions for "foo3"

int foo3()
{
   float c;

//  <TOP-LEVEL:d[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*d)[3] = 2.0;                                                       /*0044*/

//  <c-W-EXACT-{}>
//  <TOP-LEVEL:d[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>

   c = (*d)[3];                                                         /*0045*/

//  <TOP-LEVEL:d[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   d[1][3] = 2.0;                                                       /*0046*/

//  <c-W-EXACT-{}>
//  <TOP-LEVEL:d[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>

   c = d[1][3];                                                         /*0047*/

//  <TOP-LEVEL:d[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <TOP-LEVEL:d[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*d)[3]++;                                                           /*0049*/

//  <TOP-LEVEL:d[PHI1][PHI2]-R-EXACT-{PHI1==0, PHI2==3}>
//  <TOP-LEVEL:d[PHI1][PHI2]-W-EXACT-{PHI1==0, PHI2==3}>

   (*d)[3] += 5.0;                                                      /*0050*/

//  <TOP-LEVEL:d[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <TOP-LEVEL:d[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   d[1][3]++;                                                           /*0051*/

//  <TOP-LEVEL:d[PHI1][PHI2]-R-EXACT-{PHI1==1, PHI2==3}>
//  <TOP-LEVEL:d[PHI1][PHI2]-W-EXACT-{PHI1==1, PHI2==3}>

   d[1][3] += 5.0;                                                      /*0052*/

   return 1;                                                            /*0054*/
} 
