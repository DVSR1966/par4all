void dereferencing02()
{
   double x = 1.;
   double *p = &x;
   double **q = &p;
   int i = 1;
//               <may be written >: TOP-LEVEL:*MEMORY*
//               <must be read   >: q
   

   **q = 2.;
//               <may be read    >: q[*]
//               <must be read   >: q
//               <must be written>: i
//               <may be written - pre >: *(q[*])
   **(q+(i = 2)) = 3.;
//               <may be read    >: TOP-LEVEL:*MEMORY*
//               <must be read   >: q
//               <must be written>: x
   x = **q;
//               <must be read   >: q
//               <must be written>: q
   q++;
//               <must be read   >: q[0] q
//               <must be written>: q
   *q++;
}
