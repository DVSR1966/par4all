
Parsed printed file for "dereferencing02"

void dereferencing02()
{
   double x = 1.;
   double *p = &x;
   double **q = &p;
   int i = 1;
   

   **q = 2.;
   **(q+(i = 2)) = 3.;
   x = **q;
   q++;
   *q++;
}

Proper effetcs for "dereferencing02"

void dereferencing02()
{
   double x = 1.;
   double *p = &x;
   double **q = &p;
   int i = 1;
//               <may be written >: TOP-LEVEL:*MEMORY*
//               <must be read   >: q
   

   **q = 2.;
//               <must be read   >: q q[i = 2]
//               <must be written>: i
//               <must be written - pre>: *(q[i = 2])
   **(q+(i = 2)) = 3.;
//               <may be read    >: TOP-LEVEL:*MEMORY*
//               <must be read   >: q
//               <must be written>: x
   x = **q;
//               <must be read   >: q
//               <must be written>: q
   q++;
//               <must be read   >: q[0] q
//               <must be written>: q
   *q++;
}

