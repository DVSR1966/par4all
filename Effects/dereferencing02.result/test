void dereferencing02()
{
   double x = 1.;
   double *p = &x;
   double **q = &p;
   int i = 1;
//               <must be read   >: q
//               <must be written>: q[0][0]
   

   **q = 2.;
//               <must be read   >: q
//               <must be written>: q[2][0]
   **(q+(i = 2)) = 3.;
//               <must be read   >: q q[0][0]
//               <must be written>: x
   x = **q;
//               <must be read   >: q
//               <must be written>: q
   q++;
//               <must be read   >: q q[0]
//               <must be written>: q
   *q++;
}

