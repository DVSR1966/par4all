
Proper effects

int main()
{
   int tab[3] = {0, 1, 2};                                              /*0006*/
//               <must be read   >: tab[0]
//               <must be written>: tab[0]

   tab[0] = tab[0]+2;                                                   /*0008*/
   return 0;                                                            /*0009*/
}

Cumulated effects 

int main()
{
   int tab[3] = {0, 1, 2};                                              /*0006*/
//               <must be read   >: tab[0]
//               <must be written>: tab[0]

   tab[0] = tab[0]+2;                                                   /*0008*/
   return 0;                                                            /*0009*/
}
