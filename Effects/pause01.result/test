      PROGRAM PAUSE01
C               <must be read   >: __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)

C     Check effects for PAUSE so that it is not removed as dead code!

      PRINT *, "I'm going to pause!"                                    0001
C               <must be read   >: __IO_EFFECTS:LUNS(0)
C               <must be written>: __IO_EFFECTS:LUNS(0)
      PAUSE                                                             0002
C               <must be read   >: __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)
      PRINT *, "I'm done!"                                              0003

      END

PAUSE must not be use-def eliminated

      PROGRAM PAUSE01

C     Check effects for PAUSE so that it is not removed as dead code!

      PRINT *, "I'm going to pause!"                                    0001
      PAUSE                                                             0002
      PRINT *, "I'm done!"                                              0003

      END

