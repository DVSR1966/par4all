C       Uninitialized scalar variables
	PROGRAM SCALAR03
	INTEGER X,Y
C               <must be exported>: X
      CALL SUB(X, Y)                                                    0001
      PRINT *, X, Y                                                     0002
      END
C               <may be exported >: X
        SUBROUTINE SUB(X,Y)
	INTEGER X,Y
C               <must be exported>: X
      X = Y                                                             0001
      END
