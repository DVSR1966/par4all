void call10(int i, int x[10], int j, int y[10])
{
   int k;
   int l;
//               <must be read   >: i
//               <must be written>: k

   for(k = 0; k <= i-1; k += 1)
//               <must be read   >: i
//               <must be written>: x[i]
      x[i] = 0;
//               <must be read   >: j
//               <must be written>: l
   for(l = 0; l <= j-1; l += 1)
//               <must be read   >: i j
//               <must be written>: y[i]
      y[i] = 0;
}

