! out simple effects...
      PROGRAM OUT1
      INTEGER I, K
C               <may be exported >: __IO_EFFECTS:LUNS(*)
C               <must be exported>: I
      READ *, I, K                                                      0001
C               <may be exported >: __IO_EFFECTS:LUNS(*)
      PRINT *, I                                                        0002
C               <must be exported>: K
      K = 3                                                             0003
      PRINT *, K                                                        0004
      END
