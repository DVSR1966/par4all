s_t make_s(int i, float x)
{
   s_t *p;
   extern char *malloc(int);
//               <must be written>: p

   p = (s_t *) malloc(sizeof(s_t));
//               <must be read   >: i p
//               <must be written - post>: (*p)[1]
   p->entier = i;
//               <must be read   >: p x
//               <must be written - post>: (*p)[2]
   p->reel = x;
//               <must be read   >: p
//               <must be read    - post>: (*p)
   return *p;
}

