C               <may be imported >: __IO_EFFECTS:LUNS(*)
C     Bug with IN regions, there is no IN region for NDIM

      PROGRAM IOEFFECT

      COMMON/PAR1/NDIM,LSIZE(4)
C               <may be imported >: __IO_EFFECTS:LUNS(*)

      READ (5, *) NDIM, (LSIZE(N), N = 1, NDIM)                         0001

      END

