
Parsed printed file for "struct05"

int struct05()
{
   struct two {
      int first;
      int second;
   } x[10];
   int i = 0;

   for (i = 0;i<10;i++)
      if (1)
         x[i].first = x[i].second;
      else
         x[i].second = x[i].first;
}

Proper effetcs for "struct05"

int struct05()
{
   struct two {
      int first;
      int second;
   } x[10];
//               <must be written>: i
   int i = 0;
//               <must be written>: i

   for(i = 0; i <= 9; i += 1)
      if (1)
//               <must be read   >: i x[i][2]
//               <must be written>: x[i][1]
         x[i].first = x[i].second;
      else
//               <must be read   >: i x[i][1]
//               <must be written>: x[i][2]
         x[i].second = x[i].first;
}

Cumulated effetcs for "struct05"

int struct05()
{
   struct two {
      int first;
      int second;
   } x[10];
//               <must be written>: i
   int i = 0;
//               <may be read    >: i x[*][1] x[*][2]
//               <may be written >: x[*][1] x[*][2]
//               <must be written>: i

   for(i = 0; i <= 9; i += 1)
//               <may be read    >: x[*][1] x[*][2]
//               <may be written >: x[*][1] x[*][2]
//               <must be read   >: i
      if (1)
//               <may be read    >: x[*][2]
//               <may be written >: x[*][1]
//               <must be read   >: i
         x[i].first = x[i].second;
      else
//               <may be read    >: x[*][1]
//               <may be written >: x[*][2]
//               <must be read   >: i
         x[i].second = x[i].first;
}

Transformers for "main"


//  T() {}

int struct05()
{

//  T() {}

   struct two {
      int first;
      int second;
   } x[10];

//  T(i) {i==0}

   int i = 0;

//  T(i) {i#init==0, 0<=i}


   for(i = 0; i <= 9; i += 1)

//  T() {0<=i, i<=9}

      if (1)

//  T() {0<=i, i<=9}

         x[i].first = x[i].second;
      else

//  T() {0==-1}

         x[i].second = x[i].first;
}

Preconditions for "main"


//  P() {}

int struct05()
{

//  P() {}

   struct two {
      int first;
      int second;
   } x[10];

//  P() {}

   int i = 0;

//  P() {i==0}


   for(i = 0; i <= 9; i += 1)

//  P(i) {0<=i, i<=9}

      if (1)

//  P(i) {0<=i, i<=9}

         x[i].first = x[i].second;
      else

//  P() {0==-1}

         x[i].second = x[i].first;
}

Printed files with regions

int struct05()
{
   struct two {
      int first;
      int second;
   } x[10];
   int i = 0;

//  <x[PHI1][PHI2]-R-EXACT-{PHI2==2, 0<=PHI1, PHI1<=9}>
//  <x[PHI1][PHI2]-W-EXACT-{PHI2==1, 0<=PHI1, PHI1<=9}>


   for(i = 0; i <= 9; i += 1)

//  <x[PHI1][PHI2]-R-EXACT-{PHI1==i, PHI2==2, 0<=i, i<=9}>
//  <x[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==1, 0<=i, i<=9}>

      if (1)

//  <x[PHI1][PHI2]-R-EXACT-{PHI1==i, PHI2==2, 0<=i, i<=9}>
//  <x[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==1, 0<=i, i<=9}>

         x[i].first = x[i].second;
      else
         x[i].second = x[i].first;
}

Printed files after parallelization

int struct05()
{
   struct two {
      int first;
      int second;
   } x[10];
   int i = 0;

//  <x[PHI1][PHI2]-R-EXACT-{PHI2==2, 0<=PHI1, PHI1<=9}>
//  <x[PHI1][PHI2]-W-EXACT-{PHI2==1, 0<=PHI1, PHI1<=9}>


#pragma omp parallel for 
   for(i = 0; i <= 9; i += 1)

//  <x[PHI1][PHI2]-R-EXACT-{PHI1==i, PHI2==2, 0<=i, i<=9}>
//  <x[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==1, 0<=i, i<=9}>

      if (1)

//  <x[PHI1][PHI2]-R-EXACT-{PHI1==i, PHI2==2, 0<=i, i<=9}>
//  <x[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==1, 0<=i, i<=9}>

         x[i].first = x[i].second;
      else
         x[i].second = x[i].first;
}
