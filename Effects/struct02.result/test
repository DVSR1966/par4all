
Parsed printed file for "struct02"

int struct02()
{
   struct two {
      int first;
      int second;
   };
   struct two x[10];
   int i = 0;

   for (i = 0;i<10;i++)
      if (1) {
         x[i].first = 1;
         x[i].second = 0;
      }
}

Proper effetcs for "struct02"

int struct02()
{
   struct two {
      int first;
      int second;
   };
   struct two x[10];
   int i = 0;
//               <must be written>: i

   for(i = 0; i <= 9; i += 1)
      if (1) {
//               <must be read   >: i
//               <must be written>: x[i][1]
         x[i].first = 1;
//               <must be read   >: i
//               <must be written>: x[i][2]
         x[i].second = 0;
      }
}

Cumulated effetcs for "struct02"

int struct02()
{
   struct two {
      int first;
      int second;
   };
   struct two x[10];
   int i = 0;
//               <may be read    >: i
//               <may be written >: x[*][*]
//               <must be written>: i

   for(i = 0; i <= 9; i += 1)
//               <may be read    >: i
//               <may be written >: x[*][*]
      if (1) {
//               <may be written >: x[*][*]
//               <must be read   >: i
         x[i].first = 1;
//               <may be written >: x[*][*]
//               <must be read   >: i
         x[i].second = 0;
      }
}

Transformers for "main"


Printed files with regions

int struct02()
{
   struct two {
      int first;
      int second;
   };
   struct two x[10];
   int i = 0;

//  <x[PHI1][PHI2]-W-MAY-{1<=PHI2, PHI2<=2}>


   for(i = 0; i <= 9; i += 1)

//  <x[PHI1][PHI2]-W-MAY-{PHI1==i, 1<=PHI2, PHI2<=2}>

      if (1) {

//  <x[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==1}>

         x[i].first = 1;

//  <x[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==2}>

         x[i].second = 0;
      }
}
