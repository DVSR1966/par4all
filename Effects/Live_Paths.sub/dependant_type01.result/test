}

OUT Regions for "main"

int main()
{
   int i;
   int size = 10;
   int a[size];

   for(i = 0; i <= 9; i += 1)
      a[i] = i;
}

PROPER_EFFECTS

int main()
{
//             <    is declared>: i
   int i;
//             <    is written>: size
//             <    is referenced>: size
//             <    is declared>: size
   int size = 10;
//             <    is read   >: size
//             <    is referenced>: size
//             <    is declared>: a
   int a[size];
//             <    is written>: i
//             <    is referenced>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: a[i]
//             <    is referenced>: a i
      a[i] = i;
}

CUMULATED_EFFECTS

int main()
{
//             <    is declared>: i
   int i;
//             <    is written>: size
//             <    is referenced>: size
//             <    is declared>: size
   int size = 10;
//             <    is read   >: size
//             <    is referenced>: size
//             <    is declared>: a
   int a[size];
//             <may be written>: a[*]
//             <    is read   >: i
//             <    is written>: i
//             <    is referenced>: a i

   for(i = 0; i <= 9; i += 1)
//             <may be written>: a[*]
//             <    is read   >: i
//             <    is referenced>: a i
      a[i] = i;
}

IN_EFFECTS

int main()
{
   int i;
   int size = 10;
//             <    is imported>: size
   int a[size];

   for(i = 0; i <= 9; i += 1)
//             <    is imported>: i
      a[i] = i;
}

OUT_EFFECTS

int main()
{
   int i;
   int size = 10;
   int a[size];

   for(i = 0; i <= 9; i += 1)
      a[i] = i;
}

LIVE_IN_PATHS

int main()
{
   int i;
   int size = 10;
//             <    is alive (in)>: size
   int a[size];

   for(i = 0; i <= 9; i += 1)
//             <    is alive (in)>: i
      a[i] = i;
}

LIVE_OUT_PATHS

int main()
{
   int i;
//             <    is alive (out)>: size
   int size = 10;
   int a[size];

   for(i = 0; i <= 9; i += 1)
//             <    is alive (out)>: i
      a[i] = i;
}

