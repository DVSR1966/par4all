
Init File

int main()
{
   int i;
   int size = 10;
   int a[size];

   for(i = 0; i <= 9; i += 1)
      a[i] = i;
}

Regions for "main"

int main()
{

//  <i-WE-EXACT-{}>

   int i;

//  <size-RE-EXACT-{}>
//  <size-W-EXACT-{}>
//  <size-WE-EXACT-{}>

   int size = 10;

//  <a-WE-EXACT-{}>
//  <size-RE-EXACT-{}>
//  <size-R-EXACT-{}>

   int a[size];

//  <a-RE-EXACT-{}>
//  <a[PHI1]-W-MAY-{0<=PHI1, PHI1<=9, size==10}>
//  <i-RE-EXACT-{}>
//  <i-R-EXACT-{}>
//  <i-W-EXACT-{}>


   for(i = 0; i <= 9; i += 1)

//  <a-RE-EXACT-{}>
//  <a[PHI1]-W-EXACT-{PHI1==i, size==10, 0<=i, i<=9}>
//  <i-RE-EXACT-{}>
//  <i-R-EXACT-{}>

      a[i] = i;
}

INV Regions for "main"

int main()
{
   int i;
   int size = 10;
   int a[size];

   for(i = 0; i <= 9; i += 1)

//  <a-RE-EXACT-{}>
//  <a[PHI1]-W-EXACT-{PHI1==i, size==10, 0<=i, i<=9}>
//  <i-RE-EXACT-{}>
//  <i-R-EXACT-{}>

      a[i] = i;
}

IN Regions for "main"

int main()
{
   int i;
   int size = 10;

//  <size-IN-EXACT-{}>

   int a[size];

   for(i = 0; i <= 9; i += 1)

//  <i-IN-EXACT-{}>

