int main()
{
   char *s = "sortie de puts";
   puts(s);
}
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
int main()
{
   char *s = "sortie de puts";
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
//               <must be read   >: s
   puts(s);
}
