int main()
{
   char *s = "sortie de puts";
   puts(s);
}
//               <may be read    >: TOP-LEVEL:*MEMORY*
//               <must be read   >: __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
int main()
{
   char *s = "sortie de puts";
//               <may be read    >: s[*]
//               <must be read   >: __IO_EFFECTS:LUNS[1] s
//               <must be written>: __IO_EFFECTS:LUNS[1]
   puts(s);
}
