
Proper effects for "foo"

void foo(int *p)
{
//               <must be read   >: p
//               <must be written>: p[0]
   
   /* Some versions of <linux/posix_types.h> define these macros.  */
   
   
   
   /* It's easier to assume 8-bit bytes than to get CHAR_BIT.  */
   
   
   
   
   /* fd_set for select and pselect.  */
   
   
   /* XPG4.2 requires this member name.  Otherwise avoid the name
          from the global namespace.  */
   
   
   
   
   *p = 4;                                                              /*0075*/
   return;                                                              /*0005*/
}

Proper effects for "main"

int main()
{
   int *x;                                                              /*0019*/
   int tab[10];                                                         /*0020*/
   int *tab2[10];                                                       /*0021*/
   int tab3[10][10];                                                    /*0022*/
   int **tab4;                                                          /*0023*/
   int y;                                                               /*0024*/
//               <must be read   >: x
//               <must be written>: x[0]
   
   
   foo(x);                                                              /*0027*/
//               <must be written>: tab[0]
   foo(tab);                                                            /*0028*/
//               <must be read   >: tab2[4]
//               <must be written>: tab2[4][0]
   foo(tab2[4]);                                                        /*0029*/
//               <must be written>: tab3[5][0]
   foo(tab3[5]);                                                        /*0030*/
//               <must be read   >: tab4 tab4[6]
//               <must be written>: tab4[6][0]
   foo(tab4[6]);                                                        /*0031*/
//               <must be written>: y
   foo(&y);                                                             /*0032*/
//               <must be written>: tab[1]
   foo(&tab[1]);                                                        /*0033*/
//               <must be read   >: __IO_EFFECTS:LUNS[1] x x[0]
//               <must be written>: __IO_EFFECTS:LUNS[1]

   printf("%d\n", *x);                                                  /*0035*/
//               <must be read   >: __IO_EFFECTS:LUNS[1] tab[0]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab[0]);                                              /*0036*/
//               <must be read   >: __IO_EFFECTS:LUNS[1] tab2[4] 
//                                  tab2[4][0]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab2[4][0]);                                          /*0037*/
//               <must be read   >: __IO_EFFECTS:LUNS[1] tab3[5][0]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab3[5][0]);                                          /*0038*/
//               <must be read   >: __IO_EFFECTS:LUNS[1] tab4 tab4[6] 
//                                  tab4[6][0]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab4[6][0]);                                          /*0039*/
//               <must be read   >: __IO_EFFECTS:LUNS[1] y
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", y);                                                   /*0040*/
//               <must be read   >: __IO_EFFECTS:LUNS[1] tab[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab[1]);                                              /*0041*/
   return 1;                                                            /*0042*/
}

Regions for "foo"


//  <p-R-EXACT-{}>
//  <p[PHI1]-W-EXACT-{PHI1==0}>

void foo(int *p)
{

//  <p-R-EXACT-{}>
//  <p[PHI1]-W-EXACT-{PHI1==0}>

   
   /* Some versions of <linux/posix_types.h> define these macros.  */
   
   
   
   /* It's easier to assume 8-bit bytes than to get CHAR_BIT.  */
   
   
   
   
   /* fd_set for select and pselect.  */
   
   
   /* XPG4.2 requires this member name.  Otherwise avoid the name
          from the global namespace.  */
   
   
   
   
   *p = 4;                                                              /*0075*/
   return;                                                              /*0005*/
}

Regions for "main"


//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>

int main()
{
   int *x;                                                              /*0019*/
   int tab[10];                                                         /*0020*/
   int *tab2[10];                                                       /*0021*/
   int tab3[10][10];                                                    /*0022*/
   int **tab4;                                                          /*0023*/
   int y;                                                               /*0024*/

//  <x-R-EXACT-{}>
//  <x[PHI1]-W-EXACT-{PHI1==0}>

   
   
   foo(x);                                                              /*0027*/

//  <tab[PHI1]-W-EXACT-{PHI1==0}>

   foo(tab);                                                            /*0028*/

//  <tab2[PHI1]-R-EXACT-{PHI1==4}>
//  <tab2[PHI1][PHI2]-W-EXACT-{PHI1==4, PHI2==0}>

   foo(tab2[4]);                                                        /*0029*/

//  <tab3[PHI1][PHI2]-W-EXACT-{PHI1==5, PHI2==0}>

   foo(tab3[5]);                                                        /*0030*/

//  <tab4-R-EXACT-{}>
//  <tab4[PHI1]-R-EXACT-{PHI1==6}>
//  <tab4[PHI1][PHI2]-W-EXACT-{PHI1==6, PHI2==0}>

   foo(tab4[6]);                                                        /*0031*/

//  <y-W-EXACT-{}>

   foo(&y);                                                             /*0032*/

//  <tab[PHI1]-W-EXACT-{PHI1==1}>

   foo(&tab[1]);                                                        /*0033*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>
//  <x[PHI1]-R-EXACT-{PHI1==0}>
//  <x-R-EXACT-{}>


   printf("%d\n", *x);                                                  /*0035*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>
//  <tab[PHI1]-R-EXACT-{PHI1==0}>

   printf("%d\n", tab[0]);                                              /*0036*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>
//  <tab2[PHI1]-R-EXACT-{PHI1==4}>
//  <tab2[PHI1][PHI2]-R-EXACT-{PHI1==4, PHI2==0}>

   printf("%d\n", tab2[4][0]);                                          /*0037*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>
//  <tab3[PHI1][PHI2]-R-EXACT-{PHI1==5, PHI2==0}>

   printf("%d\n", tab3[5][0]);                                          /*0038*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>
//  <tab4-R-EXACT-{}>
//  <tab4[PHI1]-R-EXACT-{PHI1==6}>
//  <tab4[PHI1][PHI2]-R-EXACT-{PHI1==6, PHI2==0}>

   printf("%d\n", tab4[6][0]);                                          /*0039*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>
//  <y-R-EXACT-{}>

   printf("%d\n", y);                                                   /*0040*/

//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>
//  <tab[PHI1]-R-EXACT-{PHI1==1}>

   printf("%d\n", tab[1]);                                              /*0041*/
   return 1;                                                            /*0042*/
}

out regions for "main"

int main()
{
   int *x;                                                              /*0019*/
   int tab[10];                                                         /*0020*/
   int *tab2[10];                                                       /*0021*/
   int tab3[10][10];                                                    /*0022*/
   int **tab4;                                                          /*0023*/
   int y;                                                               /*0024*/

//  <x[PHI1]-OUT-EXACT-{PHI1==0}>

   
   
   foo(x);                                                              /*0027*/

//  <tab[PHI1]-OUT-EXACT-{PHI1==0}>

   foo(tab);                                                            /*0028*/

//  <tab2[PHI1][PHI2]-OUT-EXACT-{PHI1==4, PHI2==0}>

   foo(tab2[4]);                                                        /*0029*/

//  <tab3[PHI1][PHI2]-OUT-EXACT-{PHI1==5, PHI2==0}>

   foo(tab3[5]);                                                        /*0030*/

//  <tab4[PHI1][PHI2]-OUT-EXACT-{PHI1==6, PHI2==0}>

   foo(tab4[6]);                                                        /*0031*/

//  <y-OUT-EXACT-{}>

   foo(&y);                                                             /*0032*/

//  <tab[PHI1]-OUT-EXACT-{PHI1==1}>

   foo(&tab[1]);                                                        /*0033*/

//  <__IO_EFFECTS:LUNS[PHI1]-OUT-EXACT-{PHI1==1}>


   printf("%d\n", *x);                                                  /*0035*/

//  <__IO_EFFECTS:LUNS[PHI1]-OUT-EXACT-{PHI1==1}>

   printf("%d\n", tab[0]);                                              /*0036*/

//  <__IO_EFFECTS:LUNS[PHI1]-OUT-EXACT-{PHI1==1}>

   printf("%d\n", tab2[4][0]);                                          /*0037*/

//  <__IO_EFFECTS:LUNS[PHI1]-OUT-EXACT-{PHI1==1}>

   printf("%d\n", tab3[5][0]);                                          /*0038*/

//  <__IO_EFFECTS:LUNS[PHI1]-OUT-EXACT-{PHI1==1}>

   printf("%d\n", tab4[6][0]);                                          /*0039*/

//  <__IO_EFFECTS:LUNS[PHI1]-OUT-EXACT-{PHI1==1}>

   printf("%d\n", y);                                                   /*0040*/
   printf("%d\n", tab[1]);                                              /*0041*/
   return 1;                                                            /*0042*/
}

