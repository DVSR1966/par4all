      PROGRAM PROG
      REAL A(100), B(2:5,6:8)
C               <must be written>: A(1)
      CALL TOTO(A)                                                      0001
C               <must be written>: B(2,6)
      CALL TOTO(B)                                                      0002
C               <must be written>: B(3,7)
      CALL TOTO(B(3,7))                                                 0003
      END
