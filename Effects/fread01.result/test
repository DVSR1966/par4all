int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
   fp = fopen("file.txt", "rb");
   n = fread(buf, 8, 50, fp);
   if (n==-1) 
      printf("fread failed");
   fclose(fp);
}
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
int main()
{
   FILE *fp;
   size_t n;
   char buf[200];
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
//               <must be written>: fp
   fp = fopen("file.txt", "rb");
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*] buf[*]
//               <must be read   >: fp
//               <must be written>: fp n
   n = fread(buf, 8, 50, fp);
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
//               <must be read   >: n
   if (n==-1) 
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
      printf("fread failed");
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
//               <must be read   >: fp
//               <must be written>: fp
   fclose(fp);
}
