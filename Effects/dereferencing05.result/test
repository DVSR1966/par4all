void dereferencing05()
{
   double *p;
   int i;
//               <must be read   >: p
//               <must be written>: p[0] p

   *p++ = 0.;
//               <must be read   >: p
//               <must be written>: p[0] p
   *p-- = 0.;
//               <must be read   >: p
//               <must be written>: p[1] p
   *++p = 0.;
//               <must be read   >: p
//               <must be written>: p[-1] p
   *--p = 0.;
}

