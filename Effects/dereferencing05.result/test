void dereferencing05()
{
   double *p;
   int i;
//               <must be read   >: p
//               <must be written>: p p[0]

   *p++ = 0.;
//               <must be read   >: p
//               <must be written>: p p[0]
   *p-- = 0.;
//               <must be read   >: p
//               <must be written>: p p[1]
   *++p = 0.;
//               <must be read   >: p
//               <must be written>: p p[-1]
   *--p = 0.;
}

