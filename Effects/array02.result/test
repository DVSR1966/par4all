Printed files with regions

void array02()
{
   float x[11][2];
   int i;

//  <x[PHI1][PHI2]-W-EXACT-{PHI2==1, 1<=PHI1, PHI1<=9}>


   for(i = 1; i <= 9; i += 1)

//  <x[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==1, 1<=i, i<=9}>

      x[i][1] = 0.;
}

Printed files after parallelization

void array02()
{
   float x[11][2];
   int i;

   forall(i = 1; i <= 9; i += 1)
      x[i][1] = 0.;
}
