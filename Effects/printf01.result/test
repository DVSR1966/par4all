int main()
{
   int i = 5;
   printf("i=%d", i);
}
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
int main()
{
   int i = 5;
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
//               <must be read   >: i
   printf("i=%d", i);
}
