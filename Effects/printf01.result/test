int main()
{
   int i = 5;
   printf("i=%d", i);
}
//               <must be read   >: __IO_EFFECTS:LUNS[1]
//               <must be written>: __IO_EFFECTS:LUNS[1]
int main()
{
//               <must be written>: i
   int i = 5;
//               <must be read   >: __IO_EFFECTS:LUNS[1] i
//               <must be written>: __IO_EFFECTS:LUNS[1]
   printf("i=%d", i);
}
