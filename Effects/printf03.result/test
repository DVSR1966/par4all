int main()
{
   int i = 5;
   int j = 6;
   float x;
   double y[10];
   char *fmt = "i=%d, %d, %f\n";

   y[0] = 0.;
   printf(fmt, i, i+j, x);
}
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
int main()
{
   int i = 5;
   int j = 6;
   float x;
   double y[10];
   char *fmt = "i=%d, %d, %f\n";
//               <may be written >: y[*]

   y[0] = 0.;
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
//               <must be read   >: fmt i j x
   printf(fmt, i, i+j, x);
}
