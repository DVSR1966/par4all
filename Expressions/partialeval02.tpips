setproperty ABORT_ON_USER_ERROR TRUE
setenv WS="partialeval02"
delete $WS
create $WS $WS.c

echo input code
display PRINTED_FILE(%ALLFUNC)

echo partially evaled code without normalization
apply PARTIAL_EVAL(foo)
display PRINTED_FILE(foo)

echo partially evaled code with normalization
setproperty PARTIAL_EVAL_LINEARIZE TRUE
apply PARTIAL_EVAL(bar)
display PRINTED_FILE(bar)

close
delete $WS

