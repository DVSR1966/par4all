
//  T(main) {main==0}

int main()
{

//  T(i) {i==0}

   int i = 0, *a = malloc(10*sizeof(int));

//  T(main) {i==0}

   if (!a)

//  T(main) {i==0, main==1}

      return 1;

//  T(i) {i#init==0, 0<=i}

   for(i = 0; i <= 4; i += 2) {

//  T(i) {0<=i#init, i#init<=4}

      *(a+i) = 3;

//  T(i) {}

      *(a+2*i+1) = 4;
   }

//  T() {5<=i, i<=6}

   printf("%d", *(a+2));

//  T(main) {main==0, 5<=i, i<=6}

   return 0;
}
int main()
{
   int i = 0, *a = malloc(10*sizeof(int));
   //PIPS generated variable
   int *a0, *a1;
   if (!a)
      return 1;
   a1 = a;
   a0 = a;
   for(i = 0; i <= 4; i += 2) {
      *a0 = 3;
      *(a1+1) = 4;
      a1 += 4;
      a0 += 2;
   }
   a = a1;
   a = a0;
   printf("%d", *(a+2));
   return 0;
}
int main()
{
   int i = 0, *a = malloc(10*sizeof(int));
   //PIPS generated variable
   int *a0, *a1;
   if (!a)
      return 1;
   a1 = a;
   a0 = a;
   for(i = 0; i <= 4; i += 2) {
      *a0 = 3;
      *(a1+1) = 4;
      a1 += 4;
      a0 += 2;
   }
   a = a0;
   printf("%d", *(a+2));
   return 0;
}

