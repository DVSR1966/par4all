
//  T(main) {main==0}

int main()
{

//  T(i) {i==0}

   int i = 0, *a = malloc(10*sizeof(int));

//  T(main) {i==0}

   if (!a)

//  T(main) {i==0, main==1}

      return 1;

//  T(i) {i#init==0, 0<=i}

   for(i = 0; i <= 9; i += 2) {

//  T(i) {0<=i#init, i#init<=9}

      *(a+i) = 3;

//  T(i) {}

      *(a+i+1) = 4;
   }

//  T() {10<=i, i<=11}

   printf("%d", *(a+2));

//  T(main) {main==0, 10<=i, i<=11}

   return 0;
}
int main()
{
   int i = 0, *a = malloc(10*sizeof(int));
   //PIPS generated variable
   int *a0;
   if (!a)
      return 1;
   a0 = a;
   for(i = 0; i <= 9; i += 2) {
      *a0 = 3;
      *(a0+1) = 4;
      a0 += 2;
   }
   printf("%d", *(a+2));
   return 0;
}
int main()
{
   int i = 0, *a = malloc(10*sizeof(int));
   //PIPS generated variable
   int *a0;
   if (!a)
      return 1;
   a0 = a;
   for(i = 0; i <= 9; i += 2) {
      *a0 = 3;
      *(a0+1) = 4;
      a0 += 2;
   }
   printf("%d", *(a+2));
   return 0;
}

