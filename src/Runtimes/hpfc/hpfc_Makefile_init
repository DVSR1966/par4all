MAKEFILE = Makefile
HPFC_MAKEFILE = hpfc_Makefile
INCLUDED = hpfc_Makefile.include
MOVE = mv
M4 = $(HPFC_M4)

.INIT:	$(INCLUDED)

all:

$(INCLUDED): $(HPFC_MAKEFILE)
	$(M4) -D ARCHITECTURE=$(PVM_ARCH) hpfc_architecture_m4_macros \
		$(HPFC_MAKEFILE) > $(INCLUDED)

#

include $(INCLUDED)

#
# that is all
#
