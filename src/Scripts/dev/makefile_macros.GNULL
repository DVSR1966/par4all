# $Date: 1996/07/15 15:45:25 $, 
#
# this file contains macro definitions 

# the GNU libc is *required* because "%lld" is not supported on sunos4
GLIBC	= /usr5/gnu/SPARC/glibc

AR	= gar
ARFLAGS	= rv
CC	= gcc
CFLAGS	= -O2 -g -Wall -mv8 -pipe
CPPFLAGS= -DLINEAR_VALUE_IS_LONGLONG -D__USE_FIXED_PROTOTYPES__ -nostdinc -I$(GLIBC)/include -I$(NEWGEN_INCDIR) -I$(LINEAR_INCDIR) -I$(PIPS_INCDIR) -I$(PIPS_EXTEDIR)
LD	= $(CC) 
LDFLAGS	= -g -nostdlib $(GLIBC)/lib/crt0.o -L$(GLIBC)/lib -L./$(ARCH) -L$(PIPS_LIBDIR)/$(ARCH) -L$(NEWGEN_LIBDIR)/$(ARCH) -L$(LINEAR_LIBDIR)/$(ARCH) -L$(PIPS_EXTEDIR)/$(ARCH) -lc -lgcc
RANLIB	= granlib
LEX	= flex
LFLAGS	= -l
FC	= g77
FFLAGS	= -O2 -g -Wimplicit -pipe
LINT	= lint
LINTFLAGS= -habxu
YACC	= bison
YFLAGS	= -y
PROTOIZE= cproto -vcf2
ETAGS	= etags
TAR	= gtar
ZIP	= gzip -v9
DIFF	= diff
M4	= gm4

# end of $RCSfile: makefile_macros.GNULL,v $
#
