#!/bin/sh
# 
# $RCSfile: checked-out,v $ (version $Revision$)
# $Date: 1995/08/08 17:11:24 $, 

USERNAME=$1
# echo $USERNAME

if [ -z "$USERNAME" ]
then
	find $PIPS_DEVEDIR -name 'p.*' -ls 
	find $LINEARDIR/Development -name 'p.*' -ls
else
	find $PIPS_DEVEDIR -name 'p.*' -ls | grep "$USERNAME"
	find $LINEARDIR/Development -name 'p.*' -ls | grep "$USERNAME"
fi
