# this file contains macro definitions 

AR	= gar
ARFLAGS	= rv
CC	= acc
# -Xc removed because of bison generated files:-(
CFLAGS	= -fast -g -Xa
CPPFLAGS= -I$(NEWGEN_INCLUDEDIR) -I$(LINEAR_INCLUDEDIR) -I$(PIPS_INCLUDEDIR) -I$(PIPS_EXTEDIR)
LD	= $(CC) -bsdmalloc
LDFLAGS	= -fast -g -L./$(ARCH) -L$(PIPS_LIBDIR)/$(ARCH) -L$(NEWGEN_LIBDIR)/$(ARCH) -L$(LINEAR_LIBDIR)/$(ARCH) -L$(PIPS_EXTEDIR)/$(ARCH) -L$(PIPS_EXTEDIR)
RANLIB	= granlib
LEX	= lex
LFLAGS	=
FC	= f77
FFLAGS	= -O -g -U -u -C 
LINT	= lint
LINTFLAGS= -habxu
YACC	= yacc
YFLAGS	=
PROTO	= cproto
PRFLAGS	= -evcf2
ETAGS	= etags
TAR	= gtar
ZIP	= gzip -v9
DIFF	= diff
M4	= gm4

# end of $RCSfile: makefile_macros.SUN4,v $
